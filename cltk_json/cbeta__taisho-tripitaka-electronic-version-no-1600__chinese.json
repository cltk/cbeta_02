002\u91cb\u96e3\u6545\u3002\u81ea\u6027\u6545\u8005\u3002\u8b02\u4e09\u81ea\u6027\u3002", "224": "\u5373\u5713\u6210\u5be6\u904d\u8a08\u6240\u57f7\u53ca\u4f9d\u4ed6\u8d77\u3002\u662f\u521d\u4e09\u53e5\u5982", "225": "\u6b21\u61c9\u77e5\u3002\u6240\u7de3\u6545\u8005\u5373\u4e09\u81ea\u6027\u3002\u7121\u5206\u5225\u6545\u8005\u3002\u8b02", "226": "\u7531\u6b64\u7121\u5206\u5225\u5373\u7121\u5206\u5225\u667a\u3002\u53ca\u65bc\u6b64\u7121\u5206\u5225\u5373", "227": "\u672c\u6027\u6e05\u6de8\u3002\u5982\u6b21\u61c9\u77e5\u5b89\u7acb\u5883\u667a\u3002\u8b02\u4e09\u81ea\u6027\u53ca", "228": "\u7121\u5206\u5225\u3002\u91cb\u96e3\u6545\u8005\u3002\u8b02\u6240\u9918\u53e5\u3002\u4e14\u6709\u96e3\u8a00\u3002\u904d\u8a08", "229": "\u6240\u57f7\u4f9d\u4ed6\u8d77\u76f8\u3002\u82e5\u5be6\u662f\u7121\u4e91\u4f55\u53ef\u5f97\u3002\u82e5\u5be6\u662f", "230": "\u6709\u4e0d\u61c9\u8af8\u6cd5\u672c\u6027\u6e05\u6de8\u3002\u70ba\u91cb\u6b64\u96e3\u8aaa\u5e7b\u7b49\u55bb\u3002", "231": "\u5982\u5e7b\u4e8b\u7b49\u96d6\u5be6\u662f\u7121\u800c\u73fe\u53ef\u5f97\u3002\u5fa9\u6709\u96e3\u8a00\u3002\u82e5", "232": "\u4e00\u5207\u6cd5\u672c\u6027\u6e05\u6de8\u3002\u5982\u4f55\u5f97\u6709\u5148\u67d3\u5f8c\u6de8\u3002\u70ba\u91cb", "233": "\u6b64\u96e3\u8aaa\u6709\u67d3\u6de8\u53ca\u865b\u7a7a\u55bb\u3002\u8b02\u5982\u865b\u7a7a\u96d6\u672c\u6027", "234": "\u6de8\u3002\u800c\u6709\u96dc\u67d3\u53ca\u6e05\u6de8\u6642\u3002\u5fa9\u6709\u96e3\u8a00\u3002\u6709\u7121\u91cf\u4f5b", "235": "\u51fa\u73fe\u65bc\u4e16\u3002\u4e00\u4e00\u80fd\u5ea6\u7121\u91cf\u6709\u60c5\u3002\u4ee4\u51fa\u751f\u6b7b", "236": "\u5165\u65bc\u6d85\u69c3\u3002\u4e91\u4f55\u751f\u6b7b\u7121\u65b7\u6ec5\u5931\u3002\u6d85\u69c3\u754c\u4e2d\u7121", "237": "\u589e\u76ca\u904e\u3002\u70ba\u91cb\u6b64\u96e3\u8aaa\u67d3\u53ca\u6de8\u7121\u6e1b\u7121\u589e\u3002\u53c8\u6709", "238": "\u60c5\u754c\u53ca\u6e05\u6de8\u54c1\u4ff1\u7121\u91cf\u6545\u3002\u7b2c\u4e8c\u5b89\u7acb\u5f7c\u81ea\u6027", "239": "\u8005\u3002\u5982\u6709\u980c\u8a00\u3002", "240": "\u4e82\u5883\u81ea\u6027\u56e0\u7121\u4e82\u81ea\u6027\u5883", "241": "\u4e82\u7121\u4e82\u4e8c\u679c\u53ca\u5f7c\u4e8c\u908a\u969b", "242": "\u5982\u662f\u5df2\u8aaa\u96a8\u6cd5\u6b63\u884c\u3002\u96e2\u4e8c\u908a\u6b63\u884c\u4e91\u4f55\u61c9\u77e5\u3002", "243": "\u5982\u5bf6\u7a4d\u7d93\u6240\u8aaa\u4e2d\u9053\u884c\u3002\u6b64\u884c\u9060\u96e2\u4f55\u7b49\u4e8c\u908a\u3002", "244": "\u980c\u66f0\u3002", "245": "\u7570\u6027\u8207\u4e00\u6027\u5916\u9053\u53ca\u8072\u805e", "246": "\u589e\u76ca\u640d\u6e1b\u908a\u6709\u60c5\u6cd5\u5404\u4e8c", "247": "\u6240\u6cbb\u53ca\u80fd\u6cbb\u5e38\u4f4f\u8207\u65b7\u6ec5", "248": "\u6240\u53d6\u80fd\u53d6\u908a\u67d3\u6de8\u4e8c\u4e09\u7a2e", "249": "\u5206\u5225\u4e8c\u908a\u6027\u61c9\u77e5\u5fa9\u6709\u4e03", "250": "\u8b02\u6709\u975e\u6709\u908a\u6240\u80fd\u5bc2\u6016\u754f", "251": "\u6240\u80fd\u53d6\u6b63\u90aa\u6709\u7528\u5e76\u7121\u7528", "252": "\u4e0d\u8d77\u53ca\u6642\u7b49\u662f\u5206\u5225\u4e8c\u908a", "253": "\u8ad6\u66f0\u3002\u82e5\u65bc\u8272\u7b49\u57f7\u6211\u6709\u7570\u3002\u6216\u57f7\u662f\u4e00\u540d\u70ba\u4e00", "254": "\u908a\u3002\u70ba\u96e2\u6b64\u57f7\u8aaa\u4e2d\u9053\u884c\u3002\u8b02\u89c0\u7121\u6211\u4e43\u81f3\u5112\u7ae5\u3002", "255": "\u898b\u6709\u6211\u8005\u5b9a\u8d77\u6b64\u57f7\u3002\u6211\u7570\u65bc\u8eab\u6216\u5373\u8eab\u6545\u3002\u82e5", "256": "\u65bc\u8272\u7b49\u57f7\u70ba\u5e38\u4f4f\u662f\u5916\u9053\u908a\u3002\u57f7\u7121\u5e38\u8005\u662f\u8072", "257": "\u805e\u908a\u3002\u70ba\u96e2\u6b64\u57f7\u8aaa\u4e2d\u9053\u884c\u3002\u8b02\u89c0\u8272\u7b49\u975e\u5e38\u7121", "258": "\u5e38\u5b9a\u57f7\u6709\u6211\u3002\u662f\u589e\u76ca\u6709\u60c5\u908a\u3002\u5b9a\u57f7\u7121\u6211\u662f\u640d", "259": "\u6e1b\u6709\u60c5\u908a\u3002\u5f7c\u4ea6\u64a5\u7121\u5047\u6709\u60c5\u6545\u3002\u70ba\u96e2\u6b64\u57f7\u8aaa", "260": "\u4e2d\u9053\u884c\u3002\u8b02\u6211\u7121\u6211\u4e8c\u908a\u4e2d\u667a\u3002\u5b9a\u57f7\u5fc3\u6709\u5be6\u3002\u662f", "261": "\u589e\u76ca\u6cd5\u908a\u3002\u5b9a\u57f7\u5fc3\u7121\u5be6\u3002\u662f\u640d\u6e1b\u6cd5\u908a\u3002\u70ba\u96e2\u6b64", "262": "\u57f7\u8aaa\u4e2d\u9053\u884c\u3002\u8b02\u65bc\u662f\u8655\u7121\u5fc3\u7121\u601d\u7121\u610f\u7121\u8b58\u3002", "263": "\u57f7\u6709\u4e0d\u5584\u7b49\u8af8\u96dc\u67d3\u6cd5\u3002\u662f\u6240\u6cbb\u908a\u3002\u57f7\u6709\u5584\u7b49", "264": "\u8af8\u6e05\u6de8\u6cd5\u3002\u662f\u80fd\u6cbb\u908a\u3002\u70ba\u96e2\u6b64\u57f7\u8aaa\u4e2d\u9053\u884c\u3002\u8b02", "265": "\u65bc\u4e8c\u908a\u4e0d\u96a8\u89c0\u8aaa\u3002\u65bc\u6709\u60c5\u6cd5\u5b9a\u57f7\u70ba\u6709\u3002\u662f\u5e38", "266": "\u4f4f\u908a\u3002\u5b9a\u57f7\u975e\u6709\u662f\u65b7\u6ec5\u908a\u3002\u70ba\u96e2\u6b64\u57f7\u8aaa\u4e2d\u9053", "267": "\u884c\u3002\u8b02\u5373\u65bc\u6b64\u4e8c\u908a\u4e2d\u667a\u57f7\u6709\u7121\u660e\u6240\u53d6\u80fd\u53d6", "268": "\u5404\u70ba\u4e00\u908a\u3002\u82e5\u57f7\u6709\u660e\u6240\u53d6\u80fd\u53d6\u5404\u70ba\u4e00\u908a\u3002\u5982", "269": "\u662f\u57f7\u6709\u6240\u6cbb\u8af8\u884c\u80fd\u6cbb\u7121\u70ba\u3002\u4e43\u81f3\u8001\u6b7b\u53ca\u80fd", "270": "\u6ec5\u3002\u5f7c\u8af8\u5c0d\u6cbb\u9053\u6240\u53d6\u80fd\u53d6\u5404\u70ba\u4e00\u908a\u3002\u6b64\u6240\u80fd", "271": "\u6cbb\u6240\u53d6\u80fd\u53d6\u3002\u5373\u662f\u9ed1\u54c1\u767d\u54c1\u5dee\u5225\u3002\u70ba\u96e2\u6b64\u57f7", "272": "\u8aaa\u4e2d\u9053\u884c\u3002\u8b02\u660e\u8207\u7121\u660e\u7121\u4e8c\u7121\u4e8c\u5206\u3002\u4e43\u81f3\u5ee3", "273": "\u8aaa\u3002\u660e\u7121\u660e\u7b49\u6240\u53d6\u80fd\u53d6\u7686\u975e\u6709\u6545\u3002\u96dc\u67d3\u6709\u4e09\u3002", "274": "\u8b02\u7169\u60f1\u96dc\u67d3\u3002\u696d\u96dc\u67d3\u3002\u751f\u96dc\u67d3\u3002\u7169\u60f1\u96dc\u67d3\u5fa9\u6709", "275": "\u4e09\u7a2e\u3002\u4e00\u8af8\u898b\u3002\u4e8c\u8caa\u778b\u7661\u76f8\u3002\u4e09\u5f8c\u6709\u9858\u3002\u6b64\u80fd\u5c0d", "276": "\u6cbb\u3002\u8b02\u7a7a\u667a\u7121\u76f8\u667a\u7121\u9858\u667a\u3002\u696d\u96dc\u67d3\u3002\u8b02\u6240\u4f5c\u5584", "277": "\u60e1\u696d\u3002\u6b64\u80fd\u5c0d\u6cbb\u3002\u8b02\u4e0d\u4f5c\u667a\u3002\u751f\u96dc\u67d3\u6709\u4e09\u7a2e\u3002\u4e00", "278": "\u5f8c\u6709\u751f\u3002\u4e8c\u751f\u5df2\u5fc3\u5fc3\u6240\u5ff5\u5ff5\u8d77\u3002\u4e09\u5f8c\u6709\u76f8\u7e8c\u3002", "279": "\u6b64\u80fd\u5c0d\u6cbb\u3002\u8b02\u7121\u751f\u667a\u7121\u8d77\u667a\u7121\u81ea\u6027\u667a\u3002\u5982\u662f", "280": "\u4e09\u7a2e\u96dc\u67d3\u9664\u6ec5\u8aaa\u70ba\u6e05\u6de8\u3002\u7a7a\u7b49\u667a\u5883\u3002\u8b02\u7a7a\u7b49", "281": "\u6cd5\u4e09\u7a2e\u96dc\u67d3\u3002\u96a8\u5176\u6240\u61c9\u3002\u975e\u7a7a\u7b49\u667a\u4ee4\u4f5c\u7a7a\u7b49\u3002", "282": "\u7531\u5f7c\u672c\u6027\u662f\u7a7a\u6027\u7b49\u3002\u6cd5\u754c\u672c\u4f86\u6027\u7121\u67d3\u6545\u3002\u82e5", "283": "\u65bc\u6cd5\u754c\u6216\u57f7\u96dc\u67d3\u6216\u57f7\u6e05\u6de8\u5404\u70ba\u4e00\u908a\u3002\u672c\u6027", "284": "\u7121\u67d3\u975e\u67d3\u6de8\u6545\u3002\u70ba\u96e2\u6b64\u57f7\u8aaa\u4e2d\u9053\u884c\u3002\u8b02\u4e0d\u7531", "285": "\u7a7a\u80fd\u7a7a\u65bc\u6cd5\u3002\u6cd5\u6027\u81ea\u7a7a\u4e43\u81f3\u5ee3\u8aaa\u3002", "286": "\u5fa9\u6709\u4e03\u7a2e\u5206\u5225\u4e8c\u908a\u3002\u4f55\u7b49\u70ba\u4e03\u3002\u8b02\u5206\u5225\u6709\u5206", "287": "\u5225\u975e\u6709\u5404\u70ba\u4e00\u908a\u3002\u5f7c\u57f7\u5be6\u6709\u88dc\u7279\u4f3d\u7f85\u4ee5\u70ba", "288": "\u58de\u6ec5\u3002\u7acb\u7a7a\u6027\u6545\u3002\u6216\u65bc\u7121\u6211\u5206\u5225\u70ba\u7121\u70ba\u3002\u96e2\u5982", "289": "\u662f\u4e8c\u908a\u5206\u5225\u8aaa\u4e2d\u9053\u884c\u3002\u8b02\u4e0d\u70ba\u6ec5\u88dc\u7279\u4f3d\u7f85", "290": "\u65b9\u7acb\u7a7a\u6027\u3002\u7136\u5f7c\u7a7a\u6027\u672c\u6027\u81ea\u7a7a\u3002\u524d\u969b\u4ea6\u7a7a\u5f8c", "291": "\u969b\u4ea6\u7a7a\u4e2d\u969b\u4ea6\u7a7a\u3002\u4e43\u81f3\u5ee3\u8aaa\u3002\u5206\u5225\u6240\u5bc2\u5206\u5225", "292": "\u80fd\u5bc2\u5404\u70ba\u4e00\u908a\u3002\u57f7\u6709\u6240\u65b7\u53ca\u6709\u80fd\u65b7\u3002\u6016\u754f\u7a7a", "293": "\u6545\u3002\u70ba\u96e2\u5982\u662f\u4e8c\u908a\u5206\u5225\u3002\u8aaa\u865b\u7a7a\u55bb\u3002\u5206\u5225\u6240\u6016", "294": "\u5206\u5225\u5f9e\u5f7c\u6240\u751f\u53ef\u754f\u5404\u70ba\u4e00\u908a\u3002\u57f7\u6709\u904d\u8a08\u6240", "295": "\u57f7\u8272\u7b49\u3002\u53ef\u751f\u6016\u6545\u3002\u57f7\u6709\u5f9e\u5f7c\u6240\u751f\u82e6\u6cd5\u3002\u53ef\u751f", "296": "\u754f\u6545\u3002\u70ba\u96e2\u5982\u662f\u4e8c\u908a\u5206\u5225\u8aaa\u756b\u5e2b\u55bb\u3002\u524d\u865b\u7a7a", "297": "\u55bb\u70ba\u8072\u805e\u8aaa\u3002\u4eca\u756b\u5e2b\u55bb\u70ba\u83e9\u85a9\u8aaa\u3002\u5206\u5225\u6240\u53d6", "298": "\u5206\u5225\u80fd\u53d6\u5404\u70ba\u4e00\u908a\u3002\u70ba\u96e2\u5982\u662f\u4e8c\u908a\u5206\u5225\u8aaa\u5e7b", "299": "\u5e2b\u55bb\u3002\u7531\u552f\u8b58\u667a\u7121\u5883\u667a\u751f\u3002\u7531\u7121\u5883\u667a\u751f\u5fa9\u6368", "300": "\u552f\u8b58\u3002\u667a\u5883\u65e2\u975e\u6709\u8b58\u3002\u4ea6\u662f\u7121\u8981\u3002\u8a17\u6240\u7de3\u8b58\u65b9", "301": "\u751f\u6545\u3002\u7531\u65af\u6240\u55bb\u8207\u55bb\u540c\u6cd5\u3002\u5206\u5225\u6b63\u6027\u5206\u5225\u90aa", "302": "\u6027\u5404\u70ba\u4e00\u908a\u3002\u57f7\u5982\u5be6\u89c0\u70ba\u6b63\u70ba\u90aa\u3002\u4e8c\u7a2e\u6027\u6545\u3002", "303": "\u70ba\u96e2\u5982\u662f\u4e8c\u908a\u5206\u5225\u8aaa\u5169\u6728\u751f\u706b\u55bb\u3002\u8b02\u5982\u5169", "304": "\u6728\u96d6\u7121\u706b\u76f8\u3002\u7531\u76f8\u947d\u622a\u800c\u80fd\u751f\u706b\u3002\u706b\u65e2\u751f\u5df2", "305": "\u9084\u71d2\u5169\u6728\u3002\u6b64\u5982\u5be6\u89c0\u4ea6\u5fa9\u5982\u662f\u3002\u96d6\u7121\u8056\u9053\u6b63", "306": "\u6027\u4e4b\u76f8\u3002\u800c\u80fd\u767c\u751f\u6b63\u6027\u8056\u6167\u3002\u5982\u662f\u6b63\u6027\u8056\u6167", "307": "\u751f\u5df2\u3002\u5fa9\u80fd\u9664\u9063\u6b64\u5982\u5be6\u89c0\u3002\u7531\u65af\u6240\u55bb\u8207\u55bb\u540c", "308": "\u6cd5\u3002\u7136\u5982\u5be6\u89c0\u96d6\u7121\u6b63\u6027\u76f8\u3002\u9806\u6b63\u6027\u6545\u4ea6\u7121\u90aa", "309": "\u6027\u76f8\u3002\u5206\u5225\u6709\u7528\u5206\u5225\u7121\u7528\u5404\u70ba\u4e00\u908a\u3002\u5f7c\u57f7\u8056", "310": "\u667a\u8981\u5148\u5206\u5225\u65b9\u80fd\u9664\u67d3\u3002\u6216\u5168\u7121\u7528\u3002\u70ba\u96e2\u5982\u662f", "311": "\u4e8c\u908a\u5206\u5225\u8aaa\u521d\u71c8\u55bb\u3002\u5206\u5225\u4e0d\u8d77\u5206\u5225\u6642\u7b49\u5404\u70ba", "312": "\u4e00\u908a\u3002\u5f7c\u57f7\u80fd\u6cbb\u7562\u7adf\u4e0d\u8d77\u3002\u6216\u57f7\u8207\u67d3\u61c9\u7b49\u6642", "313": "\u9577\u3002\u70ba\u96e2\u5982\u662f\u4e8c\u908a\u5206\u5225\u8aaa\u5f8c\u71c8\u55bb\u3002\u5982\u662f\u5df2\u8aaa", "314": "\u96e2\u4e8c\u908a\u6b63\u884c\u5dee\u5225\u3002\u7121\u5dee\u5225\u6b63\u884c\u4e91\u4f55\u3002\u980c\u66f0\u3002", "315": "\u5dee\u5225\u7121\u5dee\u5225\u61c9\u77e5\u65bc\u5341\u5730", "316": "\u5341\u6ce2\u7f85\u871c\u591a\u589e\u4e0a\u7b49\u4fee\u96c6", "317": "\u8ad6\u66f0\u3002\u65bc\u5341\u5730\u4e2d\u5341\u5230\u5f7c\u5cb8\u3002\u96a8\u4e00\u589e\u4e0a\u800c\u4fee\u96c6", "318": "\u8005\u3002\u61c9\u77e5\u8aaa\u70ba\u5dee\u5225\u6b63\u884c\u3002\u65bc\u4e00\u5207\u5730\u7686\u7b49\u4fee\u96c6", "319": "\u5e03\u65bd\u7b49\u5341\u6ce2\u7f85\u871c\u591a\u3002\u5982\u662f\u6b63\u884c\u540d\u7121\u5dee\u5225\u516d", "320": "\u6b63\u884c\u7e3d\u7fa9\u8005\u3002\u8b02\u5373\u5982\u662f\u54c1\u985e\u6700\u52dd\u3002\u7531\u6b64\u601d\u60df", "321": "\u5982\u6240\u65bd\u8a2d\u5927\u4e58\u6cd5\u7b49\u3002\u7531\u5982\u662f\u54c1\u7121\u4e82\u8f49\u8b8a\u4fee", "322": "\u5962\u6469\u4ed6\u3002\u53ca\u7121\u5012\u8f49\u8b8a\u4fee\u6bd8\u9262\u820d\u90a3\u70ba\u5982\u662f\u7fa9", "323": "\u4fee\u4e2d\u9053\u884c\u800c\u6c42\u51fa\u96e2\u3002\u65bc\u5341\u5730\u4e2d\u4fee\u7fd2\u5dee\u5225\u7121", "324": "\u5dee\u5225\u884c\u3002\u5982\u662f\u5df2\u8aaa\u6b63\u884c\u7121\u4e0a\u3002\u6240\u7de3\u7121\u4e0a\u5176\u76f8", "325": "\u4e91\u4f55\u3002\u980c\u66f0\u3002", "326": "\u6240\u7de3\u8b02\u5b89\u754c\u6240\u80fd\u7acb\u4efb\u6301", "327": "\u5370\u5167\u6301\u901a\u9054\u589e\u8b49\u904b\u6700\u52dd", "328": "\u8ad6\u66f0\u3002\u5982\u662f\u6240\u7de3\u6709\u5341\u4e8c\u7a2e\u3002\u4e00\u5b89\u7acb\u6cd5\u65bd\u8a2d\u6240", "329": "\u7de3\u3002\u4e8c\u6cd5\u754c\u6240\u7de3\u3002\u4e09\u6240\u7acb\u6240\u7de3\u3002\u56db\u80fd\u7acb\u6240\u7de3\u3002\u4e94", "330": "\u4efb\u6301\u6240\u7de3\u516d\u5370\u6301\u6240\u7de3\u3002\u4e03\u5167\u6301\u6240\u7de3\u3002\u516b\u901a\u9054", "331": "\u6240\u7de3\u3002\u4e5d\u589e\u9577\u6240\u7de3\u3002\u5341\u5206\u8b49\u6240\u7de3\u3002\u5341\u4e00\u7b49\u904b", "332": "\u6240\u7de3\u3002\u5341\u4e8c\u6700\u52dd\u6240\u7de3\u3002\u6b64\u4e2d\u6700\u521d\u8b02\u6240\u5b89\u7acb\u3002\u5230", "333": "\u5f7c\u5cb8\u7b49\u5dee\u5225\u6cd5\u9580\u3002\u7b2c\u4e8c\u8b02\u771f\u5982\u3002\u7b2c\u4e09\u7b2c\u56db\u5982", "334": "\u6b21\u61c9\u77e5\u3002\u5373\u524d\u4e8c\u7a2e\u5230\u5f7c\u5cb8\u7b49\u5dee\u5225\u6cd5\u9580\u3002\u8981\u7531", "335": "\u901a\u9054\u6cd5\u754c\u6210\u6545\u3002\u7b2c\u4e94\u8b02\u805e\u6240\u6210\u6167\u5883\u3002\u4efb\u6301\u6587", "336": "\u6545\u3002\u7b2c\u516d\u8b02\u601d\u6240\u6210\u6167\u5883\u3002\u5370\u6301\u7fa9\u6545\u3002\u7b2c\u4e03\u8b02\u4fee", "337": "\u6240\u6210\u6167\u5883\u3002\u5167\u5225\u6301\u6545\u3002\u7b2c\u516b\u8b02\u521d\u5730\u4e2d\u898b\u9053\u5883\u3002", "338": "\u7b2c\u4e5d\u8b02\u4fee\u9053\u4e2d\u4e43\u81f3\u4e03\u5730\u5883\u3002\u7b2c\u5341\u8b02\u5373\u4e03\u5730", "339": "\u4e2d\u4e16\u51fa\u4e16\u9053\u54c1\u985e\u5dee\u5225\u5206\u5206\u8b49\u5883\u3002\u7b2c\u5341\u4e00\u8b02", "340": "\u7b2c\u516b\u5730\u5883\u3002\u7b2c\u5341\u4e8c\u8b02\u7b2c\u4e5d\u7b2c\u5341\u5982\u4f86\u5730\u5883\u3002\u61c9", "341": "\u77e5\u6b64\u4e2d\u5373\u521d\u7b2c\u4e8c\u3002\u96a8\u8af8\u7fa9\u4f4d\u5f97\u5f7c\u5f7c\u540d\u3002\u5982\u662f", "342": "\u5df2\u8aaa\u6240\u7de3\u7121\u4e0a\u3002\u4fee\u8b49\u7121\u4e0a\u5176\u76f8\u4e91\u4f55\u3002\u980c\u66f0\u3002", "343": "\u4fee\u8b49\u8b02\u7121\u95d5\u4e0d\u6bc0\u52d5\u5713\u6eff", "344": "\u8d77\u5805\u56fa\u8abf\u67d4\u4e0d\u4f4f\u7121\u969c\u606f", "345": "\u8ad6\u66f0\u3002\u5982\u662f\u4fee\u8b49\u7e3d\u6709\u5341\u7a2e\u3002\u4e00\u7a2e\u6027\u4fee\u8b49\u7de3", "346": "\u7121\u95d5\u6545\u3002\u4e8c\u4fe1\u89e3\u4fee\u8b49\u3002\u4e0d\u8b17\u6bc0\u5927\u4e58\u6545\u3002\u4e09\u767c\u5fc3", "347": "\u4fee\u8b49\u3002\u975e\u4e0b\u52a3\u4e58\u6240\u64fe\u52d5\u6545\u3002\u56db\u6b63\u884c\u4fee\u8b49\u3002\u6ce2\u7f85", "348": "\u871c\u591a\u5f97\u5713\u6eff\u6545\u3002\u4e94\u5165\u96e2\u751f\u4fee\u8b49\u3002\u8d77\u8056\u9053\u6545\u3002\u516d", "349": "\u6210\u719f\u6709\u60c5\u4fee\u8b49\u3002\u5805\u56fa\u5584\u6839\u9577\u6642\u96c6\u6545\u3002\u4e03\u6de8\u571f", "350": "\u4fee\u8b49\u3002\u5fc3\u8abf\u67d4\u6545\u516b\u5f97\u4e0d\u9000\u5730\u53d7\u8a18\u4fee\u8b49\u3002\u4ee5\u4e0d", "351": "\u4f4f\u8457\u751f\u6b7b\u6d85\u69c3\u3002\u975e\u6b64\u4e8c\u7a2e\u6240\u9000\u8f49\u6545\u3002\u4e5d\u4f5b\u5730", "352": "\u4fee\u8b49\u3002\u7121\u4e8c\u969c\u6545\u3002\u5341\u793a\u73fe\u83e9\u63d0\u4fee\u8b49\u3002\u7121\u4f11\u606f\u6545\u3002", "353": "\u7121\u4e0a\u4e58\u7e3d\u7fa9\u8005\u3002\u7565\u6709\u4e09\u7a2e\u7121\u4e0a\u4e58\u7fa9\u3002\u8b02\u6b63\u884c", "354": "\u7121\u4e0a\u6545\u3002\u6b63\u884c\u6301\u7121\u4e0a\u6545\u3002\u6b63\u884c\u679c\u7121\u4e0a\u6545\u3002\u4f55\u6545", "355": "\u6b64\u8ad6\u540d\u8faf\u4e2d\u908a\u3002\u980c\u66f0\u3002", "356": "\u6b64\u8ad6\u8faf\u4e2d\u908a\u6df1\u5bc6\u5805\u5be6\u7fa9", "357": "\u5ee3\u5927\u4e00\u5207\u7fa9\u9664\u8af8\u4e0d\u5409\u7965", "358": "\u8ad6\u66f0\u3002\u6b64\u8ad6\u80fd\u8faf\u4e2d\u908a\u884c\u6545\u540d\u8faf\u4e2d\u908a\u3002\u5373\u662f\u986f", "359": "\u4e86\u8655\u4e2d\u4e8c\u908a\u80fd\u7de3\u884c\u7fa9\u3002\u53c8\u6b64\u80fd\u8faf\u4e2d\u908a\u5883\u6545", "360": "\u540d\u8faf\u4e2d\u908a\u3002\u5373\u662f\u986f\u4e86\u8655\u4e2d\u4e8c\u908a\u6240\u7de3\u5883\u7fa9\u3002\u6216", "361": "\u6b64\u6b63\u8faf\u96e2\u521d\u5f8c\u908a\u4e2d\u9053\u6cd5\u6545\u540d\u8faf\u4e2d\u908a\u3002\u6b64\u8ad6", "362": "\u6240\u8faf\u662f\u6df1\u5bc6\u7fa9\u3002\u975e\u8af8\u5c0b\u601d\u6240\u884c\u8655\u6545\u3002\u662f\u5805\u5be6", "363": "\u7fa9\u3002\u80fd\u6467\u4ed6\u8faf\u975e\u5f7c\u4f0f\u6545\u3002\u662f\u5ee3\u5927\u7fa9\u3002\u80fd\u8faf\u5229", "364": "\u6a02\u81ea\u4ed6\u4e8b\u6545\u3002\u662f\u4e00\u5207\u7fa9\u3002\u666e\u80fd\u6c7a\u4e86\u4e09\u4e58\u6cd5\u6545\u3002", "365": "\u53c8\u80fd\u9664\u6ec5\u8af8\u4e0d\u5409\u7965\u3002\u6c38\u65b7\u7169\u60f1\u6240\u77e5\u969c\u6545\u3002", "366": "\u6211\u8faf\u6b64\u8ad6\u8af8\u529f\u5fb7\u54b8\u6301\u666e\u65bd\u7fa4\u751f\u985e", "367": "\u4ee4\u7372\u52dd\u751f\u589e\u798f\u6167\u75be\u8b49\u5ee3\u5927\u4e09\u83e9\u63d0", "368": "\u8faf\u4e2d\u908a\u8ad6\u5377\u4e0b"}, "originalTitle": "Taisho Tripitaka, Electronic version, No. 1600 \u8faf\u4e2d\u908a\u8ad6: \u8faf\u4e2d\u908a\u8ad6: \u8faf\u4e2d\u908a\u8ad6", "source": "CBETA"}