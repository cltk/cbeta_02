u5927\u795e\u901a\u529b\u667a\u6167\u5230\u5f7c\u5cb8", "83": "\u9060\u96e2\u65bc\u8f2a\u8ff4\u6c38\u5f97\u4e0d\u9000\u8f49", "84": "\u52c7\u731b\u5927\u7cbe\u9032\u4e86\u77e5\u4e00\u5207\u6cd5", "85": "\u5927\u88dc\u7279\u4f3d\u7f85\u5f97\u8d85\u6700\u4e0a\u5730", "86": "\u4ee5\u667a\u6167\u6cd5\u96e8\u666e\u6f64\u8af8\u773e\u751f", "87": "\u5ba3\u793a\u56db\u6cd5\u5370\u5f15\u5c0e\u65bc\u4e09\u4e58", "88": "\u6e05\u6de8\u6700\u4e0a\u7fa9\u80fd\u4f5c\u5927\u5409\u7965", "89": "\u83e9\u85a9\u6240\u7a31\u8b9a\u6210\u5c31\u91d1\u525b\u5c0a", "90": "\u773e\u751f\u7121\u5104\u6578\u77bb\u4ef0\u6b78\u547d\u79ae", "91": "\u6b78\u547d\u5927\u7a7a\u85cf\u8b9a\u6b4e\u5927\u7a7a\u85cf", "92": "\u6b78\u547d\u4f5b\u89ba\u9053\u8b9a\u6b4e\u4f5b\u89ba\u9053", "93": "\u6b78\u547d\u8af8\u4f5b\u8eab\u8b9a\u6b4e\u8af8\u4f5b\u8eab", "94": "\u6b78\u547d\u4f5b\u6b23\u6085\u8b9a\u6b4e\u4f5b\u6b23\u6085", "95": "\u6b78\u547d\u4f5b\u529f\u5fb7\u8b9a\u6b4e\u4f5b\u529f\u5fb7", "96": "\u6b78\u547d\u8af8\u4f5b\u5ff5\u8b9a\u6b4e\u8af8\u4f5b\u5ff5", "97": "\u6b78\u547d\u4f5b\u559c\u7b11\u8b9a\u6b4e\u4f5b\u559c\u7b11", "98": "\u6b78\u547d\u8af8\u4f5b\u8a9e\u8b9a\u6b4e\u8af8\u4f5b\u8a9e", "99": "\u6b78\u547d\u4f5b\u6240\u611b\u8b9a\u6b4e\u4f5b\u6240\u611b", "100": "\u6b78\u547d\u8af8\u4f5b\u751f\u8b9a\u6b4e\u8af8\u4f5b\u751f", "101": "\u6b78\u547d\u4f5b\u667a\u751f\u8b9a\u6b4e\u4f5b\u667a\u751f", "102": "\u6b78\u547d\u4f5b\u6232\u821e\u8b9a\u6b4e\u4f5b\u6232\u821e", "103": "\u6b78\u547d\u4e00\u5207\u667a\u8b9a\u6b4e\u4e00\u5207\u667a", "104": "\u6027\u7a7a\u53ca\u5e7b\u7db2\u6b78\u8b9a\u4ea6\u5982\u662f", "105": "\u723e\u6642\u91d1\u525b\u638c\u83e9\u85a9\u6469\u8a36\u85a9\u3002\u91d1\u525b\u624b\u83e9\u85a9\u6469\u8a36", "106": "\u85a9\u3002\u767d\u4f5b\u8a00\u4e16\u5c0a\u3002\u5982\u4f86\u667a\u89c0\u4e00\u5207\u667a\u8eab\u751a\u6df1\u5fae", "107": "\u5999\u3002\u6587\u6b8a\u83e9\u85a9\u6469\u8a36\u85a9\u3002\u70ba\u6b32\u5229\u76ca\u4e00\u5207\u773e\u751f\u3002\u8aaa", "108": "\u6b64\u6e05\u6de8\u6700\u52dd\u540d\u7fa9\u3002\u82e5\u8af8\u773e\u751f\u672a\u6de8\u4e09\u696d\u3002\u4ee4\u65bc", "109": "\u4f5b\u5730\u6ce2\u7f85\u871c\u9580\u798f\u667a\u85cf\u4e2d\u3002\u651d\u6301\u4e09\u696d\u5713\u6eff\u6e05", "110": "\u6de8\u3002\u65bc\u6700\u4e0a\u7fa9\u672a\u4e86\u89e3\u8005\u4ee4\u5f97\u4e86\u89e3\u3002\u4e43\u81f3\u4e00\u5207", "111": "\u8af8\u4f5b\u6cd5\u85cf\u3002\u7686\u70ba\u958b\u767c\u4ee4\u5f97\u609f\u89e3\u3002\u51fa\u751f\u773e\u5584\u70ba", "112": "\u529f\u5fb7\u9580\u3002\u5fa9\u6b21\u91d1\u525b\u638c\u83e9\u85a9\u6469\u8a36\u85a9\u91d1\u525b\u624b\u83e9", "113": "\u85a9\u6469\u8a36\u85a9\u8a00\u3002\u6b64\u6700\u52dd\u540d\u7fa9\u51fa\u751f\u4e00\u5207\u6de8\u5730\u6ce2", "114": "\u7f85\u871c\u6cd5\u7955\u5bc6\u795e\u546a\u5713\u6eff\u6210\u5c31\u4e00\u5207\u667a\u667a\u8af8\u529f\u5fb7", "115": "\u6d77\u3002\u6de8\u8eab\u8a9e\u610f\u4e09\u7955\u5bc6\u9580\u3002\u89c0\u60f3\u8af8\u4f5b\u6b63\u7b49\u6b63\u89ba\u3002", "116": "\u6210\u5c31\u4f5b\u667a\u5927\u4e09\u6469\u5730\u3002\u4e00\u5207\u5982\u4f86\u6e05\u6de8\u6cd5\u754c\u3002\u6700", "117": "\u52dd\u5341\u529b\u7834\u58de\u9b54\u6028\u3002\u5177\u4e00\u5207\u667a\u4e00\u5207\u7a2e\u667a\u3002\u6839\u672c", "118": "\u65b9\u4fbf\u9952\u76ca\u773e\u751f\u3002\u798f\u5fb7\u667a\u85cf\u6e05\u6de8\u5713\u6eff\u3002\u51fa\u751f\u83e9", "119": "\u85a9\u8072\u805e\u7de3\u89ba\u3002\u4e8c\u4e58\u8056\u7a2e\u4e00\u5207\u4eba\u5929\u3002\u5b89\u4f4f\u5927\u4e58", "120": "\u8af8\u83e9\u85a9\u884c\u3002\u5165\u6b63\u8056\u9053\u7686\u5f97\u89e3\u812b\u3002\u5fa9\u80fd\u589e\u9577\u83e9", "121": "\u85a9\u5584\u6839\u3002\u651d\u53d7\u4e00\u5207\u5916\u9053\u7570\u8ad6\u3002\u5a01\u5fb7\u6467\u4f0f\u56db\u7a2e", "122": "\u9b54\u6028\u3002\u4ee4\u8af8\u773e\u751f\u540c\u6b78\u8056\u9053\u3002\u89e3\u812b\u7e6b\u7e1b\u96e2\u8af8\u6563", "123": "\u4e82\u3002\u5177\u8db3\u4e00\u5207\u8af8\u5584\u4e8b\u696d\u3002\u65b7\u9664\u8f2a\u8ff4\u5f97\u771f\u8056\u9053\u3002", "124": "\u4ee5\u5999\u9999\u83ef\u5e62\u5e61\u5098\u84cb\u3002\u666e\u904d\u4f9b\u990a\u4e00\u5207\u5982\u4f86\u3002\u901f", "125": "\u5f97\u6210\u5c31\u8af8\u546a\u90e8\u9580\u3002\u65bc\u8af8\u83e9\u85a9\u751f\u611b\u6a02\u60f3\u3002\u76f8\u61c9", "126": "\u822c\u82e5\u6ce2\u7f85\u871c\u591a\u3002\u4e86\u77e5\u83e9\u85a9\u7a7a\u884c\u7121\u4e8c\u3002\u5177\u8db3\u4e00", "127": "\u5207\u6ce2\u7f85\u871c\u85cf\u3002\u5713\u6eff\u4e00\u5207\u6e05\u6de8\u4f5b\u5730\u3002\u5f97\u56db\u771f\u8ae6", "128": "\u8056\u667a\u73fe\u524d\u3002\u4e00\u5fc3\u5b89\u4f4f\u56db\u6b63\u5ff5\u8655\u3002\u4e43\u81f3\u5177\u8db3\u8af8", "129": "\u4f5b\u529f\u5fb7\u3002\u5fa9\u6b21\u91d1\u525b\u638c\u83e9\u85a9\u6469\u8a36\u85a9\u91d1\u525b\u624b\u83e9", "130": "\u85a9\u6469\u8a36\u85a9\u8a00\u3002\u6b64\u6700\u52dd\u540d\u7fa9\u7d93\u3002\u80fd\u9664\u4e00\u5207\u773e\u751f", "131": "\u8eab\u8a9e\u610f\u696d\u4e0d\u5584\u7f6a\u57a2\u3002\u4ee4\u5f97\u9060\u96e2\u4e00\u5207\u60e1\u9053\u3002\u53ca", "132": "\u5f97\u65b7\u9664\u4e00\u5207\u696d\u969c\u3002\u516b\u96e3\u6016\u754f\u7686\u5f97\u6d88\u9664\u3002\u96e2\u60e1", "133": "\u7720\u5922\u7372\u5927\u5409\u7965\u3002\u53ca\u96e2\u4e00\u5207\u8af8\u9b54\u6028\u7d50\u3002\u4fee\u8af8\u5584", "134": "\u6839\u798f\u5fb7\u5229\u76ca\u3002\u65b7\u9664\u4e00\u5207\u589e\u4e0a\u6211\u6162\u3002\u6c38\u96e2\u4e00\u5207", "135": "\u82e6\u60f1\u8f2a\u8f49\u3002\u4e00\u5207\u4f5b\u5fc3\u4e86\u77e5\u5982\u5be6\u83e9\u85a9\u5bc6\u884c\u4e86", "136": "\u77e5\u5982\u5be6\u3002\u4e09\u4e58\u8056\u667a\u4e86\u77e5\u5982\u5be6\u3002\u4e00\u5207\u546a\u5370\u4e86\u77e5", "137": "\u5982\u5be6\u3002\u6700\u52dd\u6cd5\u7fa9\u751f\u5927\u667a\u6167\u3002\u4f4f\u5b89\u6a02\u884c\u8272\u529b\u81ea", "138": "\u5728\u3002\u7372\u5f97\u6e05\u6de8\u5927\u5409\u7965\u4e8b\u3002\u6b61\u559c\u8e34\u8e8d\u4ee5\u5999\u53e5\u5048\u3002", "139": "\u7a31\u63da\u8b9a\u6b4e\u6b64\u540d\u7fa9\u7d93\u3002\u4ea6\u80fd\u6d88\u9664\u4e00\u5207\u75be\u75c5\u53ca", "140": "\u5927\u6050\u6016\u3002\u82e5\u773e\u751f\u5fc3\u6709\u6240\u6a02\u6b32\u3002\u81f3\u5fc3\u8aa6\u6301\u6700\u52dd", "141": "\u540d\u7fa9\u6089\u5f97\u5982\u610f\u3002\u6b32\u5f97\u6e05\u6de8\u5373\u5f97\u6e05\u6de8\u3002\u6b32\u5f97\u6551", "142": "\u8b77\u5373\u5f97\u6551\u8b77\u3002\u6b32\u5f97\u5bcc\u9952\u5373\u5f97\u5bcc\u9952\u3002\u5982\u6f02\u6eba\u4eba", "143": "\u5f97\u7372\u6fdf\u5ea6\u3002\u672a\u5f97\u9053\u8005\u4ee4\u5f97\u9053\u679c\u3002\u4e58\u822c\u82e5\u821f\u5230", "144": "\u83e9\u63d0\u5cb8\u3002\u5982\u5927\u91ab\u738b\u80fd\u9664\u773e\u75c5\u3002\u4ee5\u65b9\u4fbf\u667a\u6551\u8b77", "145": "\u773e\u751f\u3002\u611a\u7661\u6697\u853d\u6089\u5f97\u6c38\u96e2\u3002\u7336\u5982\u610f\u5bf6\u96a8\u5176\u6240", "146": "\u6b32\u3002\u5229\u76ca\u4e00\u5207\u7686\u4ee4\u5713\u6eff\u3002\u6587\u6b8a\u83e9\u85a9\u6469\u8a36\u85a9\u3002\u5982", "147": "\u5be6\u4e86\u77e5\u4e00\u5207\u667a\u667a\u3002\u5177\u8db3\u4e94\u773c\u516d\u6ce2\u7f85\u871c\u3002\u5f97\u56db", "148": "\u7121\u754f\u5b89\u4f4f\u5341\u5730\u3002\u5927\u798f\u667a\u85cf\u4e09\u6469\u5730\u9580\u7686\u5f97\u5713", "149": "\u6eff\u3002\u5982\u5be6\u4e86\u77e5\u6cd5\u6027\u7121\u4e8c\u3002\u5982\u5be6\u4e86\u77e5\u8272\u76f8\u5dee\u5225\u3002", "150": "\u5982\u5be6\u4e86\u77e5\u7a2e\u7a2e\u5104\u6578\u8272\u76f8\u6e05\u6de8\u3002\u5982\u4f86\u81ea\u6027\u6089", "151": "\u7686\u7a7a\u6545\u3002\u6b64\u540d\u7fa9\u7d93\u7121\u4e8c\u6cd5\u7fa9\u3002\u82e5\u6709\u53d7\u6301\u958b\u793a", "152": "\u986f\u767c\u3002\u5247\u80fd\u5229\u76ca\u4e00\u5207\u773e\u751f\u3002\u4ee4\u96e2\u90aa\u898b\u7169\u60f1\u7a20", "153": "\u6797\u3002\u5fa9\u6b21\u91d1\u525b\u638c\u83e9\u85a9\u6469\u8a36\u85a9\u91d1\u525b\u624b\u83e9\u85a9\u6469", "154": "\u8a36\u85a9\u3002\u767d\u4f5b\u8a00\u4e16\u5c0a\u3002\u6587\u6b8a\u5e2b\u5229\u83e9\u85a9\u6469\u8a36\u85a9\u3002\u53ca", "155": "\u4e00\u5207\u5982\u4f86\u667a\u89c0\u7121\u4e8c\u3002\u6b64\u540d\u7fa9\u7d93\u6700\u52dd\u5c0a\u91cd\u3002\u5982", "156": "\u4f5b\u9802\u4e2d\u5927\u6469\u5c3c\u73e0\u3002\u82e5\u5584\u7537\u5b50\u5584\u5973\u4eba\u4f9d\u6b64\u6700", "157": "\u52dd\u7955\u5bc6\u546a\u9580\u53e5\u5048\u7fa9\u7406\u3002\u6bcf\u65e5\u4e09\u6642\u53d7\u6301\u8b80\u8aa6", "158": "\u89e3\u8aaa\u66f8\u5beb\u3002\u5229\u76ca\u773e\u751f\u986f\u793a\u4e09\u4e58\u666e\u4ee4\u609f\u5165\u3002\u723e", "159": "\u6642\u6587\u6b8a\u5e2b\u5229\u83e9\u85a9\u6469\u8a36\u85a9\u3002\u8aaa\u6b64\u6700\u52dd\u540d\u7fa9\u3002\u6b32", "160": "\u4ee4\u773e\u751f\u4e00\u5fc3\u4fe1\u53d7\u3002\u5f97\u52dd\u89e3\u5fc3\u4e86\u9054\u4e00\u5207\u6700\u4e0a", "161": "\u6cd5\u9580\u3002\u4fee\u7121\u4f4f\u884c\u5177\u8db3\u667a\u6167\u3002\u6e05\u6de8\u4e09\u696d\u767c\u83e9\u63d0", "162": "\u5fc3\u3002\u4e00\u5207\u8af8\u4f5b\u8af8\u5927\u83e9\u85a9\u3002\u666e\u7686\u793a\u73fe\u5e73\u7b49\u6cd5\u9580\u3002", "163": "\u4ee4\u8af8\u773e\u751f\u7686\u5f97\u609f\u5165\u3002\u5927\u91d1\u525b\u638c\u83e9\u85a9\u73fe\u5fff\u6012", "164": "\u76f8\u3002\u4ee5\u5927\u5a01\u529b\u964d\u4f0f\u9b54\u6028\u3002\u666e\u7686\u5229\u76ca\u4e00\u5207\u773e\u751f", "165": "\u4ee4\u5f97\u5b89\u6a02\u3002\u986f\u793a\u7955\u5bc6\u4e09\u6627\u9053\u5834\u4e00\u5207\u546a\u5370\u3002\u5f15", "166": "\u5c0e\u773e\u751f\u5165\u6b63\u5b9a\u805a\u5713\u6eff\u7121\u9918\u3002\u5927\u660e\u546a\u738b\u9664\u8af8", "167": "\u969c\u96e3\u3002\u6d88\u4f0f\u9b54\u6028\u5177\u5927\u5a01\u5fb7\u3002\u65bc\u665d\u591c\u4e2d\u5e38\u7576\u64c1", "168": "\u8b77\u3002\u7ae5\u771f\u83e9\u85a9\u68b5\u738b\u5e1d\u91cb\u3002\u5695\u9640\u7f85\u795e\u90a3\u7f85\u5ef6\u5929\u3002", "169": "\u5927\u81ea\u5728\u5929\u5e76\u5b50\u6b4c\u54e9\u5e95\u8a08\u5927\u9ed1\u5929\u795e\u3002\u96e3\u5e95\u8a08", "170": "\u8aaa\u56c9\u5927\u795e\u7130\u9b54\u5929\u738b\u3002\u6c34\u754c\u5927\u795e\u6bd8\u6c99\u9580\u5929\u738b", "171": "\u8cc0\u54e9\u5e1d\u6bcd\u3002\u65bc\u665d\u591c\u4e2d\u5e38\u7576\u64c1\u8b77\u3002\u82e5\u884c\u82e5\u4f4f\u82e5", "172": "\u5750\u82e5\u81e5\u4e00\u5207\u6642\u4e2d\u3002\u8af8\u4f5b\u83e9\u85a9\u5a01\u795e\u52a0\u8b77\u3002\u4e00\u5207", "173": "\u8af8\u4f5b\u53ca\u8af8\u83e9\u85a9\u3002\u9952\u76ca\u651d\u53d7\u5f7c\u8af8\u773e\u751f\u3002\u8eab\u610f\u8a9e", "174": "\u696d\u7686\u5f97\u6e05\u6de8\u3002\u4e00\u5207\u7f85\u6f22\u8072\u805e\u7de3\u89ba\u3002\u8b77\u5ff5\u651d\u53d7", "175": "\u5f7c\u8af8\u773e\u751f\u3002\u65bc\u4e00\u5207\u6cd5\u5f97\u7121\u6240\u754f\u3002\u5982\u662f\u6700\u52dd\u540d", "176": "\u7fa9\u8056\u6cd5\u3002\u65bc\u8af8\u7d93\u4e2d\u6700\u70ba\u4e0a\u9996\u3002\u82e5\u80fd\u65bc\u6b64\u4fe1\u89e3", "177": "\u53d7\u6301\u3002\u662f\u4eba\u5247\u70ba\u5f97\u83e9\u63d0\u9053\u3002\u6216\u8655\u79aa\u5b9a\u6216\u5c45\u95e0", "178": "\u9b27\u3002\u6216\u5165\u738b\u57ce\u805a\u843d\u4e4b\u8655\u3002\u6c5f\u6cb3\u5712\u6797\u4e00\u5207\u4f4f\u8655\u3002", "179": "\u65bc\u665d\u591c\u4e2d\u5e38\u7576\u64c1\u8b77\u4ee4\u7121\u6016\u754f\u3002\u5929\u9f8d\u516b\u90e8\u4eba", "180": "\u53ca\u975e\u4eba\u3002\u4e43\u81f3\u6bd8\u820d\u906e\u5973\u5e76\u8af8\u7737\u5c6c\u3002\u5e38\u7576\u64c1\u8b77", "181": "\u662f\u8af8\u773e\u751f\u3002\u4ee4\u96e2\u8af8\u60f1\u5f97\u5927\u5b89\u96b1\u3002\u5fa9\u6b21\u91d1\u525b\u638c", "182": "\u83e9\u85a9\u91d1\u525b\u624b\u83e9\u85a9\u8a00\u3002\u6b64\u6700\u52dd\u540d\u7fa9\u7d93\u3002\u5982\u4f5b\u9802", "183": "\u73e0\u6700\u4e0a\u5fae\u5999\u3002\u529f\u5fb7\u6b8a\u52dd\u4e0d\u53ef\u601d\u8b70\u3002\u6bcf\u65e5\u4e09\u6642", "184": "\u53d7\u6301\u8b80\u8aa6\u3002\u6b63\u5ff5\u601d\u60df\u7cbe\u9032\u7121\u61c8\u3002\u65bc\u4f5b\u83e9\u63d0\u901f", "185": "\u80fd\u8da3\u5165\u3002\u5fa9\u767d\u4f5b\u8a00\u4e16\u5c0a\u3002\u6587\u6b8a\u5e2b\u5229\u83e9\u85a9\u8272\u76f8", "186": "\u5177\u8db3\u89c0\u5bdf\u601d\u60df\u3002\u4ee5\u5927\u9858\u529b\u5ea6\u812b\u773e\u751f\u3002\u6216\u65bc\u7a7a", "187": "\u4e2d\u73fe\u4e00\u5207\u4f5b\u3002\u4e00\u5207\u83e9\u85a9\u7a2e\u7a2e\u5316\u8eab\u3002\u96a8\u9806\u773e\u751f", "188": "\u6240\u6a02\u793a\u73fe\u3002\u6f14\u8aaa\u5fae\u5999\u751a\u6df1\u53e5\u7fa9\u3002\u5f15\u5c0e\u773e\u751f\u9060", "189": "\u96e2\u60e1\u8da3\u3002\u4e0d\u751f\u5351\u8ce4\u4e0d\u58ae\u908a\u5883\u3002\u751f\u4e0d\u919c\u964b\u4e0d\u58ae", "190": "\u90aa\u898b\u3002\u5e38\u751f\u4f5b\u524e\u807d\u805e\u6b63\u6cd5\u96e2\u7121\u60f3\u8655\u3002\u4e0d\u751f\u9951", "191": "\u9949\u9b2a\u6230\u52ab\u4e2d\u3002\u4e0d\u751f\u4e94\u6fc1\u53ca\u8cca\u96e3\u8655\u3002\u4e0d\u751f\u8ca7\u7aae", "192": "\u53ca\u56f0\u82e6\u8655\u3002\u4e0d\u751f\u975e\u6cd5\u5984\u8aaa\u53e5\u5048\u70ba\u6c42\u540d\u805e\u3002\u751f", "193": "\u8ce2\u5584\u5bb6\u5c0a\u8cb4\u4eba\u4e2d\u3002\u5713\u6eff\u8272\u76f8\u7aef\u56b4\u5177\u8db3\u3002\u898b\u8005", "194": "\u6b61\u559c\u7121\u4e0d\u611b\u6a02\u3002\u5f97\u5bbf\u4f4f\u901a\u5927\u5c0a\u91cd\u76f8\u5177\u5927\u5a01", "195": "\u5fb7\u3002\u5e03\u65bd\u6301\u6212\u5fcd\u8fb1\u7cbe\u9032\u3002\u79aa\u5b9a\u667a\u6167\u65b9\u4fbf\u9858\u529b\u3002", "196": "\u6148\u60b2\u559c\u6368\u56db\u7121\u91cf\u5fc3\u5177\u8db3\u5713\u6eff\u3002\u53ca\u65bc\u4e00\u5207\u5de5", "197": "\u5de7\u6280\u85dd\u3002\u8b9a\u8a60\u5916\u66f8\u7686\u5f97\u4e86\u89e3\u3002\u51fa\u5bb6\u6c42\u9053\u65bc\u4e00", "198": "\u5207\u667a\u5fc3\u7121\u6563\u5931\u3002\u4e86\u9054\u4e09\u4e58\u8af8\u6cd5\u7fa9\u5229\u3002\u6b64\u7d93\u529f", "199": "\u5fb7\u51fa\u751f\u7121\u91cf\u667a\u6167\u7121\u91cf\u5584\u6cd5\u3002\u82e5\u5584\u7537\u5b50\u5584\u5973", "200": "\u4eba\u3002\u53d7\u6301\u8b80\u8aa6\u89e3\u5176\u7fa9\u7406\u70ba\u4eba\u6f14\u8aaa\u3002\u7576\u77e5\u662f\u4eba", "201": "\u65bc\u5982\u4f86\u85cf\u4e2d\u5f97\u4f5b\u529f\u5fb7\u3002\u4e0d\u4e45\u7576\u8b49\u6700\u4e0a\u83e9\u63d0\u3002", "202": "\u5177\u4e00\u5207\u667a\u4e00\u5207\u7a2e\u667a\u3002\u5b89\u4f4f\u4e16\u9593\u632f\u5927\u6cd5\u9f13\u5efa", "203": "\u5927\u6cd5\u5e62\u3002\u4f5c\u5927\u6cd5\u738b\u6f14\u5927\u6cd5\u546a\u3002\u5373\u8aaa\u546a\u66f0\u3002", "204": "\u5535\u5f15\u85a9\u54e9\u56a9\u4e8c\u5408\u9054\u54e9\u6469\u4e8c\u5408\u5f15\u963f\u5a46\u5f15\u56a9\u5a11\u56a9\u4e8c", "205": "\u5408\u5a46\u5f15\u56a9\u5c3e\udb82\udc4a\u99b1\u56a9\u60f9\u56c9\u4e8c\u5408\u904f\u963f\u6697\u5641", "206": "\u4e00\u5207\u6cd5\u76f8\u81ea\u6027\u6e05\u6de8\u3002\u6240\u8b02\u4e00\u5207\u5982\u4f86\u667a\u8eab\u3002\u6587", "207": "\u6b8a\u83e9\u85a9\u6e05\u6de8\u51fa\u751f\u3002\u546a\u66f0\u3002", "208": "\u5641\u85a9\u54e9\u56a9\u4e8c\u5408\u601b\u9640\u5f15\u8a90\u591a\u7d07\u54e9\u4e8c\u5408\u637a\u91ce\u559d\u56c9", "209": "\u559d\u56c9\u5535\u5f15\u543d\u5f15\u7d07\u54e9\u4e8c\u5408", "210": "\u6587\u6b8a\u83e9\u85a9\u8a9e\u81ea\u5728\u738b\u3002\u5ee3\u5927\u5ba3\u8aaa\u4e00\u5207\u6cd5\u6027\u3002\u7336", "211": "\u5982\u865b\u7a7a\u5713\u6eff\u6e05\u6de8\u3002\u6cd5\u754c\u667a\u85cf\u546a\u66f0\u3002\u963f", "212": "\u723e\u6642\u91d1\u525b\u638c\u83e9\u85a9\u6469\u8a36\u85a9\u91d1\u525b\u624b\u83e9\u85a9\u6469\u8a36", "213": "\u85a9\u3002\u6b61\u559c\u8e34\u8e8d\u606d\u656c\u5408\u638c\u77bb\u4ef0\u5982\u4f86\u3002\u8af8\u4f5b\u8056\u773e", "214": "\u5927\u7955\u5bc6\u738b\u3002\u96a8\u559c\u7a31\u8b9a\u5ee3\u5927\u9053\u5834\u3002\u723e\u6642\u91cb\u8fe6\u725f", "215": "\u5c3c\u4f5b\u3002\u8b9a\u91d1\u525b\u638c\u83e9\u85a9\u91d1\u525b\u624b\u83e9\u85a9\u8a00\u3002\u5584\u54c9\u5584", "216": "\u54c9\u3002\u6c5d\u80fd\u986f\u793a\u6587\u6b8a\u6240\u8aaa\u8af8\u4f5b\u7955\u5bc6\u6700\u52dd\u540d\u7fa9\u3002", "217": "\u5229\u76ca\u5b89\u6a02\u4e00\u5207\u773e\u751f\u3002\u6c5d\u7b49\u4e0d\u4e45\u7576\u5f97\u963f\u8028\u591a", "218": "\u7f85\u4e09\u85d0\u4e09\u83e9\u63d0\u3002\u5f97\u5927\u89e3\u812b\u65bc\u5e7b\u7db2\u4e2d\u4f5c\u5927\u5c0e", "219": "\u5e2b\u3002\u4ee5\u5927\u6e05\u6de8\u751a\u6df1\u5999\u7fa9\u958b\u767c\u5f15\u5c0e\u3002\u4ee4\u8af8\u773e\u751f", "220": "\u5165\u4f5b\u5883\u754c\u3002\u6211\u4eca\u8b49\u660e\u3002\u6c5d\u7b49\u6240\u8aaa\u6700\u52dd\u540d\u7fa9\u3002\u6b64", "221": "\u7d93\u65bc\u4e00\u842c\u516d\u5343\u5927\u7955\u5bc6\u6559\u667a\u6167\u85cf\u4e2d\u3002\u6b21\u7b2c\u6d41", "222": "\u51fa\u70ba\u4e09\u6469\u5730\u8f2a\u3002\u6467\u4f0f\u9b54\u6028\u6d88\u9664\u7169\u60f1\u3002\u5ea6\u8af8\u773e", "223": "\u751f\u9060\u96e2\u8f2a\u8ff4\u5230\u83e9\u63d0\u5cb8\u3002", "224": "\u6587\u6b8a\u6240\u8aaa\u6700\u52dd\u540d\u7fa9\u7d93\u5377\u4e0b"}, "originalTitle": "Taisho Tripitaka, Electronic version, No. 1188 \u6587\u6b8a\u6240\u8aaa\u6700\u52dd\u540d\u7fa9\u7d93", "source": "CBETA"}