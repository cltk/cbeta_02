8\u70ba\u9ec3\uff0c\u666e\u4e0a\u4e0b\u904d\uff0c\u4e0d\u4e8c\uff0c\u7121\u6709\u91cf\u3002", "379": "\u300c\u4e03\u70ba\u5728\u884c\u8005\u6bd4\u4e18\u70ba\u8d64\uff0c\u666e\u4e0a\u4e0b\u904d\uff0c\u4e0d\u4e8c\uff0c\u7121\u6709\u91cf\u3002", "38": "\u8457\u8863\u6301\u61c9\u5668\uff0c\u5165\u90e1\u7e23\u6c42\u98df\u3002\u610f\u8a08\uff1a\u300e\u7576\u591a\u53ef\u610f", "380": "\u300c\u516b\u70ba\u5728\u884c\u8005\u6bd4\u4e18\u70ba\u767d\uff0c\u666e\u4e0a\u4e0b\u904d\uff0c\u4e0d\u4e8c\uff0c\u7121\u6709\u91cf\u3002", "381": "\u300c\u4e5d\u70ba\u5728\u884c\u8005\u6bd4\u4e18\u70ba\u7a7a\uff0c\u666e\u4e0a\u4e0b\u904d\uff0c\u4e0d\u4e8c\uff0c\u7121\u6709\u91cf\u3002", "382": "\u300c\u5341\u70ba\u5728\u884c\u8005\u6bd4\u4e18\u70ba\u8b58\uff0c\u666e\u4e0a\u4e0b\u904d\uff0c\u4e0d\u4e8c\uff0c\u7121\u6709", "383": "\u91cf\u3002", "384": "\u300c\u4f5b\u5341\u529b\u3002\u4f55\u8b02\u70ba\u5341\u529b\uff1f\u4e00\u8005\uff0c\u4f5b\u70ba\u8655\u8655\u5982\u6709", "385": "\u77e5\uff0c\u7576\u723e\u4e0d\u723e\u3001\u8655\u4e0d\u8655\u5982\u6709\u77e5\uff0c\u5f9e\u6167\u884c\u5f97\u81ea\u77e5\uff0c", "386": "\u662f\u70ba\u4e00\u529b\u3002", "387": "\u300c\u4e8c\u8005\uff0c\u4f5b\u70ba\u904e\u53bb\u672a\u4f86\u73fe\u5728\u884c\u7f6a\u8655\u672c\u7a2e\u6b83\u5982\u6709", "388": "\u77e5\uff0c\u662f\u70ba\u4e8c\u529b\u3002", "389": "\u300c\u4e09\u8005\u4f5b\u70ba\u4e00\u5207\u5728\u8655\u53d7\u884c\u5982\u6709\u77e5\uff0c\u81ea\u66f4\u6167\u884c", "39": "\u5649\u98df\u3002\u300f\u81ea\u5f97\u591a\u53ef\u5649\u98df\uff0c\u81ea\u610f\u5ff5\uff1a\u300e\u6211\u70ba\u671d\u5f97\u591a\u53ef", "390": "\u5f97\u77e5\u662f\uff0c\u662f\u70ba\u4e09\u529b\u3002", "391": "\u300c\u56db\u8005\uff0c\u4f5b\u70ba\u68c4\u89e3\u5b9a\u884c\u4ea6\u5b9a\u77e5\uff0c\u5f9e\u662f\u7e1b\u4ea6\u77e5\uff0c\u5f9e\u662f", "392": "\u89e3\u4ea6\u77e5\uff0c\u5f9e\u662f\u8d77\u5982\u6709\u77e5\uff0c\u662f\u70ba\u56db\u529b\u3002", "393": "\u300c\u4e94\u8005\uff0c\u4f5b\u70ba\u5982\u5fc3\u9858\u4ed6\u5bb6\u4ed6\u4eba\u5982\u6709\u77e5\uff0c\u662f\u70ba\u4e94", "394": "\u529b\u3002", "395": "\u300c\u516d\u8005\uff0c\u4f5b\u70ba\u96dc\u7a2e\u7121\u6709\u91cf\u7a2e\u5929\u4e0b\u884c\u5982\u662f\u6709\u77e5\uff0c", "396": "\u662f\u70ba\u516d\u529b\u3002", "397": "\u300c\u4e03\u8005\uff0c\u4f5b\u70ba\u4ed6\u5bb6\u4ed6\u6839\u5177\u4e0d\u5177\u5982\u6709\u77e5\uff0c\u662f\u70ba\u4e03", "398": "\u529b\u3002", "399": "\u300c\u516b\u8005\uff0c\u4f5b\u70ba\u7121\u6709\u91cf\u5206\u5225\u672c\u4e0a\u982d\u81f3\u66f4\u81ea\u5ff5\u5982\u6709", "4": "\u8005\u540c\u5b78\u8005\uff0c\u662f\u672c\u884c\uff0c\u672a\u5f97\u6167\u4fbf\u5f97\u6167\u3002\u662f\u70ba\u4e00\u6cd5", "40": "\u5649\u98df\u3002\u300f\u4fbf\u81ea\u5ff5\uff1a\u300e\u671d\u5f97\u591a\u53ef\u5649\u98df\uff0c\u70ba\u6211\u8eab\u91cd\uff0c\u4e0d\u80fd", "400": "\u77e5\uff0c\u662f\u70ba\u516b\u529b\u3002", "401": "\u300c\u4e5d\u8005\uff0c\u4f5b\u70ba\u5929\u773c\u5df2\u6de8\u904e\u5ea6\u4eba\u9593\uff0c\u898b\u4eba\u5f80\u4f86\u6b7b\u751f", "402": "\u5982\u6709\u77e5\uff0c\u662f\u70ba\u4e5d\u529b\u3002\u300d", "403": "\u300c\u5341\u8005\uff0c\u4f5b\u70ba\u5df2\u7e1b\u7d50\u76e1\u7121\u6709\u4f7f\u7e1b\u7d50\uff0c\u610f\u5df2\u89e3\u812b\uff0c", "404": "\u5f9e\u6167\u70ba\u884c\u812b\uff0c\u898b\u6cd5\u81ea\u6167\u8b49\uff0c\u66f4\u77e5\u53d7\u6b62\u76e1\uff0c\u751f\u7adf\uff0c", "405": "\u884c\u6240\u884c\u5df2\u8db3\uff0c\u4e0d\u5fa9\u5f80\u4f86\u4e16\u9593\uff0c\u5df2\u5ea6\u4e16\uff0c\u5982\u6709\u77e5\uff0c", "406": "\u662f\u70ba\u5341\u529b\u3002", "407": "\u300c\u7b2c\u4e5d\u5341\u6cd5\uff0c\u81ea\u8b49\u77e5\u5341\u8db3\u5b78\u4e0d\u5fa9\u5b78\u3002\u4f55\u7b49\u70ba", "408": "\u5341\uff1f\u4e00\u70ba\u76f4\u898b\u5df2\u8db3\uff0c\u4e0d\u5fa9\u5b78\u76f4\u898b\u3002", "409": "\u300c\u4e8c\u8005\uff0c\u76f4\u601d\u60df\u8a08\u5df2\u8db3\uff0c\u4e0d\u5fa9\u5b78\u76f4\u601d\u60df\u8a08\u3002", "41": "\u884c\u4e0d\u80fd\u5750\uff0c\u4ee4\u6211\u50be\u81e5\u3002\u300f\u4fbf\u81e5\u3002\u7121\u6709\u5ea6\u4e16\u65b9\u4fbf\uff0c", "410": "\u300c\u4e09\u8005\u76f4\u8a00\u5df2\u8db3\uff0c\u4e0d\u5fa9\u5b78\u76f4\u8a00\u3002", "411": "\u300c\u56db\u8005\uff0c\u76f4\u884c\u5df2\u8db3\uff0c\u4e0d\u5fa9\u5b78\u76f4\u884c\u3002", "412": "\u300c\u4e94\u8005\uff0c\u76f4\u696d\u5df2\u8db3\uff0c\u4e0d\u5fa9\u5b78\u76f4\u696d\u3002", "413": "\u300c\u516d\u8005\uff0c\u76f4\u65b9\u4fbf\u5df2\u8db3\uff0c\u4e0d\u5fa9\u5b78\u76f4\u65b9\u4fbf\u3002", "414": "\u300c\u4e03\u8005\uff0c\u76f4\u5ff5\u5df2\u8db3\uff0c\u4e0d\u5fa9\u5b78\u76f4\u5ff5\u3002", "415": "\u300c\u516b\u8005\uff0c\u76f4\u5b9a\u5df2\u8db3\uff0c\u4e0d\u5fa9\u5b78\u76f4\u5b9a\u3002", "416": "\u300c\u4e5d\u8005\uff0c\u76f4\u5f97\u5ea6\u4e16\u5df2\u8db3\uff0c\u4e0d\u5fa9\u5b78\u76f4\u5f97\u5ea6\u4e16\u3002", "417": "\u300c\u5341\u8005\uff0c\u76f4\u6167\u5df2\u8db3\uff0c\u4e0d\u5fa9\u5b78\u76f4\u6167\u3002\u662f\u70ba\u5b78\u884c\u8005\u767e", "418": "\u6cd5\u6cd5\u767e\u8aaa\u3002\u662f\u4e0d\u975e\u3001\u662f\u4e0d\u7570\uff0c\u6709\u8ae6\u5982\uff0c\u6709\u4e0d\u60d1\u4e0d", "419": "\u5012\uff0c\u662f\u5982\u6709\u6301\u6167\u610f\u89c0\u3002", "42": "\u6240\u61c9\u5f97\u4e0d\u5f97\uff0c\u6240\u61c9\u89e3\u7121\u89e3\uff0c\u6240\u61c9\u81ea\u77e5\u4e0d\u81ea", "420": "\u300c\u6240\u4e0a\u8aaa\u5b78\u8005\u807d\u8aaa\u6cd5\uff0c\u4e0a\u8aaa\u4ea6\u6de8\uff0c\u4e2d\u8aaa\u4ea6\u6de8\uff0c", "421": "\u5df2\u7adf\u8981\u8aaa\u4ea6\u6de8\uff0c\u6709\u5229\u6709\u597d\u8db3\u5177\u6de8\u7adf\u884c\u5df2\u898b\uff0c", "422": "\u662f\u540d\u70ba\u5341\u5831\u6cd5\uff0c\u5982\u61c9\u662f\u4e0a\u8aaa\uff0c\u70ba\u662f\u6545\u8aaa\u3002\u300d\u820d\u5229", "423": "\u66f0\u5df2\u8aaa\u7adf\uff0c\u8af8\u53d7\u8457\u5fc3\u8499\u6069\u3002", "424": "\u9577\u963f\u542b\u5341\u5831\u6cd5\u7d93\u5377\u4e0b", "43": "\u77e5\u3002\u662f\u70ba\u4e8c\u77a2\u77a2\u7a2e\u3002", "44": "\u300c\u6216\u6709\u6642\u884c\u8005\uff0c\u6216\u6642\u61c9\u51fa\u884c\u9053\uff0c\u4fbf\u610f\u751f\uff1a\u300e\u6211\u70ba\u61c9", "45": "\u51fa\u884c\u9053\uff0c\u6211\u4e0d\u80fd\u51fa\u884c\u9053\uff0c\u4e0d\u80fd\u53d7\u6559\u8aa1\u884c\uff0c\u4ee4", "46": "\u6211\u50be\u81e5\u3002\u300f\u4e0d\u5fa9\u6c42\u5ea6\u4e16\u65b9\u4fbf\uff0c\u7576\u5f97\u672a\u5f97\uff0c\u7576\u89e3", "47": "\u672a\u89e3\uff0c\u7576\u81ea\u77e5\u672a\u81ea\u77e5\u3002\u662f\u70ba\u4e09\u77a2\u77a2\u7a2e\u3002", "48": "\u300c\u6216\u6642\u884c\u8005\uff0c\u665d\u65e5\u884c\u9053\u3002\u610f\u8a08\uff1a\u300e\u671d\u884c\u9053\u4f86\uff0c\u5ff5\u8eab", "49": "\u7fb8\u4e0d\u80fd\u5750\u884c\uff0c\u4ee4\u6211\u50be\u81e5\u3002\u300f\u4fbf\u50be\u81e5\u3002\u7121\u6709\u5ea6\u4e16\u65b9", "5": "\u56e0\u7de3\u3002", "50": "\u4fbf\uff0c\u7576\u5f97\u4e0d\u5f97\uff0c\u7576\u89e3\u4e0d\u89e3\uff0c\u7576\u81ea\u77e5\u8b49\u4e0d\u81ea\u77e5", "51": "\u8b49\u3002\u662f\u70ba\u56db\u77a2\u77a2\u7a2e\u3002", "52": "\u300c\u6216\u6642\u884c\u8005\uff0c\u61c9\u597d\u884c\uff0c\u4fbf\u8a08\uff1a\u300e\u6211\u61c9\u597d\u884c\uff0c\u6211\u4e0d\u80fd", "53": "\u884c\uff0c\u4e0d\u80fd\u5949\u53d7\u6559\u8aa1\uff0c\u4ee4\u6211\u9808\u81fe\u9593\u50be\u81e5\u3002\u300f\u4fbf\u50be", "54": "\u81e5\u3002\u4e0d\u6c42\u5ea6\u4e16\u65b9\u4fbf\uff0c\u61c9\u53ca\u8005\u4e0d\u53ca\uff0c\u61c9\u89e3\u8005\u4e0d", "55": "\u89e3\uff0c\u61c9\u81ea\u77e5\u8b49\u4e0d\u81ea\u77e5\u8b49\u3002\u662f\u70ba\u4e94\u77a2\u77a2\u7a2e\u3002", "56": "\u300c\u6216\u6642\u884c\u8005\u8a08\uff1a\u300e\u6211\u671d\u4ee5\u884c\u9053\uff0c\u5df2\u8eab\u7fb8\u4e0d\u80fd\u5750\uff0c", "57": "\u4ee4\u6211\u50be\u81e5\u3002\u300f\u5df2\u50be\u81e5\u3002\u4e0d\u6c42\u5ea6\u4e16\u65b9\u4fbf\uff0c\u61c9\u5f97\u4e0d", "58": "\u5f97\uff0c\u61c9\u89e3\u4e0d\u89e3\uff0c\u61c9\u81ea\u77e5\u8b49\u4e0d\u81ea\u77e5\u8b49\u3002\u662f\u70ba\u516d\u77a2", "59": "\u77a2\u7a2e\u3002", "6": "\u300c\u5df2\u4f9d\u4f5b\u4ea6\u9918\u6167\u8005\u540c\u5b78\u8005\uff0c\u5f97\u6642\u6642\u805e\u5fae\u6cd5\u7d93\uff0c", "60": "\u300c\u6216\u6642\u884c\u8005\uff0c\u5df2\u5f97\u75c5\u82e6\uff0c\u4fbf\u5ff5\uff1a\u300e\u6211\u5df2\u82e6\u5f97\u75c5\uff0c\u8eab", "61": "\u7fb8\u4e0d\u80fd\u884c\u4e0d\u80fd\u5750\uff0c\u4ee4\u6211\u50be\u81e5\u3002\u300f\u4fbf\u50be\u81e5\uff0c\u7121\u6709\u5ea6", "62": "\u4e16\u65b9\u4fbf\uff0c\u7576\u5f97\u4e0d\u5f97\uff0c\u7576\u89e3\u4e0d\u89e3\uff0c\u7576\u81ea\u77e5\u8b49\u4e0d", "63": "\u81ea\u77e5\u8b49\u3002\u662f\u70ba\u4e03\u77a2\u77a2\u7a2e\u3002", "64": "\u300c\u6216\u6642\u884c\u8005\uff0c\u9069\u5f9e\u75c5\u8d77\uff0c\u4e0d\u4e45\u4fbf\u5ff5\uff1a\u300e\u6211\u70ba\u9069\u5f9e", "65": "\u75c5\u8d77\uff0c\u8eab\u7fb8\u4e0d\u80fd\u884c\u5750\uff0c\u4ee4\u6211\u50be\u81e5\u3002\u300f\u4fbf\u50be\u81e5\uff0c\u4e0d", "66": "\u6c42\u5ea6\u4e16\u65b9\u4fbf\uff0c\u7576\u5f97\u4e0d\u5f97\uff0c\u7576\u89e3\u4e0d\u89e3\uff0c\u7576\u81ea\u77e5", "67": "\u8b49\u4e0d\u81ea\u77e5\u8b49\u3002\u662f\u70ba\u516b\u77a2\u77a2\u7a2e\u3002", "68": "\u300c\u7b2c\u516d\u516b\u6cd5\uff0c\u884c\u589e\u9053\uff0c\u516b\u7cbe\u9032\u65b9\u4fbf\u9053\u3002\u884c\u8005\uff0c\u82e5\u5728", "69": "\u90e1\u3001\u5728\u7e23\u3001\u5728\u805a\uff0c\u4ea6\u9918\u8655\u4f9d\u884c\uff0c\u6e05\u671d\u8d77\uff0c\u8457\u8863\u6301\u61c9", "7": "\u662f\u589e\u672c\u884c\uff0c\u4e0d\u5f97\u672c\u6167\u4fbf\u5f97\u672c\u6167\u3002\u662f\u70ba\u4e8c\u6cd5\u56e0", "70": "\u5668\uff0c\u5165\u90e1\u7e23\u6c42\u98df\u3002\u610f\u8a08\uff1a\u300e\u7576\u5f97\u591a\u53ef\u5649\u98df\u3002\u300f\u4e0d\u5f97", "71": "\u591a\u53ef\u5649\u98df\uff0c\u81ea\u610f\u8a08\uff1a\u300e\u6211\u671d\u4e0d\u5f97\u591a\u53ef\u5649\u98df\u3002\u8eab", "72": "\u8f15\u80fd\u884c\u5750\uff0c\u4ee4\u6211\u4f5c\u65b9\u4fbf\u3002\u300f\u672a\u5f97\u4ee4\u5f97\uff0c\u672a\u89e3\u4ee4", "73": "\u89e3\uff0c\u672a\u81ea\u77e5\u4ee4\u81ea\u77e5\u3002\u662f\u70ba\u4e00\u7cbe\u9032\u65b9\u4fbf\u3002", "74": "\u300c\u6216\u6642\u884c\u8005\u3002\u82e5\u5728\u90e1\u3001\u5728\u7e23\u3001\u5728\u805a\uff0c\u4ea6\u9918\u8655\u4f9d\u884c\uff0c\u6e05", "75": "\u671d\u8d77\uff0c\u8457\u8863\u6301\u61c9\u5668\uff0c\u5165\u90e1\u7e23\u6c42\u98df\u3002\u610f\u8a08\uff1a\u300e\u7576\u5f97", "76": "\u591a\u53ef\u5649\u98df\u3002\u300f\u4fbf\u5f97\u591a\u53ef\u5649\u98df\uff0c\u4fbf\u5ff5\uff1a\u300e\u5df2\u671d\u5f97\u591a", "77": "\u53ef\u5649\u98df\uff0c\u8eab\u6709\u529b\u80fd\u524d\u5750\u884c\uff0c\u4ee4\u6211\u6c42\u65b9\u4fbf\u3002\u300f\u672a", "78": "\u5f97\u7576\u5f97\uff0c\u672a\u89e3\u7576\u89e3\uff0c\u672a\u81ea\u77e5\u7576\u81ea\u77e5\u3002\u662f\u70ba", "79": "\u4e8c\u7cbe\u9032\u65b9\u4fbf\u3002", "8": "\u7de3\u3002\u300c\u5df2\u805e\u6cd5\uff0c\u5374\u8eab\u5374\u610f\uff0c\u5f9e\u662f\u672c\u884c\u56e0\u7de3\uff0c\u4e0d\u5f97\u6167", "80": "\u300c\u6216\u6642\u884c\u8005\uff0c\u7576\u51fa\u884c\u610f\u751f\uff1a\u300e\u6211\u70ba\u61c9\u51fa\uff0c\u8eab\u4e0d\u80fd", "81": "\u884c\uff0c\u4ea6\u4e0d\u80fd\u53d7\u6559\u8aa1\u884c\uff0c\u4ee4\u6211\u6559\u52c5\u6c42\u65b9\u4fbf\u3002\u300f\u70ba", "82": "\u81ea\u4f5c\u9053\u65b9\u4fbf\uff0c\u672a\u5f97\u8005\u81f4\u5f97\uff0c\u672a\u89e3\u8005\u81f4\u89e3\uff0c\u672a", "83": "\u81ea\u77e5\u81f4\u81ea\u77e5\u3002\u662f\u70ba\u4e09\u7cbe\u9032\u65b9\u4fbf\u3002", "84": "\u300c\u6216\u6642\u884c\u8005\uff0c\u5df2\u884c\u9053\u751f\uff1a\u300e\u6211\u5df2\u884c\u9053\u4f86\uff0c\u4e0d\u80fd\u81ea", "85": "\u884c\u9053\uff0c\u4e0d\u80fd\u5949\u4e8b\u6559\u8aa1\uff0c\u4ee4\u6211\u958b\u6240\u72af\u3002\u300f\u4ee4\u6709\u65b9", "86": "\u4fbf\uff0c\u672a\u5f97\u7576\u5f97\uff0c\u672a\u89e3\u7576\u89e3\uff0c\u672a\u81ea\u77e5\u7576\u81ea\u77e5\uff0c\u662f", "87": "\u70ba\u56db\u7cbe\u9032\u65b9\u4fbf\u3002", "88": "\u300c\u6216\u6642\u884c\u8005\uff0c\u61c9\u884c\u4fbf\u5ff5\uff1a\u300e\u6211\u4e0d\u80fd\u4f5c\u884c\u6210\u6559\uff0c\u6216", "89": "\u4ee4\u6211\u5c45\u524d\u6c42\u65b9\u4fbf\u3002\u300f\u4fbf\u524d\u884c\u65b9\u4fbf\uff0c\u672a\u5f97\u7576\u5f97\uff0c", "9": "\u4fbf\u5f97\u6167\u3002\u662f\u70ba\u4e09\u6cd5\u56e0\u7de3\u3002", "90": "\u672a\u89e3\u7576\u89e3\uff0c\u672a\u81ea\u77e5\u7576\u81ea\u77e5\u3002\u662f\u70ba\u4e94\u7cbe\u9032\u65b9", "91": "\u4fbf\u3002", "92": "\u300c\u6216\u6642\u884c\u8005\uff0c\u76e1\u884c\u4fbf\u5ff5\uff1a\u300e\u6211\u5df2\u76e1\u884c\uff0c\u4e0d\u80fd\u5fa9\u884c\uff0c", "93": "\u6210\u6559\u8aa1\uff0c\u4ee4\u6211\u80fd\u5f97\u9589\u6240\u72af\u3002\u300f\u4fbf\u6c42\u65b9\u4fbf\u6240\u72af", "94": "\u9589\uff0c\u672a\u5f97\u7576\u5f97\uff0c\u672a\u89e3\u7576\u89e3\uff0c\u672a\u81ea\u77e5\u7576\u81ea\u77e5\u3002\u662f", "95": "\u70ba\u516d\u7cbe\u9032\u65b9\u4fbf\u3002", "96": "\u300c\u6216\u6642\u884c\u8005\uff0c\u8eab\u6709\u75c5\u82e6\u6975\uff0c\u4fbf\u5ff5\uff1a\u300e\u6211\u6709\u75c5\u82e6\u6975\uff0c\u6709", "97": "\u6642\u5f9e\u662f\u75c5\u6b7b\uff0c\u5ff5\u6211\u9808\u81fe\u9593\u6c42\u65b9\u4fbf\u884c\u3002\u300f\u672a\u5f97\u7576", "98": "\u5f97\uff0c\u672a\u89e3\u7576\u89e3\uff0c\u672a\u81ea\u77e5\u7576\u81ea\u77e5\u3002\u662f\u70ba\u4e03\u7cbe\u9032\u65b9", "99": "\u4fbf\u3002"}, "source": "CBETA", "language": "chinese", "originalTitle": "Taisho Tripitaka, Electronic version, No. 0013 \u9577\u963f\u542b\u5341\u5831\u6cd5\u7d93: \u9577\u963f\u542b\u5341\u5831\u6cd5\u7d93: \u9577\u963f\u542b\u5341\u5831\u6cd5\u7d93", "author": "\u5f8c\u6f22 \u5b89\u4e16\u9ad8\u8b6f", "sourceLink": "http://www.cbeta.org/"}