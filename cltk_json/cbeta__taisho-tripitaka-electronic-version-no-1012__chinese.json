u8af8\u5929\u4eba\u4e00\u5207\u9858\u4e0d\u96e3", "115": "\u6b64\u70ba\u9053\u5fc3\u751f\u4f46\u7576\u6b63\u610f\u884c", "116": "\u5fa9\u6b21\u820d\u5229\u5f17\u3002\u83e9\u85a9\u6210\u5c31\u56db\u6cd5\u3002\u75be\u5f97\u662f\u6301\u3002\u4f55\u7b49", "117": "\u70ba\u56db\u3002\u53ad\u96e2\u611b\u6b32\u3002\u4e0d\u751f\u75be\u5fc3\u3002\u65bc\u4e00\u5207\u773e\u751f\u65bd", "118": "\u4e00\u5207\u6709\u3002\u665d\u591c\u6c42\u6cd5\u5fc3\u4e0d\u61c8\u60d3\u3002\u723e\u6642\u4e16\u5c0a\u800c\u8aaa", "119": "\u5048\u8a00\u3002", "120": "\u53ad\u96e2\u65bc\u8272\u6b32\u9b54\u754c\u5730\u7344\u7de3", "121": "\u653e\u9038\u9020\u773e\u60e1\u60e1\u4ee4\u58ae\u60e1\u9053", "122": "\u65bc\u4ed6\u4e0d\u8d77\u59ac\u4e0d\u70ba\u65bc\u540d\u7a31", "123": "\u610d\u611b\u8996\u773e\u751f\u5c0a\u8cb4\u8272\u7aef\u56b4", "124": "\u773e\u751f\u8d77\u6028\u8acd\u7686\u4ee5\u8caa\u70ba\u672c", "125": "\u662f\u6545\u7576\u6368\u611b\u611b\u6368\u6210\u5999\u6301", "126": "\u665d\u591c\u5e38\u6a02\u6cd5\u63a8\u7570\u63a8\u9053\u6c42", "127": "\u5c08\u7cbe\u6c42\u9053\u8005\u75be\u5f97\u6210\u5999\u6301", "128": "\u53c8\u820d\u5229\u5f17\u3002\u83e9\u85a9\u5fa9\u6709\u56db\u6cd5\u3002\u6210\u5c31\u75be\u5f97\u662f\u6301\u3002\u4f55", "129": "\u7b49\u70ba\u56db\u3002\u9591\u5c45\u5bc2\u5fd7\u3002\u5f97\u6df1\u6cd5\u5fcd\u3002\u4e0d\u6c42\u5229\u990a\u3002\u4e00\u5207", "130": "\u5167\u5916\u76e1\u65bd\u7121\u907a\u3002\u662f\u70ba\u56db\u3002\u723e\u6642\u4e16\u5c0a\u800c\u8aaa\u5048\u8a00\u3002", "131": "\u9591\u5c45\u5bc2\u5fd7\u4f5b\u6240\u8b7d\u81ea\u5b88\u7a7a\u975c\u4e0d\u8f15\u5f7c", "132": "\u5e38\u6a02\u4fee\u7fd2\u6df1\u6cd5\u5fcd\u5982\u6551\u982d\u7136\u4e0d\u653e\u9038", "133": "\u4e0d\u6c42\u540d\u8b7d\u53ca\u5229\u990a\u56e0\u65af\u9020\u8af8\u8ac2\u66f2\u884c", "134": "\u8655\u8655\u77e5\u8db3\u5982\u98db\u9ce5\u7bc0\u8eab\u640d\u5df1\u4fee\u4ec1\u884c", "135": "\u5feb\u54c9\u5f97\u4f5b\u6559\u51fa\u5bb6\u53d7\u5177\u6212", "136": "\u6368\u5bb6\u96e2\u773e\u82e6\u9075\u4fee\u65bc\u4f5b\u6559", "137": "\u82e5\u4e0d\u5f97\u4f5b\u6559\u51fa\u5bb6\u662f\u6212\u5229", "138": "\u4fe1\u6212\u65bd\u805e\u6167\u7686\u6089\u4e0d\u6210\u5c31", "139": "\u6b64\u904e\u96e2\u4f5b\u9060\u662f\u6545\u6368\u8caa\u5229", "140": "\u53c8\u820d\u5229\u5f17\u3002\u83e9\u85a9\u5fa9\u6709\u56db\u6cd5\u3002\u6210\u5c31\u75be\u5f97\u662f\u6301\u3002\u4f55", "141": "\u7b49\u70ba\u56db\u3002\u4e00\u66f0\u516b\u5b57\u662f\u7fa9\u96a8\u7fa9\u6089\u5165\u5176\u4e2d\u3002\u516b\u5b57", "142": "\u8005\u6ce2\u7f85\u5a46\u8fe6\u95cd\u9640\u8cd2\u53c9\u3002\u6ce2\u8005\u7b2c\u4e00\u7fa9\u3002\u4e00\u5207", "143": "\u8af8\u6cd5\u7121\u6211\u6089\u4f86\u5165\u9580\u3002\u7f85\u8005\u76f8\u597d\u7121\u76f8\u3002\u5982\u5982\u4f86", "144": "\u8eab\u5165\u65bc\u6cd5\u6027\u3002\u5a46\u8005\u611a\u7661\u4e4b\u6cd5\u53ca\u667a\u6167\u6cd5\u3002\u96a8\u9806", "145": "\u5165\u7fa9\u3002\u8fe6\u8005\u696d\u884c\u679c\u5831\u53ca\u975e\u696d\u884c\u5831\u3002\u7686\u6089\u7a76\u7adf", "146": "\u96a8\u9806\u5165\u7fa9\u3002\u95cd\u8005\u751f\u7de3\u8001\u6b7b\u7686\u6089\u96a8\u5165\u4e0d\u751f\u4e0d", "147": "\u6ec5\u3002\u9640\u8005\u4e00\u5207\u8af8\u6301\u6089\u96a8\u5165\u7a7a\u7121\u76f8\u7121\u9858\u3002\u8cd2\u8005", "148": "\u4e00\u5207\u8af8\u6301\u7686\u96a8\u9806\u5165\u3002\u53c9\u8005\u76e1\u4e00\u5207\u8af8\u6cd5\u6089\u5165", "149": "\u65bc\u5982\u662f\u3002\u5982\u662f\u516b\u5b57\u662f\u7fa9\u6089\u5165\u5176\u4e2d\u3002\u4e8c\u66f0\u5584", "150": "\u66f8\u6b64\u7d93\u57f7\u6301\u4e0d\u6368\u3002\u4e09\u66f0\u534a\u6708\u534a\u6708\u8aa6\u7fd2\u6b64\u7d93\u3002", "151": "\u56db\u66f0\u5e1d\u7576\u4f9b\u7d66\u4f9b\u990a\u6301\u6b64\u7d93\u8005\u3002\u723e\u6642\u4e16\u5c0a\u800c", "152": "\u8aaa\u5048\u8a00\u3002", "153": "\u7576\u601d\u516b\u5b57\u7fa9\u5584\u66f8\u6301\u6b64\u7d93", "154": "\u534a\u6708\u6b21\u4f86\u8aa6\u52f8\u767c\u884c\u6301\u8005", "155": "\u4e00\u5207\u5341\u65b9\u6052\u6c99\u570b\u7121\u91cf\u8af8\u4f5b\u5728\u5176\u524d", "156": "\u5f97\u5750\u9053\u5834\u5999\u76f8\u6eff\u7576\u52e4\u4fee\u7fd2\u6b64\u6301\u884c", "157": "\u5fa9\u6b21\u820d\u5229\u5f17\u3002\u83e9\u85a9\u5b78\u6b64\u6301\u8005\u5f97\u56db\u529f\u5fb7\u5229\u3002\u4f55", "158": "\u7b49\u70ba\u56db\u3002\u4e00\u8005\u5341\u65b9\u8af8\u4f5b\u6240\u5ff5\u3002\u4e8c\u8005\u96e2\u8af8\u9b54\u4e8b\u3002", "159": "\u4e09\u8005\u60e1\u7f6a\u696d\u969c\u75be\u5f97\u6e05\u6de8\u3002\u56db\u8005\u7121\u7919\u65b7\u8faf\u624d", "160": "\u7686\u6089\u6210\u5c31\u3002\u662f\u70ba\u56db\u3002\u723e\u6642\u4e16\u5c0a\u800c\u8aaa\u5048\u8a00\u3002", "161": "\u8af8\u4f5b\u5e38\u8b77\u5ff5\u773e\u9b54\u4e0d\u5f97\u4fbf", "162": "\u7f6a\u696d\u969c\u6089\u9664\u5f97\u5999\u7121\u91cf\u6301", "163": "\u4f5b\u544a\u820d\u5229\u5f17\u3002\u4e43\u6614\u904e\u53bb\u7121\u6578\u52ab\u6642\u3002\u6709\u4f5b\u4e16\u5c0a", "164": "\u865f\u5bf6\u9996\u7130\u738b\u5982\u4f86\u61c9\u4f9b\u7b49\u6b63\u89ba\u660e\u884c\u6210\u5584\u901d\u4e16", "165": "\u9593\u89e3\u7121\u4e0a\u58eb\u8abf\u5fa1\u4e08\u592b\u5929\u4eba\u5e2b\u4f5b\u4e16\u5c0a\u3002\u820d\u5229", "166": "\u5f17\u5f7c\u5bf6\u9996\u7130\u738b\u5982\u4f86\u81e8\u6ec5\u5ea6\u6642\u3002\u6709\u8f49\u8f2a\u738b\u540d", "167": "\u66f0\u6301\u5149\u660e\u3002\u738b\u56db\u5929\u4e0b\u5feb\u5f97\u81ea\u5728\u3002\u738b\u6709\u592a\u5b50\u540d", "168": "\u7121\u5ff5\u5fb7\u9053\u3002\u5176\u521d\u751f\u6642\u5f62\u5982\u5341\u516d\u6b72\u7ae5\u5b50\u3002\u5f9e\u5f7c", "169": "\u4e16\u5c0a\u5f97\u805e\u6b64\u6301\u3002\u805e\u6b64\u6301\u5df2\u5c08\u7cbe\u6c42\u5b78\u3002\u7d93\u4e03\u5343", "170": "\u6b72\u4e0d\u70ba\u7761\u7720\u4e4b\u6240\u852d\u84cb\u3002\u4e03\u5343\u6b72\u4e2d\u672a\u66fe\u8d77\u8eab", "171": "\u611b\u3002\u672a\u66fe\u5ff5\u738b\u4e8b\u3002\u53c8\u4e03\u5343\u6b72\u4e2d\u8eab\u4e0d\u7317\u81e5\u7aef\u5750", "172": "\u6b63\u53d7\u3002\u65bc\u662f\u5373\u898b\u4e5d\u5341\u5104\u90a3\u7531\u4ed6\u8af8\u4f5b\u3002\u5f9e\u5176\u805e", "173": "\u6cd5\u7686\u80fd\u53d7\u6301\u3002\u4fbf\u51fa\u5bb6\u5b78\u9053\u3002\u4e5d\u842c\u6b72\u4e2d\u4ee5\u662f\u7121", "174": "\u91cf\u9580\u51fa\u751f\u4e4b\u6301\u5ee3\u70ba\u4eba\u8aaa\u3002\u65bc\u4e00\u751f\u4e2d\u6210\u5c31\u516b", "175": "\u5341\u5104\u90a3\u7531\u4ed6\u4eba\u3002\u7acb\u963f\u8028\u591a\u7f85\u4e09\u85d0\u4e09\u83e9\u63d0\u5fc3\u3002", "176": "\u52f8\u9032\u6559\u5316\u4ee4\u4e0d\u9000\u8f49\u3002\u5f7c\u6642\u773e\u4e2d\u6709\u9577\u8005\u5b50\u540d", "177": "\u66f0\u6708\u5e62\u3002\u5f97\u805e\u662f\u6301\u81f3\u5fc3\u96a8\u559c\u3002\u5f7c\u4ee5\u96a8\u559c\u5584\u6839", "178": "\u56e0\u7de3\u6545\u3002\u5e38\u5f97\u89aa\u8fd1\u4e5d\u5341\u5104\u4f5b\u3002\u5f97\u6700\u4e0a\u6301\u5f97\u6700", "179": "\u4e0a\u8655\u7121\u91cf\u8faf\u624d\u3002\u5fa9\u65bc\u4e09\u52ab\u89aa\u8fd1\u8af8\u4f5b\u3002\u4e09\u52ab\u672b", "180": "\u6642\u6708\u5e62\u5f97\u4f5b\u865f\u71c8\u5149\u5982\u4f86\u61c9\u4f9b\u7b49\u6b63\u89ba\u3002\u5f7c\u6642", "181": "\u738b\u5b50\u7121\u5ff5\u5fb7\u9053\u8005\u3002\u4eca\u897f\u65b9\u7121\u91cf\u58fd\u4f5b\u662f\u3002\u820d\u5229", "182": "\u5f17\u6211\u4ea6\u8207\u8ce2\u52ab\u8af8\u83e9\u85a9\u3002\u66fe\u805e\u6b64\u6301\u805e\u5df2\u96a8\u559c\u3002", "183": "\u4ee5\u6b64\u96a8\u559c\u5584\u6839\u56e0\u7de3\u3002\u9664\u5374\u56db\u5341\u842c\u52ab\u751f\u6b7b\u4e4b", "184": "\u7f6a\u3002\u53c8\u5f97\u89aa\u8fd1\u4e5d\u5341\u5104\u4f5b\u3002\u662f\u6545\u820d\u5229\u5f17\u82e5\u83e9\u85a9", "185": "\u6469\u8a36\u85a9\u3002\u6b32\u5f97\u75be\u6210\u963f\u8028\u591a\u7f85\u4e09\u85d0\u4e09\u83e9\u63d0\u8005\u3002", "186": "\u65bc\u6b64\u5999\u6301\u7576\u52e4\u96a8\u559c\u3002\u96a8\u559c\u529f\u5fb7\u3002\u4e43\u81f3\u83e9\u85a9\u4e0d", "187": "\u9000\u8f49\u5730\u4f5c\u5927\u56e0\u7de3\u3002\u6cc1\u5fa9\u66f8\u6301\u53d7\u8aa6\u5982\u8aaa\u4fee\u884c\u3002", "188": "\u4e00\u5207\u4eba\u6c11\u83ab\u80fd\u601d\u91cf\u7a31\u8aaa\u5176\u798f\u3002\u723e\u6642\u4e16\u5c0a\u800c", "189": "\u8aaa\u5048\u8a00\u3002", "190": "\u805e\u7d93\u5fc3\u96a8\u559c\u66f8\u6301\u656c\u8af7\u8aa6", "191": "\u5929\u4eba\u83ab\u80fd\u8a08\u798f\u5831\u4e4b\u9650\u91cf", "192": "\u751f\u751f\u5e38\u898b\u4f5b\u4fe1\u5411\u4e0d\u8ff4\u52d5", "193": "\u9ad4\u89e3\u6df1\u6cd5\u8981\u75be\u89ba\u7121\u4e0a\u9053", "194": "\u4e09\u6627\u4e0d\u9000\u6e1b\u4e09\u660e\u7e3d\u6301\u9580", "195": "\u8ca1\u8272\u5e38\u898b\u4f5b\u4e43\u81f3\u7121\u4e0a\u9053", "196": "\u61b6\u5ff5\u904e\u53bb\u4e16\u9577\u8005\u5b50\u805e\u6301", "197": "\u5373\u898b\u4e5d\u5341\u5104\u90a3\u7531\u4ed6\u8af8\u4f5b", "198": "\u6240\u6c42\u5982\u5176\u9858\u5f97\u6210\u7121\u4e0a\u9053", "199": "\u82e5\u6b32\u75be\u6210\u4f5b\u9053\u529b\u964d\u773e\u9b54", "200": "\u6b32\u6eff\u767e\u798f\u76f8\u884c\u6b64\u975e\u6709\u96e3", "201": "\u5982\u6eff\u6052\u6c99\u524e\u7a4d\u5bf6\u4ee5\u5e03\u65bd", "202": "\u6bd4\u66f8\u6301\u6b64\u7d93\u7d42\u4e0d\u5f97\u70ba\u6bd4", "203": "\u53c8\u820d\u5229\u5f17\u83e9\u85a9\u884c\u6b64\u6301\u8005\u3002\u6709\u516b\u5927\u9b3c\u662f\u96ea\u5c71", "204": "\u4e4b\u795e\u3002\u5171\u8b77\u8996\u4e4b\u3002\u5176\u540d\u66f0\u52c7\u5065\u795e\u5f37\u529b\u795e\u3002\u81ea\u5728", "205": "\u795e\u96c4\u731b\u795e\u3002\u77e5\u884c\u795e\u96e3\u52dd\u795e\u3002\u9ce9\u6469\u7f85\u795e\u5584\u81c2\u795e\u3002", "206": "\u662f\u70ba\u516b\u3002\u5f7c\u6b32\u4ee4\u8af8\u795e\u4f86\u8005\u3002\u7576\u6fa1\u6d74\u5176\u8eab\u6de8\u5176", "207": "\u8863\u670d\u3002\u6b63\u8272\u7d93\u884c\u6148\u5ff5\u773e\u751f\u3002\u601d\u662f\u6cd5\u8981\u5982\u5176\u61b6", "208": "\u5ff5\u3002\u5f7c\u8af8\u795e\u7b49\u5c0b\u73fe\u5728\u524d\u3002\u5fa9\u6709\u516b\u83e9\u85a9\u751f\u6b32\u884c", "209": "\u5929\u3002\u4ea6\u5e38\u8b77\u5ff5\u3002\u4f55\u7b49\u70ba\u516b\u3002\u7167\u660e\u5341\u65b9\u5929\u5b50\u96e2", "210": "\u6b32\u884c\u5929\u5b50\u3002\u6167\u5149\u5929\u5b50\u5982\u65e5\u5929\u5b50\u3002\u771f\u8ae6\u5929\u5b50\u9858", "211": "\u6eff\u5929\u5b50\u3002\u661f\u738b\u5929\u5b50\u77e5\u884c\u5929\u5b50\u3002\u662f\u70ba\u516b\u3002\u662f\u7b49", "212": "\u83e9\u85a9\u5e38\u4f86\u8b77\u5ff5\u3002\u4ee4\u5f7c\u5c08\u7cbe\u75be\u5f97\u5999\u6301\u3002\u884c\u662f\u6301", "213": "\u8005\u771f\u5be6\u70ba\u672c\u3002\u800c\u5e38\u606d\u656c\u7576\u5ff5\u77e5\u6069\u3002\u5b78\u6df1\u6cd5\u5fcd", "214": "\u7b49\u89c0\u7121\u751f\u7121\u6ec5\u4e4b\u6cd5\u3002\u5e73\u7b49\u6301\u6212\u81ea\u8b77\u517c\u5f7c\u3002\u8aaa", "215": "\u662f\u6cd5\u6642\u4e09\u5341\u4e8c\u6052\u6cb3\u6c99\u7b49\u83e9\u85a9\u5c0b\u5f97\u662f\u6301\u3002\u4e0d", "216": "\u9000\u8f49\u65bc\u963f\u8028\u591a\u7f85\u4e09\u85d0\u4e09\u83e9\u63d0\u3002\u516d\u5341\u842c\u4eba\u5f97", "217": "\u4e0d\u8d77\u6cd5\u5fcd\u3002\u4e09\u842c\u4e8c\u5343\u5929\u8207\u4eba\u3002\u7686\u767c\u7121\u4e0a\u6b63\u771f", "218": "\u9053\u610f\u3002\u662f\u6642\u4e09\u5343\u5927\u5343\u4e16\u754c\u516d\u7a2e\u9707\u52d5\u3002\u5929\u96e8\u773e", "219": "\u83ef\u666e\u904d\u4e16\u754c\u3002\u5929\u767c\u4f0e\u6a02\u4e0d\u9f13\u81ea\u9cf4\u3002\u662f\u6642\u820d\u5229", "220": "\u5f17\u767d\u4f5b\u8a00\u3002\u4e16\u5c0a\u7576\u4f55\u540d\u6b64\u7d93\u3002\u4f5b\u544a\u820d\u5229\u5f17\u3002\u6b64", "221": "\u7d93\u4e4b\u8981\u540d\u51fa\u751f\u7121\u91cf\u9580\u6301\u3002\u4ea6\u540d\u4e00\u751f\u88dc\u8655\u9053", "222": "\u884c\u3002\u4ea6\u540d\u6210\u9053\u964d\u9b54\u5f97\u4e00\u5207\u667a\u3002\u7576\u5949\u6301\u4e4b\u3002\u8aaa\u662f", "223": "\u6cd5\u6642\u5c0a\u8005\u820d\u5229\u5f17\u3002\u53ca\u8af8\u5341\u65b9\u4f86\u6703\u83e9\u85a9\u3002\u5e76\u8af8", "224": "\u773e\u6703\u5929\u8207\u4eba\u4e7e\u95e5\u5a46\u963f\u4fee\u7f85\u7b49\u3002\u805e\u4f5b\u6240\u8aaa\u7686", "225": "\u5927\u6b61\u559c\u3002", "226": "\u4f5b\u8aaa\u51fa\u751f\u7121\u91cf\u9580\u6301\u7d93"}, "originalTitle": "Taisho Tripitaka, Electronic version, No. 1012 \u4f5b\u8aaa\u51fa\u751f\u7121\u91cf\u9580\u6301\u7d93", "source": "CBETA"}