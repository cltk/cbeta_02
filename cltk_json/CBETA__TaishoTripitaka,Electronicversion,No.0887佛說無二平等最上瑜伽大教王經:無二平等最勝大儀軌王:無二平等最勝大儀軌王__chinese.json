60c5\u754c\u3002\u80fd\u6eff\u4e00\u5207\u9858\u3002\u70ba\u4e00\u5207\u5982\u4f86\u5171\u6240\u7a31\u8b9a\u3002", "230": "\u53c8\u89c0\u6e05\u6de8\u5927\u4e73\u6d77\u6709\u7a2e\u7a2e\u8272\u5999\u5149\u660e", "231": "\u6708\u66fc\u62cf\u7f85\u73fe\u5176\u4e2d\u65bc\u4e2d\u5fa9\u60f3\u65e5\u8f2a\u76f8", "232": "\u6b21\u89c0\u81ea\u8eab\u5373\u4f5b\u8eab\u76f8\u61c9\u51fa\u751f\u8af8\u5100\u6cd5", "233": "\u7531\u60f3\u81ea\u8eab\u904d\u4e00\u5207\u5468\u5ee3\u4e00\u5207\u4e16\u754c\u91cf", "234": "\u53c8\u60f3\u5927\u6d77\u65bc\u5fc3\u73fe\u65e5\u66fc\u62cf\u7f85\u5728\u6d77\u4e2d", "235": "\u4f9d\u6cd5\u89c0\u60f3\u8af8\u4f5b\u76f8\u5468\u531d\u5149\u660e\u8f2a\u570d\u7e5e", "236": "\u5f7c\u5149\u660e\u4e2d\u73fe\u6587\u5b57\u5468\u5ee3\u4e00\u5207\u4e16\u754c\u91cf", "237": "\u5420\u6ce5\u90aa\u7b49\u6240\u4f5c\u7528\u4f9d\u6cd5\u76f8\u61c9\u800c\u89c0\u60f3", "238": "\u5fa9\u6b21\u4f9d\u6cd5\u800c\u89c0\u60f3\u6708\u66fc\u62cf\u7f85\u4e00\u8098\u91cf", "239": "\u65bc\u4e2d\u51fa\u73fe\u91d1\u525b\u528d\u80fd\u4f5c\u4e16\u9593\u6210\u5c31\u6cd5", "24": "\u53d7\u4e00\u5207\u5982\u4f86\u52f8\u8acb\u8aaa\u6cd5\u3002", "240": "\u6b21\u60f3\u7130\u5149\u767d\u84ee\u83ef\u8868\u793a\u83e9\u63d0\u5fc3\u838a\u56b4", "241": "\u4e2d\u60f3\u672c\u90e8\u8ce2\u8056\u76f8\u5de6\u53f3\u5b89\u5e03\u8e30\u59cb\u591a", "242": "\u5982\u662f\u4f9d\u6cd5\u800c\u651d\u53d7\u81ea\u91d1\u525b\u6cd5\u7686\u6210\u5c31", "243": "\u666e\u904d\u4e00\u5207\u4e16\u754c\u4e2d\u5982\u662f\u4f9b\u990a\u4e00\u5207\u4f5b", "244": "\u6b64\u540d\u666e\u8ce2\u7955\u5bc6\u6cd5\u9580\u3002", "245": "\u53c8\u60f3\u65e5\u8f2a\u66fc\u62cf\u7f85\u5176\u91cf\u7b49\u4e00\u591a\u7f85\u6a39", "246": "\u4e2d\u60f3\u6f38\u7565\u5927\u660e\u5b57\u4e43\u81f3\u4e00\u5207\u76f8\u61c9\u751f", "247": "\u5805\u56fa\u5b89\u4f4f\u81ea\u5f71\u50cf\u7136\u7576\u89c0\u60f3\u8ce2\u8056\u7b49", "248": "\u5fc3\u5927\u660e\u5b57\u5404\u5b89\u5df2\u666e\u7528\u4f9b\u990a\u4e00\u5207\u4f5b", "249": "\u6b21\u60f3\u56db\u5370\u66fc\u62cf\u7f85\u91cf\u5404\u7b49\u4e03\u591a\u7f85\u6a39", "25": "\u5fa9\u6709\u6b91\u4f3d\u6c99\u6578\u7b49\u8af8\u4f5b\u5982\u4f86\u3002\u5f9e\u5341\u65b9\u754c\u4f86\u96c6", "250": "\u66fc\u62cf\u7f85\u4e2d\u8af8\u8ce2\u8056\u5404\u60f3\u5fc3\u660e\u5b57\u5b89\u5e03", "251": "\u6b64\u540d\u91d1\u525b\u4e09\u6627\u6cd5\u9580\u3002", "252": "\u6b21\u7576\u89c0\u60f3\u66fc\u62cf\u7f85\u5176\u91cf\u7b49\u4e00\u4ff1\u76e7\u820d", "253": "\u4e2d\u60f3\u81ea\u8eab\u6210\u4f5b\u8eab\u53ca\u5f7c\u4f5b\u773c\u83e9\u85a9\u7b49", "254": "\u4e00\u5207\u7686\u5f9e\u81ea\u5fc3\u60f3\u5927\u667a\u6240\u89c0\u800c\u6210\u5c31", "255": "\u5f7c\u5fc3\u5982\u662f\u5f97\u76f8\u61c9\u5373\u6210\u5927\u58eb\u76f8\u61c9\u89c0", "256": "\u53c8\u60f3\u81ea\u5fc3\u66fc\u62cf\u7f85\u91cf\u5fa9\u7b49\u4e00\u4ff1\u76e7\u820d", "257": "\u89c0\u81ea\u5f71\u50cf\u7121\u6240\u751f\u4e43\u81f3\u865b\u7a7a\u7121\u904e\u8d8a", "258": "\u81ea\u76f8\u61c9\u9580\u5805\u56fa\u4f4f\u89c0\u60f3\u65bc\u4f5b\u5373\u76f8\u61c9", "259": "\u4f5b\u76f8\u61c9\u9580\u5805\u56fa\u4f4f\u4f5b\u4f5b\u89c0\u60f3\u5373\u76f8\u61c9", "26": "\u6b64\u6703\u3002\u7336\u5982\u80e1\u9ebb\u6eff\u95bb\u6d6e\u63d0\u3002\u662f\u8af8\u5982\u4f86\u5404\u65bc\u5f7c", "260": "\u89c0\u60f3\u76f8\u61c9\u83e9\u63d0\u5fc3\u5176\u91cf\u5ee3\u5927\u904d\u4e00\u5207", "261": "\u4e00\u5207\u8ce2\u8056\u5f9e\u662f\u751f\u4f9d\u6cd5\u89c0\u60f3\u5927\u660e\u5b57", "262": "\u89c0\u60f3\u76f8\u61c9\u5999\u84ee\u83ef\u5176\u91cf\u5ee3\u4e00\u591a\u7f85\u6a39", "263": "\u81ea\u5fc3\u5b89\u4f4f\u800c\u4e0d\u52d5\u60f3\u6210\u81ea\u5fc3\u66fc\u62cf\u7f85", "264": "\u53c8\u60f3\u81ea\u5fc3\u66fc\u62cf\u7f85\u5176\u91cf\u5ee3\u4e00\u591a\u7f85\u6a39", "265": "\u60f3\u81ea\u5f71\u50cf\u73fe\u5176\u4e2d\u4e00\u5207\u6240\u4f5c\u5584\u5805\u56fa", "266": "\u53c8\u60f3\u7130\u9b18\u767d\u6de8\u5149\u4e94\u516d\u4f9d\u6b21\u800c\u89c0\u60f3", "267": "\u60f3\u81ea\u5f71\u50cf\u8655\u5176\u4e2d\u5de6\u53f3\u8e30\u59cb\u591a\u5b89\u5e03", "268": "\u5ee3\u5927\u5149\u660e\u4f9d\u6cd5\u73fe\u81ea\u5fc3\u767d\u7b49\u9918\u4e03\u8272", "269": "\u5982\u662f\u5149\u660e\u904d\u865b\u7a7a\u65bc\u4e2d\u60f3\u73fe\u8af8\u4f5b\u76f8", "27": "\u5f7c\u4f5b\u524e\u3002\u4ea6\u5e38\u8aaa\u6b64\u7121\u4e8c\u5e73\u7b49\u7955\u5bc6\u6cd5\u9580\u3002", "270": "\u60f3\u81ea\u5f71\u50cf\u904d\u865b\u7a7a\u4e00\u5207\u7686\u5f9e\u81ea\u5fc3\u73fe", "271": "\u81ea\u8eab\u4f5b\u8eab\u672c\u76f8\u61c9\u4e00\u5207\u76f8\u7b49\u4f9d\u6cd5\u7528", "272": "\u6b21\u7576\u89c0\u60f3\u8af8\u83e9\u85a9\u5468\u531d\u904d\u6eff\u865b\u7a7a\u754c", "273": "\u81ea\u76f8\u5982\u7406\u800c\u76f8\u61c9\u8af8\u4f5b\u83e9\u85a9\u540c\u89c0\u60f3", "274": "\u4f9d\u6cd5\u60f3\u53ec\u8af8\u8ce2\u8056\u5b89\u4f4f\u865b\u7a7a\u66fc\u62cf\u7f85", "275": "\u81ea\u76f8\u771f\u5be6\u7406\u76f8\u61c9\u5373\u6210\u83e9\u63d0\u5fc3\u89c0\u60f3", "276": "\u5927\u660e\u6587\u5b57\u7b49\u76f8\u61c9\u5982\u5be6\u89c0\u5f7c\u6587\u5b57\u76f8", "277": "\u5f9e\u83e9\u63d0\u5fc3\u6240\u51fa\u751f\u71be\u76db\u5149\u660e\u800c\u56b4\u98fe", "278": "\u5f9e\u5149\u51fa\u73fe\u8af8\u5100\u6cd5\u81ea\u5f71\u50cf\u8207\u8af8\u4f5b\u7b49", "279": "\u81ea\u4ed6\u5f71\u50cf\u6089\u76f8\u61c9\u7130\u9b18\u5149\u660e\u7b49\u7167\u8000", "28": "\u723e\u6642\u91d1\u525b\u624b\u5927\u57f7\u91d1\u525b\u8005\u3002\u5b89\u4f4f\u4e00\u5207\u5982\u4f86\u5fc3\u3002", "280": "\u6b21\u60f3\u4f9d\u6cd5\u800c\u51fa\u73fe\u4f5b\u773c\u83e9\u85a9\u7b49\u8ce2\u8056", "281": "\u4ee5\u5f7c\u5f7c\u5fc3\u5927\u660e\u5b57\u6708\u66fc\u62cf\u7f85\u4e2d\u5b89\u5e03", "282": "\u91d1\u525b\u85a9\u57f5\u771f\u5be6\u76f8\u904d\u5165\u5fae\u5999\u5149\u660e\u4e2d", "283": "\u6b64\u540d\u6301\u91d1\u525b\u6cd5\u9580\u3002", "284": "\u53c8\u60f3\u91d1\u525b\u85a9\u57f5\u751f\u81ea\u5fc3\u5f71\u50cf\u66fc\u62cf\u7f85", "285": "\u662f\u76f8\u5982\u7406\u89c0\u76f8\u61c9\u7570\u6b64\u800c\u89c0\u975e\u6210\u5c31", "286": "\u53c8\u60f3\u91d1\u525b\u85a9\u57f5\u751f\u65e5\u8f2a\u66fc\u62cf\u7f85\u4e2d\u73fe", "287": "\u91d1\u525b\u9264\u7d22\u7b49\u8af8\u76f8\u651d\u53d7\u5f1f\u5b50\u5165\u58c7\u5834", "288": "\u53c8\u60f3\u91d1\u525b\u85a9\u57f5\u751f\u4e00\u5207\u6cd5\u5f9e\u81ea\u5fc3\u73fe", "289": "\u4ee5\u6587\u5b57\u7b49\u5047\u65bd\u8a2d\u662f\u6545\u58ae\u65bc\u6587\u5b57\u6578", "29": "\u4fee\u7fd2\u4e00\u5207\u5982\u4f86\u884c\u3002\u96a8\u4e00\u5207\u5982\u4f86\u8eab\u6240\u884c\u3002\u9806\u4e00", "290": "\u6301\u8aa6\u4e43\u81f3\u6240\u4f5c\u4e8b\u4e00\u4e00\u4f4f\u65bc\u79aa\u5b9a\u5fc3", "291": "\u82e5\u5982\u662f\u4fee\u5373\u6210\u5c31\u5f7c\u6301\u660e\u8005\u540d\u6700\u52dd", "292": "\u6b64\u540d\u4e00\u5207\u5982\u4f86\u4e09\u6627\u6cd5\u9580\u3002", "293": "\u5f9e\u81ea\u5f71\u50cf\u800c\u51fa\u751f\u672c\u90e8\u6cd5\u4e2d\u8ae6\u89c0\u60f3", "294": "\u7531\u60f3\u81ea\u5fc3\u4f9d\u6cd5\u4f4f\u4e00\u5207\u9b54\u6028\u7686\u7834\u6563", "295": "\u6b64\u4e2d\u5982\u662f\u6c42\u6089\u5730\u8af8\u89c0\u60f3\u7b49\u4f9d\u5100\u8ecc", "296": "\u65e5\u8f2a\u66fc\u62cf\u7f85\u76f8\u61c9\u89c0\u60f3\u73fe\u8af8\u660e\u738b\u76f8", "297": "\u7130\u9b18\u90e8\u4e2d\u6240\u73fe\u5149\u7686\u7576\u89c0\u60f3\u5fff\u6012\u76f8", "298": "\u5f7c\u6240\u8aaa\u76f8\u4f9d\u6cd5\u5100\u662f\u540d\u6c7a\u5b9a\u771f\u5be6\u8a9e", "299": "\u61c9\u7576\u5982\u662f\u5982\u7406\u89c0\u7570\u6b64\u800c\u89c0\u975e\u6210\u5c31", "3": "\u897f\u5929\u8b6f\u7d93\u4e09\u85cf\u671d\u5949\u5927\u592b\u8a66\u5149\u797f\u537f", "30": "\u5207\u5982\u4f86\u5fc3\u6240\u4f5c\u3002\u5982\u662f\u7e3d\u651d\u672c\u90e8\u76f8\u61c9\u4f9d\u672c\u6cd5", "300": "\u6b64\u540d\u4e00\u5207\u5982\u4f86\u666e\u8ce2\u6cd5\u9580\u3002", "301": "\u60f3\u81ea\u5f71\u50cf\u66fc\u62cf\u7f85\u4e2d\u6709\u71be\u76db\u5927\u5149\u660e", "302": "\u5176\u5149\u5ee3\u7167\u8af8\u4e16\u754c\u7686\u5f9e\u771f\u5be6\u667a\u5fc3\u73fe", "303": "\u81ea\u8eab\u5fa9\u73fe\u84ee\u83ef\u4e2d\u6e05\u6de8\u5149\u660e\u6240\u7167\u8000", "304": "\u662f\u8eab\u96d6\u73fe\u6709\u76f8\u8eab\u81ea\u6027\u5e73\u7b49\u800c\u7121\u8457", "305": "\u4f9d\u6cd5\u89c0\u60f3\u5fc3\u6587\u5b57\u662f\u5373\u91d1\u525b\u85a9\u57f5\u76f8", "306": "\u91d1\u525b\u85a9\u57f5\u76f8\u5e73\u7b49\u662f\u5373\u89c0\u60f3\u8af8\u4f5b\u76f8", "307": "\u4e86\u77e5\u5927\u660e\u6559\u51fa\u751f\u7576\u7372\u4e00\u5207\u6089\u5730\u6cd5", "308": "\u662f\u6545\u5927\u660e\u6700\u5c0a\u52dd\u96e2\u5927\u660e\u6559\u7121\u6709\u4e0a", "309": "\u6b64\u540d\u4e00\u5207\u5982\u4f86\u5805\u56fa\u52c7\u731b\u6cd5\u9580\u3002", "31": "\u4f4f\u3002\u65bc\u6b64\u6703\u4e2d\u96b1\u8eab\u4e0d\u73fe\u3002\u662f\u6642\u5341\u65b9\u6240\u96c6\u4e00\u5207", "310": "\u7576\u60f3\u65e5\u8f2a\u66fc\u62cf\u7f85\u4e2d\u73fe\u6bd8\u76e7\u906e\u90a3\u4f5b", "311": "\u601d\u5ff5\u5f7c\u4f5b\u5fc3\u5927\u660e\u5f7c\u660e\u6210\u5c31\u4e00\u5207\u6cd5", "312": "\u5f9e\u6b64\u5927\u660e\u5fa9\u51fa\u751f\u5e0c\u6709\u6700\u4e0a\u7955\u5bc6\u5100", "313": "\u4e00\u5207\u5f9e\u5fc3\u5982\u7406\u73fe\u7336\u6708\u6e05\u6de8\u800c\u7167\u8000", "314": "\u5fa9\u60f3\u5f9e\u81ea\u5f71\u50cf\u751f\u91d1\u525b\u528d\u7b49\u6210\u5c31\u76f8", "315": "\u800c\u5f7c\u51fa\u751f\u4f9d\u6cd5\u5100\u60f3\u73fe\u5ee3\u5927\u5149\u660e\u805a", "316": "\u5177\u7a2e\u7a2e\u8272\u751a\u5fae\u5999\u666e\u7167\u4e16\u754c\u7121\u908a\u91cf", "317": "\u6b64\u540d\u4e00\u5207\u5982\u4f86\u4e09\u6627\u773c\u6cd5\u9580\u3002", "318": "\u4f5b\u8aaa\u7121\u4e8c\u5e73\u7b49\u6700\u4e0a\u745c\u4f3d\u5927\u6559\u738b\u7d93\u5377\u7b2c\u4e00", "32": "\u5982\u4f86\u3002\u65e2\u65bc\u6703\u4e2d\u4e0d\u898b\u91d1\u525b\u624b\u83e9\u85a9\u3002\u5373\u6642\u7570\u53e3", "33": "\u540c\u97f3\u8aaa\u4f3d\u9640\u66f0\u3002", "34": "\u5927\u54c9\u91d1\u525b\u624b\u5982\u6211\u7b49\u7236\u6bcd", "35": "\u4e0d\u73fe\u6b64\u6703\u4e2d\u5927\u58eb\u4eca\u4f55\u5f80", "36": "\u5f7c\u8af8\u5982\u4f86\u4f5c\u662f\u8aaa\u5df2\u3002\u5404\u5404\u65bc\u5176\u5927\u91d1\u525b\u5bf6\u52a0", "37": "\u6301\u6cd5\u4e2d\u3002\u9ed8\u7136\u800c\u4f4f\u3002", "38": "\u723e\u6642\u91d1\u525b\u624b\u5927\u57f7\u91d1\u525b\u8005\u3002\u5373\u5f9e\u4e00\u5207\u5982\u4f86\u4e94", "39": "\u8655\u800c\u51fa\u3002\u4ee5\u81ea\u8eab\u76f8\u73fe\u5927\u6bd8\u76e7\u906e\u90a3\u4f5b\u76f8\u3002\u5fa9\u5f9e", "4": "\u50b3\u6cd5\u5927\u5e2b\u8cdc\u7d2b\u81e3\u65bd\u8b77\u5949\u3000\u8a54\u8b6f", "40": "\u4e00\u5207\u5982\u4f86\u5fc3\u3002\u73fe\u5927\u6708\u66fc\u62cf\u7f85\u3002\u6709\u767e\u5343\u7a2e\u5713", "41": "\u5149\u7167\u8000\u3002\u5982\u662f\u73fe\u5df2\u3002\u5fa9\u5f9e\u4e00\u5207\u5982\u4f86\u5fc3\u3002\u73fe\u5927\u65e5", "42": "\u66fc\u62cf\u7f85\u3002\u91cf\u5ee3\u7121\u6578\u767e\u5343\u4e16\u754c\u3002\u9084\u5fa9\u672c\u8eab\u3002\u65bc\u5176", "43": "\u4e2d\u9593\u5b89\u8a73\u800c\u5750\u3002", "44": "\u723e\u6642\u6240\u96c6\u4e00\u5207\u5982\u4f86\u3002\u5373\u6642\u5404\u5404\u5411\u91d1\u525b\u624b\u83e9", "45": "\u85a9\u3002\u5408\u638c\u9802\u79ae\u52f8\u8acb\u8aaa\u6cd5\u3002\u8aaa\u4f3d\u9640\u66f0\u3002", "46": "\u5584\u54c9\u91d1\u525b\u6700\u4e0a\u6cd5\u5584\u54c9\u91d1\u525b\u6e05\u6de8\u773c", "47": "\u5584\u54c9\u91d1\u525b\u6700\u52dd\u667a\u666e\u651d\u8af8\u4f5b\u4e00\u5207\u667a", "48": "\u6211\u7b49\u52f8\u8acb\u91d1\u525b\u624b\u666e\u70ba\u8af8\u4f5b\u4f5c\u5229\u6a02", "49": "\u5ba3\u8aaa\u81ea\u4ed6\u667a\u6240\u751f\u6700\u4e0a\u5927\u4e58\u7955\u5bc6\u6cd5", "5": "1 \u5f71\u50cf\u5206\u7121\u4e8c\u5e73\u7b49\u6700\u52dd\u5927\u5100\u8ecc\u738b\u5f71\u50cf\u5206\u7b2c\u4e00", "50": "\u723e\u6642\u91d1\u525b\u624b\u5927\u57f7\u91d1\u525b\u8005\u3002\u767d\u8af8\u5982\u4f86\u8a00\u3002\u8af8\u4f5b", "51": "\u5982\u4f86\u4e0d\u61c9\u52f8\u8acb\u3002\u4ee4\u6211\u5ba3\u8aaa\u7955\u5bc6\u6cd5\u9580\u3002\u8af8\u5982\u4f86", "52": "\u8a00\u4f55\u56e0\u7de3\u6545\u800c\u6c5d\u4e0d\u8aaa\u3002\u91d1\u525b\u624b\u8a00\u8af8\u4f5b\u7576\u77e5\u3002", "53": "\u4eca\u6b64\u5927\u6703\u8af8\u83e9\u85a9\u6469\u8a36\u85a9\u773e\u4e2d\u3002\u6216\u6709\u805e\u8aaa\u5982", "54": "\u662f\u5927\u4e58\u7b2c\u4e00\u7fa9\u8ae6\u7955\u5bc6\u6cd5\u5df2\u3002\u4e43\u6709\u591a\u7a2e\u7834\u58de", "55": "\u7b49\u76f8\u3002\u8af8\u5982\u4f86\u8a00\u3002\u6211\u7b49\u5404\u4ee5\u795e\u529b\u52a0\u6301\u6703\u773e\u3002\u5fa9", "56": "\u4ee4\u6089\u6210\u5927\u91d1\u525b\u773e\u3002\u6c5d\u4eca\u4f46\u7576\u65bd\u4f5c\u4f5b\u4e8b\u3002", "57": "\u723e\u6642\u91d1\u525b\u624b\u5927\u57f7\u91d1\u525b\u8005\u3002\u7199\u6021\u5fae\u7b11\u77bb\u4ef0\u8af8", "58": "\u4f5b\u3002\u8b9a\u8af8\u4f5b\u8a00\u5584\u54c9\u5584\u54c9\u3002\u5373\u5165\u4e00\u5207\u5982\u4f86\u5927\u8f2a", "59": "\u5149\u660e\u666e\u7167\u4e00\u5207\u5982\u4f86\u5fc3\u4e09\u6469\u5730\u3002\u5f9e\u662f\u4e09\u6469\u5730", "6": "\u5982\u662f\u6211\u805e\u3002\u4e00\u6642\u4e16\u5c0a\u3002\u5728\u4ed6\u5316\u81ea\u5728\u5929\u5bae\u3002\u8207\u4e5d", "60": "\u51fa\u5df2\u3002\u65bc\u5176\u56db\u65b9\u73fe\u56db\u5927\u65e5\u66fc\u62cf\u7f85\u3002\u6771\u65b9\u540d\u6301", "61": "\u7121\u76e1\u8272\u5927\u65e5\u66fc\u62cf\u7f85\u3002\u5357\u65b9\u540d\u5982\u610f\u5bf6\u91d1\u525b\u5927", "62": "\u65e5\u66fc\u62cf\u7f85\u3002\u897f\u65b9\u540d\u7121\u91cf\u91d1\u525b\u5927\u65e5\u66fc\u62cf\u7f85\u3002\u5317", "63": "\u65b9\u540d\u4e0d\u7a7a\u4e09\u6627\u91d1\u525b\u5927\u65e5\u66fc\u62cf\u7f85\u3002\u5982\u662f\u73fe\u5df2\u3002", "64": "\u4e43\u70ba\u4e00\u5207\u5982\u4f86\u4e00\u5207\u83e9\u85a9\u773e\u6703\u3002\u5ba3\u8aaa\u7121\u4e8c\u5e73", "65": "\u7b49\u6700\u52dd\u5927\u5100\u8ecc\u738b\u7955\u5bc6\u6cd5\u9580\u3002\u5176\u6240\u5ba3\u8aaa\u8207\u4e00", "66": "\u5207\u83e9\u85a9\u5982\u7406\u76f8\u61c9\u3002\u4ee4\u4e00\u5207\u83e9\u85a9\u6210\u5c31\u4e00\u5207\u5982", "67": "\u4f86\u6700\u52dd\u529f\u5fb7\u795e\u901a\u4e8b\u696d\u3002\u666e\u904d\u773e\u6703\u3002\u4e43\u81f3\u76e1\u5341", "68": "\u65b9\u754c\u4e00\u5207\u4f5b\u83e9\u85a9\u5982\u7406\u800c\u4f4f\u3002\u8b6c\u5982\u865b\u7a7a\u5149\u660e", "69": "\u904d\u7167\u4e09\u5343\u5927\u5343\u4e16\u754c\u3002\u6240\u6709\u4e00\u5207\u65e5\u8f2a\u5149\u660e\u3002\u96a8", "7": "\u5341\u4e5d\u9808\u5f4c\u5c71\u91cf\u5fae\u5875\u6578\u83e9\u85a9\u6469\u8a36\u85a9\u773e\u4ff1\u3002\u6b64", "70": "\u91cf\u800c\u7167\u4ea6\u6089\u5468\u904d\u3002\u6240\u6709\u4e00\u5207\u5982\u4f86\u5149\u660e\u91d1\u525b", "71": "\u624b\u83e9\u85a9\u5149\u660e\u4ea6\u5fa9\u5982\u662f\u3002\u5149\u5149\u6df7\u878d\u5e73\u7b49\u7167\u8000\u3002", "72": "\u5982\u5be6\u89c0\u8005\u5982\u7406\u800c\u4f4f\u3002", "73": "\u723e\u6642\u4e00\u5207\u5982\u4f86\u3002\u5373\u4ee5\u4e00\u5207\u5982\u4f86\u666e\u8ce2\u51fa\u751f\u4f9b", "74": "\u990a\u96f2\u6d77\u3002\u4f9b\u990a\u91d1\u525b\u624b\u83e9\u85a9\u3002\u4f5c\u4f9b\u990a\u5df2\u3002\u5f7c\u8af8\u5982", "75": "\u4f86\u9ed8\u7136\u800c\u4f4f\u3002", "76": "\u723e\u6642\u91d1\u525b\u624b\u5927\u57f7\u91d1\u525b\u8005\u3002\u5373\u5165\u89c0\u7167\u4e00\u5207\u83e9", "77": "\u85a9\u5fc3\u667a\u90e8\u51fa\u751f\u4e00\u5207\u5982\u4f86\u6700\u4e0a\u7955\u5bc6\u7834\u8af8\u6697\u56e0", "78": "\u81ea\u4e09\u6469\u5730\u3002\u5f9e\u662f\u4e09\u6469\u5730\u51fa\u5df1\u3002\u5ba3\u8aaa\u667a\u90e8\u4e09\u6469", "79": "\u5730\u6cd5\u9580\u3002", "8": "\u8af8\u83e9\u85a9\u4e00\u4e00\u7686\u662f\u91d1\u525b\u85a9\u57f5\u91d1\u525b\u7737\u5c6c\u3002\u5176\u540d\u66f0", "80": "\u7576\u4f4f\u79aa\u5b9a\u5fc3\u89c0\u6e05\u6de8\u4e73\u6d77", "81": "\u4e2d\u6709\u5927\u84ee\u83ef\u5982\u6c34\u7cbe\u6708\u5149", "82": "\u89c0\u81ea\u5fc3\u7a2e\u5b50\u8207\u81ea\u6cd5\u76f8\u61c9", "83": "\u7531\u662f\u89c0\u60f3\u6210\u81ea\u5fc3\u66fc\u62cf\u7f85", "84": "\u800c\u6b64\u66fc\u62cf\u7f85\u7e31\u5ee3\u4e00\u8098\u91cf", "85": "\u56db\u65b9\u53ca\u56db\u9685\u56db\u5bf6\u52dd\u838a\u56b4", "86": "\u4f9d\u6cd5\u800c\u60f3\u6210\u4f4f\u5fc3\u4e0d\u6563\u4e82", "87": "\u5468\u531d\u6709\u5149\u660e\u71be\u76db\u666e\u7167\u8000", "88": "\u5f7c\u4e2d\u4f9d\u6cd5\u5100\u89c0\u81ea\u8eab\u6e05\u6de8", "89": "\u96e2\u6211\u7b49\u5206\u5225\u5373\u60f3\u6210\u4f5b\u8eab", "9": "\u91d1\u525b\u624b\u83e9\u85a9\u6469\u8a36\u85a9\u3002\u666e\u8ce2\u83e9\u85a9\u6469\u8a36\u85a9\u3002\u89c0\u81ea", "90": "\u5982\u662f\u89c0\u60f3\u5df2\u6210\u5c31\u66fc\u62cf\u7f85", "91": "\u6b21\u4f9d\u667a\u90e8\u5100\u5927\u660e\u5982\u662f\u7a31", "92": "\u5535\u5f15\u4e00\u53e5\u85a9\u54e9\u56a9\u4e8c\u5408\u601b\u4ed6\u5f15\u8a90\u591a\u4e8c\u502a\u4e5f\u4e8c\u5408\u5f15\u90a3\u838e", "93": "\u5a46\u5f15\u56a9\u5f15\u601b\u6469\u4e8c\u5408\u9164\u5f15\u3c20\u547c\u90ce\u5207\u4e09", "94": "\u5927\u660e\u6240\u7528\u5370\u4e26\u4f9d\u667a\u90e8\u5100", "95": "\u800c\u6b64\u5927\u660e\u529b\u80fd\u6ec5\u4e00\u5207\u7f6a", "96": "\u89c0\u60f3\u6709\u767e\u5343\u7121\u57a2\u6700\u4e0a\u5149", "97": "\u5fa9\u4f9d\u667a\u90e8\u5100\u89c0\u60f3\u660e\u738b\u7b49", "98": "\u5fae\u5999\u767d\u6de8\u5149\u4f9d\u6cd5\u60f3\u5b89\u5e03", "99": "\u6240\u4f5c\u4f9d\u6b64\u8aaa\u7570\u6b64\u975e\u89c0\u60f3"}, "source": "CBETA", "language": "chinese", "originalTitle": "Taisho Tripitaka, Electronic version, No. 0887 \u4f5b\u8aaa\u7121\u4e8c\u5e73\u7b49\u6700\u4e0a\u745c\u4f3d\u5927\u6559\u738b\u7d93: \u7121\u4e8c\u5e73\u7b49\u6700\u52dd\u5927\u5100\u8ecc\u738b: \u7121\u4e8c\u5e73\u7b49\u6700\u52dd\u5927\u5100\u8ecc\u738b", "author": "\u5b8b \u65bd\u8b77\u8b6f", "sourceLink": "http://www.cbeta.org/"}