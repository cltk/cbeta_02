02\u4eca\u61c9", "191": "\u66f4\u7576\u6368\u81ea\u5167\u8ca1\u6240\u6709\u8eab\u547d\u3002\u4f9b\u990a\u8af8\u4f5b\u83e9\u85a9\u8056", "192": "\u773e\u3002\u5373\u61c9\u767c\u9858\u4f5c\u5982\u662f\u8a00\u3002\u4f0f\u9858\u8af8\u8056\u773e\u8207\u6211\u4f5c", "193": "\u5927\u8b77\u52a0\u6301\u3002\u5e38\u651d\u53d7\u3002\u5982\u662f\u4e09\u767d\u8acb\u3002\u8056\u773e\u4e0d\u9055\u9858\u3002", "194": "\u4fbf\u5373\u61fa\u8af8\u7f6a\u3002", "195": "\u7121\u59cb\u5341\u60e1\u696d\u9858\u7686\u76e1\u92b7\u6ec5", "196": "\u5fa9\u61c9\u66f4\u8ff4\u5411\u4eca\u6211\u6240\u4fee\u5584", "197": "\u5ff5\u8aa6\u8af8\u529f\u5fb7\u8ff4\u8207\u8af8\u6709\u60c5", "198": "\u8ff4\u5411\u83e9\u63d0\u679c\u8aa6\u6b64\u660e\u52a0\u6301", "199": "\u4ee4\u9858\u4e0d\u865b\u767c\u5176\u660e\u8aa6\u516b\u904d", "200": "\u660e\u66f0\u3002", "201": "\u66e9\u6469\u85a9\u56a9\u6c92\u99b1\udb80\udc9b\u5730\u85a9\u601b\u56a9\u4e8c\u5408\u5357\u66e9\u8b28\u7d20", "202": "\u89a9\u4e8c\u5408\u5e1d\u83bd\u8cc0\u56a9\u65e5\u56c9\u546c\u85a9\u56a9\u546c\u8e6c\u8fe6\u56c9\u5e95", "203": "\u745f\u4f98\u4e8c\u5408\u85a9\u35da\u8b0e\u4e8c\u5408\u56c9\u9ebc\u4e8c\u5408\u601b\u56c9\u4e8c\u5408\u85a9", "204": "\u35da\u8b0e\u4e8c\u5408\u6bba\u9054\u6469\u62cf\u83bd\u5730\u745f\u4f98\u4e8c\u5408\u8212\u5a11\u56a9", "205": "\u4e8c\u5408\u5f15\u8a36\u5f15", "206": "\u6b21\u61c9\u66f4\u7576\u7d50\u524d\u704c\u9802\u5370\u3002\u8aa6\u660e\u81ea\u704c\u9802\u5df2\u3002\u6b21\u7d50", "207": "\u865b\u7a7a\u773c\u5370\u3002\u8b77\u81ea\u8eab\u53ca\u8b77\u672c\u5c0a\u3002\u5176\u5370\u865b\u5fc3\u5408", "208": "\u638c\u3002\u5c48\u4e8c\u982d\u6307\u982d\u81f3\u4e2d\u6307\u7b2c\u4e00\u7bc0\u3002\u4e8c\u5927\u6307\u4e26\u7aea", "209": "\u5373\u6210\u3002\u8aa6\u6b64\u660e\u66f0\u3002", "210": "\u66e9\u6469\u6089\u5e95\u54e9\u4e8c\u5408\u91ce\u5a1c\u56a9\u4e8c\u5408\u62cf\u5b7d\u5e1d\udb84\udc3d\u85a9", "211": "\u56a9\u601b\u4f57\u5f15\u5b7d\u591a\udb84\udc3d\u5535\u8a90\u8a90\u66e9\u8005\u511e\u8a90\u8a90\u4e09", "212": "\u6469\u85a9\u56a9\u89a9\u5695\u4e8c\u5408\u8a90\u591a\u5e95\u5a11\u56c9\u4e09\u5a46\u5420\u5165\u56a9", "213": "\u4e8c\u5408\u56c9\u66e9\u8b28\u963f\u4f3d\u5357\u5a11\u56a9\u4e8c\u5408\u5f15\u8a36", "214": "\u8aa6\u660e\u7d50\u5370\u8b77\u8eab\u8a16\u3002\u5373\u4ef0\u958b\u6b64\u5370\u3002\u5411\u4e0a\u5982\u6367\u72c0\u3002", "215": "\u4fbf\u6210\u6367\u5ff5\u73e0\u5370\u3002\u5373\u8aa6\u6b64\u660e\u3002\u7528\u52a0\u6301\u5ff5\u8aa6\u901f\u6210", "216": "\u5c31\u660e\u66f0\u3002", "217": "\u66e9\u8b28\u56a9\u65e5\u56c9\u4e8c\u5408\u76ee\u5951\u85a9\u56a9\u601b\u4ed6\u5f15\udb80\udef2\u5e1d\u6bd8", "218": "\u5ebe\u4e8c\u5408\u5a46\u8a90\u9441\u6301\u56a9\u4e8c\u5408\u6bd8\u85e5\u4e8c\u5408\u601b\u511e\u4e5f\u4e8c", "219": "\u5408\u4ed6\u77ef\u550e\u5f65\u99b1\u550e\u6230\u8336\u91cc\u9ebc\u8e6c\u7947\u8cd3\u5f15", "220": "\u8a90\u91cc\u601b\u4ed6\u5f15\u5b7d\u591a\u5fae\u66f3\u4e8c\u5408\u4f7f\u591a\u837c\u5e95\u543d", "221": "\u5165\u56a9\u4e8c\u5408\u91cc\u591a\u5e1d\u901d\u4f0a\u80fd\u8fe6\u56c9\u7130\u53e5\u5695\u4e8c\u5408\u5a11", "222": "\u56a9\u4e8c\u5408\u8a36", "223": "\u6b21\u7d50\u6cd5\u754c\u751f\u5370\u3002\u81ea\u52a0\u6301\u4ee4\u8af8\u969c\u4e0d\u751f\u3002\u5176\u5370\u4ee5", "224": "\u5de6\u53f3\u982d\u6307\u4e8c\u7121\u540d\u6307\u3002\u5c48\u5165\u65bc\u638c\u4e2d\u9762\u76f8\u9264\u3002\u7aea\u4e8c", "225": "\u5927\u6307\u4e8c\u4e2d\u6307\u4e8c\u76f8\u3002\u982d\u76f8\u62c4\u958b\u3002\u4ee5\u5370\u6309\u5de6\u81c2\u3002", "226": "\u6b21\u53f3\u81f3\u9802\u4e0a\u6563\u3002\u6bcf\u6309\u8655\u8aa6\u6b64\u660e\u4e00\u904d\u660e\u66f0\u3002", "227": "\u66e9\u83ab\u85a9\u56a9\u6c92\u99b1\u80c3\u5730\u85a9\u601b\u56a9\u4e8c\u5408\u5357\u963f\u5f15\u85a9", "228": "\u56a9\u4ed6\u85a9\u56a9\u601b\u56c9\u4e8c\u5408\u8def\u8a08\u5a11\u56a9\u4e8c\u5408\u5f15\u8a36\u5f15", "229": "\u6b21\u5373\u66f4\u7576\u7d50\u524d\u865b\u7a7a\u773c\u5370\u8aa6\u660e\u4e03\u904d\u4ea6\u540d\u90e8\u5fc3\u8aa6", "230": "\u5df2\u5373\u7576\u8ae6\u89c0\u3002\u8af8\u4f5b\u83e9\u85a9\u6b77\u7136\u6eff\u7a7a\u3002\u672c\u5c0a\u8056\u773e", "231": "\u89aa\u5c0d\u5728\u5df1\u76ee\u524d\u3002\u5b89\u5fc3\u5b9a\u610f\u5169\u624b\u57f7\u6301\u5ff5\u73e0\u3002\u7576", "232": "\u5fc3\u5f90\u5ff5\u8aa6\u3002\u4e43\u81f3\u75b2\u6975\u904d\u6578\u4efb\u610f\u3002\u6bcf\u65e5\u4e09\u6642", "233": "\u5ff5\u8aa6\u4e0d\u4ee4\u9593\u65b7\u3002\u904d\u6578\u81f3\u6975\u4e0b\u5c11\u4e0d\u5f97\u4e0b\u65bc\u4e00", "234": "\u767e\u516b\u904d\u3002\u5ff5\u8aa6\u6578\u8a16\u5df2\u3002\u5373\u8aa6\u865b\u7a7a\u773c\u660e\u52a0\u6301\u3002\u7d50", "235": "\u8b77\u5ff5\u73e0\u7f6e\u65bc\u672c\u8655\u3002\u6b21\u5373\u5374\u7d50\u6839\u5370\u8aa6\u767e\u5b57", "236": "\u660e\u3002\u52a0\u6301\u81ea\u8eab\u4ee4\u901f\u6210\u5c31\u660e\u66f0\u3002", "237": "\u5535\u963f\u4e09\u9ebc\u4e09\u9ebc\u4e09\u66fc\u591a\u89a9\u5a1c\u591a\u53bb\u601b\u56a9\u5fc5", "238": "\u5e95\u4e8c\u5408\u820d\u85a9\u511e\u8cc0\u56c9\u8cc0\u56c9\u5a11\u9ebc\u4e8c\u5408\u56c9\u62cf\u5a11\u9ebc", "239": "\u4e8c\u5408\u62cf\u5fae\u9913\u591a\u6c92\u99b1\u9054\u6469\u5e1d\u5a11\u7f85\u5a11\u7f85\u3002", "240": "\u4e09\u9ebc\u56a9\u56c9\u8377\u56c9\u8377\u56c9\u601b\u56c9\u4e8c\u5408\u91ce\u601b\u56c9\u4e8c\u5408", "241": "\u91ce\u4f3d\u90a3\u4f3d\u90a3\u6469\u8cc0\u672b\u651e\ud843\ude2a\u4e5e\u53c9\u4e8c\u5408\u511e\u5165\u56a9", "242": "\u4e8c\u5408\u56c9\u5165\u56a9\u4e8c\u5408\u56c9\u90a3\u5a11\u4f3d\u3611\u5a11\u56a9\u4e8c\u5408\u8a36\u5f15", "243": "\u8aa6\u767e\u5b57\u52a0\u6301\u5df2\u3002\u5fa9\u7576\u60f3\u524d\u6240\u89c0\u672c\u5c0a\u8af8\u4f5b\u8056", "244": "\u773e\u6b77\u7136\u3002\u651d\u53d7\u65bc\u6211\u5728\u5df1\u76ee\u524d\u3002\u5373\u61c9\u66f4\u9673\u524d\u8af8", "245": "\u7a2e\u7a2e\u5ee3\u5927\u4f9b\u990a\u3002\u7a7a\u4e2d\u6240\u898b\u4e00\u5207\u8056\u773e\u4f9b\u990a\u7562", "246": "\u5df2\u3002\u5fa9\u61c9\u66f4\u7576\u81f3\u5fc3\u8ff4\u5411\u767c\u9858\u3002\u4f5c\u5982\u662f\u8a00\u3002", "247": "\u9858\u6211\u6240\u4fee\u884c\u4e00\u5207\u8af8\u5584\u696d", "248": "\u5ff5\u8aa6\u52a0\u6301\u529b\u8ff4\u65bd\u8af8\u6709\u60c5", "249": "\u51fa\u96e2\u4e09\u754c\u82e6\u901f\u8b49\u65bc\u83e9\u63d0", "250": "\u8ff4\u5411\u767c\u9858\u5df2", "251": "\u5373\u61c9\u7d50\u524d\u6240\u7d50\u8af8\u754c\u5370\u3002\u5de6\u8f49\u5373\u4fbf\u89e3\u5df2\u3002\u5fa9", "252": "\u7d50\u524d\u704c\u9802\u5370\u3002\u7aea\u4e8c\u5c0f\u6307\u982d\u76f8\u62c4\u3002\u5373\u8aa6\u71c8\u7130\u5982", "253": "\u4f86\u660e\u3002\u4ee5\u5370\u5de6\u8f49\u4e4b\u3002\u4e00\u5207\u8af8\u8056\u773e\u5404\u6b78\u672c\u6de8\u571f", "254": "\u660e\u66f0\u3002", "255": "\u66e9\u83ab\u6089\u5e95\u54e9\u4e09\u5408\u91ce\u5a1c\u56a9\u4e8c\u5408\u52aa\u8a90\u591a\u5357\u5535", "256": "\u7d07\u54e9\u4e8c\u5408", "257": "\u6b21\u7d50\u524d\u4e09\u6627\u8036\u3002\u9802\u79ae\u8af8\u8056\u773e\u3002\u8d77\u51fa\u65bc\u9053\u5834\u3002\u8f49", "258": "\u8b80\u6469\u8a36\u884d\u82b1\u56b4\u7b49\u7d93\u5178\u3002\u4efb\u610f\u81ea\u7d93\u884c\u3002\u884c\u8005", "259": "\u82e5\u81ea\u55ab\u98ef\u98df\u53ca\u8af8\u85e5\u7269\u3002\u7576\u5373\u4ee5\u6b64\u660e\u52a0\u6301\u98df\u3002", "260": "\u8aa6\u660e\u516b\u904d\u7136\u5f8c\u53d6\u55ab\u969c\u8005\u4e0d\u80fd\u70ba\u5bb3\u660e\u66f0\u3002", "261": "\u66e9\u9ebc\u85a9\u56a9\u6c92\u99b1\udb80\udc9b\u5730\u85a9\u601b\u56a9\u4e8c\u5408\u5357\u5f15\u5535\u56a9", "262": "\u5f15\u85cd\u5a1c\u6ce5\u5e1d\u5112\u83bd\u5229\u511e\u5a11\u56a9\u4e8c\u5408\u5f15\u8a36", "263": "\u884c\u8005\u82e5\u80fd\u5e38\u4fee\u884c\u4f9b\u990a\u3002\u61c9\u6bcf\u65e5\u55ab\u98ef\u98df\u837c", "264": "\u85e5\u7b49\u3002\u7686\u7559\u5c11\u6b98\u7f6e\u4e00\u5225\u5668\u4e2d\u3002\u5373\u7d50\u8056\u8005\u672c\u528d", "265": "\u5370\u3002\u52a0\u6301\u98df\u4e0a\u8aa6\u660e\u4e03\u904d\u3002\u9001\u7f6e\u6de8\u8655\u81f3\u5fc3\u4f9b\u990a", "266": "\u4e4b\u3002\u5176\u5370\u53f3\u624b\u4f5c\u91d1\u525b\u62f3\u3002\u76f4\u4e26\u7533\u982d\u4e8c\u6307\u3002\u5927", "267": "\u6307\u3002\u7121\u540d\u6307\u7532\u5074\u3002\u52a0\u6301\u4e0a\u660e\u66f0\u3002", "268": "\u66e9\u83ab\u4e09\u6eff\u591a\u56a9\u65e5\u56c9\u4e8c\u5408\u62cf\u53bb\u601b\u56c9\u4e8c\u5408\u5412\u963f", "269": "\u76ee\u4f3d\u6230\u62cf\u6469\u8a36\u5695\u6bba\u62cf\u5a11\u9817\u4e8c\u5408\u5412\u91ce\u543d", "270": "\u601b\u56c9\u4e8c\u5408\u9ebc\u91ce\u601b\u56c9\u4e8c\u5408\u9ebc\u91ce\u543d\u601b\u56c9\u4e8c\u5408\u5412", "271": "\u5505\ud851\udea5", "272": "\u6240\u7528\u98f2\u98df\u4f9b\u990a\u8056\u8005\u3002\u6b64\u5c0a\u672c\u9858\u3002\u5927\u60b2\u6368\u8eab\u5949", "273": "\u4f8d\u4e00\u5207\u6301\u8aa6\u8005\u3002\u8eab\u5982\u5974\u50d5\u73fe\u7121\u4e00\u76ee\u76f8\u3002\u53d7\u6b64", "274": "\u6b98\u98df\u4f9b\u990a\u3002\u884c\u8005\u82e5\u6bcf\u98df\u4e4b\u6642\u5fc3\u4e0d\u5fd8\u8005\u3002\u6211\u7576", "275": "\u665d\u591c\u5e38\u96a8\u64c1\u8b77\u3002\u4e0d\u4ee4\u8af8\u9b54\u6bd8\u90a3\u591c\u8fe6\u4f5c\u8af8\u969c", "276": "\u96e3\u3002\u4ee4\u4e0d\u96a8\u610f\u901f\u6eff\u6210\u5c31\u3002\u884c\u8005\u82e5\u591c\u5206\u5be2\u606f\u6642\u3002", "277": "\u5373\u7576\u5148\u7d50\u6de8\u5ba4\u838a\u56b4\u5370\u3002\u5148\u5f4e\u5de6\u624b\u80cc\u3002\u6301\u6309\u5fc3", "278": "\u4e0a\u3002\u5f8c\u5f4e\u53f3\u624b\u3002\u65bc\u53f3\u624b\u4e0a\u638c\u76f8\u5408\u3002\u8209\u5370\u7f6e\u9802", "279": "\u4e0a\u3002\u4fbf\u5206\u958b\u4e8c\u624b\u3002\u9806\u8eab\u4e0b\u6469\u3002\u7576\u8aa6\u6b64\u660e\u66f0\u3002", "280": "\u66e9\u9ebc\u6089\u5e95\u54e9\u4e8c\u5408\u91ce\u5a1c\u56a9\u4e8c\u5408\u52aa\u8a90\u591a\u5357\u85a9\u56a9", "281": "\u601b\u4ed6\u5f15\u5b7d\u591a\u5357\u6469\u8cc0\u4e09\u9ebc\u5f15\u91ce\u5b7d\u5e1d\u4e09\u9ebc\u5b7d", "282": "\u56c9\u4e8c\u5408\u9ebc\u9ebc\u4ed6\u5b7d\u56c9\u4e8c\u5408\u9ebc\u601b\u56c9\u8def\u8a08\u8a08\u9054", "283": "\u9ebc\u99b1\u56a9\u4e8c\u5408\u5e97\u591a\u50e7\u4f3d\u8ae6\u5a11\u56a9\u4e8c\u5408\u5f15\u8a36", "284": "\u6de8\u52a0\u6301\u5df2\u3002\u5373\u5408\u638c\u9577\u8212\u5169\u81c2\u3002\u65bc\u9802\u4e0a\u3002\u5411\u6771\u65b9", "285": "\u9762\u8457\u5730\u3002\u4ea6\u8212\u4e8c\u8db3\u81f3\u5fc3\u4f5c\u79ae\u3002\u79ae\u6642\u89c0\u60f3\u4e00\u5207", "286": "\u8af8\u4f5b\u83e9\u85a9\u5404\u5728\u672c\u524e\u3002\u4f5c\u662f\u5ff5\u8a00\u3002\u6211\u4eca\u6368\u6b64\u8eab", "287": "\u70ba\u5974\u50d5\u3002\u4f9b\u990a\u5949\u4f8d\u4e00\u5207\u4f5b\u3002\u552f\u9858\u651d\u53d7\u54c0\u610d", "288": "\u65bc\u6211\u3002\u4f5c\u6700\u4e0a\u6210\u5c31\u3002\u5982\u662f\u4e09\u767d\u5df2\u3002\u7136\u5f8c\u96a8\u610f", "289": "\u5be2\u606f\u3002\u5e38\u5ff5\u660e\u76f8\u4f5c\u901f\u8d77\u610f\u3002\u53c8\u4e0d\u52d5\u5c0a\u6cd5\u54c1\u4e91\u3002", "290": "\u4f5b\u8a00\u4e0d\u52d5\u4f7f\u8005\u3002\u80fd\u5229\u76ca\u6210\u5c31\u4e00\u5207\u4e8b\u696d\u3002\u884c\u8005", "291": "\u82e5\u6b32\u4fee\u884c\u4f5c\u8af8\u6cd5\u8005\u3002\u5148\u884c\u6cd5\u8aa6\u5341\u842c\u904d\u5df2\u3002\u5373", "292": "\u65bc\u6708\u516b\u65e5\u6216\u5341\u4e94\u65e5\u3002\u4e00\u65e5\u4e00\u591c\u5927\u4f5c\u4f9b\u990a\u3002\u65bc", "293": "\u50cf\u524d\u4ee5\u82e6\u7df4\u4e00\u5343\u516b\u679a\u548c\u9165\u71d2\u3002\u4e00\u8aa6\u53d6\u4e00\u679a\u3002", "294": "\u71d2\u6eff\u4e00\u5343\u516b\u904d\u5df2\u3002\u5f8c\u6240\u4f5c\u6cd5\u7686\u5f97\u6210\u5c31\u3002\u884c\u8005", "295": "\u6240\u6709\u8a00\u8aaa\u3002\u4eba\u7686\u656c\u91cd\u7121\u6562\u9055\u8005\u3002\u82e5\u6b32\u7e1b\u64b2\u554f", "296": "\u4e8b\u7b56\u4f7f\u5d29\u6467\u4efb\u610f\u7686\u61c9\u3002\u53c8\u65bc\u6708\u8755\u65e5\u3002\u53d6\u672a\u8457", "297": "\u5730\u725b\u7cde\u3002\u5857\u66fc\u837c\u7f85\u96a8\u5176\u5927\u5c0f\u3002\u65bc\u5176\u58c7\u4e0a\u6563\u7a2e", "298": "\u7a2e\u96dc\u82b1\u3002\u58c7\u4e2d\u7f6e\u5927\u822c\u82e5\u7d93\u593e\u3002\u53d6\u7d14\u8272\u7258\u5b50\u6bcd", "299": "\u725b\u9165\u4e00\u531d\u7f6e\u719f\u9285\u6900\u4e2d\u3002\u53d6\u4f49\u9640\u7f85\u6728\u5982\u9f52\u6728", "300": "\u5927\u9577\u5341\u4e8c\u6307\u652a\u4e4b\u3002\u65bc\u9053\u5834\u524d\u5982\u6301\u5ff5\u8aa6\u4e0d\u9650", "301": "\u904d\u6578\u3002\u4ee4\u4e09\u76f8\u73fe\u3002\u8755\u7562\u5373\u6b62\u3002\u53c8\u65bc\u5c71\u5cef\u4e0a\u5857", "302": "\u58c7\u65b7\u98df\u5ff5\u8aa6\u6eff\u5341\u842c\u904d\u3002\u5373\u898b\u5730\u4e2d\u4e00\u5207\u8af8\u4f0f", "303": "\u85cf\u3002", "304": "\u53c8\u6cd5\u53d6\u4e73\u7e8c\u7e8c\u6295\u706b\u4e2d\u3002\u8b77\u6469\u5ff5\u8aa6\u6eff\u4e00\u5343\u904d\u3002", "305": "\u80fd\u9664\u75ab\u75c5\u3002", "306": "\u53c8\u53d6\u4ff1\u5c62\u8349\u548c\u9165\u4e73\u871c\u7b49\u3002\u6c83\u706b\u4e2d\u71d2\u5341\u842c\u904d\u3002", "307": "\u80fd\u9664\u5927\u75ab\u75c5\u3002", "308": "\u53c8\u6cd5\u53d6\u84ee\u83ef\u548c\u9165\u916a\u871c\u3002\u6c83\u706b\u4e2d\u71d2\u8aa6\u5341\u842c\u904d\u3002", "309": "\u84ee\u83ef\u5409\u7965\u5929\u5973\u7b49\u80fd\u8207\u6eff\u9858\u3002", "310": "\u53c8\u81e8\u6cb3\u6d77\u53e3\u5165\u6c34\u81f3\u80f8\u8aa6\u4e09\u5341\u842c\u904d\u5f97\u5c3e\u6c99", "311": "\u8036\u3002", "312": "\u53c8\u53d6\u96dc\u82b1\u64f2\u706b\u4e2d\u71d2\u3002\u96a8\u82b1\u8272\u5f97\u8863\u3002\u71d2\u4e94\u7a40\u5b50\u3002", "313": "\u5f97\u7a40\u7c73\u96a8\u610f\u53d7\u7528\u3002\u53c8\u53d6\u5c3e\u7f85\u56a9\u4e8c\u5408\u6728\u3002\u64f2\u706b", "314": "\u4e2d\u71d2\u5ff5\u8aa6\u5341\u842c\u904d\u3002\u5373\u5f97\u56c9\u95cd\u738b\u611b\u656c\u3002", "315": "\u53c8\u53d6\u5fc5\u54e9\u5546\u9685\u6728\u3002\u64f2\u706b\u4e2d\u71d2\u3002\u80fd\u4ee4\u4e00\u5207\u4eba\u656c", "316": "\u611b\u3002\u71d2\u9f52\u6728\u3002\u5373\u5f97\u7121\u91cf\u50d5\u5f9e\u3002\u71d2\u5927\u9ea5\u5373\u70ba\u9577", "317": "\u8005\u3002\u5df2\u4e0a\u5ff5\u8aa6\u5404\u6eff\u5341\u842c\u904d\u3002\u6b21\u8aaa\u756b\u4e0d\u52d5\u5c0a\u50cf", "318": "\u6cd5\u4ee5\u597d\u6de8\u3cb2\u756b\u4e0d\u52d5\u5c0a\u3002\u8457\u8d64\u571f\u8272\u88d9\u3002\u5de6\u5782", "319": "\u8fae\u9aee\u9afb\u3002\u773c\u659c\u8996\u3002\u5de6\u624b\u57f7\u528d\u53f3\u624b\u57f7\u7d22\u3002\u5750\u5bf6\u84ee", "320": "\u83ef\u3002\u8e59\u7709\u9762\u778b\u76f8\u3002\u4f5c\u964d\u4e09\u4e16\u72c0\u3002\u5982\u662f\u756b\u5df2\u3002\u5c07\u6b64", "321": "\u50cf\u65bc\u6cb3\u6d77\u5cb8\u4e0a\u3002\u5982\u6cd5\u5857\u58c7\u5b89\u50cf\u3002\u884c\u8005\u4ea6\u8457\u8d64", "322": "\u8272\u8863\u3002\u5fc3\u4e0d\u67d3\u8457\u5bc2\u975c\u5b89\u5fc3\u4e5e\u98df\u70ba\u6d3b\u3002\u5373\u65bc\u50cf", "323": "\u524d\u5ff5\u8aa6\u4e94\u5341\u842c\u904d\u5df2\u3002\u5373\u65bc\u591c\u4e2d\u53d6\u8a79\u672b\u4e00", "324": "\u842c\u6bb5\u3002\u4e00\u8aa6\u4e00\u64f2\u4e00\u64f2\u706b\u4e2d\u71d2\u4e4b\u3002\u6eff\u5df2\u5373\u4e0d\u52d5", "325": "\u5c0a\u81ea\u73fe\u5176\u8eab\u3002\u6eff\u884c\u8005\u9858\u6240\u4f5c\u7686\u5f97\u6210\u5c31\u3002\u884c\u8005", "326": "\u81ea\u8eab\u70ba\u5982\u4f86\u4f7f\u8005\u3002\u8b49\u4e09\u6469\u5730\u5171\u8af8\u83e9\u85a9\u540c\u4f4d\u3002", "327": "\u53c8\u6b32\u5f97\u964d\u4f0f\u4e00\u5207\u60e1\u4eba\u8005\u3002\u53d6\u5c38\u9640\u6797\u5e1b\u756b\u4e0d", "328": "\u52d5\u5c0a\u3002\u4ee5\u81ea\u5df1\u8840\u6de1\u4f5c\u50cf\u8272\u3002\u50cf\u7f6e\u897f\u5411\u3002\u884c\u8005\u6771", "329": "\u9762\u5750\u5ff5\u8aa6\u3002\u6bcf\u65e5\u4e09\u6642\u6d17\u6d74\u8457\u6fd5\u8863\u3002\u65bc\u50cf\u524d", "330": "\u8aa6\u6eff\u5341\u842c\u904d\u5df2\u3002\u5373\u4e00\u5207\u6240\u4f5c\u7686\u96a8\u6210\u5c31\u3002\u4ecd\u6bcf", "331": "\u65e5\u65bd\u4e00\u5207\u9b3c\u795e\u98df\u3002", "332": "\u53c8\u6cd5\u9ed1\u6708\u516b\u65e5\u591c\u3002\u65bc\u5bd2\u6797\u4e2d\u53d6\u6bcd\u8036\u6469\u5974\u6c99\u3002", "333": "\u5750\u5176\u4e0a\u5ff5\u8aa6\u6eff\u4e00\u842c\u904d\u3002\u5f7c\u6469\u5974\u6c99\u5373\u52d5\u8033\u3002\u5fc5", "334": "\u4e0d\u5f97\u6015\u3002\u5f7c\u4fbf\u958b\u53e3\u51fa\u5927\u958b\u6577\u84ee\u83ef\u3002\u5373\u4fbf\u628a\u53d6\u3002", "335": "\u80fd\u4ee4\u5df1\u8eab\u5982\u5341\u516d\u7ae5\u5b50\u3002\u9aee\u5982\u84ee\u74b0\u3002\u6607\u7a7a\u904a", "336": "\u65bc\u68b5\u5929\u3002\u5f97\u5927\u660e\u738b\u4e3b\u3002\u53c8\u65bc\u50cf\u524d\u6bcf\u65e5\u4e09\u6642\u5ff5", "337": "\u8aa6\u3002\u96a8\u529b\u4f9b\u990a\u71d2\u6c88\u6c34\u9999\u3002\u5982\u662f\u4f9b\u990a\u6eff\u65bc\u516d\u6708\u3002", "338": "\u81f3\u5fc3\u4e0d\u65b7\u5373\u5f97\u5c3e\u6c99\u8036\u81f3\u3002", "339": "\u53c8\u6cd5\u6b32\u4ee4\u4ed6\u8ecd\u9663\u7834\u6563\u8005\u3002\u52a0\u6301\u81ea\u8ecd\u65cc\u4e00\u5341", "340": "\u904d\u3002\u57f7\u51fa\u5728\u8ecd\u524d\u3002\u5f7c\u8ecd\u9663\u7834\u6563\u9000\u8d70\u3002", "341": "\u53c8\u6cd5\u6b32\u7981\u4ed6\u8ecd\u9663\u773e\u4ee4\u4e0d\u52d5\u8005\u3002\u65bc\u81ea\u65cc\u4e0a\u756b", "342": "\u4e0d\u52d5\u5c0a\u3002\u56db\u9762\u56db\u81c2\u8eab\u4f5c\u9ec3\u8272\u3002\u4e0a\u4e0b\u51fa\u7259\u4f5c\u5927", "343": "\u5fff\u6012\u778b\u6016\u754f\u72c0\u3002\u904d\u8eab\u706b\u5149\u4f5c\u5929\u5175\u52e2\u3002\u884c\u8005\u4ee5", "344": "\u65cc\u793a\u5f7c\u8ecd\u773e\u3002\u5fa9\u60f3\u8056\u8005\u4ee5\u7f82\u7d22\u7e1b\u5f7c\u5175\u773e\u3002\u5373", "345": "\u5f7c\u8ecd\u773e\u76e1\u4e0d\u80fd\u52d5\u3002", "346": "\u53c8\u6cd5\u6b32\u4ee4\u4ed6\u8ecd\u773e\u81ea\u9b2a\u8acd\u9000\u6563\u8005\u3002\u53d6\u8001\u9d76\u9d1f", "347": "\u689f\u9d3f\u6bdb\u3002\u8aa6\u660e\u52a0\u6301\u64f2\u706b\u4e2d\u71d2\u6eff\u4e00\u5343\u904d\u3002\u5f7c\u8ecd", "348": "\u773e\u5373\u81ea\u76f8\u9b2a\u8acd\u3002", "349": "\u53c8\u6cd5\u6b32\u4ee4\u6368\u89a9\u5695\u7d42\u4ea1\u8005\u3002\u53d6\u7a3b\u7ce0\u8aa6\u660e\u52a0\u6301", "350": "\u64f2\u706b\u4e2d\u71d2\u3002\u53c8\u60f3\u5f7c\u6368\u89a9\u5695\u3002\u88ab\u4f7f\u8005\u4ee5\u7d22\u7e1b\u3002\u5c07", "351": "\u5411\u5357\u65b9\u60b6\u82e6\u5410\u8840\u800c\u7d42\u3002\u5f7c\u7b49\u65cf\u985e\u3002\u7686\u4e0d\u5f97\u75ca", "352": "\u4e00\u7121\u5b58\u5728\u3002", "353": "\u53c8\u6cd5\u6b32\u4ee4\u4ed6\u8ecd\u4e3b\u7d42\u4ea1\u8005\u3002\u53d6\u9e7d\u571f\u881f\u82e6\u7df4\u8449\u3002", "354": "\u548c\u64e3\u70ba\u57ff\u3002\u4f5c\u5f7c\u5f62\u72c0\u7f6e\u65bc\u5730\u4e0a\u3002\u8aa6\u660e\u52a0\u6301", "355": "\u65ab\u65b7\u3002\u5f7c\u5373\u7d42\u3002", "356": "\u53c8\u6cd5\u6b32\u4ee4\u4ed6\u8ecd\u8ca7\u7aae\u7d55\u7ce7\u8005\u3002\u53d6\u7a3b\u7a40\u52a0\u6301\u5f7c", "357": "\u5373\u8ca7\u77e3\u3002", "358": "\u53c8\u6cd5\u6b32\u4ee4\u4ed6\u8ecd\u964d\u4f0f\u4f86\u8005\u3002\u5373\u7d50\u4e0d\u52d5\u5c0a\u773c\u5370\u3002", "359": "\u4f5c\u778b\u6012\u8072\u7a31\u543d\u5b57\u3002\u60f3\u8056\u8005\u4f7f\u8af8\u9b3c\u795e\u6349\u7e1b\u5c07", "360": "\u4f86\u3002\u5f7c\u5373\u81ea\u964d\u3002", "361": "\u53c8\u6cd5\u6b32\u4ee4\u5927\u611b\u6a02\u8005\u3002\u4ee5\u4e03\u9e7d\u4f5c\u5f7c\u5f62\u72c0\u3002\u6bb5", "362": "\u6bb5\u65b7\u4e4b\u5ff5\u8aa6\u6eff\u4e03\u65e5\u3002\u5f7c\u5373\u611b\u6a02\u3002", "363": "\u53c8\u53d6\u4ff1\u8607\u6469\u82b1\u71d2\u8aa6\u660e\u5341\u842c\u904d\u3002\u5f97\u85e5\u53c9\u5973\u4f86\u3002", "364": "\u65bc\u4e09\u4e8b\u4e2d\u6240\u6c42\u7686\u5f97\u3002\u53c8\u53d6\u66fc\u9640\u7f85\u82b1\u3002\u7a31\u5f7c\u4eba", "365": "\u540d\u52a0\u6301\u3002\u5373\u4ee4\u8352\u4e82\u3002\u53c8\u53d6\u9e7d\u52a0\u6301\u71d2\u3002\u5373\u5f97\u5929\u5973", "366": "\u4f86\u6240\u4f7f\u96a8\u610f\u3002\u53c8\u52a0\u6301\u5b89\u6089\u9999\u71d2\u3002\u5373\u5f97\u738b\u81e3\u61b6", "367": "\u5ff5\u3002\u53c8\u8aaa\u756b\u50cf\u6cd5\u3002\u4e2d\u756b\u91cb\u8fe6\u725f\u5c3c\u4f5b\u3002\u5de6\u756b\u66fc\u6b8a", "368": "\u7ae5\u5b50\u3002\u53f3\u756b\u57f7\u91d1\u525b\u83e9\u85a9\u3002\u4f5c\u5fae\u7b11\u9762\u624b\u57f7\u91d1\u525b", "369": "\u6775\u3002\u65bc\u57f7\u91d1\u525b\u4e0b\u756b\u4e0d\u52d5\u5c0a\u3002\u7a2e\u7a2e\u838a\u56b4\u3002\u5373\u65bc", "370": "\u50cf\u524d\u8aa6\u4e94\u5341\u842c\u904d\u3002\u7136\u5f8c\u4f5c\u4e00\u5207\u4e8b\u696d\u7686\u5f97\u96a8", "371": "\u610f\u3002", "372": "\u53c8\u6cd5\u53d6\u71d2\u5c4d\u7070\u8aa6\u660e\u4e03\u904d\u8207\u5f7c\u4eba\u5373\u5f97\u611b\u6a02\u3002", "373": "\u53c8\u6cd5\u53d6\u725b\u9ec3\u52a0\u6301\u4e03\u904d\u3002\u9ede\u5df1\u984d\u4e0a\u3002\u80fd\u4ee4\u773e\u4eba", "374": "\u6240\u898b\u7686\u751f\u656c\u91cd\u3002\u6bd8\u90a3\u591c\u8fe6\u4e0d\u80fd\u640d\u5bb3\u71be\u76db\u6210", "375": "\u5c31\u3002\u53c8\u65bc\u5df1\u8eab\u4e0a\u5e03\u660e\u68b5\u5b57\u3002\u5f7c\u7f85\u524e\u773e\u8af8\u4f5c\u969c", "376": "\u8005\u3002\u767e\u7531\u5de1\u5167\u7686\u6089\u9000\u6563\u3002", "377": "\u53c8\u6cd5\u82e5\u4eba\u88ab\u86c7\u86df\u3002\u7d93\u516d\u6708\u4e0d\u5dee\u3002\u8aa6\u660e\u52a0\u6301\u3002", "378": "\u65bc\u5176\u81c2\u4e0a\u756b\u528d\u5951\u3002\u7acb\u5dee\u3002", "379": "\u53c8\u6cd5\u756b\u5f8b\u8fe6\u5927\u86c7\u3002\u7e8f\u528d\u4e0a\u3002\u528d\u570d\u9076\u756b\u706b\u7130\u3002\u8aa6", "380": "\u660e\u52a0\u6301\u6eff\u4e00\u5343\u904d\u3002\u4ee5\u793a\u75c5\u8005\u5373\u81ea\u4e0b\u8a9e\u3002\u82e5", "381": "\u8aa6\u52a0\u6301\u75c5\u8005\u4e00\u767e\u516b\u904d\u3002\u5373\u5e38\u8499\u8056\u8005\u64c1\u8b77\u3002\u82e5", "382": "\u6bcf\u65e5\u52a0\u6301\u6b98\u98df\u7f6e\u6de8\u8655\u3002\u4f9b\u990a\u4f7f\u8005\u5e38\u5982\u9858\u3002", "383": "\u53c8\u6cd5\u82e5\u60e1\u96e8\u96e8\u3002\u884c\u8005\u778b\u6012\u5fc3\u5927\u8072\u7a31\u543d\u5b57\u3002\u60e1", "384": "\u96f2\u9000\u6563\u3002\u53c8\u53d6\u68d8\u523a\u3002\u548c\u7f85\u8996\u8fe6\u6cb9\u52a0\u6301\u64f2\u706b\u4e2d", "385": "\u71d2\u3002\u80fd\u6b62\u5927\u96e8\u3002\u4ea6\u4ee4\u884c\u8005\u6210\u5927\u7d50\u8b77\u3002\u4ea6\u6210\u5c31\u4e00", "386": "\u5207\u4e8b\u696d\u3002", "387": "\u53c8\u6cd5\u756b\u4e0d\u52d5\u5c0a\u8457\u8d64\u571f\u8272\u8863\u3002\u5de6\u5782\u8fae\u9aee\u773c\u659c", "388": "\u8996\u3002\u7ae5\u5b50\u5f62\u3002\u53f3\u624b\u57f7\u91d1\u525b\u6775\u7576\u5fc3\u3002\u5de6\u624b\u57f7\u5bf6\u68d2\u3002", "389": "\u773c\u5fae\u8d64\u5750\u84ee\u82b1\u4e0a\u3002\u778b\u6012\u76f8\u904d\u8eab\u706b\u7130\u3002\u65bc\u50cf\u524d", "390": "\u7d50\u6240\u611b\u6a02\u5370\u5ff5\u8aa6\u3002\u4e00\u5207\u7686\u5f97\u6210\u5c31\u3002\u4f9d\u524d\u5ff5\u8aa6\u3002", "391": "\u6607\u7a7a\u96b1\u5f62\u4e00\u5207\u611b\u6a02\u4e8b\u3002\u7686\u96a8\u610f\u6210\u5c31\u3002\u82e5\u7121\u756b", "392": "\u50cf\u3002\u7368\u8655\u9591\u975c\u6216\u65bc\u5bfa\u4e2d\u3002\u6216\u5728\u5c71\u7a9f\u96e2\u96dc\u9b27\u8655\u3002", "393": "\u6c42\u4e00\u5207\u4e8b\u6cd5\u7686\u4ea6\u6210\u5c31\u3002", "394": "\u53c8\u6cd5\u52a0\u6301\u7627\u75c5\u4ee4\u81ea\u7e1b\u4e0b\u8a9e\u3002\u53c8\u52a0\u6301\u93e1\u3002\u65bc\u4e2d", "395": "\u8056\u8005\u73fe\u554f\u4e8b\u7686\u8a9e\u3002\u53c8\u53d6\u4e00\u7ae5\u5b50\u6216\u7ae5\u5973\u3002\u4ee4\u6de8", "396": "\u6d17\u6d74\u8457\u9bae\u6de8\u8863\u7f6e\u9053\u5834\u4e2d\u3002\u53ec\u8acb\u8056\u8005\u5165\u9053\u5834", "397": "\u52a0\u6301\u3002\u88ab\u6b64\u7ae5\u5b50\u554f\u4e00\u5207\u4e8b\u7686\u5f97\u3002", "398": "\u53c8\u6cd5\u82e5\u6b32\u6210\u5c31\u7e6b\u8fe6\u56c9\u6cd5\u8005\u3002\u65bc\u767d\u6708\u4e00\u65e5\u65e5", "399": "\u4e2d\u6642\u3002\u65bc\u50cf\u524d\u8457\u7a2e\u7a2e\u9999\u82b1\u4f9b\u990a\u3002\u4e0d\u6b47\u8aa6\u660e\u4e00", "400": "\u767e\u516b\u904d\u3002\u60f3\u5ff5\u58c7\u4e2d\u4e00\u5207\u8af8\u4f5b\u83e9\u85a9\u651d\u53d7\u3002\u6bcf\u65e5", "401": "\u5982\u662f\u5ff5\u8aa6\u6eff\u4e00\u6708\u3002\u53c8\u53d6\u82e6\u7df4\u6728\u9999\u3002\u53c8\u53d6\u904f", "402": "\u8fe6\u6728\u4ee5\u9165\u5857\u4e0a\u3002\u548c\u767d\u82a5\u5b50\u52a0\u6301\u64f2\u706b\u4e2d\u71d2\u3002\u5f9e", "403": "\u620c\u81f3\u5b50\u4e43\u81f3\u5bc5\u6642\u3002\u7e6b\u8fe6\u56c9\u5373\u4f86\u8a9e\u884c\u8005\u8a00\u3002\u4f7f", "404": "\u6211\u4f5c\u4f55\u4e8b\u3002\u884c\u8005\u651d\u53d7\u3002\u5df2\u5f8c\u5e38\u96a8\u884c\u8005\u610f\u6240\u4f7f\u3002", "405": "\u96a8\u9806\u4f9b\u7d66\u6240\u9808\u98f2\u98df\u9f52\u6728\u6de8\u6c34\u7b49\u3002\u5e38\u5728\u5de6\u53f3\u3002", "406": "\u4e43\u81f3\u4f7f\u5f80\u4e0a\u5929\u53d6\u5929\u5973\u3002\u4ea6\u5373\u5c07\u4f86\u3002", "407": "\u53c8\u66f4\u8aaa\u6839\u672c\u5370\u660e\u7b49\u3002\u5176\u6839\u672c\u5370\u3002\u4ee5\u4e8c\u4e2d\u6307\u5167", "408": "\u76f8\u53c9\u70ba\u9264\u3002\u4e8c\u982d\u6307\u5074\u76f8\u62c4\u3002\u4e8c\u5927\u6307\u5404\u637b\u81ea\u7121", "409": "\u540d\u6307\u7532\u4e0a\u660e\u66f0\u3002", "410": "\u66e9\u9ebc\u4e09\u66fc\u591a\u56a9\u65e5\u56c9\u4e8c\u5408\u62cf\u53bb\u543d\u5c3e\u5409\u54e9\u4e8c\u5408", "411": "\u591a\u5c3e\u8fe6\u56c9\u9ebc\u8cc0\u7562\u3611\u4e8c\u5408\u591a\u5df2\u745f\u4f98\u4e8c\u5408\u5951", "412": "\u6012\u59cb\u745f\u4f98\u4e8c\u5408\u8cc0\u56c9\u6848\u601b\u56c9\u4e8c\u5408\u9ebc\u56c9\u99b1\u56c9\u8005", "413": "\u5484\u56c9\u76ee\u4f49\u5165\u56a9\u4e8c\u5408\u908f\u6bd4\u8def\u56c9\u58ae\u8a08\u820d\u543d\u56a9", "414": "\u65e5\u56c9\u4e8c\u5408\u5b7d\u56c9\u4e8c\u5408\u543d\u6cee\u5412\u534a\u97f3", "415": "\u6b21\u8aaa\u5fc3\u5370\u3002\u4ee5\u4e8c\u5c0f\u6307\u4e8c\u7121\u540d\u6307\u5167\u76f8\u53c9\u3002\u4e8c\u4e2d", "416": "\u6307\u7aea\u982d\u76f8\u62c4\u3002\u4e8c\u982d\u6307\u65bc\u4e2d\u6307\u5fa9\u66f2\u5982\u9264\u3002\u4e8c\u5927", "417": "\u6307\u4e26\u637b\u4e2d\u6307\u4e2d\u7bc0\u660e\u66f0\u3002", "418": "\u66e9\u9ebc\u4e09\u66fc\u591a\u56a9\u65e5\u56c9\u4e8c\u5408\u62cf\u601b\u56c9\u4e8c\u5408\u5412\u963f\u6bcd", "419": "\u4f3d\u6230\u62cf\u6469\u8cc0\u5695\u6bba\u62cf\u5a11\u9817\u4e8c\u5408\u5412\u91ce\u543d\u601b\u56c9", "420": "\u4e8c\u5408\u9ebc\u91ce\u601b\u56c9\u4e8c\u5408\u9ebc\u91ce\u543d\u601b\u56c9\u4e8c\u5408\u5412\u5505", "421": "\ud851\udea5", "422": "\u6b21\u8aaa\u528d\u5370\u3002\u5de6\u624b\u5927\u6307\u3002\u637b\u7121\u540d\u6307\u5c0f\u6307\u7532\u3002\u76f4\u982d", "423": "\u4e2d\u4e8c\u6307\u3002\u53f3\u624b\u982d\u4e2d\u4e8c\u6307\u3002\u5165\u5de6\u638c\u4e2d\u3002\u63e1\u4ee5\u70ba\u62f3\u3002", "424": "\u53f3\u7121\u540d\u5c0f\u4e8c\u6307\u5927\u6307\u637b\u5176\u7532\u4e0a\u3002\u53c8\u4e91\u5de6\u982d\u6307", "425": "\u5c48\u3002\u637b\u5927\u6307\u5982\u74b0\u660e\u66f0\u3002", "426": "\u5535\u5a1c\u8005\u908f\u52a0\u62cf\u6c92\u99b1\u5236\u5412\u8fe6\u543d\u543d\u4f49\u546c", "427": "\u4f49\u546c\u4f0a\u80fd\u9b5a\u54e9\u4e8c\u5408\u546c\ud851\udea5\u8cc0\u550e\u5c3e\u6c99\u7d22\u9262\u591a", "428": "\u4e8c\u5408\u60e1\u7d07\u54e9\u4e8c\u5408\u8cc0\u6cee\u5412\u963f\u54e9\u91ce\u4e8c\u5408\u8005\u908f\u963f", "429": "\u5b7d\u8eca\u7dca\u65e8\u56c9\u591c\u601d\u4f0a\u5f15\u80fd\u8fe6\u5f15\u54e9\u91ce\u4e8c\u5408\u53e5\u5695", "430": "\u66e9\u9ebc\u5a11\u56a9\u4e8c\u5408\u8a36\u5f15", "431": "\u6b21\u8aaa\u91d1\u525b\u6775\u5370\u3002\u4ee5\u53f3\u624b\u5927\u6307\u3002\u637b\u982d\u6307\u7532\u5982\u74b0\u3002", "432": "\u6563\u958b\u9918\u4e09\u6307\u4ea6\u540d\u6210\u5c31\u4e00\u5207\u4e8b\u696d\u5370\u660e\u66f0\u3002", "433": "\u66e9\u83ab\u4e09\u66fc\u591a\u56a9\u65e5\u56c9\u4e8c\u5408\u5f15\u62cf\u5535\u5a40\u8005\u908f\u8fe6\u5f15", "434": "\u8005\u5695\u5a11\u5f15\u99b1\u91ce\u543d\ud843\udc22\u5412", "435": "\u6b21\u8aaa\u5bf6\u5c71\u5370\u3002\u4ee5\u5169\u624b\u5341\u6307\u5167\u76f8\u53c9\u3002\u5408\u7d50\u70ba\u62f3\u3002", "436": "\u6b21\u8aaa\u982d\u5370\u3002\u5de6\u624b\u4ee5\u56db\u6307\u63e1\u5927\u6307\u3002\u70ba\u62f3\u7f6e\u982d\u4e0a\u3002", "437": "\u6b21\u8aaa\u5782\u8fae\u9aee\u9aee\u5370\u3002\u4ee5\u4e8c\u7121\u540d\u6307\u5167\u76f8\u53c9\u3002\u7aea\u4e8c", "438": "\u4e2d\u6307\u982d\u5074\u5408\u3002\u4e8c\u982d\u6307\u5404\u637b\u4e2d\u6307\u7532\u4e0a\u3002\u4e8c\u5927\u6307", "439": "\u5165\u7121\u540d\u6307\u5c0f\u6307\u9593\u3002\u7532\u76f8\u80cc\u5408\u3002\u4e8c\u5c0f\u6307\u5408\u800c\u7aea", "440": "\u4ee5\u5370\u7f6e\u982d\u5728\u89d2\u4e0a\u5373\u6210\u3002\u53c8\u7528\u6b64\u5370\u3002\u7ffb\u982d\u5411\u5167", "441": "\u5230\u5782\u3002\u7f6e\u984d\u4e0a\u3002\u4fbf\u6210\u8056\u8005\u773c\u5370\u3002", "442": "\u6b21\u8aaa\u53e3\u5370\u3002\u4ee5\u4e8c\u5c0f\u6307\u5167\u76f8\u4ea4\u3002\u4ee5\u4e8c\u7121\u540d\u6307\u63e1", "443": "\u4e4b\u3002\u4e8c\u4e2d\u6307\u5074\u76f8\u62c4\u3002\u4e8c\u982d\u6307\u5404\u637b\u4e2d\u6307\u7532\u3002\u4e8c\u5927", "444": "\u6307\u5411\u5916\u4e26\u76f4\u7aea\u7f6e\u53e3\u4e0a\u3002", "445": "\u6b21\u8aaa\u7532\u5370\u3002\u5408\u5370\u5408\u638c\u4e8c\u982d\u6307\u4e8c\u7121\u540d\u6307\u80cc\u76f8", "446": "\u8457\u3002\u4e8c\u5927\u6307\u4e8c\u4e2d\u6307\u4e8c\u5c0f\u6307\u5408\u800c\u6429\u958b\u3002", "447": "\u6b21\u8aaa\u5e2b\u5b50\u596e\u8fc5\u5370\u3002\u5148\u5408\u3002\u4ee5\u7121\u540d\u4e8c\u6307\u5c48\u5165\u638c", "448": "\u4e2d\u80cc\u76f8\u8457\u3002\u4e8c\u982d\u6307\u65bc\u4e2d\u6307\u5f8c\u5c48\u5982\u9264\u3002\u4e8c\u5927\u6307", "449": "\u4e8c\u5c0f\u6307\u5404\u4e26\u76f4\u5411\u5916\u7533\u3002\u5373\u8d77\u4f5c\u983b\u7533\u7121\u71b1\u3002\u9076", "450": "\u58c7\u884c\u9053\u3002", "451": "\u6b21\u8aaa\u706b\u7130\u5370\u3002\u53f3\u624b\u5927\u6307\u5c48\u3002\u62bc\u4e2d\u6307\u9918\u4e09\u6307\u3002", "452": "\u76f4\u7aea\u982d\u62c4\u53f3\u638c\u5fc3\u3002\u53f3\u624b\u4e94\u6307\u6563\u958b\u3002", "453": "\u6b21\u8aaa\u5236\u706b\u7130\u5370\u3002\u4e8c\u624b\u5404\u70ba\u62f3\u3002\u4e8c\u5927\u6307\u65bc\u982d\u6307", "454": "\u9593\u51fa\u982d\u3002\u62f3\u76f8\u5408\u3002", "455": "\u6b21\u8aaa\u5546\u4f49\u5370\u3002\u4ee5\u4e8c\u5c0f\u6307\u5167\u76f8\u4ea4\u3002\u4e8c\u4e2d\u6307\u4e8c\u7121", "456": "\u540d\u6307\u982d\u76f8\u62c4\u3002\u5de6\u982d\u6307\u76f4\u7533\u9644\u4e2d\u6307\u80cc\u3002\u53f3\u982d\u6307", "457": "\u5c48\u637b\u4e2d\u6307\u4e0a\u7bc0\u3002\u4e8c\u5927\u6307\u5404\u637b\u7121\u540d\u6307\u3002\u5f9e\u5bf6\u5c71", "458": "\u4ee5\u4e0b\u8af8\u5370\u3002\u53d6\u672c\u90e8\u4e2d\u6240\u611b\u6a02\u660e\u3002\u52a0\u6301\u7528\u4e4b\u5fc5", "459": "\u9a57\u6210\u5c31\u3002", "460": "\u6b21\u8aaa\u7d22\u5370\u3002\u4e8c\u624b\u5404\u4f5c\u91d1\u525b\u62f3\u3002\u5404\u7aea\u982d\u6307\u3002\u53f3", "461": "\u982d\u6307\u5167\u5165\u5de6\u638c\u4e2d\u63e1\u4e4b\u3002\u7aea\u5370\u7576\u5fc3\u8aa6\u52a0\u6301\u660e", "462": "\u66f0\u3002", "463": "\u66e9\u9ebc\u4e09\u66fc\u591a\u56a9\u65e5\u56c9\u4e8c\u5408\u62cf\u963f\u6ce2\u820d\u4f34\u95cd\u66e9", "464": "\u543d\u6cee\u5412", "465": "\u5e95\u54e9\u4e09\u6627\u8036\u4e0d\u52d5\u5c0a\u5a01\u6012\u738b\u4f7f\u8005\u5ff5\u8aa6\u6cd5\u4e00", "466": "\u5377"}, "originalTitle": "Taisho Tripitaka, Electronic version, No. 1200 \u5e95\u54e9\u4e09\u6627\u8036\u4e0d\u52d5\u5c0a\u5a01\u6012\u738b\u4f7f\u8005\u5ff5\u8aa6\u6cd5", "source": "CBETA"}