32\u5f97\u6c42\u89e3\u812b\u667a\u6167\u8005\u547c\u4f7f\u8005\u3002", "253": "\u7b2c\u516d\u4e8c\u4e09\u56db\u79aa\u5927\u660e\u738b\u3002\u662f\u666e\u8ce2\u83e9\u85a9\u6240\u8b8a\u8eab", "254": "\u53f3\u6301\u6756\u5de6\u62bc\u8170\u3002\u8d64\u8272\u9aee\u7aea\u5f62\u3002\u82e5\u4eba\u6b32\u5f97\u6c42\u4e00\u5207\u610f\u9858\u8005\u662f\u547c\u4f7f\u8005\u3002", "255": "\u7b2c\u4e03\u4e09\u5341\u4e09\u5929\u5404\u5404\u5929\u738b\u3002\u662f\u6587\u6b8a\u6240\u8b8a\u8eab\u53f3\u624b", "256": "\u7528\u5927\u5200\u5de6\u624b\u62bc\u8170\u3002\u9ed1\u8272\u5f62\u4e5f\u3002\u82e5\u4eba\u6b32\u5f97\u5927\u667a\u8005\u662f\u547c\u4f7f\u8005\u3002", "257": "\u7b2c\u516b\u963f\u8fe6\u5c3c\u591a\u5929\u738b\u3002\u662f\u89c0\u97f3\u6240\u8b8a\u8eab\u53f3\u624b\u7528\u932b\u6756\u5de6", "258": "\u62bc\u8170\u3002\u8d64\u8272\u5f62\u4e5f\u3002\u82e5\u4eba\u6b32\u5f97\u570b\u738b\u5927\u81e3\u611b\u5ff5\u8005\u662f\u547c\u4f7f\u8005\u3002", "259": "\u7b2c\u4e5d\u592e\u4ff1\u5c07\u8fe6\u7f85\u738b\u3002\u662f\u5f4c\u52d2\u6240\u8b8a\u8eab\u53f3\u7528\u7d22\u5de6\u62f3\u62bc\u8170\u3002", "26": "\u4e09\u6469\u5730\u9a5a\u89ba\u3002\u53ec\u96c6\u4e00\u5207\u8072\u805e\u8f9f\u652f\u4f5b\u4e00\u5207\u5929", "260": "\u8d64\u8272\u5f62\u4e5f\u3002\u82e5\u4eba\u6b32\u5f97\u738b\u540e\u91c7\u5973\u8005\u662f\u547c\u4f7f\u8005\u3002", "261": "\u7b2c\u5341\u4fee\u7f85\u91d1\u7e1b\u738b\u3002\u662f\ud851\ude3d\u6a13\u5b6b\u4f5b\u6240\u8b8a\u8eab\u9ed1\u8272\u53f3\u57f7\u5200\u5de6", "262": "\u62bc\u8170\u3002\u82e5\u4eba\u6b32\u5f97\u5927\u81e3\u516c\u537f\u611b\u8005\u6b64\u547c\u4f7f\u8005\u3002", "263": "\u7b2c\u5341\u4e00\u5927\u9262\u6c99\u7f85\u738b\u3002\u662f\ud851\ude3d\u90a3\u542b\u725f\u5c3c\u4f5b\u6240\u8b8a", "264": "\u8eab\u53f3\u624b\u7528\u679a\u5de6\u62bc\u8170\u3002\u72d7\u6253\u5f62\u8d64\u8272\u5f62\u4e5f\u3002\u82e5\u4eba\u6b32\u5f97\u5927\u798f\u5b98\u8005\u6b64\u547c\u4f7f\u8005\u3002", "265": "\u7b2c\u5341\u4e8c\u62d4\u82e6\u5a46\u591a\u7f85\u738b\u3002\u662f\u6bd8\u5a46\u652f\u4f5b\u6240\u8b8a\u8eab", "266": "\u53f3\u624b\u7528\u8f2a\u5de6\u62bc\u8170\u3002\u9ed1\u8272\u5f62\u3002\u82e5\u4eba\u6b32\u5f97\u52dd\u4eba\u8005\u662f\u547c\u4f7f\u8005\u3002", "267": "\u7b2c\u5341\u4e09\u591a\u7f85\u8fe6\u738b\u3002\u662f\u767c\u6c99\u4f5b\u6240\u8b8a\u8eab\u5de6\u53f3\u624b\u5408\u638c\u3002\u8d64\u8272\u5f62\u3002", "268": "\u82e5\u4eba\u6b32\u5f97\u9042\u4e00\u5207\u6240\u671b\u4e16\u9593\u8005\u547c\u4f7f\u8005\u3002", "269": "\u7b2c\u5341\u56db\u725b\u982d\u5bc6\u546a\u738b\u3002\u662f\u8fe6\u8449\u4f5b\u6240\u8b8a\u8eab\u53f3\u624b\u6301\u7d93", "27": "\u9f8d\u85e5\u53c9\u4e7e\u64bb\u7e1b\u963f\u4fee\u7f85\u8616\u9b6f\u62cf\u7dca\u90a3\u7f85\u6469\u8b77", "270": "\u5de6\u624b\u528d\u5370\u62bc\u8170\u3002\u8d64\u8272\u5f62\u3002\u82e5\u4eba\u6b32\u5f97\u793a\u5316\u4eba\u5927\u9748\u9a57\u8005\u662f\u547c\u4f7f\u8005\u3002", "271": "\u7b2c\u5341\u4e94\u5149\u706b\u708e\u6469\u738b\u662f\u89c0\u97f3\u6240\u8b8a\u8eab\u5de6\u624b\u62bc\u8170\u53f3\u624b\u6301\u5bf6", "272": "\u73e0\u3002\u638c\u5fc3\u5c45\u3002\u9ed1\u8272\u5f62\u3002\u82e5\u4eba\u6b32\u5f97\u9a57\u5229\u6b64\u547c\u4f7f\u8005\u3002", "273": "\u7b2c\u5341\u516d\u4e94\u5929\u4eba\u6563\u7f85\u738b\u3002\u662f\u6bd8\u6c99\u9580\u5929\u6240\u8b8a\u8eab", "274": "\u53f3\u624b\u6301\u91d1\u525b\u6775\u5de6\u624b\u62bc\u8170\u3002\u8d64\u8272\u5f62\u4e5f\u3002\u82e5\u4eba\u6b32\u5f97\u4e00\u5207\u4eba\u611b\u5ff5\u8005\u662f\u547c\u4f7f\u8005\u3002", "275": "\u7b2c\u5341\u4e03\u795e\u6bcd\u5927\u5c0f\u8af8\u738b\u662f\u5ef6\u547d\u83e9\u85a9\u6240\u8b8a\u8eab", "276": "\u5de6\u6301\u7e1b\u6c99\u7f85\u53f3\u62bc\u8170\u3002\u8d64\u8272\u5f62\u4e5f\u3002\u82e5\u4eba\u6b32\u5f97\u52dd\u8ad6\u8b70\u8005\u662f\u547c\u4f7f\u8005\u3002", "277": "\u7b2c\u5341\u516b\u6425\u9418\u8fe6\u7f85\u5927\u738b\u3002\u662f\u51c6\u63d0\u89c0\u97f3\u6240\u8b8a\u8eab", "278": "\u53f3\u624b\u6301\u4e09\u53e4\u5de6\u62bc\u8170\u3002\u8d64\u8272\u5f62\u3002\u82e5\u4eba\u6b32\u5f97\u60e1\u4eba\u964d\u4f0f\u8005\u6b64\u547c\u4f7f\u8005\u3002", "279": "\u7b2c\u5341\u4e5d\u8fe6\u6bd8\u7f85\u4fee\u6cd5\u738b\u3002\u662f\u5343\u624b\u89c0\u97f3\u6240\u8b8a\u8eab", "28": "\u7f85\u8a90\u4eba\u53ca\u975e\u4eba\u7b49\u3002\u4e00\u5207\u7fa4\u751f\u985e\u7686\u4f86\u96c6\u6703\u8005\u3002", "280": "\u53f3\u6301\u84ee\u83ef\u5de6\u62bc\u8170\u3002\u8d64\u8272\u5f62\u3002\u82e5\u4eba\u6b32\u5f97\u6b62\u71b1\u75c5\u8005\u6b64\u547c\u4f7f\u8005\u3002", "281": "\u7b2c\u4e8c\u5341\u85e5\u53c9\u8af8\u5929\u738b\u3002\u662f\u5999\u898b\u83e9\u85a9\u6240\u8b8a\u8eab", "282": "\u53f3\u6301\u5927\u5200\u5de6\u62bc\u8170\u3002\u8d64\u8272\u5f62\u4e5f\u3002\u82e5\u4eba\u6b32\u5f97\u6b62\u75ab\u75c5\u8005\u662f\u3002\u547c\u4f7f\u8005\u3002", "283": "\u7b2c\u4e8c\u5341\u4e00\u4e09\u754c\u6388\u5929\u5927\u738b\u3002\u662f\u85e5\u738b\u83e9\u85a9\u6240\u8b8a", "284": "\u8eab\u53f3\u6301\u5854\u5de6\u6301\u5927\u5200\u3002\u8d64\u8272\u8863\u5f62\u4e5f\u3002\u82e5\u4eba\u6b32\u5f97\u5b98\u4f4d\u8005\u6b64\u547c\u4f7f\u8005\u3002", "285": "\u7b2c\u4e8c\u5341\u4e8c\u4ff1\u591a\u9077\u5316\u5929\u738b\u3002\u662f\u85e5\u4e0a\u83e9\u85a9\u6240\u8b8a", "286": "\u8eab\u53f3\u6301\u5f13\u5de6\u6301\u52a0\u5229\u6469\u591a\u3002\u8d64\u8272\u5f62\u3002\u82e5\u6b32\u5f97\u6210\u4e00\u5207\u6cd5\u5e2b\u738b\u8005\u6b64\u547c\u4f7f\u8005\u3002", "287": "\u7b2c\u4e8c\u5341\u4e09\u706b\u7f85\u8af8\u5929\u738b\u3002\u662f\u5730\u85cf\u83e9\u85a9\u6240\u8b8a\u8eab", "288": "\u53f3\u6301\u7e69\u7e1b\u6c99\u7f85\u5de6\u62bc\u8170\u3002\u8d64\u8272\u5f62\u3002\u82e5\u6b32\u5f97\u8af8\u5bcc\u4eba\u8005\u6b64\u547c\u4f7f\u8005\u3002", "289": "\u7b2c\u4e8c\u5341\u56db\u7686\u651d\u6301\u5929\u738b\u3002\u662f\u9f8d\u6a39\u83e9\u85a9\u6240\u8b8a\u8eab", "29": "\u53c8\u62bd\u64ae\u5f7c\u7fa4\u751f\u773e\u5dee\u5225\u4e4b\u5fc3\u3002\u4ee4\u540c\u4e00\u9ad4\u7b49\u4f4f", "290": "\u5de6\u53f3\u624b\u91d1\u525b\u5408\u638c\u5370\u3002\u5fff\u6012\u5f62\u3002\u4e8c\u76ee\u55d4\u6012\u9762\u8d64\u72c0\u9ed1\u8272\u5f62\u3002\u82e5\u6b32\u5f97\u5929\u72d0\u7d50\u7e33\u8005\u6b64\u547c\u4f7f\u8005\u3002\u5df2\u4e0a\u5de6\u4f7f\u8005\u4e8c\u5341\u56db\u4eba\u4e5f", "291": "\u662f\u767c\u5fc3\u4fee\u884c\u7fa9\u3002\u53f3\u4e8c\u5341\u56db\u4eba\u4f7f\u8005\u3002\u70c8\u5475\u7acb\u5f62\u4e5f\u3002\u662f\u4fee\u679c\u6210\u4f5b\u7fa9\u3002", "292": "\u7b2c\u4e00\u91d1\u525b\u4fee\u7f85\u738b\u3002\u662f\u52e2\u81f3\u83e9\u85a9\u6240\u8b8a\u8eab\u5de6\u8107\u7368\u53e4\u7528\u53f3", "293": "\u5de6\u5408\u638c\u5370\u3002\u9ed1\u8272\u5f62\u4e5f\u3002\u6b32\u5f97\u60e1\u9748\u90aa\u6c23\u7d50\u7e33\u8005\u662f\u547c\u4f7f\u8005\u3002", "294": "\u7b2c\u4e8c\u795e\u738b\u5f15\u651d\u5927\u58eb\u738b\u3002\u662f\u5409\u7965\u5929\u5973\u6240\u8b8a\u8eab", "295": "\u53f3\u6301\u6756\u5de6\u62bc\u8170\u3002\u8d64\u8272\u5f62\u4e5f\u3002\u6b32\u5f97\u9664\u6ec5\u4e00\u5207\u9b3c\u795e\u7e33\u7919\u8005\u662f\u547c\u4f7f\u8005\u3002", "296": "\u7b2c\u4e09\u4e8c\u5341\u516b\u5bbf\u8af8\u5927\u738b\u3002\u662f\u89c0\u97f3\u6240\u8b8a\u8eab\u53f3\u624b\u6301\u84ee\u83ef\u5de6", "297": "\u62bc\u8170\u3002\u767d\u8272\u5f62\u4e5f\u3002\u6216\u8d64\u8272\u5f62\u3002\u6b32\u5f97\u8af8\u4eba\u6c7a\u5b9a\u696d\u969c\u9664\u6ec5\u8005\u662f\u547c\u4f7f\u8005\u3002", "298": "\u7b2c\u56db\u4e00\u5207\u8af8\u6cd5\u53d7\u7528\u738b\u3002\u662f\u5c3e\u76e7\u820d\u90a3\u6240\u8b8a\u8eab", "299": "\u5de6\u6301\u9234\u53f3\u57f7\u528d\u3002\u9ed1\u8272\u5f62\u4e5f\u3002\u6b32\u5f97\u8af8\u5c0f\u5152\u77ed\u547d\u592e\u58fd\u6d88\u9664\u8005\u662f\u547c\u4f7f\u8005\u3002", "3": "\u5927\u8208\u5584\u5bfa\u4e09\u85cf\u6c99\u9580\u5927\u5ee3\u667a\u4e0d\u7a7a", "30": "\u4e09\u6469\u5730\u3002\u591a\u4ff1\u80dd\u4e16\u754c\u711a\u706b\u6ec5\u3002\u552f\u6210\u4e00\u5927\u706b", "300": "\u7b2c\u4e94\u8fe6\u8449\u5927\u546a\u5927\u58eb\u738b\u3002\u662f\u7121\u76e1\u610f\u83e9\u85a9\u6240\u8b8a", "301": "\u8eab\u53f3\u624b\u7528\u932b\u6756\u5de6\u6301\u8888\u88df\u3002\u5f62\u6b32\u5f97\u4e00\u5207\u5973\u4eba\u96e3\u7522\u751f\u8005\u662f\u547c\u4f7f\u8005\u3002", "302": "\u7b2c\u516d\u4e00\u4e00\u5404\u6709\u5927\u58eb\u738b\u3002\u662f\u5999\u97f3\u83e9\u85a9\u6240\u8b8a\u8eab", "303": "\u53f3\u624b\u6301\u6756\u9006\u5de6\u62bc\u8170\u3002\u8d64\u8272\u5f62\u4e5f\u3002\u6b32\u5f97\u6b62\u4e00\u5207\u60e1\u9b54\u9b3c\u795e\u985b\u72c2\u8005\u662f\u547c\u4f7f\u8005\u3002", "304": "\u7b2c\u4e03\u8b77\u6301\u8af8\u6cd5\u738b\u3002\u662f\u9640\u7f85\u5c3c\u83e9\u85a9\u6240\u8b8a\u8eab", "305": "\u53f3\u6301\u7d93\u5377\u5de6\u624b\u6301\u5ff5\u73e0\u3002\u8d64\u8272\u5f62\u4e5f\u3002\u82e5\u6b32\u5f97\u4e00\u5207\u4eba\u8ca1\u7269\u8005\u662f\u547c\u4f7f\u8005\u3002", "306": "\u7b2c\u516b\u543d\u767c\u591a\u7f85\u738b\u3002\u662f\u865b\u7a7a\u85cf\u83e9\u85a9\u6240\u8b8a\u8eab", "307": "\u53f3\u57f7\u5854\u5de6\u57f7\u5927\u5200\u3002\u8d64\u8272\u5f62\u4e5f\u3002\u6b32\u5f97\u9ad8\u8cb4\u5a66\u4eba\u8005\u662f\u547c\u4f7f\u8005\u3002", "308": "\u7b2c\u4e5d\u8607\u5c0f\u62d4\u82e6\u738b\u3002\u662f\u8336\u5409\u5c3c\u6240\u8b8a\u8eab\u53f3\u57f7\u9ad1\u9acf\u5de6\u624b\u62f3\u62bc", "309": "\u8170\u3002\u773c\u778b\u6012\u5f62\u4e5f\u3002\u9ed1\u8272\u5f62\u3002\u6b32\u5f97\u8fd4\u60e1\u4eba\u546a\u5480\u8005\u6b64\u547c\u4f7f\u8005\u3002", "31": "\u805a\u3002\u5982\u4e03\u65e5\u5149\u7167\u5927\u99ac\u53e3\u7b49\u3002\u773e\u6d41\u4f46\udb84\udd0d\u541e\u7d0d", "310": "\u7b2c\u5341\u6025\u6025\u5927\u5c0f\u795e\u5929\u738b\u3002\u662f\u5949\u6559\u5bae\u6240\u8b8a\u8eab", "311": "\u53f3\u5de6\u624b\u91d1\u525b\u5408\u638c\u5370\u3002\u8d64\u8272\u5f62\u4e5f\u3002\u6b32\u5f97\u6b62\u4e00\u5207\u707d\u96e3\u8005\u6b64\u547c\u4f7f\u8005\u3002", "312": "\u7b2c\u5341\u4e00\u90a3\u7e1b\u8fe6\u7f85\u738b\u3002\u662f\u6708\u5929\u6240\u8b8a\u8eab\u6b32\u5f97\u52dd\u8ecd\u9673\u8005\u6b64\u547c\u4f7f\u8005\u3002", "313": "\u7b2c\u5341\u4e8c\u6089\u5e95\u5730\u5927\u58eb\u738b\u3002\u662f\u6697\u591c\u5929\u6240\u8b8a\u8eab", "314": "\u53f3\u6301\u7368\u53e4\u5de6\u62f3\u62bc\u8170\u3002\u9ed1\u8272\u4e5f\u3002\u6b32\u5f97\u9664\u75c5\u6d41\u884c\u707d\u8005\u6b64\u547c\u4f7f\u8005\u3002", "315": "\u7b2c\u5341\u4e09\u795e\u738b\u7737\u5c6c\u5927\u667a\u738b\u3002\u662f\u7130\u9b54\u7f85\u738b\u6240\u8b8a", "316": "\u8eab\u53f3\u6301\u6756\u5de6\u62bc\u8170\u3002\u8d64\u8272\u5f62\u4e5f\u3002\u6b32\u5f97\u6975\u6a02\u5f80\u751f\u4e0a\u54c1\u4e0a\u751f\u8005\u662f\u547c\u4f7f\u8005\u3002", "317": "\u7b2c\u5341\u56db\u6469\u767b\u8fe6\u7f85\u5929\u4eba\u738b\u3002\u662f\u90a3\u7f85\u5ef6\u5929\u6240\u8b8a", "318": "\u8eab\u53f3\u7528\u4e09\u53e4\u6775\u5de6\u6301\u7d22\u3002\u9ed1\u8272\u5f62\u3002\u6b32\u5f97\u5176\u4e03\u4e16\u7236\u6bcd\u8005\u89c0\u4e00\u5207\u773e\u751f\u5730\u7344\u82e6\u8005\u662f\u547c\u4f7f\u8005\u3002", "319": "\u7b2c\u5341\u4e94\u5929\u5730\u53d7\u7528\u5927\u660e\u738b\u3002\u662f\u5ee3\u76ee\u5929\u6240\u8b8a\u8eab", "32": "\u7121\u9918\u76e1\u6210\u731b\u7130\u3002\u8aaa\u662f\u5927\u5a01\u6012\u738b\u52dd\u8ecd\u4e0d\u52d5\u660e", "320": "\u53f3\u6301\u528d\u5de6\u62bc\u8170\u3002\u8d64\u8272\u5f62\u3002\u6b32\u5f97\u592b\u5a66\u611b\u76f8\u8005\u662f\u547c\u4f7f\u8005\u3002", "321": "\u7b2c\u5341\u516d\u8af8\u795e\u7686\u5f97\u5927\u738b\u3002\u662f\u6c34\u5929\u6240\u8b8a\u8eab\u53f3\u57f7\u7368\u53e4\u5de6\u62bc", "322": "\u8170\u3002\u9ed1\u8272\u5f62\u3002\u6b32\u5f97\u6b62\u4e00\u5207\u807e\u7602\u75c5\u8005\u662f\u547c\u4f7f\u8005\u3002", "323": "\u7b2c\u5341\u4e03\u4e00\u4e00\u6771\u897f\u5357\u5317\u738b\u3002\u662f\u8fa8\u624d\u5929\u6240\u8b8a\u8eab\u53f3\u6301", "324": "\u5ff5\u73e0\u5de6\u62bc\u8170\u3002\u767d\u8272\u5f62\u3002\u6b32\u5f97\u4ee4\u592b\u5a66\u76f8\u96e2\u60e1\u662f\u547c\u4f7f\u8005\u3002", "325": "\u7b2c\u5341\u516b\u5bc6\u546a\u53d7\u6301\u738b\u3002\u662f\u7f85\u524e\u5929\u6240\u8b8a\u8eab\u53f3\u57f7\u5982\u610f\u73e0\u5de6", "326": "\u62bc\u8170\u3002\u8d64\u8272\u5f62\u3002\u6b32\u5f97\u4e00\u5207\u7269\u8005\u662f\u547c\u4f7f\u8005\u3002", "327": "\u7b2c\u5341\u4e5d\u8fe6\u8449\u5927\u738b\u3002\u662f\u963f\u4fee\u7f85\u738b\u6240\u8b8a\u8eab\u53f3\u57f7\u5927\u5200\u5de6\u624b", "328": "\u57f7\u7d22\u62bc\u8170\u3002\u8d64\u8272\u3002\u4e94\u9808\u5982\u5929\u738b\u3002\u6b32\u5f97\u9664\u76dc\u4eba\u96e3\u8005\u662f\u547c\u4f7f\u8005\u3002", "329": "\u7b2c\u4e8c\u5341\u6c99\u7f85\u4ed9\u5927\u795e\u738b\u3002\u662f\u6eff\u610f\u5929\u6240\u8b8a\u8eab", "33": "\u738b\u5fae\u5999\u5fc3\u3002\u4ea6\u5927\u99ac\u53e3\u541e\u5649\u4e00\u5207\u6709\u60c5\u3002\u4e00\u5207\u773e", "330": "\u53f3\u624b\u6301\u93e1\u5de6\u62bc\u8170\u3002\u767d\u8272\u3002\u6b32\u5f97\u5f9e\u8005\u4ed6\u6240\u8fd4\u8005\u662f\u547c\u4f7f\u8005\u3002", "331": "\u7b2c\u4e8c\u5341\u4e00\u83ab\u546a\u5927\u546a\u5927\u660e\u738b\u3002\u662f\u5927\u65e5\u6240\u8b8a\u8eab", "332": "\u53f3\u624b\u57f7\u528d\u5de6\u6301\u7d22\u3002\u773c\u778b\u6012\u9ed1\u8272\u5f62\u3002\u6b32\u5f97\u725b\u99ac\u8fef\u904a\u8005\u662f\u547c\u4f7f\u8005\u3002", "333": "\u7b2c\u4e8c\u5341\u4e8c\u6703\u96c6\u795e\u738b\u3002\u662f\u904d\u97f3\u5929\u6240\u8b8a\u8eab\u53f3\u6301\u927e\u5de6\u62bc\u8170\u3002", "334": "\u8d64\u8272\u5f62\u3002\u6b32\u5f97\u7530\u7560\u8fd4\u8005\u6b64\u547c\u4f7f\u8005\u3002", "335": "\u7b2c\u4e8c\u5341\u4e09\u592a\u4e00\u5fb7\u738b\u3002\u662f\u666e\u4e16\u5929\u6240\u8b8a\u8eab\u53f3\u6301\u4e09\u53e4\u6775\u5de6", "336": "\u4f5c\u528d\u5370\u62bc\u8170\u3002\u9ed1\u8272\u5f62\u3002\u6b32\u5f97\u5e2b\u9577\u611b\u656c\u8005\u662f\u547c\u4f7f\u8005\u3002", "337": "\u7b2c\u4e8c\u5341\u56db\u4e00\u5207\u8af8\u795e\u738b\u3002\u662f\u706b\u5929\u6240\u8b8a\u8eab\u53f3\u57f7\u4e94\u53e4\u9234\u5de6", "338": "\u62bc\u8170\u3002\u8d64\u8272\u8863\u3002\u773c\u778b\u6012\u5f62\u3002\u6b32\u5f97\u4e00\u5207\u8af8\u4f5b\u611b\u8b77\u8005\u662f\u547c\u4f7f\u8005\u5fc5\u6210\u5c31\u3002", "339": "\u56db\u5341\u516b\u4eba\u4f7f\u8005\u3002\u4e0d\u52d5\u660e\u738b\u5b88\u8b77\u6301\u6cd5\u884c\u8005\u6545\u3002", "34": "\u751f\u82e5\u5e72\u696d\u7169\u60f1\u5fc3\u56e0\u7b49\u3002\u6210\u5927\u706b\u708e\u4e16\u754c\u3002\u723e\u6642", "340": "\u56db\u5341\u516b\u5165\u8af8\u9b3c\u738b\u8eab\u4e5f\u3002\u4e00\u5207\u5982\u4f86\u4e00\u5207\u83e9\u85a9", "341": "\u5929\u7b49\u3002\u70ba\u4f5b\u6cd5\u8b77\u6301\u4ee5\u6b0a\u8eab\u3002\u660e\u738b\u6301\u5ff5\u884c\u8005\u6210", "342": "\u7a2e\u7a2e\u9b3c\u738b\u8eab\u3002\u665d\u591c\u5b88\u8b77\u4e5f\u3002\u662f\u8af8\u4f5b\u83e9\u85a9\u5929", "343": "\u7b49\u65b9\u4fbf\u8eab\u4e5f\u3002\u723e\u6642\u822c\u82e5\u83e9\u85a9\u3002\u544a\u4e00\u5207\u8af8\u9b3c\u738b", "344": "\u8a00\u3002\u82e5\u4e09\u5343\u5927\u5343\u4e16\u754c\u9b54\u738b\u3002\u4e0d\u9806\u4e00\u5207\u5982\u4f86", "345": "\u52c5\u547d\u80fd\u4ee4\u964d\u4f0f\u3002\u5584\u9053\u6211\u4f4f\u4e09\u6627\u5730\u3002\u800c\u4ee4\u7121\u9a5a", "346": "\u754f\u3002\u4e91\u4f55\u4e0d\u4f4f\u5373\u5176\u8af8\u4f5b\u6240\u3002\u65bc\u5927\u66fc\u9640\u7f85\u4e2d\u800c", "347": "\u5f97\u6cd5\u5229\u3002\u6642\u52dd\u8ecd\u5927\u52d5\u660e\u738b\u767d\u4f5b\u8a00\u3002\u6b64\u5927\u81ea\u5728", "348": "\u5929\u7576\u7526\u4e91\u4f55\u3002\u4f5b\u8a00\u6c5d\u61c9\u7526\u4e4b\u6642\u3002\u4e0d\u52d5\u660e\u738b\u5373", "349": "\u8aaa\u6cd5\u754c\u751f\u771f\u8a00\u3002\u723e\u6642\u5927\u81ea\u5728\u8005\u5373\u5fa9\u8607\u606f\u751f", "35": "\u822c\u82e5\u83e9\u85a9\u767d\u4f5b\u8a00\u3002\u6211\u4e00\u5207\u5982\u4f86\u4e00\u5207\u83e9\u85a9\u8072", "350": "\u5927\u6b61\u5584\u767d\u4f5b\u8a00\u3002\u751a\u5e0c\u6709\u4e5f\u3002\u6211\u521d\u5404\u81f3\u5df2\u554f\u4f5b\u3002", "351": "\u6b64\u591c\u53c9\u662f\u4f55\u7b49\u985e\u3002\u6211\u6240\u4e0d\u89e3\u3002\u4f5b\u8a00\u662f\u8af8\u4f5b\u4e4b", "352": "\u4e3b\u6211\u4f5c\u662f\u5ff5\u3002\u8af8\u4f5b\u4e00\u5207\u4e4b\u5c0a\u3002\u4e91\u4f55\u4ee5\u6b64\u800c\u66f4", "353": "\u70ba\u4e3b\u8036\u3002\u662f\u6240\u4e0d\u89e3\u3002\u4eca\u4e43\u77e5\u4e4b\u3002\u7531\u6b64\u5927\u738b\u529b\u6545\u3002", "354": "\u4ee4\u6211\u73fe\u524d\u5f97\u8a18\u4f5c\u4f5b\u3002\u7576\u77e5\u662f\u8af8\u4f5b\u4e4b\u5c0a\u4e5f\u3002\u6240", "355": "\u4ee5\u7136\u8005\u3002\u5927\u81ea\u5728\u5929\u4e09\u5343\u4e16\u754c\u4e4b\u4e3b\u3002\u5373\u662f\u773e\u751f", "356": "\u81ea\u5fc3\u3002\u6240\u8b02\u7121\u59cb\u7121\u660e\u4f4f\u5730\u3002\u65bc\u8af8\u60d1\u4e2d\u800c\u5f97\u81ea", "357": "\u5728\u3002\u552f\u9664\u5927\u83e9\u63d0\u5fc3\u7121\u80fd\u4f0f\u8005\u3002\u65b7\u5176\u547d\u5df2\u5373\u662f", "358": "\u65bc\u5bc2\u7136\u4e16\u754c\u4f5c\u8b49\u3002\u6240\u8b02\u751f\u8005\u5373\u662f\u8d77\u4f5b\u6167\u9580\u3002", "359": "\u662f\u6545\u771f\u8a00\u884c\u8005\u3002\u61c9\u4e00\u4e00\u601d\u60df\u8af8\u4f5b\u5bc6\u8a9e\u4e5f\u3002", "36": "\u805e\u7de3\u89ba\u8f9f\u652f\u4f5b\u3002\u4e43\u81f3\u5929\u9f8d\u516b\u90e8\u4e00\u5207\u6709\u60c5\u773e", "360": "\u82e5\u6709\u51f6\u5b85\u6050\u61fc\u4e4b\u8655\u6216\u6709\u5bae\u4e8b\u903c\u60f1\u8005\u3002\u7576\u4e00", "361": "\u5fc3\u8aa6\u6839\u672c\u546a\u3002\u4ea6\u53ef\u66f8\u546a\u91d8\u65bc\u5ead\u4e2d\u3002\u4ee4\u706b\u5730\u4e2d", "362": "\u842c\u59d3\u4e0d\u6562\u52d5\u4f5c\u3002\u4ea6\u91d8\u6b64\u6148\u6551\u546a\u3002\u4ea6\u6e05\u6de8\u5ba4\u4e2d", "363": "\u7d50\u754c\u8b77\u8eab\u3002\u7e6b\u5fc3\u4e0d\u52d5\u4f7f\u8005\u3002\u5c08\u5ff5\u4e0d\u7d55\u3002\u591a\u65e5\u8aa6", "364": "\u5f4c\u5f80\u4e43\u81f3\u5341\u6d1b\u53c9\u3002\u6bcf\u98df\u51fa\u4e00\u5206\u3002\u606d\u656c\u4f9b\u990a\u4e0d", "365": "\u52d5\u4f7f\u8005\u3002\u81ea\u60f3\u904e\u53bb\u8af8\u4f5b\u3002\u70ba\u5316\u5ea6\u5229\u76ca\u4e00\u5207", "366": "\u773e\u751f\u3002\u73fe\u4e00\u5207\u9b3c\u738b\u5927\u8eab\u5fff\u6012\u738b\u3002\u4e00\u5207\u9b54\u738b", "367": "\u70ba\u7d50\u7e1b\u4e00\u5207\u9b3c\u738b\u3002\u73fe\u5927\u8eab\u7686\u4ee4\u5165\u4f5b\u679c\u3002\u4f3c\u5404", "368": "\u5404\u5f62\u72c0\u3002\u4ee5\u5f62\u70ba\u5f7c\u964d\u4f0f\u6545\u793a\u73fe\u8eab\u5fc3\u4e5f\u3002\u9b54\u8005", "369": "\u662f\u83e9\u85a9\u7fa9\u3002\u9b3c\u8005\u6d85\u69c3\u7fa9\u3002\u738b\u8005\u81ea\u5728\u901a\u7fa9\u3002\u662f\u4e00", "37": "\u751f\u754c\u3002\u70ba\u6b32\u6190\u610d\u5316\u5ea6\u5229\u76ca\u3002\u52dd\u8ecd\u660e\u738b\u73fe\u5fff\u8eab\u3002", "370": "\u5207\u4e16\u9593\u6210\u5c31\u7fa9\u3002\u6b64\u6cd5\u5831\u61c9\u4e09\u8eab\u3002\u4ee5\u4e09\u5982\u4f86\u9b54", "371": "\u738b\u8eab\u53ef\u4e91\u9b54\u738b\u3002\u5982\u4f86\u662f\u5927\u4e8b\u56e0\u7de3\u5584\u77e5\u8b58\u4e5f\u3002", "372": "\u4e00\u5207\u773e\u751f\u6240\u5f97\u4e09\u7a2e\u61fa\u6094\u4e5f\u3002\u8caa\u55d4\u7661\u662f\u4e00\u5207", "373": "\u773e\u751f\u6839\u672c\u4e5f\u3002\u6210\u4f5b\u662f\u61fa\u6094\u4e0d\u76e1\u3002\u53c8\u6210\u4f5b\u5f8c\u70ba", "374": "\u5316\u5ea6\u4e00\u5207\u773e\u751f\u3002\u8caa\u55d4\u7661\u662f\u6839\u672c\u61fa\u6094\u4e5f\u3002\u5ff5\u5ff5", "375": "\u52e4\u52e4\u83ab\u9593\u3002\u6b64\u6700\u6839\u672c\u771f\u8a00\u8aa6\u3002\u8aa6\u901f\u5f97\u6210\u5c31\u9a57", "376": "\u529f\u3002\u6211\u70ba\u6301\u5ff5\u884c\u8005\u7121\u9918\u5ff5\u3002\u884c\u4f4f\u5750\u81e5\u81f4\u5ff5\u8aa6", "377": "\u4e0d\u6563\u4e82\u3002\u5fc3\u70ba\u6cd5\u6210\u5c31\u6545\u3002\u6240\u8a93\u9858\u4e5f\u3002\u61c8\u6020\u4e4b\u5f1f", "378": "\u5b50\u3002\u4e0d\u4fe1\u4e4b\u6545\u6cd5\u4e0d\u6210\u5c31\u3002\u4f9d\u4e4b\u767c\u90aa\u57f7\u3002\u81ea\u77e5\u83ab", "379": "\u5411\u4eba\u8aaa\u3002\u4e00\u6d1b\u53c9\u5341\u842c\u904d\u4e5f\u3002\u52dd\u8ecd\u660e\u738b\u6cd5\u7565\u8981", "38": "\u5f9e\u6469\u8a36\u5c3e\u76e7\u906e\u90a3\u5fc3\u4e94\u660e\u738b\u51fa\u751f\u3002\u4e00\u5207\u8af8\u4f5b", "380": "\u76e1\u6b64\u3002\u4fe1\u53d7\u5949\u884c\u4e8c\u7a2e\u5370\u53e3\u50b3\u53ef\u805e\u8005\u3002", "381": "\u52dd\u8ecd\u4e0d\u52d5\u7955\u5bc6\u5100\u8ecc\u4e00\u5377", "382": "\u897f\u9662\u5b8f\u6559\u5f8b\u5e2b\u3002\u752b\u6587\u6284\u4e5d\u3002\u4e0d\u52d5\u8a18\u4e91\u3002\u52dd\u8ecd", "383": "\u5100\u8ecc\u4e2d\u4e91\u3002\u4e00\u6301\u7955\u5bc6\u546a\u751f\u751f\u52a0\u8b77\u3002\u6c42\u8993\u5176", "384": "\u672c\u7136\u4e0d\u5f97\u65e2\u4e45\u3002\u4eca\u6b72\u590f\u516d\u6708\u3002\u5e78\u5f97\u6b64\u8ecc\u5c0f", "385": "\u6c60\u574a\u5bf6\u5eab\u3002\u662f\u4fe1\u6055\u50e7\u6b63\u50b3\u6301\u4e4b\u672c\u4e5f\u3002\u4eab\u548c", "386": "\u6539\u5143\u8f9b\u9149\u51ac\u3002\u5171\u4e8c\u4e09\u4e4b\u540c\u5fd7\u3002\u630d\u9ede\u4e4b\u547d\u5de5", "387": "\u58fd\udb84\udc5d\u3002\u5f18\u660e\u738b\u53ca\u56db\u5341\u516b\u4f7f\u8005\u4e4b\u5a01\u5fb7\u3002", "388": "\u8c50\u5c71\u7e3d\u6301\u9662\u5feb\u9053\u8a8c", "389": "\u58a8\u7b46", "39": "\u5982\u6b64\u3002\u662f\u5927\u65e5\u4e94\u660e\u738b\u4e94\u6ce2\u7f85\u871c\u9580\u3002\u53c8\u51fa\u751f\u5176", "390": "\u6b64\u8ecc\u672a\u5f97\u4ed6\u672c\u3002\u4e0d\u5f97\u5c0d\u6821\u4ee5\u6545\u3002\u62ab\u95b1\u4f7f\u8005", "391": "\u6cd5\u7b49\u6578\u90e8\u3002\u7a0d\u52a0\u7b46\u524a\u8033\u3002\u91cd\u5f97\u6821\u672c\u53ef\u6c7a\u70cf", "392": "\u7109\u4e5f\u3002\u4eab\u548c\u7678\u4ea5\u56db\u6708\u4e8c\u5341\u4e00\u65e5\u6b63\u5348\u3002", "393": "\u6148\u9806", "394": "\u6731\u7b46", "395": "\u6587\u5316\u5143\u5e74\u7532\u5b50\u4e03\u6708\u5341\u65e5\u3002\u4ee5\u9ad8\u5c71\u5bfa\u5beb\u672c", "396": "\u9042\u4e00\u6821\u7562\u3002\u3000\u3000\u3000\u3000\u3000\u3000\u3000\u3000\u3000\u3000\u6148\u9806", "4": "\u5929\u7afa\u570b\u5a46\u7f85\u9580\u50e7\u904d\u667a\u5949\u3000\u8a54\u96c6", "40": "\u5206\u8eab\u3002\u4e00\u660e\u738b\u540d\u66f0\u52dd\u8ecd\u4e0d\u52d5\u3002\u4e09\u5343\u5927\u5343\u754c\u4e2d", "41": "\u73fe\u81ea\u5728\u708e\u660e\u5a01\u5fb7\u8eab\u3002\u5373\u81ea\u6027\u8f2a\u8eab\u822c\u82e5\u83e9\u85a9\u3002", "42": "\u70ba\u6298\u4f0f\u651d\u53d7\u96a8\u985e\u773e\u751f\u3002\u70ba\u57f7\u91d1\u525b\u5f1f\u5b50\u6756\u3002\u70ba", "43": "\u52a0\u6301\u7737\u5c6c\u6467\u7834\u4e00\u5207\u969c\u6545\u3002\u5982\u4e0d\u99b1\u5927\u529b\u958b\u793a", "44": "\u5fff\u6012\u91d1\u525b\u9ad4\u3002\u5373\u4f4f\u706b\u751f\u4e09\u6627\u3002\u8aaa\u6467\u5927\u969c\u8005\u771f", "45": "\u8a00\u3002\u6b64\u771f\u8a00\u5927\u5a01\u52e2\u80fd\u9664\u4e00\u5207\u3002\u6467\u771f\u884c\u8005\u969c\u96e3\u3002", "46": "\u5f9e\u767c\u83e9\u63d0\u5fc3\u5b88\u8b77\u589e\u9577\u3002\u5343\u5168\u81f3\u6210\u4f5b\u679c\u3002\u7d42\u4e0d", "47": "\u9000\u5931\u4e0d\u58ae\u5728\u975e\u9053\u4e2d\u3002\u4e43\u81f3\u5728\u4f5b\u9053\u901f\u75be\u8b49\u679c\u3002", "48": "\u660e\u738b\u56db\u5f18\u9858\u4e91\u3002", "49": "\u898b\u6211\u8eab\u8005\u767c\u83e9\u63d0\u5fc3\u805e\u6211\u540d\u8005", "5": "\u723e\u6642\u822c\u82e5\u83e9\u85a9\u5165\u4e09\u6469\u5730\u3002\u540d\u91d1\u525b\u7b49\u81f3\u3002\u71be\u76db", "50": "\u65b7\u60e1\u4fee\u5584\u805e\u6211\u8aaa\u8005\u5f97\u5927\u667a\u6167", "51": "\u77e5\u6211\u5fc3\u8005\u5373\u8eab\u6210\u4f5b", "52": "\u4e00\u6301\u7955\u5bc6\u546a\u3002\u751f\u751f\u800c\u52a0\u8b77\u3002\u6301\u5ff5\u6211\u8005\u4e0d\u53ef\u81f3", "53": "\u7591\u5fc3\u3002\u4f55\u6cc1\u4e16\u9593\u8af8\u969c\u54c9\u3002\u552f\u6709\u4e8c\u7a2e\u3002\u4e00\u8005\u5167", "54": "\u969c\u3002\u8b02\u5f9e\u81ea\u5fc3\u751f\u3002\u5176\u985e\u751a\u591a\u4e0d\u53ef\u8aaa\u3002\u4e8c\u8005\u5916", "55": "\u969c\u3002\u5f9e\u5916\u4e8b\u751f\u3002\u5176\u985e\u4ea6\u591a\u3002\u6b64\u771f\u8a00\u7686\u80fd\u6467\u6ec5", "56": "\u8f9f\u9664\u3002\u4ee4\u8eab\u7121\u57a2\u6e05\u6de8\u3002\u4f9d\u8aaa\u5373\u8eab\u6210\u4f5b\u3002\u662f\u6df1", "57": "\u7955\u4e4b\u4e2d\u6975\u5bc6\u6559\u4e5f\u3002\u4e91\u4f55\u56db\u7a2e\u56e0\u7de3\u3002\u4e00\u8005\u65b7\u98df\u3002", "58": "\u4e8c\u8005\u670d\u6c23\u98df\u3002\u4e09\u8005\u83dc\u98df\u3002\u56db\u8005\u7bc0\u98df\u3002\u96a8\u529b\u6240\u8fa6", "59": "\u81ea\u7d04\u8eab\u5df2\u3002\u8aa6\u6839\u672c\u9640\u7f85\u5c3c\u3002\u4e00\u6d1b\u53c9\u4e43\u81f3\u4e09\u6d1b", "6": "\u708e\u7130\u3002\u5176\u708e\u666e\u7167\u4e00\u5207\u4f5b\u571f\u3002\u5468\u904d\u711a\u71d2\u4e09\u5343", "60": "\u53c9\u5df2\u5373\u5df2\u3002\u4e00\u65e5\u4e00\u591c\u6c34\u4e0d\u98f2\u3002\u5ee3\u5927\u4f9b\u990a\u838a\u56b4", "61": "\u9053\u5834\u3002\u65bc\u756b\u50cf\u524d\u71d2\u82e6\u7df4\u6728\u3002\u5982\u5927\u6307\u9577\u5341\u4e8c", "62": "\u6307\u3002\u5169\u982d\u6435\u8457\u8607\u871c\u3002\u8aa6\u546a\u4e00\u904d\u71d2\u4e00\u679d\u6728\u3002\u6eff\u4e00", "63": "\u5343\u516b\u904d\u71d2\u4e86\u3002\u5373\u5c11\u5c11\u4e16\u9593\u4e8b\u4fbf\u5982\u9858\u6eff\u3002\u5fa9\u6b21", "64": "\u8aa6\u6578\u6eff\u5df2\u3002\u5165\u6c5f\u6cb3\u6d77\u3002\u6df1\u81f3\u9838\u8655\u9762\u5411\u6771\u3002\u6bcf", "65": "\u65e5\u5ff5\u8aa6\u6578\u6eff\u4e09\u6d1b\u53c9\u5df2\u3002\u5fc3\u4e2d\u6240\u53d7\u798f\u7530\u7686\u6eff", "66": "\u9858\u3002\u5176\u6c34\u4ee4\u81f4\u86c7\u9f9c\u4e4b\u8655\u3002\u6cc1\u546a\u529b\u672a\u6210\u70ba\u4ed6\u6240", "67": "\u640d\u5e38\u9808\u7d50\u754c\u3002\u82e5\u5e38\u52a0\u529f\u6301\u8aa6\u4e0d\u52d5\u6839\u672c\u771f", "68": "\u8a00\u3002\u73fe\u8eab\u96a8\u529b\u80fd\u7e1b\u4e00\u5207\u9b3c\u795e\u3002\u4ea6\u80fd\u6467\u65ab\u4e00\u5207", "69": "\u6a39\u6728\u3002\u4ea6\u4ee4\u7a7a\u4e2d\u98db\u9ce5\u884c\u8005\u524d\u58ae\u589c\u3002\u4ea6\u80fd\u57f7\u4e7e", "7": "\u5927\u5343\u4e16\u754c\u3002\u5176\u4e2d\u6240\u6709\u4e00\u5207\u9b54\u7f85\u4f5c\u96e3\u8005\u3002\u4e00\u5207", "70": "\u7aed\u9f8d\u6dc0\u3002\u82e5\u6b32\u8ad6\u8b70\u53ca\u7834\u5916\u9053\u8fd4\u60e1\u4eba\u7b49\u3002\u7686\u80fd", "71": "\u964d\u4f0f\u8005\u3002\u5fa9\u6b21\u5148\u5019\u6708\u6b32\u98df\u6642\u3002\u4ee4\u8aa6\u546a\u6eff\u6578\u5148", "72": "\u98f2\u3002\u4e00\u65e5\u4e00\u591c\u4e0d\u98df\u3002\u53d6\u7948\u725b\u7cde\u3002\u4ee5\u672a\u843d\u5730\u8005\u3002", "73": "\u4ee5\u5668\u627f\u53d6\u83ab\u4ee4\u81f3\u5730\u3002\u4e8c\u8005\u4ea6\u843d\u5730\u6642\u5875\u571f\u672a", "74": "\u6c61\u3002\u53d6\u4e0a\u4ee4\u4e0d\u8457\u5730\u3002\u5c07\u58c7\u53d6\u7a2e\u7a2e\u83ef\u6563\u4ee5\u4f9b\u990a\u3002", "75": "\u53d6\u5927\u822c\u82e5\u7d93\u5b89\u7f6e\u4e2d\u5fc3\u8457\u3002\u53d6\u540c\u8272\u8001\u72a2\u68d5\u725b", "76": "\u4e73\u4f5c\u916a\u65bd\u3002\u6b21\u53d6\u8607\u8207\u719f\u9285\u697e\u76db\u3002\u4ee5\u53ef\u91cc\u7f85", "77": "\u6728\u4f5c\u7be6\u652a\u8607\u3002\u65bc\u6708\u521d\u98f2\u5373\u5ff5\u8aa6\u3002\u4e43\u81f3\u898b\u4e09", "78": "\u7a2e\u76f8\u3002\u6240\u8b02\u7130\u7b49\u3002\u6696\u76f8\u8005\u670d\u4e4b\u5dee\u4e00\u5207\u75c5\u3002\u5f97\u70df", "79": "\u76f8\u8005\u4ee5\u5c07\u5857\u5df1\u8eab\u53ef\u96b1\u853d\u3002\u4e0d\u4ee4\u60e1\u4eba\u5f97\u898b\u4e5f\u3002", "8": "\u5c3e\u66e9\u4e5f\u8fe6\u7f85\u524e\u5a11\u7b49\u3002\u88ab\u662f\u5927\u706b\u711a\u71d2\u75ab\u695a", "80": "\u5f97\u706b\u708e\u76f8\u8005\u670d\u4e4b\u795e\u901a\u8eab\u80fd\u98db\u884c\u3002\u6240\u8b02\u8eab\u901a", "81": "\u4e5f\u3002\u8001\u72a2\u725b\u4e73\u8005\u3002\u72a2\u751f\u7d93\u5e74\u5df2\u4e0a\u72a2\u5927\u5982\u6bcd\u3002\u800c", "82": "\u7336\u5b64\u98df\u4e73\u3002\u5176\u72a2\u6bdb\u8272\u9808\u8207\u6bcd\u540c\u3002\u5982\u662f\u725b\u4e73\u6e5b", "83": "\u4f5c\u8607\u7528\u3002\u5fa9\u6b21\u884c\u4eba\u5c71\u9ad8\u9802\u4e4b\u8655\u3002\u65b7\u7a40\u4e0d\u98df\u3002\u66f4", "84": "\u8aa6\u4e00\u6d1b\u53c9\u3002\u5fc3\u5fc3\u76f8\u7e8c\u66f4\u83ab\u7570\u7de3\u3002\u5929\u4e4b\u4f0f\u85cf\u81ea", "85": "\u7136\u51fa\u73fe\u3002\u51e1\u4f0f\u85cf\u6709\u5929\u6709\u795e\u6709\u4eba\u3002\u4eba\u6240\u57cb\u85cf\u540d", "86": "\u4eba\u85cf\u3002\u9b3c\u795e\u6240\u5b88\u540d\u66f0\u795e\u4f0f\u85cf\u3002\u4e14\u540d\u5730\u4f0f\u85cf\u3002", "87": "\u8af8\u5929\u7b49\u8b77\u8005\u70ba\u5929\u4f0f\u85cf\u3002\u5c1a\u80fd\u5f97\u898b\u3002\u6085\u5730\u85cf\u5b88", "88": "\u4eba\u85cf\u4e4e\u3002\u61c9\u4f5c\u798f\u4e8b\u96a8\u610f\u66f4\u7528\u3002\u5fa9\u6b21\u4e00\u71d2\u546a\u6eff", "89": "\u4e00\u5343\u904d\u3002\u80fd\u9664\u570b\u4e2d\u5927\u75ab\u75be\u4e5f\u3002\u8b77\u6469\u6cd5\u8005\u3002\u5730", "9": "\u904d\u5165\u3002\u652f\u9ad4\u82e6\u75db\u7d5e\u7d50\u7e8f\u8eab\u3002\u5fc3\u8ff7\u60b6\u7d55\u54b8\u7686\u9ad8", "90": "\u4f5c\u7210\u8005\u8457\u706b\u4ee4\u71be\u3002\u6753\u5b50\u6cd5\u8005\u4ee5\u5805\u6728\u524b\u4e4b\u3002\u982d", "91": "\u5982\u6753\u53ef\u53d7\u9dc4\u5b50\u9ec3\u5df2\u4e0b\u4e73\u3002\u5176\u70b3\u7aef\u5176\u9577\u4e8c\u5c3a", "92": "\u8a31\u3002\u5225\u4ee5\u6de8\u5668\u76db\u4e73\u3002\u4ee5\u6b64\u6753\u5b50\u914c\u800c\u546a\u4e4b\u3002\u800c", "93": "\u570b\u75ab\u7642\u3002\u6216\u4ee5\u5bb6\u4e00\u6301\u9054\u3002\u76f8\u67d3\u8457\u7686\u6b7b\u76e1\u3002\u4ee4", "94": "\u5433\u8700\u5dba\u5357\u6709\u4e8b\u4f5c\u80fd\u9664\u3002\u5fa9\u6b21\u53d6\u5411\u8349\u83ef\u548c", "95": "\u8607\u871c\u3002\u4e00\u546a\u4e00\u71d2\u6240\u6c42\u8863\u670d\u5982\u6b64\u83ef\u8272\u3002\u7686\u5f97\u7a31", "96": "\u610f\u3002\u82e5\u6c42\u7dcb\u8005\u7576\u71d2\u8d64\u83ef\u3002\u9918\u7686\u51c6\u6b64\u3002\u4292\u4e0d\u601d", "97": "\u8b70\u795e\u529b\u6eff\u884c\u4eba\u9858\u3002\u53ef\u91cf\u5206\u7d04\u4e8b\u3002\u800c\u4e8c\u672d\u5247\u7121", "98": "\u6709\u5fd7\u3002\u82e5\u975e\u5206\u5984\u53d6\u795e\u4ea6\u6162\u4eba\u3002\u96d6\u5f97\u4e0d\u8cb4\u3002\u82e5", "99": "\u71d2\u871c\u7f85\u7e1b\u4e00\u6d1b\u53c9\u3002\u71d2\u4e8c\u546a\u5f97\u570b\u4e2d\u7b2c\u4e00\u5b98\u4f4d\u3002"}, "source": "CBETA", "language": "chinese", "originalTitle": "Taisho Tripitaka, Electronic version, No. 1205 \u52dd\u8ecd\u4e0d\u52d5\u660e\u738b\u56db\u5341\u516b\u4f7f\u8005\u7955\u5bc6\u6210\u5c31\u5100\u8ecc", "author": "\u5510 \u904d\u667a\u96c6", "sourceLink": "http://www.cbeta.org/"}