05", "127": "\u61c9\u53d7\u4eba\u5929\u4f9b\u591a\u7f85\u5927\u60b2\u8005", "128": "\u4e00\u5207\u4e4b\u6148\u6bcd\u5929\u4eba\u53ca\u85e5\u53c9", "129": "\u7121\u4e00\u975e\u5b50\u8005\u6545\u865f\u4e16\u9593\u6bcd", "130": "\u53ca\u8207\u51fa\u4e16\u9593\u89c0\u97f3\u5927\u52e2\u81f3", "131": "\u91d1\u525b\u8207\u5584\u624d\u6587\u6b8a\u9808\u83e9\u63d0", "132": "\u6148\u6c0f\u8207\u9999\u8c61\u6708\u5149\u7121\u76e1\u610f", "133": "\u96e2\u57a2\u865b\u7a7a\u85cf\u5999\u773c\u53ca\u5927\u6167", "134": "\u7dad\u6469\u7b49\u83e9\u85a9\u7686\u662f\u591a\u7f85\u5b50", "135": "\u4ea6\u662f\u6ce2\u82e5\u6bcd\u4e09\u4e16\u8af8\u5982\u4f86", "136": "\u4e00\u5207\u6469\u8a36\u85a9\u7121\u4e00\u975e\u5b50\u8005", "137": "\u7686\u7a31\u662f\u6211\u6bcd\u6148\u80b2\u8af8\u6709\u60c5", "138": "\u5b89\u8f09\u5982\u5927\u5730", "139": "\u723e\u6642\u89c0\u81ea\u5728\u8aaa\u662f\u5048\u5df2\u3002\u5373\u70ba\u591a\u7f85\u83e9\u85a9\u3002\u8aaa\u9640", "140": "\u7f85\u5c3c\u66f0\u3002", "141": "\u90a3\u6155\u56c9\u601b\u5a1c\u4e8c\u5408\u601b\u56c9\u4e8c\u5408\u591c\u8036\u5a1c\u83ab\u963f\u5229\u8036", "142": "\u56a9\u5695\u5409\u5e1d\u6fd5\u56a9\u4e8c\u5408\u56c9\u8036\u5192\u5730\u85a9\u57f5\u8036\u6469\u8a36\u85a9", "143": "\u57f5\u8036\u6469\u8a36\u8fe6\u8def\u5c3c\u8fe6\u8036\u601b\u59ea\u4ed6\u5535\u591a\u5229\u5484\u591a", "144": "\u550e\u5484\u550e\u838e\u56a9\u4e8c\u5408\u8a36", "145": "\u6642\u89c0\u81ea\u5728\u83e9\u85a9\u8aaa\u9640\u7f85\u5c3c\u5df2\u3002\u4ee5\u591a\u7f85\u83e9\u85a9\u5a01", "146": "\u795e\u529b\u6545\u3002\u4e00\u5207\u4e16\u754c\u6240\u6709\u773e\u751f\u3002\u96e2\u8af8\u82e6\u60f1\u7686\u7372", "147": "\u5b89\u6a02\u3002\u6089\u767c\u7121\u4e0a\u83e9\u63d0\u4e4b\u5fc3\u3002\u6089\u8207\u6cd5\u754c\u9ad4\u6027\u76f8", "148": "\u61c9\u3002\u5165\u65bc\u51fa\u751f\u7121\u908a\u9580\u85cf\u3002\u4e00\u5207\u773e\u6703\u5fc3\u751f\u5947\u7279", "149": "\u6b4e\u672a\u66fe\u6709\u3002", "150": "2 \u66fc\u837c\u7f85\u54c1\u89c0\u81ea\u5728\u591a\u7f85\u83e9\u85a9\u7d93\u66fc\u837c\u7f85\u54c1\u7b2c\u4e8c", "151": "\u723e\u6642\u89c0\u81ea\u5728\u83e9\u85a9\u6469\u8a36\u85a9\u3002\u544a\u591a\u7f85\u83e9\u85a9\u8a00\u3002\u82e5", "152": "\u5973\u4eba\u70ba\u6b32\u6210\u5c31\u4e00\u5207\u7a2e\u667a\u3002\u53ca\u6b32\u6eff\u8db3\u4e16\u9593\u52dd", "153": "\u9858\u3002\u61c9\u7576\u4fee\u7fd2\u5982\u662f\u7955\u8981\u3002\u5176\u66fc\u837c\u7f85\u3002\u4e00\u5982\u4eca\u65e5", "154": "\u91cb\u8fe6\u5982\u4f86\u5728\u6de8\u5c45\u5929\u5bae\u3002\u8207\u8af8\u83e9\u85a9\u96c6\u6703\u4e4b\u4f4d\u3002", "155": "\u5176\u4fee\u884c\u8005\u5148\u61c9\u64c7\u5730\u3002\u6216\u65bc\u5c71\u5cef\u6216\u65bc\u6cb3\u5cb8\u3002\u6216", "156": "\u8fd1\u5927\u6d77\u83ef\u83d3\u6cc9\u6c60\u3002\u5bc2\u975c\u4e4b\u8655\u96e2\u8af8\u5371\u96e3\u3002\u53ca\u8511", "157": "\u623e\u8eca\u6028\u8cca\u6bd2\u87f2\u3002\u65c3\u9640\u7f85\u7b49\u96dc\u7a62\u4e4b\u8655\u3002\u91cf\u53d6", "158": "\u56db\u8098\u6216\u516b\u8098\u4e43\u81f3\u5341\u516d\u8098\u3002\u6398\u6df1\u4e00\u8098\u53bb\u8af8\u9aa8", "159": "\u9aee\u7070\u70ad\u834a\u68d8\u4e0d\u6de8\u4e4b\u7269\u3002\u53d6\u6cb3\u5cb8\u571f\u53ca\u8af8\u6de8\u571f\u3002", "160": "\u5148\u4ee5\u4e94\u6de8\u7051\u5df2\u3002\u5373\u8aa6\u672c\u5c0a\u9640\u7f85\u5c3c\u4e00\u767e\u516b\u904d\u3002", "161": "\u52a0\u6301\u5176\u571f\u7528\u586b\u5176\u5730\u3002\u6e05\u6de8\u4fee\u7bc9\u6975\u4ee4\u5e73\u6574\u3002\u53d6", "162": "\u9ec3\u725b\u7cde\u4e0d\u58ae\u5730\u8005\u3002\u4ea6\u8aa6\u9640\u7f85\u5c3c\u800c\u52a0\u6301\u5df2\u3002\u7136", "163": "\u5f8c\u5857\u5730\u3002\u65bc\u795e\u901a\u6708\u53ca\u5409\u5bbf\u65e5\u3002\u6216\u6b63\u6708\u5341\u4e94\u65e5", "164": "\u4e8c\u6708\u516b\u65e5\u5341\u4e94\u65e5\u7b49\u3002\u5f9e\u591c\u8d77\u9996\u3002\u4ee5\u9752\u84ee\u83ef\u5370", "165": "\u52a0\u6301\u58c7\u5730\u3002\u53d6\u4e94\u8272\u7db5\u7dda\u62fc\u5176\u754c\u9053\u3002\u4ee5\u4e94\u8272", "166": "\u7c89\u649a\u756b\u70ba\u4e4b\u3002\u6216\u4e03\u5bf6\u7c89\u96a8\u529b\u800c\u8fa6\u3002\u548c\u8af8\u9999\u672b", "167": "\u8aa6\u9640\u7f85\u5c3c\u3002\u56db\u65b9\u4e09\u9662\u3002\u5148\u65bc\u4e2d\u80ce\u756b\u91cb\u8fe6\u725f", "168": "\u5c3c\u4f5b\u3002\u5750\u5bf6\u5e2b\u5b50\u5ea7\u4f5c\u8aaa\u6cd5\u76f8\u3002\u53f3\u908a\u61c9\u756b\u89c0\u81ea", "169": "\u5728\u83e9\u85a9\u5750\u84ee\u83ef\u4e0a\u3002\u77bb\u4ef0\u5408\u638c\u6301\u767d\u84ee\u83ef\u3002\u8eab\u767d", "170": "\u7d05\u8272\u56b4\u98fe\u74d4\u73de\u3002\u9996\u6234\u5bf6\u51a0\u5de6\u7d61\u767d\u795e\u7d22\u3002\u5de6\u908a", "171": "\u756b\u91d1\u525b\u85cf\u83e9\u85a9\u3002\u5de6\u624b\u6301\u91d1\u525b\u6775\u8eab\u6dfa\u7da0\u8272\u3002\u6b21", "172": "\u5f8c\u61c9\u756b\u516b\u5927\u83e9\u85a9\u3002\u6240\u8b02\u5f4c\u52d2\u83e9\u85a9\u3002\u5927\u52e2\u81f3\u83e9", "173": "\u85a9\u3002\u66fc\u6b8a\u5ba4\u5229\u83e9\u85a9\u3002\u5730\u85cf\u83e9\u85a9\u3002\u865b\u7a7a\u5eab\u83e9\u85a9\u3002\u9664", "174": "\u84cb\u969c\u83e9\u85a9\u3002\u85a9\u9640\u6ce2\u5d19\u83e9\u85a9\u3002\u865b\u7a7a\u85cf\u83e9\u85a9\u3002\u65bc\u91d1", "175": "\u525b\u85cf\u83e9\u85a9\u4e0b\u3002\u5fa9\u756b\u964d\u4e09\u4e16\u660e\u738b\u83e9\u85a9\u3002\u4f5c\u5fff\u6012", "176": "\u5f62\u3002\u53ca\u756b\u6708\u9768\u5fff\u6012\u83e9\u85a9\u3002\u4f5c\u633c\u638c\u6467\u4f0f\u8af8\u9b54", "177": "\u52e2\u3002\u8fd1\u89c0\u81ea\u5728\u83e9\u85a9\u3002\u756b\u6bd8\u4ff1\u80dd\u3002\u767d\u8863\u89c0\u4e16\u97f3\u3002\u99ac", "178": "\u982d\u660e\u738b\u3002\u5404\u5982\u672c\u8272\u3002\u8fd1\u99ac\u982d\u83e9\u85a9\u3002\u756b\u5927\u5409\u7965\u89c0", "179": "\u4e16\u97f3\u3002\u5927\u767d\u89c0\u4e16\u97f3\u3002\u6708\u89c0\u4e16\u97f3\u3002\u8c50\u8ca1\u89c0\u4e16\u97f3\u3002", "180": "\u540d\u7a31\u89c0\u4e16\u97f3\u3002\u65bc\u91cb\u8fe6\u5982\u4f86\u5e2b\u5b50\u5ea7\u4e0b\u3002\u756b\u84ee\u83ef", "181": "\u6c60\u3002\u65bc\u5176\u6c60\u4e2d\u6709\u5999\u5bf6\u84ee\u83ef\u3002\u4f5c\u8d64\u5149\u8272\u5982\u7d05\u9817", "182": "\u68a8\u653e\u5927\u5149\u660e\u3002\u5176\u84ee\u83ef\u4e2d\u5750\u591a\u7f85\u83e9\u85a9\u3002\u5de6\u624b\u6301", "183": "\u9752\u84ee\u83ef\u3002\u53f3\u624b\u4ef0\u5b89\u81cd\u4e0a\u5982\u5750\u79aa\u52e2\u3002\u773c\u4ea6\u5982\u662f\u3002", "184": "\u56b4\u98fe\u74d4\u73de\u62ab\u7d17\u7e20\u671d\u971e\u8863\u3002\u6021\u7136\u800c\u4f4f\u3002\u5176\u58c7\u56db", "185": "\u9580\u5e62\u65db\u83ef\u84cb\u3002\u6b04\u6abb\u965b\u696f\u96e3\u63d0\u5546\u4f49\u3002\u8af8\u5929\u97f3\u6a02", "186": "\u8b9a\u8a60\u8056\u773e\u3002\u58c7\u6771\u9762\u756b\u963f\u8fe6\u5c3c\u5412\u5929\u773e\u3002\u5c11\u5149", "187": "\u5929\u5b50\u3002\u7121\u71b1\u5929\u5b50\u3002\u5317\u908a\u756b\u5999\u898b\u5929\u5b50\u3002\u5584\u73fe\u5929\u5b50\u3002", "188": "\u5176\u9580\u5169\u908a\u756b\u68b5\u5929\u68b5\u8f14\u5929\u3002\u5149\u97f3\u5929\u5927\u68b5\u5929\u3002\u56db", "189": "\u65b9\u5404\u756b\u4e8c\u5929\u3002\u7686\u6234\u5bf6\u51a0\u62ab\u8d64\u8272\u8863\u3002\u6216\u9ec3\u6216\u767d", "190": "\u8eab\u7121\u74d4\u73de\u3002\u5750\u79aa\u800c\u4f4f\u3002\u5404\u4ee5\u53f3\u624b\u5b89\u65bc\u9802\u4e0a", "191": "\u4f5c\u656c\u79ae\u76f8\u3002\u5916\u9662\u9580\u5074\u3002\u756b\u8a16\u54e9\u745f\u62cf\u69c3\u5716\u9b54\u738b\u3002", "192": "\u74d4\u73de\u838a\u56b4\u5c11\u5e74\u4e4b\u8c8c\u3002\u6b21\u756b\u5316\u6a02\u5929\u53ca\u515c\u7387\u591c", "193": "\u6469\u5e1d\u91cb\u5929\u5b50\u7b49\u3002\u8fd1\u9580\u800c\u4f4f\u3002\u6b21\u756b\u56db\u5929\u738b\u5929\u8fe6", "194": "\u6a13\u7f85\u5929\u4f0a\u820d\u90a3\u9b3c\u795e\u4e3b\u3002\u53ca\u756b\u6bd8\u7d10\u5929\u6301\u8f2a\u800c", "195": "\u4f4f\u3002\u6b21\u756b\u534a\u652f\u8fe6\u5927\u85e5\u53c9\u5c07\u3002\u6b21\u756b\u67d3\u5a46\u7f85\u5927\u85e5", "196": "\u53c9\u3002\u53ca\u6eff\u8ce2\u5bf6\u8ce2\u85e5\u53c9\u738b\u7b49\u3002\u53ca\u756b\u8a36\u5229\u5e95\u5927\u85e5", "197": "\u53c9\u5973\u3002\u65e5\u6708\u661f\u5bbf\u56db\u59ca\u59b9\u3002\u5272\u5e95\u8fe6\u7ae5\u5b50\u3002\u5e76\u4e8c", "198": "\u9f8d\u738b\u96e3\u9640\u8dcb\u96e3\u9640\u7b49\u3002\u5982\u662f\u8056\u773e\u7686\u9808\u4e00\u5fc3\u8fce", "199": "\u8acb\u4ee5\u5fc3\u767c\u9063\u3002\u53d6\u767d\u83ef\u7f6e\u904f\u4f3d\u9262\u4e2d\u3002\u4f9b\u990a\u4e00\u5207", "200": "\u8af8\u4f5b\u83e9\u85a9\u7de3\u89ba\u8072\u805e\u4e00\u5207\u546a\u4ed9\u3002\u4e16\u51fa\u4e16\u9593\u7686", "201": "\u9808\u89c0\u76e1\u5fc3\u4f9b\u990a\u3002\u4ee5\u767d\u6a80\u9f8d\u8166\u6b1d\u91d1\u800c\u70ba\u9999\u6c34\u3002", "202": "\u6563\u7051\u8056\u773e\u3002\u5fa9\u4ee5\u6b64\u9999\u70ba\u672b\u71d2\u4e4b\u3002\u7576\u767d\u662f\u8a00\u3002", "203": "\u552f\u9858\u8af8\u4f5b\u8af8\u5927\u83e9\u85a9\u4e00\u5207\u8056\u8005\u8207\u6211\u6089\u5730\u3002\u4ee4", "204": "\u6211\u901f\u51fa\u751f\u6b7b\u6de4\u6ce5\u3002\u4e09\u7a31\u662f\u5df2\u3002\u96a8\u529b\u6240\u8fa6\u800c\u70ba", "205": "\u4f9b\u990a\u3002\u5fc3\u5e38\u7e6b\u8acb\u4e16\u51fa\u4e16\u9593\u4e00\u5207\u546a\u5929\u3002\u9858\u52a0\u6301", "206": "\u6211\u3002\u4e00\u4e00\u5404\u7d50\u5176\u672c\u5370\u800c\u76f8\u61c9\u4e4b\u3002\u4ee5\u516b\u65b0\u74f6\u76db", "207": "\u6eff\u9999\u6c34\u3002\u5e76\u7f6e\u4e00\u5207\u7a2e\u5b50\u53ca\u4e03\u5bf6\u91d1\u9280\u3002\u5e76\u8af8\u85e5", "208": "\u8349\u963f\u6469\u7f85\u6a39\u679d\u3002\u5e76\u694a\u67f3\u591c\u5408\u677e\u67cf\u7b49\u8449\u3002\u4ee5", "209": "\u4e0d\u622a\u7e52\u7db5\u7e6b\u8986\u74f6\u4e0a\u3002\u56db\u74f6\u7f6e\u5167\u56db\u89d2\u3002\u56db\u74f6\u7f6e", "210": "\u5916\u56db\u89d2\u3002\u53c8\u5b89\u516b\u74f6\u3002\u7b2c\u4e00\u4e00\u74f6\u4f9b\u990a\u8272\u754c\u963f\u8fe6", "211": "\u5c3c\u5412\u5929\u773e\u3002\u7b2c\u4e8c\u4e00\u74f6\u4f9b\u990a\u6de8\u5c45\u5929\u773e\u3002\u7b2c\u4e09\u4e00", "212": "\u74f6\u4f9b\u990a\u6b32\u754c\u5929\u773e\u3002\u7b2c\u56db\u4e00\u74f6\u4f9b\u990a\u8af8\u85e5\u53c9\u5929", "213": "\u5973\u53ca\u6301\u660e\u546a\u4ed9\u3002\u7b2c\u4e94\u4e00\u74f6\u4f9b\u990a\u8af8\u4f5b\u4e16\u5c0a\u3002\u7b2c", "214": "\u516d\u4e00\u74f6\u4f9b\u990a\u83e9\u85a9\u8072\u805e\u7de3\u89ba\u3002\u7b2c\u4e03\u4e00\u74f6\u65bc\u591a", "215": "\u7f85\u524d\u800c\u70ba\u4f9b\u990a\u3002\u7b2c\u516b\u4e00\u74f6\u4f9b\u990a\u4e00\u5207\u773e\u751f\u3002\u5982", "216": "\u662f\u4e00\u5207\u516d\u74f6\u3002\u7686\u9808\u4e00\u4e00\u5982\u6cd5\u5e03\u7f6e\u3002\u65bc\u58c7\u56db\u9762", "217": "\u5404\u7136\u9165\u71c8\u3002\u5857\u9999\u71d2\u9999\u83ef\u9b18\u73cd\u994c\u3002\u4e00\u4e00\u7686\u5982\u66fc", "218": "\u6b8a\u5ba4\u5229\u66fc\u837c\u7f85\u6cd5\u3002\u7686\u4ee5\u672c\u5c0a\u9640\u7f85\u5c3c\u52a0\u6301\u8af8", "219": "\u4f9b\u990a\u7269\u3002\u65bc\u58c7\u897f\u5357\u89d2\u53bb\u56db\u4e94\u8098\u3002\u61c9\u4f5c\u8b77\u6469\u8ecd", "220": "\u5412\u3002\u5176\u7210\u65b9\u56db\u89d2\u3002\u6216\u4e00\u8098\u4e8c\u8098\u6df1\u53ef\u534a\u8098\u3002\u7210\u4e2d", "221": "\u4f5c\u84ee\u83ef\u5f62\u3002\u5176\u7210\u7a7f\u9020\u5982\u64c7\u5730\u6cd5\u3002\u53d6\u65b0\u947d\u6de8\u706b", "222": "\u4ee5\u4e73\u6728\u4f5c\u67f4\u3002\u53d6\u4e00\u5207\u8349\u83ef\u53ca\u4e94\u7a2e\u5b50\u3002\u548c\u9165\u871c", "223": "\u916a\u3002\u5148\u4ee5\u9165\u4e09\u6753\u4f9b\u990a\u706b\u5929\u71d2\u4e4b\u3002\u6b21\u871c\u916a\u5404\u4e09", "224": "\u6753\u3002\u7136\u5f8c\u4ee5\u4e94\u7a2e\u5b50\u4e09\u7f6e\u706b\u4e2d\u3002\u9640\u7f85\u5c3c\u66f0\u3002", "225": "\u5535\u963f\u7947\u511e\u838e\u56a9\u4e8c\u5408\u8a36", "226": "\u8aa6\u6b64\u5bc6\u8a00\u800c\u52a0\u6301\u4e4b\u3002\u5fa9\u53d6\u767d\u6a80\u6b1d\u91d1\u9f8d\u8166\u9999", "227": "\u7b49\u76f8\u548c\u9999\u6c34\u3002\u76db\u4ee5\u74e6\u6728\u91d1\u9280\u719f\u9285\u65b0\u6de8\u4e4b\u5668", "228": "\u53f3\u908a\u5b89\u4e4b\u3002\u8acb\u706b\u5929\u5df2\u3002\u53d6\u83ef\u9999\u8aa6\u6839\u672c\u9640\u7f85\u5c3c", "229": "\u64f2\u71d2\u4f9b\u990a\u706b\u5929\u3002\u53f3\u624b\u53d6\u9999\u6c34\u53f3\u65cb\u7051\u706b\u53ca\u7528", "230": "\u672c\u6cd5\u3002\u5148\u81ea\u8b77\u8eab\u3002\u6b21\u7d50\u9752\u84ee\u83ef\u5370\u60f3\u591a\u7f85\u83e9\u85a9", "231": "\u8aa6\u4e00\u5343\u516b\u904d\u53d6\u8af8\u767d\u83ef\u7f6e\u904f\u4f3d\u6c34\u3002\u60f3\u5ff5\u672c\u5c0a", "232": "\u6367\u800c\u4f9b\u990a\u3002\u5148\u50be\u4e09\u6e27\u3002\u53c8\u53d6\u767d\u6a80\u6b1d\u91d1\u9f8d\u8166\u3002", "233": "\u548c\u9165\u871c\u916a\u3002\u5e76\u53d6\u6709\u4e73\u6728\u67f4\u7121\u7bc0\u7aef\u76f4\u3002\u5341\u4e8c\u6307", "234": "\u622a\u4e00\u5343\u516b\u6bb5\u3002\u4e00\u8aa6\u4e00\u64f2\u7210\u4e2d\u71d2\u5df2\u3002\u591a\u7f85\u83e9\u85a9", "235": "\u5373\u73fe\u5176\u8eab\u544a\u884c\u8005\u8a00\u3002\u6c5d\u6c42\u4f55\u9858\u4e00\u5207\u65bd\u8207\u3002\u7e31", "236": "\u4fee\u884c\u8005\u6709\u7a4d\u696d\u91cd\u969c\u3002\u4ea6\u73fe\u8b66\u8aa1\u3002\u6216\u653e\u5149\u660e\u6216", "237": "\u805e\u96f7\u9707\u9418\u78ec\u7b49\u8072\u3002\u6216\u65bc\u7a7a\u4e2d\u7121\u96f2\u800c\u96e8\u3002\u6216\u9999", "238": "\u83ef\u6e05\u6dbc\u5999\u98a8\u89f8\u884c\u8005\u8eab\u3002\u9047\u65af\u745e\u5df2\u751f\u5927\u6176\u6085\u3002", "239": "\u8af8\u5929\u4eba\u773e\u898b\u4fee\u884c\u8005\u5fc3\u751f\u6b61\u559c\u3002\u61c9\u77e5\u591a\u7f85\u83e9", "240": "\u85a9\u4e0d\u4e45\u6eff\u9858\u3002\u82e5\u963f\u95cd\u68a8\u70ba\u4f5c\u6b64\u8005\u3002\u767c\u9063\u8056\u773e", "241": "\u737b\u904f\u4f3d\u5df2\u3002\u5373\u65bc\u9053\u5834\u5982\u5e38\u8aa6\u5ff5\u4e00\u5343\u516b\u904d\u3002\u570d", "242": "\u7e5e\u4e09\u531d\u96a8\u610f\u7d93\u884c\u3002\u6bcf\u65e5\u4e09\u6642\u6216\u4e00\u4e03\u65e5\u3002\u4e8c", "243": "\u4e03\u65e5\u4e43\u81f3\u4e09\u4e03\u65e5\u3002\u6c42\u81ea\u672c\u9858\u5ff5\u8aa6\u3002\u5ff5\u8aa6\u5df2\u3002", "244": "\u5373\u53d6\u591a\u7f85\u83e9\u85a9\u672c\u5c0a\u524d\u74f6\u4f9b\u990a\u4e4b\u6c34\u3002\u7d50\u9752\u84ee", "245": "\u82b1\u5370\u704c\u4fee\u884c\u8005\u9802\u3002\u5176\u704c\u9802\u8655\u3002\u53bb\u58c7\u516b\u8098\u756b", "246": "\u4e00\u84ee\u83ef\u3002\u800c\u704c\u9802\u5df2\u570d\u9076\u4e09\u531d\u3002\u91cd\u737b\u904f\u4f3d\u8aa6\u8af8", "247": "\u8b9a\u6b4e\u3002\u7d50\u672c\u5c0a\u5370\u4e0a\u7f6e\u767d\u84ee\u3002\u6563\u65bc\u58c7\u4e0a\u4ee5\u70ba\u4f9b", "248": "\u990a\u3002\u7136\u5f8c\u5982\u5e38\u6642\u767c\u9063\u3002\u58c7\u4e2d\u82b1\u7c89\u7f6e\u6e05\u6de8\u6d41", "249": "\u6c34\u4e4b\u4e2d\u3002\u4e0d\u61c9\u5c65\u8e10\u3002\u66fc\u837c\u7f85\u8655\u5fa9\u7528\u77bf\u6469\u5857", "250": "\u4e4b\u3002\u4f9b\u990a\u98f2\u98df\u7576\u65bd\u773e\u50e7\u53ca\u8af8\u8ca7\u8005\u3002\u66fe\u5165\u66fc\u837c", "251": "\u7f85\u4fee\u884c\u4e4b\u4eba\u4e0d\u61c9\u98df\u6b64\u3002\u4ea6\u4e0d\u5f97\u98df\u8304\u5b50\u863f\u8514", "252": "\u84ee\u83ef\u6839\u8396\u3002\u4ea6\u4e0d\u5f97\u8e10\u5c65\u7aa3\u5835\u6ce2\u5f71\u53ca\u963f\u95cd\u68a8", "253": "\u7236\u6bcd\u4e4b\u5f71\u6e05\u6de8\u6bd4\u4e18\u4fee\u884c\u4eba\u5f71\u3002\u4e43\u81f3\u4e03\u4f5b\u83e9", "254": "\u63d0\u6a39\u5f71\u4e26\u4e0d\u61c9\u5c65\u3002", "255": "3 \u756b\u50cf\u54c1\u89c0\u81ea\u5728\u591a\u7f85\u83e9\u85a9\u7d93\u756b\u50cf\u54c1", "256": "\u723e\u6642\u91cb\u8fe6\u725f\u5c3c\u4f5b\u3002\u53c8\u5fa9\u89c0\u5bdf\u6de8\u5c45\u5929\u5bae\u3002\u544a\u89c0", "257": "\u81ea\u5728\u83e9\u85a9\u6469\u8a36\u85a9\u8a00\u3002\u6c5d\u4eca\u6b21\u61c9\u70ba\u591a\u7f85\u83e9\u85a9", "258": "\u8aaa\u756b\u50cf\u6cd5\u3002\u6642\u89c0\u81ea\u5728\u83e9\u85a9\u6469\u8a36\u85a9\u3002\u627f\u4f5b\u8056\u65e8", "259": "\u5f9e\u5750\u800c\u8d77\u3002\u79ae\u4f5b\u96d9\u8db3\u5fa9\u904d\u89c0\u5bdf\u6de8\u5c45\u5929\u5bae\u3002\u800c", "260": "\u52c5\u5929\u9f8d\u591c\u53c9\u5065\u95e5\u5a46\u963f\u8607\u7f85\u8fe6\u6a13\u7f85\u7dca\u90a3\u7f85", "261": "\u6469\u547c\u7f85\u4f3d\u3002\u53ca\u4e00\u5207\u4e16\u754c\u6301\u660e\u546a\u4ed9\u3002\u4f5c\u662f\u8a93", "262": "\u8a00\u3002\u6c5d\u7b49\u61c9\u7576\u807d\u53d7\u61b6\u672c\u4e09\u6627\u8036\u52ff\u751f\u7591\u60d1\u3002", "263": "\u82e5\u7576\u4f86\u4e16\u6709\u4fee\u884c\u8005\u61c9\u7576\u64c1\u8b77\u3002\u82e5\u8d77\u7570\u5fc3\u57f7", "264": "\u91d1\u525b\u5927\u85e5\u53c9\u5c07\u3002\u4ee5\u91d1\u525b\u706b\u7130\u6775\u6467\u788e\u6c5d\u9802\u3002\u547d", "265": "\u7d42\u4e4b\u5f8c\u58ae\u5728\u6ce5\u7281\u3002\u65bc\u7121\u91cf\u52ab\u53d7\u5927\u82e6\u60f1\u3002\u723e", "266": "\u6642\u83e9\u85a9\u544a\u8a93\u5df2\u8a16\u3002\u544a\u591a\u7f85\u83e9\u85a9\u8a00\u82e5\u672a\u4f86\u4e16", "267": "\u8af8\u4fee\u884c\u8005\u3002\u81f3\u6c42\u5713\u6eff\u589e\u4e0a\u6089\u5730\u3002\u7576\u4f9d\u6211\u6559", "268": "\u5982\u6cd5\u756b\u50cf\u3002\u53d6\u65b0\u767d\u3cb2\u53ca\u8af8\u7d79\u7d20\u4e0d\u622a\u5e45\u8005\u3002\u7d44", "269": "\u71be\u6e05\u6de8\u7121\u8af8\u6bdb\u9aee\u83ef\u53ca\u5f69\u8272\u5fc5\u9808\u65b0\u6f54\u3002\u53d6\u7b2c", "270": "\u4e00\u756b\u5de5\u53ca\u767c\u83e9\u63d0\u5fc3\u3002\u8eab\u5668\u5168\u8005\u53d7\u516b\u95dc\u6212\u3002\u6216", "271": "\u516b\u8098\u56db\u8098\u4e43\u81f3\u4e00\u8098\u3002\u5148\u65bc\u4e2d\u53f0\u756b\u91cb\u8fe6\u4e16\u5c0a\u3002", "272": "\u5750\u65bc\u773e\u5bf6\u5e2b\u5b50\u4e4b\u5ea7\u8655\u6de8\u5c45\u5929\u5bae\u3002\u8eab\u8272\u5982\u91d1", "273": "\u4f5c\u8aaa\u6cd5\u76f8\u3002\u5de6\u756b\u66fc\u6b8a\u5ba4\u5229\u7ae5\u5b50\u3002\u56b4\u98fe\u74d4\u73de\u5fae", "274": "\u4f5c\u8d64\u8272\u3002\u8457\u9752\u8272\u88d9\u88ab\u8f15\u7e20\u8863\u3002\u7d61\u4ee5\u795e\u7d22\u3002\u53f3", "275": "\u624b\u57f7\u767d\u62c2\u5de6\u624b\u6301\u9752\u84ee\u83ef\u8396\u3002\u77bb\u4ef0\u800c\u4f4f\u3002\u53f3\u756b", "276": "\u89c0\u81ea\u5728\u83e9\u85a9\u3002\u8eab\u6dfa\u7d05\u8272\u9aee\u6234\u5bf6\u51a0\u5316\u4f5b\u3002\u5e36\u767d", "277": "\u795e\u7d22\u3002\u65bc\u84ee\u83ef\u4e0a\u4812\u8dea\u800c\u5750\u3002\u5de6\u624b\u57f7\u84ee\u83ef\u53f3", "278": "\u624b\u65bc\u9802\u4e0a\u4f5c\u6563\u82b1\u52e2\u3002\u7a2e\u7a2e\u74d4\u73de\u838a\u56b4\u5176\u8eab\u3002\u4f5c", "279": "\u5fae\u7b11\u8c8c\u5df2\u3002\u6b21\u5fa9\u61c9\u756b\u591a\u7f85\u83e9\u85a9\u3002\u7121\u50f9\u96dc\u5bf6\u800c", "280": "\u70ba\u838a\u56b4\u3002\u8eab\u7da0\u9ec3\u8272\u5982\u76db\u5e74\u5f62\u3002\u4f5c\u610d\u5ff5\u5fae\u7b11\u89c0", "281": "\u884c\u8005\u8c8c\u3002\u5411\u89c0\u81ea\u5728\u66f2\u8eac\u800c\u4f4f\u3002\u5de6\u624b\u6301\u9752\u84ee\u83ef", "282": "\u53f3\u4f5c\u57f7\u5409\u7965\u679c\u52e2\u3002\u65bc\u89c0\u81ea\u5728\u83e9\u85a9\u4e0b\u3002\u61c9\u756b\u884c", "283": "\u8005\u6367\u9999\u7210\u4f5c\u9802\u79ae\u52e2\u3002\u65bc\u5ea7\u53f3\u908a\u756b\u4e00\u91d1\u9262\u3002\u76db", "284": "\u719f\u963f\u6469\u7f85\u679c\u3002\u4e0b\u65b9\u756b\u5c11\u5149\u5929\u5b50\u7121\u71b1\u5929\u5b50\u5584", "285": "\u898b\u5929\u5b50\u3002\u4f5c\u807d\u6cd5\u76f8\u3002\u4e0a\u65b9\u53f3\u908a\u756b\u963f\u8fe6\u5c3c\u5412\u5929", "286": "\u5b50\u3002\u5de6\u908a\u756b\u5584\u73fe\u5929\u5b50\u3002\u53f3\u624b\u6563\u83ef\u5de6\u624b\u4f5c\u656c\u79ae", "287": "\u76f8\u3002\u4e0a\u756b\u5bf6\u84cb\u8af8\u5929\u5993\u6a02\u3002\u56db\u908a\u7a7a\u8655\u6089\u756b\u9f8d", "288": "\u83ef\u3002\u82e5\u4fee\u884c\u8005\u81f3\u8aa0\u4f9b\u990a\u9802\u79ae\u4e00\u62dc\u3002\u6ec5\u9664\u5104\u52ab", "289": "\u751f\u6b7b\u4e4b\u7f6a\u3002", "290": "\u89c0\u81ea\u5728\u591a\u7f85\u83e9\u85a9\u7b2c\u4e8c\u756b\u50cf\u54c1", "291": "\u723e\u6642\u89c0\u81ea\u5728\u83e9\u85a9\u6469\u8a36\u85a9\u3002\u5fa9\u544a\u591a\u7f85\u83e9\u85a9\u8a00\u3002", "292": "\u82e5\u4fee\u884c\u4eba\u5fa9\u6b32\u6210\u5c31\u7b2c\u4e8c\u756b\u50cf\u6cd5\u8005\u3002\u5148\u65bc\u4e2d", "293": "\u53f0\u756b\u7121\u91cf\u58fd\u4f5b\u3002\u501a\u83e9\u63d0\u6a39\u3002\u5de6\u908a\u756b\u96e2\u57a2\u83e9\u85a9", "294": "\u767d\u8272\u7a2e\u7a2e\u838a\u56b4\u8863\u7d17\u7e20\u8863\u3002\u624b\u6301\u767d\u62c2\u5074\u9867\u5411", "295": "\u4f5b\u3002\u53f3\u908a\u756b\u56db\u81c2\u89c0\u81ea\u5728\u83e9\u85a9\u3002\u53f3\u7b2c\u4e00\u624b\u4f5c\u7121", "296": "\u754f\u5370\u3002\u4ee5\u4e2d\u6307\u5927\u6307\u637b\u6578\u73e0\u3002\u5c55\u624b\u4f5c\u6469\u9802\u52e2\u3002\u7b2c", "297": "\u4e8c\u624b\u4f5c\u57f7\u6756\u5f62\u3002\u5de6\u7b2c\u4e00\u624b\u6301\u7d05\u84ee\u83ef\u3002\u7b2c\u4e8c\u624b", "298": "\u57f7\u8ecd\u6301\u3002\u89c0\u81ea\u5728\u83e9\u85a9\u53f3\u908a\u3002\u756b\u591a\u7f85\u83e9\u85a9\u5999\u5bf6", "299": "\u838a\u56b4\u8eab\u7da0\u9ec3\u8272\u3002\u5408\u638c\u6367\u9752\u84ee\u83ef\u3002\u534a\u8dcf\u800c\u5750\u3002", "300": "\u4f5c\u606d\u656c\u66f2\u8eac\u4e4b\u76f8\u3002\u96e2\u57a2\u83e9\u85a9\u4e0b\u3002\u756b\u4e09\u76ee\u56db\u624b", "301": "\u6bd8\u4ff1\u80dd\u83e9\u85a9\u3002\u8eab\u8457\u7d20\u8863\u3002\u5de6\u7b2c\u4e00\u624b\u57f7\u84ee\u83ef\u7b2c", "302": "\u4e8c\u624b\u57f7\u8ecd\u6301\u3002\u53f3\u7b2c\u4e00\u624b\u4f5c\u7121\u754f\u5370\u7b2c\u4e8c\u624b\u57f7", "303": "\u6578\u73e0\u3002\u591a\u7f85\u83e9\u85a9\u4e0b\u3002\u756b\u4e00\u9afb\u7f85\u524e\u3002\u773c\u8d64\u9ed1\u8272", "304": "\u64d4\u809a\u5782\u4e0b\u3002\u86c7\u70ba\u74d4\u73de\u72d7\u7259\u4e0a\u51fa\u3002\u8457\u864e\u76ae\u88d9", "305": "\u87d2\u86c7\u69c3\u9aee\u3002\u53f3\u624b\u628a\u925e\u65a7\u5de6\u624b\u6301\u86c7\u7f82\u7d22\u3002\u4ee5\u8840", "306": "\u5857\u8eab\u3002\u4e8c\u624b\u5408\u638c\u6522\u7709\u6012\u76ee\u4f5c\u6050\u6016\u76f8\u3002\u722a\u7532\u7e96", "307": "\u5229\u4ea4\u7d61\u8c61\u76ae\u3002\u6bd8\u4ff1\u80dd\u4e0b\u3002\u756b\u56db\u81c2\u99ac\u982d\u83e9\u85a9\u3002\u4e8c", "308": "\u624b\u7d50\u6839\u672c\u5370\u3002\u53f3\u624b\u6301\u925e\u65a7\u5de6\u624b\u57f7\u84ee\u83ef\u3002\u4e01\u5b57", "309": "\u800c\u7acb\u4f5c\u5fff\u6012\u76f8\u3002\u50cf\u4e0b\u756b\u96e3\u9640\u8dcb\u96e3\u9640\u9f8d\u738b\u3002\u5de6", "310": "\u624b\u6367\u84ee\u83ef\u8396\u53f3\u624b\u4f5c\u656c\u79ae\u76f8\u6c60\u4e0b\u756b\u5730\u5929\u3002\u6367", "311": "\u5bf6\u69c3\u4812\u8dea\u77bb\u4ef0\u3002\u56db\u65b9\u56db\u9685\u756b\u516b\u65b9\u795e\u3002\u4e0a\u756b", "312": "\u65e5\u6708\u8af8\u5929\u5993\u6a02\u3002\u4e94\u8272\u96f2\u4e2d\u7051\u7518\u96e8\u52e2\u3002\u89c0\u81ea\u5728", "313": "\u83e9\u85a9\u4e0b\u756b\u884c\u8005\u3002\u8457\u767d\u8863\u57f7\u9999\u7210\u4812\u8dea\u77bb\u4ef0\u3002", "314": "\u82e5\u4fee\u884c\u4eba\u65bc\u6708\u516b\u65e5\u6216\u5341\u4e94\u65e5\u6216\u795e\u901a\u6708\u3002\u6216", "315": "\u9806\u5409\u5bbf\u3002\u61c9\u98df\u4e73\u7ca5\u83ef\u679c\u7b49\u98df\u3002\u6216\u552f\u98df\u9999\u3002\u4f9d\u5148", "316": "\u6301\u8aa6\u6cd5\u4e09\u6642\u6fa1\u6d74\u4e0d\u61c9\u7761\u7720\u3002\u5e38\u5750\u8305\u8349\u8eab\u8863", "317": "\u767d\u670d\u3002\u6578\u9650\u6b32\u7d42\u4e09\u65e5\u4e0d\u98df\u3002\u7121\u9650\u5ff5\u8aa6\u5c0d\u6b64\u50cf", "318": "\u524d\u3002\u65bc\u58c7\u56db\u89d2\u7f6e\u9999\u6c34\u3002\u74f6\u4e2d\u63d2\u591c\u5408\u67f3\u7b49\u8af8\u9999", "319": "\u6a39\u8449\u3002\u4ea6\u7f6e\u4e03\u5bf6\u53ca\u4e94\u7a40\u7a2e\u3002\u4ee5\u4e0d\u622a\u7db5\u65bc\u74f6", "320": "\u4e0a\u3002\u884c\u8005\u5750\u5de6\u908a\u6848\u4e0a\u3002\u7f6e\u66fc\u73e0\u822c\u82e5\u3002\u6563\u83ef\u7d93\u4e0a\u3002", "321": "\u516b\u65b9\u7f6e\u516b\u6de8\u5668\u4ea6\u76db\u9999\u6c34\u3002\u53c8\u7f6e\u516b\u6900\u4e73\u3002\u7136\u767e", "322": "\u76de\u8607\u71c8\u3002\u7a2e\u7a2e\u98f2\u98df\u76db\u4ee5\u65b0\u5668\u3002\u7f6e\u58c7\u56db\u89d2\u71d2", "323": "\u6c88\u6c34\u9999\u3002\u71d2\u4e73\u6728\u706b\u3002\u53d6\u6c88\u9999\u5341\u4e8c\u6307\u622a\u4e00\u767e\u516b", "324": "\u6bb5\u3002\u6985\u8607\u5408\u6cb9\u4e00\u8aa6\u4e00\u71d2\u6eff\u4e00\u767e\u516b\u904d\u3002", "325": "\u89c0\u81ea\u5728\u83e9\u85a9\u5927\u60b2\u8056\u8005\u3002\u5f9e\u6771\u65b9\u4f86\u3002\u624b\u6301\u4e00\u6756", "326": "\u8eab\u8863\u767d\u670d\u3002\u5999\u5bf6\u74d4\u73de\u4ee5\u70ba\u838a\u56b4\u3002\u4ee5\u9ed1\u9e7f\u76ae\u4f5c", "327": "\u53f3\u818a\u4ea4\u7d61\u3002\u9aee\u6234\u5bf6\u51a0\u3002\u73fe\u884c\u8005\u524d\u653e\u5927\u5149\u660e\u3002", "328": "\u666e\u7167\u5730\u7344\u755c\u751f\u9913\u9b3c\u3002\u82e6\u60f1\u773e\u751f\u904e\u65af\u5149\u5df2\u3002\u8eab", "329": "\u5b89\u5feb\u6a02\u767c\u83e9\u63d0\u5fc3\u3002\u884c\u8005\u898b\u5df2\u6563\u8af8\u83ef\u9999\u3002\u4e94\u9ad4", "330": "\u6295\u5730\u81f3\u8aa0\u6b78\u547d\u3002\u6301\u904f\u4f3d\u6c34\u4ee5\u737b\u83e9\u85a9\u3002\u6642\u89c0\u81ea", "331": "\u5728\u5927\u60b2\u8056\u8005\u544a\u884c\u8005\u8a00\u3002\u5584\u54c9\u884c\u8005\u6c5d\u7b49\u4f55\u9858\u3002", "332": "\u4e00\u5207\u65bd\u8207\u3002\u5f97\u5370\u53ef\u5df2\u6240\u6c42\u5fc3\u6b32\u7121\u4e0d\u6210\u5c31\u3002\u6216", "333": "\u98db\u9a30\u865b\u7a7a\u6216\u5b89\u601b\u9640\u90a3\u3002\u6216\u805e\u6301\u5ef6\u58fd\u3002\u6216\u6839\u4e0d", "334": "\u5177\u4ea6\u5f97\u5713\u6eff\u3002\u6216\u6c42\u4f0f\u85cf\u5165\u4fee\u7f85\u7a9f\u3002\u4ea6\u5f97\u96a8\u5165", "335": "\u89c0\u81ea\u5728\u5bae\u3002\u5982\u662f\u7b49\u4e00\u5207\u4e0a\u9858\u3002\u4e16\u51fa\u4e16\u9593\u7121\u4e0d", "336": "\u6210\u5c31\u3002\u5fa9\u8aa6\u6b64\u5bc6\u8a00\u767c\u9063\u8056\u8005\u3002\u9640\u7f85\u5c3c\u66f0\u3002", "337": "\u5a1c\u6155\u56c9\u601b\u90a3\u4e8c\u5408\u601b\u56c9\u4e8c\u5408\u591c\u8036\u90a3\u83ab\u963f\u550e\u8036", "338": "\u56a9\u9b6f\u5409\u5e1d\u6fd5\u56a9\u4e8c\u5408\u56c9\u8036\u83e9\u5730\u85a9\u57f5\u8036\u6469\u8a36\u85a9", "339": "\u57f5\u8036\u546c\u550e\u546c\u550e\u8607\ud844\udc14\u8607\ud844\udc14\u85a9\u56a9\u85a9\u57f5\u8fe6\ud844\udc14\u5c3c", "340": "\u8fe6\u5b7d\u8e49\u5b7d\u8e49\u8036\u963f\u550e\u8036\u56a9\u9b6f\u5409\u5e1d\u6fd5\u56a9\u4e8c\u5408\u56c9", "341": "\u4e5f\u4ed6\u4e09\u9ebc\u8036\u6eff\u52aa\u85a9\u9ebc\u4e8c\u5408\u56c9\u85a9\u56a9\u4e8c\u5408\u8a36", "342": "\u884c\u8005\u6367\u8af8\u9999\u83ef\u3002\u8aa6\u6b64\u771f\u8a00\u4e03\u904d\u52a0\u6301\u3002\u5df2\u6563\u83e9", "343": "\u85a9\u8db3\u4e0b\u5247\u6210\u767c\u9063\u4e00\u9afb\u7f85\u524e\u9640\u7f85\u5c3c\u66f0\u3002", "344": "\u5a1c\u6155\u56c9\u601b\u90a3\u4e8c\u5408\u601b\u56c9\u4e8c\u5408\u591c\u8036\u5a1c\u83ab\u963f\u550e\u8036", "345": "\u56a9\u9b6f\u5409\u5e1d\u3000\u6fd5\u56a9\u4e8c\u5408\u56c9\u8036\u5192\u5730\u85a9\u57f5\u8036\u6469\u8a36", "346": "\u85a9\u57f5\u8036\u5a1c\u6155\u7ff3\u8fe6\u60f9\u5412\u8036\u9ebc\u8a36\u56c9\u4e5e\u7051\u4e8c\u5408\u65af", "347": "\u963f\u591c\u56c9\u9ebc\u9ebc\u6469\u8a36\u56c9\u4e5e\u7051\u4e8c\u5408\u65af\u9ebc\u9ebc\u6bcd\u8fe6\u85a9", "348": "\u56a9\u8fe6\u550e\u4e5f\u62b3\u8fe6\ud844\udc14\u546c\u601b\u59ea\u4ed6\u963f\u96e3\u5e1d\u85a9\u56a9\u4e8c\u5408", "349": "\u8a36\u60f9\u591c\u4e5f\u5a11\u56a9\u4e8c\u5408\u8a36\u85a9\u56a9\u5fae\u8fd1\u5a1c\u5c3e\u90a3\u591c\u5efa", "350": "\ud843\ude2a\u4e5e\u53c9\u4e8c\u5408\ud843\ude2a\u4e5e\u53c9\u4e8c\u5408\u5a11\u56a9\u4e8c\u5408\u8a36", "351": "\u6b64\u9640\u7f85\u5c3c\u80fd\u4ee4\u7528\u529f\u5c11\u6210\u5c31\u75be\u3002\u4ea6\u662f\u591a\u7f85\u83e9", "352": "\u85a9\u4f7f\u8005\u3002\u6545\u8af8\u4fee\u884c\u4eba\u61c9\u7576\u8aa6\u5ff5\u3002", "353": "\u4f5b\u8aaa\u5927\u65b9\u5ee3\u66fc\u6b8a\u5ba4\u5229\u7d93"}, "originalTitle": "Taisho Tripitaka, Electronic version, No. 1101 \u4f5b\u8aaa\u5927\u65b9\u5ee3\u66fc\u6b8a\u5ba4\u5229\u7d93", "source": "CBETA"}