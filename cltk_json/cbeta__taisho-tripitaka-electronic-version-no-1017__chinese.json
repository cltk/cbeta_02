5177\u4e00\u5207\u83e9\u85a9\u884c\u6545\u3002\u5f97\u5165\u5982\u662f\u5165\u9580\u9640\u7f85\u5c3c\u3002", "218": "\u4f55\u7b49\u70ba\u56db\u3002\u6240\u8b02\u89aa\u8fd1\u5c71\u6797\u963f\u862d\u82e5\u8655\u5f97\u6df1\u6cd5", "219": "\u5fcd\u3002\u540d\u805e\u5229\u990a\u4e00\u5207\u606d\u656c\u3002\u6240\u611b\u4e4b\u8655\u4e00\u5207\u6368\u96e2\u3002", "220": "\u4e43\u81f3\u6c92\u547d\u7d42\u4e0d\u4e2d\u6094\u3002\u820d\u5229\u5f17\u5982\u662f\u4e4b\u6cd5\u3002\u80fd\u5177", "221": "\u83e9\u85a9\u6240\u6709\u8af8\u884c\u3002\u5f97\u5165\u5982\u662f\u5165\u9580\u9640\u7f85\u5c3c\u3002\u723e\u6642", "222": "\u4e16\u5c0a\u6b32\u91cd\u5ba3\u6b64\u7fa9\u3002\u4ee5\u5048\u980c\u66f0\u3002", "223": "\u5c71\u6797\u963f\u7df4\u82e5\u6b63\u884c\u4e4b\u6240\u6b4e", "224": "\u96d6\u5fa9\u4f4f\u5f7c\u8655\u614e\u4e0d\u6bc0\u65bc\u4ed6", "225": "\u4fee\u5fcd\u4e0d\u653e\u9038\u5e38\u5982\u6551\u982d\u7136", "226": "\u4e0d\u4f9d\u65bc\u540d\u5229\u8a50\u73fe\u8af8\u7570\u76f8", "227": "\u503c\u4f5b\u5e0c\u6709\u96e3\u6368\u5bb6\u591a\u82e6\u672c", "228": "\u6e05\u6de8\u8eab\u53e3\u610f\u606d\u656c\u4f5b\u6cd5\u50e7", "229": "\u4e0d\u6c42\u65bc\u540d\u5229\u8655\u8655\u77e5\u6b62\u8db3", "230": "\u5982\u9ce5\u98db\u865b\u7a7a\u4e00\u5207\u7121\u6240\u4f9d", "231": "\u5982\u662f\u4e4b\u4eba\u8005\u7562\u4e0d\u7a7a\u65bc\u679c", "232": "\u7121\u4fe1\u7121\u6212\u884c\u65bc\u6cd5\u4e0d\u601d\u60df", "233": "\u662f\u4eba\u53bb\u83e9\u63d0\u7336\u5982\u5929\u8207\u5730", "234": "\u662f\u6545\u4fee\u884c\u8005\u6545\u9808\u6368\u540d\u5229", "235": "\u5982\u662f\u771f\u5999\u884c\u8ce2\u8056\u4e4b\u6240\u8b9a", "236": "\u5fa9\u6b21\u820d\u5229\u5f17\u3002\u5fa9\u6709\u56db\u6cd5\u5584\u80fd\u5177\u8db3\u83e9\u85a9\u884c\u6545\u3002", "237": "\u5f97\u5165\u5982\u662f\u5165\u9580\u9640\u7f85\u5c3c\u3002\u4f55\u7b49\u70ba\u56db\u3002\u6240\u8b02\u516b\u7a2e", "238": "\u5b57\u9580\u3002\u82e5\u80fd\u5584\u5165\u6b64\u5b57\u7fa9\u8005\u3002\u5f97\u5165\u5982\u662f\u5165\u9580\u9640", "239": "\u7f85\u5c3c\u3002\u4f55\u7b49\u70ba\u516b\u3002\u6240\u8b02\u8ddb\u5b57\u908f\u5b57\u3002\u83bd\u5b57\u60f9", "240": "\u5b57\u3002\u8fe6\u5b57\u9640\u5b57\u3002\u6368\u5b57\u4e5e\u7051\u4e8c\u5408\u5b57\u3002\u662f\u516b\u5b57\u3002", "241": "\u8ddb\u8005\u5373\u662f\u771f\u5982\u3002\u4e00\u5207\u6cd5\u7121\u6211\u5165\u6545\u3002\u908f\u8005\u76f8", "242": "\u597d\u975e\u76f8\u597d\u3002\u5982\u4f86\u6cd5\u8eab\u5165\u6545\u3002\u83bd\u8005\u51e1\u592b\u6cd5\u8ce2\u8056", "243": "\u6cd5\u3002\u4e00\u5982\u7121\u4e8c\u5165\u6545\u3002\u60f9\u8005\u751f\u8001\u75c5\u6b7b\u975e\u751f\u8001\u75c5", "244": "\u6b7b\u3002\u7121\u751f\u7121\u6ec5\u5165\u6545\u3002\u8fe6\u8005\u696d\u5831\u975e\u696d\u5831\u5165\u6545\u3002\u9640", "245": "\u8005\u9640\u7f85\u5c3c\u6cd5\u672c\u7a7a\u3002\u7121\u76f8\u7121\u9858\u6cd5\u754c\u5165\u6545\u3002\u6368\u8005", "246": "\u5b9a\u6167\u975e\u5b9a\u6167\u3002\u4e00\u5207\u6cd5\u4e00\u5982\u5165\u6545\u3002\u4e5e\u7051\u4e8c\u5408\u8005", "247": "\u7a7a\u4e00\u5207\u6cd5\u7121\u76e1\u4e0d\u53ef\u7834\u58de\u3002\u672c\u4f86\u5bc2\u975c\u6d85\u69c3\u6cd5", "248": "\u5165\u6545\u3002", "249": "\u723e\u6642\u4e16\u5c0a\u4f5c\u5982\u662f\u8a00\u3002\u5982\u6b64\u516b\u5b57\u82e5\u5f97\u610f\u8005\u3002\u80fd", "250": "\u5165\u5982\u662f\u9640\u7f85\u5c3c\u6cd5\u672c\u3002\u5982\u662f\u9640\u7f85\u5c3c\u6cd5\u672c\u4fee\u591a", "251": "\u7f85\u3002\u7576\u5584\u66f8\u5beb\u53d7\u6301\u8b80\u8aa6\u82e5\u534a\u6708\u82e5\u4e00\u6708\u3002\u5fa9\u4ee5", "252": "\u6b64\u6cd5\u672c\u8f49\u6559\u9918\u4eba\u4ee4\u751f\u6b61\u559c\u3002\u52f8\u52f5\u4fee\u9032\u52a9\u5176", "253": "\u5a01\u529b\u3002\u4ee4\u4f7f\u5c08\u5fd7\u7b56\u52e4\u4e0d\u9000\u3002\u4f5c\u662f\u8aaa\u5df2\u544a\u820d\u5229", "254": "\u5f17\u8a00\u3002\u5982\u662f\u56db\u6cd5\u80fd\u6210\u83e9\u85a9\u884c\u6545\u3002\u5f97\u5165\u9640\u7f85\u5c3c", "255": "\u9580\u723e\u6642\u4e16\u5c0a\u6b32\u91cd\u5ba3\u6b64\u7fa9\u3002\u4ee5\u5048\u980c\u66f0\u3002", "256": "\u7576\u601d\u516b\u5b57\u610f\u7d93\u53ca\u9640\u7f85\u5c3c", "257": "\u66f8\u5beb\u6301\u8b80\u8aa6\u4e00\u6708\u82e5\u534a\u6708", "258": "\u5fa9\u5316\u8af8\u773e\u751f\u6b61\u559c\u4ee4\u7b56\u9032", "259": "\u5f7c\u4eba\u8fd1\u83e9\u63d0\u5e38\u5f97\u52dd\u4e0a\u89ba", "260": "\u73fe\u898b\u4e00\u5207\u4f5b\u4f4f\u5728\u65bc\u5341\u65b9", "261": "\u5b78\u884c\u5f97\u589e\u9577\u662f\u6545\u61c9\u7cbe\u52e4", "262": "\u820d\u5229\u5f17\u5fa9\u6709\u56db\u6cd5\u3002\u80fd\u6210\u83e9\u85a9\u8af8\u798f\u5fb7\u6545\u3002\u65bc\u6b64", "263": "\u9640\u7f85\u5c3c\u4e2d\u7cbe\u52e4\u4e0d\u9000\u3002\u4f55\u7b49\u70ba\u56db\u3002\u6240\u8b02\u5341\u65b9\u4e00", "264": "\u5207\u8af8\u4f5b\u5e38\u7576\u8b77\u5ff5\u3002\u7121\u6709\u4e00\u5207\u8af8\u9b54\u60e1\u696d\u3002\u80fd\u8f49", "265": "\u4e00\u5207\u8af8\u60e1\u696d\u969c\u3002\u901f\u4ee4\u6e05\u6de8\u5f97\u7121\u65b7\u8faf\u624d\u3002\u820d\u5229", "266": "\u5f17\u6c5d\u7576\u77e5\u4e4b\u3002\u5982\u662f\u56db\u6cd5\u80fd\u6210\u5c31\u8af8\u83e9\u85a9\u798f\u5fb7", "267": "\u6545\u3002\u65bc\u6b64\u9640\u7f85\u5c3c\u4e2d\u7cbe\u5fc3\u4e0d\u9000\u3002\u723e\u6642\u4e16\u5c0a\u6b32\u91cd", "268": "\u5ba3\u6b64\u7fa9\u3002\u4ee5\u5048\u980c\u66f0\u3002", "269": "\u5341\u65b9\u4e00\u5207\u4f5b\u61b6\u5ff5\u82e5\u9b54\u9b54\u696d\u4e0d\u5f97\u4fbf", "270": "\u80fd\u8f49\u696d\u969c\u4ee4\u6e05\u6de8\u901f\u7372\u7121\u4e0a\u52dd\u8faf\u624d", "271": "\u820d\u5229\u5f17\u6211\u5ff5\u904e\u53bb\u4e16\u6642\u3002\u5ee3\u5927\u7121\u91cf\u7121\u908a\u963f\u50e7", "272": "\u7947\u52ab\u3002\u5fa9\u904e\u662f\u6578\u3002\u662f\u6642\u6709\u4f5b\u865f\u66f0\u5bf6\u529f\u5fb7\u5a01\u5bbf", "273": "\u52ab\u738b\u5982\u4f86\u61c9\u4f9b\u7b49\u6b63\u89ba\u660e\u884c\u8db3\u5584\u901d\u4e16\u9593\u89e3\u7121", "274": "\u4e0a\u58eb\u8abf\u5fa1\u4e08\u592b\u5929\u4eba\u5e2b\u4f5b\u5a46\u4f3d\u5a46\u3002\u723e\u6642\u5f7c\u4f5b", "275": "\u5c07\u5165\u6d85\u69c3\u6545\u5ee3\u70ba\u5927\u773e\u8aaa\u662f\u6cd5\u672c\u9640\u7f85\u5c3c\u3002\u6642", "276": "\u6709\u5927\u8f49\u8f2a\u738b\u540d\u66f0\u6301\u706b\u3002\u4e03\u5bf6\u5177\u8db3\u4eba\u6c11\u5b89\u6a02\u3002", "277": "\u6642\u738b\u6709\u5b50\u540d\u66f0\u4e0d\u601d\u8b70\u52dd\u529f\u5fb7\u3002\u751f\u5e74\u5341\u516d\u5f9e", "278": "\u5f7c\u5982\u4f86\u3002\u805e\u662f\u9640\u7f85\u5c3c\u6cd5\u672c\u3002\u805e\u5df2\u6b61\u559c\u3002\u5373\u65bc\u4e03", "279": "\u842c\u6b72\u4e2d\u3002\u7cbe\u52e4\u4fee\u7fd2\u6b64\u9640\u7f85\u5c3c\u6cd5\u672c\u3002\u672a\u66fe\u7761\u7720", "280": "\u4ea6\u4e0d\u5043\u5074\u7aef\u5750\u4e00\u8655\u3002\u4e0d\u8caa\u8ca1\u7269\u4e0d\u8caa\u738b\u4f4d\u4e0d", "281": "\u6a02\u81ea\u8eab\u3002\u5f97\u503c\u4e5d\u5341\u5104\u767e\u5343\u90a3\u7531\u4ed6\u8af8\u4f5b\u3002\u8af8\u4f5b", "282": "\u8aaa\u6cd5\u76e1\u80fd\u807d\u805e\u805e\u5df2\u80fd\u6301\u3002\u611b\u6a02\u4fee\u7fd2\u53ad\u96e2\u5bb6", "283": "\u6cd5\u3002\u5243\u9664\u9b1a\u9aee\u800c\u4f5c\u6c99\u9580\u3002\u4f5c\u5df2\u5fa9\u65bc\u4e5d\u842c\u6b72\u4e2d\u3002", "284": "\u4fee\u7fd2\u6b64\u5165\u7121\u908a\u5165\u9580\u9640\u7f85\u5c3c\u7686\u5584\u6210\u719f\u3002\u5584\u6210", "285": "\u719f\u5df2\u5fa9\u4ee5\u6b64\u5048\u3002\u5ee3\u70ba\u773e\u751f\u5206\u5225\u986f\u793a\u3002\u4e00\u751f\u4e4b", "286": "\u4e2d\u529b\u7cbe\u6559\u5316\u3002\u4ee4\u516b\u5341\u5104\u767e\u5343\u90a3\u7531\u4ed6\u773e\u751f\u3002\u5f97", "287": "\u767c\u963f\u8028\u591a\u7f85\u4e09\u85d0\u4e09\u83e9\u63d0\u5fc3\u3002\u5f9e\u662f\u767c\u5fc3\u7a4d\u529f", "288": "\u7d2f\u5fb7\u3002\u7686\u6089\u4f4f\u65bc\u4e0d\u9000\u8f49\u5730\u3002\u820d\u5229\u5f17\u6c5d\u4e14\u77e5\u4e4b\u3002", "289": "\u7576\u65bc\u662f\u6642\u6709\u4e00\u9577\u8005\u540d\u66f0\u6708\u76f8\u3002\u5f7c\u6709\u4e00\u5b50\u540d", "290": "\u7121\u908a\u5149\u3002\u5f9e\u5f7c\u6bd4\u4e18\u805e\u662f\u5165\u7121\u908a\u9580\u6cd5\u672c\u9640\u7f85", "291": "\u5c3c\u6545\u3002\u8d77\u96a8\u559c\u5fc3\u3002\u7531\u6b64\u96a8\u559c\u5584\u6839\u56e0\u7de3\u6545\u3002\u5f97\u503c", "292": "\u4e5d\u5341\u5104\u4f5b\u3002\u5f97\u52dd\u4e0a\u9640\u7f85\u5c3c\u3002\u5f97\u52dd\u4e0a\u8d77\u4fe1\u6cd5\u9580\u3002", "293": "\u6709\u6240\u8a00\u8aaa\u4eba\u7686\u5949\u7528\u3002\u5fa9\u5f97\u52dd\u4e0a\u76f8\u7e8c\u7121\u65b7\u8faf", "294": "\u624d\u3002\u8d85\u904e\u4e00\u5207\u8af8\u83e9\u85a9\u773e\u3002\u65bc\u5f7c\u4e00\u5207\u5982\u4f86\u6240\u7d93", "295": "\u4e09\u52ab\u4e2d\u5e38\u7686\u4f9b\u990a\u3002\u904e\u4e09\u52ab\u5df2\u5f97\u963f\u8028\u591a\u7f85\u4e09", "296": "\u85d0\u4e09\u83e9\u63d0\u3002", "297": "\u820d\u5229\u5f17\u6c5d\u4eca\u7576\u77e5\u3002\u83ab\u751f\u7591\u5fc3\u800c\u4f5c\u7919\u60f3\u3002\u723e\u6642", "298": "\u6708\u76f8\u9577\u8005\u5b50\u8c48\u7570\u4eba\u4e4e\u3002\u5373\u5f7c\u7136\u71c8\u5982\u4f86\u61c9\u4f9b", "299": "\u7b49\u6b63\u89ba\u662f\u4e5f\u3002\u723e\u6642\u4e0d\u601d\u8b70\u52dd\u529f\u5fb7\u6bd4\u4e18\u8aaa\u6cd5", "300": "\u8005\u3002\u8c48\u7570\u4eba\u4e4e\u5373\u963f\u5f4c\u591a\u5a46\u8036\u5982\u4f86\u61c9\u4f9b\u7b49\u6b63", "301": "\u89ba\u662f\u4e5f\u3002\u820d\u5229\u5f17\u6211\u53ca\u8ce2\u52ab\u4e2d\u4e00\u5207\u83e9\u85a9\u3002\u805e\u6b64", "302": "\u6cd5\u672c\u7686\u751f\u96a8\u559c\u4e4b\u5fc3\u3002\u6211\u65bc\u723e\u6642\u767c\u589e\u4e0a\u96a8\u559c", "303": "\u5fc3\u6545\u3002\u4ee5\u6b64\u5584\u6839\u56e0\u7de3\u3002\u56db\u767e\u842c\u52ab\u4e2d\u6240\u6709\u7169\u60f1\u3002", "304": "\u4e00\u5207\u8d85\u904e\u4e0d\u80fd\u70ba\u7919\u3002\u65bc\u4e5d\u5341\u5104\u4f5b\u908a\u807d\u805e\u6b63", "305": "\u6cd5\u3002\u5f97\u52dd\u4e0a\u9640\u7f85\u5c3c\u3002\u5fa9\u5f97\u52dd\u4e0a\u8d77\u4fe1\u6cd5\u9580\u3002\u5fa9", "306": "\u5f97\u52dd\u4e0a\u76f8\u7e8c\u7121\u65b7\u8faf\u624d\u3002", "307": "\u820d\u5229\u5f17\u6c5d\u7576\u77e5\u4e4b\u3002\u82e5\u8af8\u83e9\u85a9\u6b32\u5f97\u901f\u81f3\u963f\u8028", "308": "\u591a\u7f85\u4e09\u85d0\u4e09\u83e9\u63d0\u8005\u3002\u5047\u4f7f\u4e0d\u80fd\u5177\u8db3\u4fee\u884c\u3002\u61c9", "309": "\u9808\u805e\u6b64\u9640\u7f85\u5c3c\u767c\u96a8\u559c\u5fc3\u3002\u4f55\u4ee5\u6545\u7531\u6b64\u807d\u805e", "310": "\u96a8\u559c\u5584\u6839\u56e0\u7de3\u3002\u80fd\u8207\u4e0d\u9000\u8f49\u5730\u4f5c\u5176\u52dd\u56e0\u3002\u4e43", "311": "\u81f3\u5f97\u963f\u8028\u591a\u7f85\u4e09\u85d0\u4e09\u83e9\u63d0\u3002\u4ea6\u5fa9\u85c9\u6b64\u5584\u6839", "312": "\u4f5c\u5176\u52dd\u56e0\u7de3\u6545\u3002\u4f55\u6cc1\u66f8\u5beb\u8b80\u8aa6\u53d7\u6301\u3002\u5982\u6cd5\u4fee", "313": "\u884c\u5ee3\u70ba\u4eba\u8aaa\u3002\u8b9a\u6b4e\u52f8\u52f5\u4ee4\u5176\u4fee\u5b78\u3002\u6b64\u8af8\u529f\u5fb7", "314": "\u805a\u552f\u9664\u8af8\u4f5b\u4e43\u80fd\u77e5\u4e4b\u3002\u4e00\u5207\u773e\u751f\u7121\u80fd\u7a31\u8aaa\u3002", "315": "\u8a08\u630d\u601d\u91cf\u6240\u80fd\u77e5\u8005\u3002\u723e\u6642\u4e16\u5c0a\u8aaa\u662f\u8a9e\u5df2\u3002\u6b32", "316": "\u91cd\u5ba3\u6b64\u7fa9\u4ee5\u5048\u980c\u66f0\u3002", "317": "\u805e\u7d93\u5fc3\u96a8\u559c\u66f8\u5beb\u5e76\u8b80\u8aa6", "318": "\u81ea\u6301\u6559\u4eba\u6301\u662f\u8af8\u529f\u5fb7\u805a", "319": "\u4e00\u5207\u8af8\u773e\u751f\u4e0d\u80fd\u6e2c\u91cf\u8005", "320": "\u73fe\u898b\u4e00\u5207\u4f5b\u7372\u5f97\u5e0c\u6709\u4fe1", "321": "\u76e1\u80fd\u77e5\u4e00\u5207\u751a\u6df1\u4fee\u591a\u7f85", "322": "\u901f\u8b49\u5999\u83e9\u63d0\u5f97\u898b\u4f5b\u7121\u6e1b", "323": "\u5fa9\u5f97\u5b9a\u795e\u901a\u5999\u8272\u5e76\u8cc7\u7ce7", "324": "\u7e3d\u6301\u7b49\u7121\u6e1b\u4e43\u81f3\u7121\u4e0a\u89ba", "325": "\u6211\u61b6\u904e\u53bb\u4e16\u5982\u662f\u9640\u7f85\u5c3c", "326": "\u7121\u908a\u723e\u6642\u805e\u5f97\u898b\u6052\u6c99\u4f5b", "327": "\u96a8\u5fc3\u6240\u6a02\u8b49\u6210\u5c31\u7136\u71c8\u5c0a", "328": "\u7121\u908a\u524d\u751f\u6642\u5f4c\u9640\u70ba\u8aaa\u6cd5", "329": "\u6211\u53ca\u8ce2\u52ab\u773e\u5f80\u4e16\u7686\u96a8\u559c", "330": "\u6b32\u901f\u8b49\u83e9\u63d0\u6467\u4f0f\u65bc\u8af8\u9b54", "331": "\u767e\u798f\u76f8\u5177\u8db3\u4fee\u6b64\u6cd5\u4e0d\u96e3", "332": "\u66f8\u6301\u5ff5\u8aa6\u798f\u5373\u662f\u771f\u4f5b\u5b50", "333": "\u5047\u4f7f\u8af8\u4e16\u754c\u5176\u6578\u7b49\u6052\u6c99", "334": "\u4e00\u5207\u6eff\u4e2d\u5bf6\u6301\u4ee5\u4f9b\u725f\u5c3c", "335": "\u76f4\u4ee5\u66f8\u6301\u798f\u80fd\u904e\u662f\u529f\u5fb7", "336": "\u662f\u7d93\u52e2\u529b\u6545\u5fc5\u5f97\u4f5b\u4e0d\u96e3", "337": "\u5fa9\u6b21\u820d\u5229\u5f17\u3002\u82e5\u83e9\u85a9\u65bc\u6b64\u5165\u7121\u908a\u9640\u7f85\u5c3c\u3002\u7cbe", "338": "\u52e4\u7528\u610f\u8005\u3002\u65bc\u5f7c\u96ea\u5c71\u4e4b\u4e2d\u6709\u516b\u591c\u53c9\u3002\u65e5\u591c\u5e38", "339": "\u4f86\u885b\u8b77\u6b64\u4eba\u3002\u4ee5\u5176\u5a01\u529b\u904d\u5165\u5f7c\u4eba\u8eab\u8af8\u6bdb\u5b54\u3002", "340": "\u4f55\u7b49\u70ba\u516b\u3002\u5176\u540d\u66f0\u52c7\u5065\u591c\u53c9\u5805\u56fa\u591c\u53c9\u3002\u773e\u591a", "341": "\u529b\u591c\u53c9\u90a3\u7f85\u5ef6\u529b\u591c\u53c9\u3002\u5be6\u884c\u591c\u53c9\u7121\u80fd\u964d\u4f0f", "342": "\u591c\u53c9\u3002\u9577\u7259\u92d2\u51fa\u591c\u53c9\u5584\u81c2\u591c\u53c9\u3002\u5982\u662f\u7b49\u820d\u5229", "343": "\u5f17\u3002\u662f\u8af8\u83e9\u85a9\u82e5\u6b32\u4ee4\u5f7c\u516b\u5927\u591c\u53c9\u4f86\u73fe\u8eab\u76f8", "344": "\u8005\u3002\u7576\u6de8\u6d17\u6d74\u8457\u597d\u6de8\u8863\u5165\u7d93\u884c\u9053\u5834\u3002\u65bc\u4e00\u5207", "345": "\u773e\u751f\u908a\u4e0d\u8d77\u60e1\u5fc3\u3002\u65bc\u6b64\u6cd5\u672c\u9640\u7f85\u5c3c\u4e00\u5fc3\u61b6", "346": "\u5ff5\u8005\u3002\u5f7c\u516b\u591c\u53c9\u901f\u73fe\u9762\u9580\u524d\u3002\u4ee4\u662f\u83e9\u85a9\u5206\u660e", "347": "\u5f97\u898b\u3002\u5fa9\u6709\u516b\u83e9\u85a9\u5728\u6b32\u754c\u5929\u4e2d\u751f\u3002\u4ea6\u5e38\u8b77\u5ff5", "348": "\u5f7c\u884c\u9640\u7f85\u5c3c\u8005\u3002\u4f55\u7b49\u70ba\u516b\u3002\u5176\u540d\u66f0\u6240\u8b02\u660e\u5fb9", "349": "\u5929\u5b50\u666e\u7167\u5929\u5b50\u3002\u667a\u5149\u5929\u5b50\u65e5\u5149\u5929\u5b50\u3002\u52f8\u8acb\u5929", "350": "\u5b50\u4e00\u5207\u9858\u6eff\u8db3\u5929\u5b50\u3002\u661f\u5bbf\u738b\u5929\u5b50\u5bf6\u610f\u5929\u5b50\u3002", "351": "\u5982\u662f\u7b49\u516b\u5929\u5b50\u5e38\u7576\u8b77\u5ff5\u3002\u82e5\u884c\u6b64\u9640\u7f85\u5c3c\u8005\u3002", "352": "\u8981\u9808\u7cbe\u5fc3\u7528\u610f\u5982\u662f\u3002\u83e9\u85a9\u61c9\u767c\u771f\u5be6\u8a93\u9858\u3002\u77e5", "353": "\u6069\u5831\u6069\u65e5\u591c\u6c42\u6cd5\u3002\u4ee5\u6cd5\u81ea\u5a1b\u4fee\u751a\u6df1\u6cd5\u5fcd\u3002\u5982", "354": "\u662f\u83e9\u85a9\u61c9\u9808\u884c\u65bd\u5206\u5e03\u65bc\u4eba\u3002\u96a8\u5df1\u6240\u6709\u4ee5\u7d66", "355": "\u65bc\u4eba\u3002\u5047\u4ee4\u552f\u6709\u6b64\u4e00\u7269\u4ea6\u9808\u65bd\u4eba\u3002\u5982\u662f\u83e9\u85a9", "356": "\u5fa9\u9808\u606d\u656c\u4e00\u5207\u5351\u5fc3\u4e0b\u9806\u3002\u5c08\u5fc3\u4e00\u610f\u5b78\u751a\u6df1", "357": "\u5fcd\u3002\u8af8\u83e9\u85a9\u7b49\u3002\u7576\u61c9\u5982\u662f\u4fee\u884c\u9640\u7f85\u5c3c\u6cd5\u672c\u3002\u723e", "358": "\u6642\u4e16\u5c0a\u8aaa\u662f\u9640\u7f85\u5c3c\u6cd5\u672c\u6642\u3002\u65bc\u5176\u773e\u4e2d\u6709\u4e09", "359": "\u5341\u5104\u767e\u5343\u90a3\u7531\u4ed6\u6052\u6cb3\u6c99\u6578\u7b49\u8af8\u83e9\u85a9\u3002\u7686\u65bc", "360": "\u963f\u8028\u591a\u7f85\u4e09\u85d0\u4e09\u83e9\u63d0\u3002\u5f97\u81f3\u4e0d\u9000\u8f49\u5730\u3002\u516d\u5341", "361": "\u5104\u767e\u5343\u90a3\u7531\u4ed6\u5929\u4eba\u7b49\u773e\u3002\u672a\u767c\u5fc3\u8005\u7686\u5f97\u767c", "362": "\u65bc\u963f\u8028\u591a\u7f85\u4e09\u85d0\u4e09\u83e9\u63d0\u5fc3\u3002\u723e\u6642\u9577\u8001\u820d\u5229", "363": "\u5f17\u767d\u4f5b\u8a00\u4e16\u5c0a\u3002\u7576\u4f55\u540d\u65af\u7d93\u3002\u6211\u7b49\u5927\u773e\u4e91\u4f55", "364": "\u5949\u6301\u3002\u4f5b\u544a\u9577\u8001\u820d\u5229\u5f17\u8a00\u3002\u6b64\u7d93\u540d\u70ba\u5165\u7121\u908a", "365": "\u6cd5\u672c\u9640\u7f85\u5c3c\u3002\u5982\u662f\u53d7\u6301\u6b64\u7d93\u3002\u4ea6\u540d\u4e00\u5411\u51fa\u751f", "366": "\u83e9\u85a9\u3002\u5982\u662f\u53d7\u6301\u6b64\u7d93\u3002\u4ea6\u540d\u964d\u4f0f\u4e00\u5207\u9b54\u6028\u5982", "367": "\u662f\u53d7\u6301\u6b64\u7d93\u3002\u4ea6\u540d\u6c7a\u5b9a\u8da3\u5411\u4e00\u5207\u667a\u6545\u5982\u662f", "368": "\u53d7\u6301\u3002\u4f5b\u8aaa\u6b64\u7d93\u5df2\u3002\u9577\u8001\u820d\u5229\u5f17\u53ca\u8af8\u8072\u805e\u3002\u83e9", "369": "\u85a9\u5927\u6703\u5929\u9f8d\u516b\u90e8\u3002\u4e00\u5207\u5927\u773e\u805e\u4f5b\u6240\u8aaa\u6b61\u559c", "370": "\u5949\u884c\u3002", "371": "\u4f5b\u8aaa\u4e00\u5411\u51fa\u751f\u83e9\u85a9\u7d93"}, "originalTitle": "Taisho Tripitaka, Electronic version, No. 1017 \u4f5b\u8aaa\u4e00\u5411\u51fa\u751f\u83e9\u85a9\u7d93", "source": "CBETA"}