81ea\u5f97\uff0c\u96a8\u4e16\u9593\u7fd2\u4fd7\u800c\u5165\uff0c\u793a\u73fe\u5982", "108": "\u662f\u3002", "109": "\u300c\u4f5b\u77e5\u4e16\u9593\u672c\u7121\u4eba\uff0c\u8af8\u6240\u6709\u672c\u7121\u5f62\uff0c\u4f5b\u73fe\u5ea6\u812b", "110": "\u7121\u592e\u6578\u4eba\uff0c\u96a8\u4e16\u9593\u7fd2\u4fd7\u800c\u5165\uff0c\u793a\u73fe\u5982\u662f\u3002", "111": "\u300c\u4f5b\u77e5\u8af8\u7d93\u6cd5\u672c\u7a7a\uff0c\u672c\u4ea6\u7121\u6240\u6709\uff0c\u73fe\u4eba\u6709\u66f4\u6b7b", "112": "\u751f\uff0c\u96a8\u4e16\u9593\u7fd2\u4fd7\u800c\u5165\uff0c\u793a\u73fe\u5982\u662f\u3002\u300c\u672c\u7121\u4eca\u4e16\u5f8c", "113": "\u4e16\u4e4b\u4e8b\uff0c\u4f5b\u73fe\u4eba\u6709\u4eca\u4e16\u5f8c\u4e16\u4e4b\u4e8b\uff0c\u96a8\u4e16\u9593\u7fd2", "114": "\u4fd7\u800c\u5165\uff0c\u793a\u73fe\u5982\u662f\u3002\u300c\u4e94\u9670\u3001\u516d\u8870\u3001\u56db\u5927\u5408\u70ba\u4e00\uff0c\u672c", "115": "\u7121\u6709\uff0c\u4f5b\u793a\u73fe\u4eba\u6b32\u754c\u3001\u8272\u754c\u3001\u7121\u601d\u60f3\u754c\uff0c\u96a8\u4e16\u9593", "116": "\u7fd2\u4fd7\u800c\u5165\uff0c\u793a\u73fe\u5982\u662f\u3002", "117": "\u300c\u672c\u7121\u904e\u53bb\u7576\u4f86\u4eca\u73fe\u5728\u4eba\uff0c\u4f5b\u73fe\u6b7b\u751f\u4e94\u9053\u4e2d", "118": "\u4eba\uff0c\u96a8\u4e16\u9593\u7fd2\u4fd7\u800c\u5165\uff0c\u793a\u73fe\u5982\u662f\u3002\u300c\u4f5b\u70ba\u6089\u793a\u611a", "119": "\u7661\u7686\u76e1\uff0c\u73fe\u4eba\u672c\u5e03\u65bd\uff0c\u96a8\u4e16\u9593\u7fd2\u4fd7\u800c\u5165\uff0c\u793a", "120": "\u73fe\u5982\u662f\u3002", "121": "\u300c\u904e\u53bb\u7576\u4f86\u4eca\u73fe\u5728\u7d93\u6cd5\uff0c\u4f5b\u6089\u77e5\u5176\u672c\uff0c\u4f5b\u793a\u4eba", "122": "\u53ef\u8aaa\u3001\u6709\u4e0d\u53ef\u8aaa\uff0c\u96a8\u4e16\u9593\u7fd2\u4fd7\u800c\u5165\uff0c\u793a\u73fe\u5982\u662f\u3002", "123": "\u300c\u4f5b\u77e5\u8af8\u7d93\u672c\u672b\u4e00\u5207\u7686\u6df1\uff0c\u4f5b\u5206\u5225\u5404\u81ea\u8aaa\u5176", "124": "\u4e8b\uff0c\u96a8\u4e16\u9593\u7fd2\u4fd7\u800c\u5165\uff0c\u793a\u73fe\u5982\u662f\u3002", "125": "\u300c\u4f5b\u6240\u8a9e\u7121\u6709\u7570\u8aaa\uff0c\u56db\u8ae6\u6cd5\u96a8\u4eba\u6240\u89e3\u800c\u8aaa\uff0c\u96a8", "126": "\u4e16\u9593\u7fd2\u4fd7\u800c\u5165\uff0c\u793a\u73fe\u5982\u662f\u3002", "127": "\u300c\u8af8\u6bd4\u4e18\u50e7\u96e3\u53ef\u6557\u58de\uff0c\u6b63\u4f7f\u6578\u5343\u5104\u842c\u9b54\u4f86\uff0c\u53ca", "128": "\u8af8\u60e1\u4e0d\u80fd\u7834\u58de\u6bd4\u4e18\u50e7\uff0c\u4f5b\u73fe\u4eba\u7834\u58de\u6bd4\u4e18\u50e7\uff0c", "129": "\u96a8\u4e16\u9593\u7fd2\u4fd7\u800c\u5165\uff0c\u793a\u73fe\u5982\u662f\u3002", "130": "\u300c\u7d93\u6cd5\u672c\u7121\u5f9e\u8ab0\u5b78\uff0c\u4ea6\u83ab\u4e0d\u5b78\u8005\uff0c\u4f5b\u73fe\u4eba\u7d93\u6cd5\uff0c\u662f", "131": "\u53d7\u6212\u3001\u662f\u4e0d\u53d7\u6212\uff0c\u96a8\u4e16\u9593\u7fd2\u4fd7\u800c\u5165\uff0c\u793a\u73fe\u5982\u662f\u3002", "132": "\u300c\u7a7a\u7a7a\u4ea6\u7121\u7e6b\uff0c\u4ea6\u7121\u812b\u4e16\u9593\uff0c\u4f5b\u793a\u4eba\u5ea6\u812b\uff0c\u96a8\u4e16", "133": "\u9593\u7fd2\u4fd7\u800c\u5165\uff0c\u793a\u73fe\u5982\u662f\u3002", "134": "\u300c\u4f5b\u822c\u6ce5\u6d39\u7121\u6240\u5411\uff0c\u963f\u7f85\u6f22\u822c\u6ce5\u6d39\u7121\u6240\u5411\uff0c\u4f5b", "135": "\u8aaa\u6cd5\u793a\u4eba\uff0c\u96a8\u4e16\u9593\u7fd2\u4fd7\u800c\u5165\uff0c\u793a\u73fe\u5982\u662f\u3002", "136": "\u300c\u4f5b\u8aaa\u7121\u6240\u751f\u7121\u6240\u6ec5\uff0c\u662f\u70ba\u8981\uff0c\u4ea6\u7121\u6240\u5f97\u4ea6\u7121", "137": "\u6240\u5931\uff0c\u96a8\u4e16\u9593\u7fd2\u4fd7\u800c\u5165\uff0c\u793a\u73fe\u5982\u662f\u3002", "138": "\u300c\u4f5b\u8aaa\u6ce5\u6d39\uff0c\u8b6c\u5982\u71c8\u6ec5\u7121\u5f62\uff0c\u4f46\u6709\u5b57\u8033\u3002\u7d93\u6cd5\u7121", "139": "\u6709\u80fd\u58de\u8005\uff0c\u96a8\u4e16\u9593\u7fd2\u4fd7\u800c\u5165\uff0c\u793a\u73fe\u5982\u662f\u3002", "140": "\u300c\u4f5b\u6089\u77e5\u8af8\u7d93\u6cd5\u672c\u7121\u5f62\uff0c\u4f5b\u73fe\u4eba\u8aaa\u7d93\u6cd5\u751a\u773e", "141": "\u591a\uff0c\u96a8\u4e16\u9593\u7fd2\u4fd7\u800c\u5165\uff0c\u793a\u73fe\u5982\u662f\u3002", "142": "\u300c\u8af8\u4f5b\u5fc3\u7686\u7121\u6240\u7f63\u7919\uff0c\u672a\u5617\u96e2\u4e09\u6627\u6642\uff0c\u73fe\u4eba", "143": "\u751f\u5ff5\uff0c\u96a8\u4e16\u9593\u7fd2\u4fd7\u800c\u5165\uff0c\u793a\u73fe\u5982\u662f\u3002", "144": "\u300c\u4f5b\u8af8\u60e1\u6089\u76e1\uff0c\u4f46\u6709\u8af8\u529f\u5fb7\u5177\u8db3\uff0c\u4f5b\u73fe\u4eba\u8af8\u60e1", "145": "\u672a\u76e1\uff0c\u96a8\u4e16\u9593\u7fd2\u4fd7\u800c\u5165\uff0c\u793a\u73fe\u5982\u662f\u3002", "146": "\u300c\u4f5b\u8eab\u884c\u53e3\u8a00\u5fc3\u5ff5\uff0c\u7576\u8207\u667a\u6167\u4ff1\u662f\u70ba\u672c\uff0c\u4f5b\u73fe", "147": "\u4eba\u4f7f\u6bd4\u4e18\u8aaa\u7d93\u81ea\u5fa9\u6b32\u805e\uff0c\u96a8\u4e16\u9593\u7fd2\u4fd7\u800c\u5165\uff0c", "148": "\u793a\u73fe\u5982\u662f\u3002", "149": "\u300c\u4f5b\u667a\u6167\u6240\u89e3\u7121\u6709\u7aed\u5e95\uff0c\u904e\u53bb\u7576\u4f86\u4eca\u73fe\u5728\u672c", "150": "\u7a7a\uff0c\u4f5b\u73fe\u4eba\u8aaa\u7d93\u6cd5\uff0c\u96a8\u5176\u6240\u559c\u5404\u5404\u70ba\u8aaa\uff0c\u96a8\u4e16", "151": "\u9593\u7fd2\u4fd7\u800c\u5165\uff0c\u793a\u73fe\u5982\u662f\u3002", "152": "\u300c\u8af8\u4f5b\u5408\u4e00\u8eab\u4ee5\u7d93\u6cd5\u70ba\u8eab\uff0c\u4f5b\u73fe\u70ba\u4eba\u8aaa\u7d93\u6cd5\uff0c", "153": "\u96a8\u4e16\u9593\u7fd2\u4fd7\u800c\u5165\uff0c\u793a\u73fe\u5982\u662f\u3002", "154": "\u300c\u4f5b\u3001\u8f9f\u652f\u4f5b\u3001\u963f\u7f85\u6f22\u3001\u672a\u5f97\u9053\u4eba\uff0c\u73fe\u6b7b\u751f\u5f97\u6ce5\u6d39\uff0c", "155": "\u96a8\u4e16\u9593\u7fd2\u4fd7\u800c\u5165\uff0c\u793a\u73fe\u5982\u662f\u3002", "156": "\u300c\u4f5b\u73fe\u70ba\u7f85\u6f22\u8aaa\u7d93\u6cd5\u6089\u5177\u8db3\uff0c\u96d6\u77e5\u5176\u5177\u8db3\uff0c\u4e0d", "157": "\u53ca\u85a9\u4e91\u82e5\uff0c\u96a8\u4e16\u9593\u7fd2\u4fd7\u800c\u5165\uff0c\u793a\u73fe\u5982\u662f\u3002", "158": "\u300c\u4f5b\u667a\u6167\u7121\u6709\u80fd\u904e\u8005\uff0c\u6089\u77e5\u7121\u6709\u904e\u53bb\u7576\u4f86\u4eca", "159": "\u73fe\u5728\uff0c\u4f5b\u73fe\u6240\u56e0\u7de3\u8aaa\u7d93\u6cd5\uff0c\u96a8\u4e16\u9593\u7fd2\u4fd7\u800c\u5165\uff0c", "160": "\u793a\u73fe\u5982\u662f\u3002", "161": "\u300c\u4e2d\u6709\u6b32\u77e5\u4f5b\u53ca\u4e86\u4f5b\u6cd5\u8005\uff0c\u7d93\u672c\u7aef\u754c\u6089\u5165\uff0c\u662f", "162": "\u4eba\u70ba\u66c9\u4e86\u4f5b\uff0c\u96a8\u4e16\u9593\u7fd2\u4fd7\u800c\u5165\uff0c\u793a\u73fe\u5982\u662f\u3002", "163": "\u300c\u83e9\u85a9\u4ea6\u4e0d\u5165\u6bcd\u8179\u4e2d\uff0c\u4ea6\u4e0d\u5f9e\u6bcd\u8179\u4e2d\u51fa\u3002\u4f55\u4ee5", "164": "\u6545\uff1f\u7d93\u6cd5\u672c\u754c\u7121\u6240\u4e0d\u5165\u3002\u83e9\u85a9\u73fe\u4eba\u5165\u6bcd\u8179\u4e2d\uff0c", "165": "\u96a8\u4e16\u9593\u7fd2\u4fd7\u800c\u5165\uff0c\u793a\u73fe\u5982\u662f\u3002", "166": "\u300c\u7121\u6240\u5f9e\u751f\u6cd5\u6a02\u8af8\u7d93\u6cd5\uff0c\u4ea6\u7121\u6240\u5f9e\u751f\uff0c\u83e9\u85a9\u73fe", "167": "\u4eba\u521d\u751f\u6642\uff0c\u96a8\u4e16\u9593\u7fd2\u4fd7\u800c\u5165\uff0c\u793a\u73fe\u5982\u662f\u3002", "168": "\u300c\u83e9\u85a9\u6bcd\u8179\u7a7a\u5b9a\uff0c\u542b\u53d7\u4e00\u4f5b\u5883\u754c\uff0c\u83e9\u85a9\u5404\u5404\u73fe", "169": "\u4eba\u56e0\u7de3\u751f\uff0c\u96a8\u4e16\u9593\u7fd2\u4fd7\u800c\u5165\uff0c\u793a\u73fe\u5982\u662f\u3002", "170": "\u300c\u4f5b\u5316\u5206\u8eab\u5728\u7121\u592e\u6578\uff0c\u4e0d\u53ef\u5fa9\u8a08\u4f5b\u524e\u6089\u904d\u81f3\uff0c", "171": "\u4f5b\u8eab\u4ea6\u4e0d\u589e\u4ea6\u4e0d\u6e1b\uff0c\u96a8\u4e16\u9593\u7fd2\u4fd7\u800c\u5165\uff0c\u793a\u73fe", "172": "\u5982\u662f\u3002", "173": "\u300c\u83e9\u85a9\u5e38\u73fe\u751f\u7121\u6709\u7d55\u6642\uff0c\u5e38\u672c\u7121\u800c\u4f4f\u4e0d\u52e4\u82e6\uff0c", "174": "\u96a8\u4e16\u9593\u7fd2\u4fd7\u800c\u5165\uff0c\u793a\u73fe\u5982\u662f\u3002", "175": "\u300c\u4f5b\u667a\u6167\u529f\u5fb7\u5a01\u795e\u4e0d\u53ef\u5fa9\u8a08\uff0c\u4f5b\u73fe\u4eba\u9650\u9577\u77ed", "176": "\u4f7f\u4eba\u77e5\u4e4b\uff0c\u96a8\u4e16\u9593\u7fd2\u4fd7\u800c\u5165\uff0c\u793a\u73fe\u5982\u662f\u3002", "177": "\u300c\u7d93\u6cd5\u672c\u754c\u7121\u6709\u80fd\u904e\u8005\uff0c\u904e\u53bb\u7576\u4f86\u4eca\u73fe\u5728\u7686", "178": "\u7a7a\u6545\u3002\u4f5b\u73fe\u4eba\u5c0a\u7d93\u6cd5\uff0c\u96a8\u4e16\u9593\u7fd2\u4fd7\u800c\u5165\uff0c\u793a\u73fe", "179": "\u5982\u662f\u3002", "180": "\u300c\u672c\u7121\u6240\u751f\u7121\u6240\u6ec5\uff0c\u7d93\u672c\u754c\u6089\u5165\uff0c\u4f5b\u73fe\u4eba\u5883\u754c", "181": "\u58de\u6557\u6642\uff0c\u96a8\u4e16\u9593\u7fd2\u4fd7\u800c\u5165\uff0c\u793a\u73fe\u5982\u662f\u3002", "182": "\u300c\u7d93\u6cd5\u672c\u7121\u6240\u5f9e\u751f\u7121\u5f62\u800c\u4f4f\uff0c\u4f5b\u73fe\u7d93\u6cd5\uff0c\u96a8\u4e16", "183": "\u9593\u7fd2\u4fd7\u800c\u5165\uff0c\u793a\u73fe\u5982\u662f\u3002", "184": "\u300c\u89c0\u672c\u7121\u4ea6\u7121\u6240\u898b\u4ea6\u7121\u6240\u8996\uff0c\u4f5b\u8996\u4eba\u6089\u898b\u6089", "185": "\u4e86\u6089\u77e5\uff0c\u96a8\u4e16\u9593\u7fd2\u4fd7\u800c\u5165\uff0c\u793a\u73fe\u5982\u662f\u3002", "186": "\u300c\u8af8\u7d93\u6cd5\u7121\u6709\u4f5c\u8005\uff0c\u4ea6\u7121\u6240\u51fa\u751f\uff0c\u4f5b\u73fe\u4eba\u7d93\u6cd5", "187": "\u672c\u7121\u6240\u51fa\u751f\uff0c\u96a8\u4e16\u9593\u7fd2\u4fd7\u800c\u5165\uff0c\u793a\u73fe\u5982\u662f\u3002", "188": "\u300c\u6ce5\u6d39\u53ca\u7a7a\u7121\u6709\u5f62\u8072\uff0c\u4ea6\u7121\u6709\u540d\uff0c\u4f5b\u73fe\u56db\u5927\u53ca", "189": "\u5f62\u8072\uff0c\u96a8\u4e16\u9593\u7fd2\u4fd7\u800c\u5165\uff0c\u793a\u73fe\u5982\u662f\u3002", "190": "\u300c\u4f5b\u529b\u7121\u6709\u96d9\u6bd4\uff0c\u4e0d\u53ef\u5fa9\u8a08\uff0c\u4ea6\u7121\u6e1b\u76e1\u6642\uff0c\u73fe\u4eba", "191": "\u8870\u8001\u6c42\u4eba\u7d66\u4f7f\uff0c\u96a8\u4e16\u9593\u7fd2\u4fd7\u800c\u5165\uff0c\u793a\u73fe\u5982\u662f\u3002", "192": "\u300c\u4f5b\u6148\u54c0\u6089\u904d\u7b49\uff0c\u7d42\u7121\u6709\u5384\u96e3\u7aae\u6975\u6642\uff0c\u4f5b\u73fe\u7661", "193": "\u4eba\u4e0d\u7576\u8207\u5f9e\u4e8b\uff0c\u96a8\u4e16\u9593\u7fd2\u4fd7\u800c\u5165\uff0c\u793a\u73fe\u5982\u662f\u3002", "194": "\u300c\u4f5b\u8af8\u529f\u5fb7\u6210\u5c31\u6089\u5177\u8db3\uff0c\u4f5b\u73fe\u529f\u5fb7\u5c11\u6240\uff0c\u96a8\u4e16", "195": "\u9593\u7fd2\u4fd7\u800c\u5165\uff0c\u793a\u73fe\u5982\u662f\u3002", "196": "\u300c\u7121\u6240\u5f9e\u751f\u672c\uff0c\u5f9e\u4e2d\u4ea6\u7121\u6240\u51fa\u751f\uff0c\u4f5b\u73fe\u4e09\u9580\u8005\uff0c", "197": "\u96a8\u4e16\u9593\u7fd2\u4fd7\u800c\u5165\uff0c\u793a\u73fe\u5982\u662f\u3002", "198": "\u300c\u4f5b\u73fe\u672c\u672b\u7121\u6240\u7f63\u7919\uff0c\u529f\u5fb7\u798f\u7121\u6709\u80fd\u904e\u8005\uff0c\u4f5b", "199": "\u73fe\u4eba\u6709\u65bd\u8207\u8005\uff0c\u4e0d\u65af\u53d7\u8da3\u8db3\u800c\u5df2\uff0c\u793a\u4e0d\u8caa\uff0c\u96a8", "200": "\u4e16\u9593\u7fd2\u4fd7\u800c\u5165\uff0c\u793a\u73fe\u5982\u662f\u3002", "201": "\u300c\u4eba\u6709\u81f3\u8aa0\u5584\u610f\u5ff5\u4f5b\u8005\uff0c\u4f5b\u5373\u70ba\u73fe\uff0c\u4f5b\u4ea6\u7121\u8655", "202": "\u6240\uff0c\u4f5b\u73fe\u8eab\u884c\u83e9\u85a9\u9053\u8005\uff0c\u96a8\u4e16\u9593\u7fd2\u4fd7\u800c\u5165\uff0c\u793a", "203": "\u73fe\u5982\u662f\u3002", "204": "\u300c\u4f5b\u5ea6\u812b\u4e0d\u53ef\u5fa9\u8a08\u963f\u50e7\u7947\u4eba\uff0c\u70ba\u4e0d\u5ea6\u4e00\u4eba\u3002\u4f55", "205": "\u4ee5\u6545\uff1f\u672c\u7121\u6545\uff0c\u96a8\u4e16\u9593\u7fd2\u4fd7\u800c\u5165\uff0c\u793a\u73fe\u5982\u662f\u3002", "206": "\u300c\u4f5b\u73cd\u5bf6\u5167\u85cf\u7d93\uff0c\u4eba\u6709\u805e\u8005\uff0c\u7121\u6709\u4e0d\u5f97\u5b89\u96b1\uff0c\u5ea6", "207": "\u6578\u5343\u842c\u5104\u52ab\u7121\u6578\u5982\u662f\uff0c\u83e9\u85a9\u4e16\u4e16\u6240\u884c\uff0c\u7528\u5341", "208": "\u65b9\u4eba\u6545\u3002\u83e9\u85a9\u4e16\u4e16\u884c\u7d93\u6212\uff0c\u672a\u66fe\u6709\u72af\u6642\uff0c\u7528\u662f", "209": "\u5f97\u4f5b\u667a\u6167\uff0c\u6709\u61c9\u662f\u884c\u5f97\u4f5b\u75be\u3002\u83e9\u85a9\u884c\u6148\u54c0\uff0c\u6709", "210": "\u76ca\u5341\u65b9\u7121\u6975\uff0c\u6709\u4f5c\u662f\u884c\u8005\uff0c\u5f97\u4f5b\u75be\u3002\u4f55\u4eba\u805e\u662f", "211": "\u4e0d\u5949\u884c\u8005\uff1f\u4f5b\u5a01\u795e\u5dcd\u5dcd\uff0c\u5176\u6709\u805e\u7d93\u6cd5\uff0c\u83ab\u4e0d\u904e", "212": "\u5ea6\u3002\u300d\u4f5b\u8aaa\u7d93\u5df2\uff0c\u6587\u6b8a\u5e2b\u5229\u83e9\u85a9\u53ca\u8af8\u83e9\u85a9\u7b49\uff0c\u7686", "213": "\u6b61\u559c\uff0c\u524d\u70ba\u4f5b\u4f5c\u79ae\u800c\u53bb\u3002", "214": "\u4f5b\u8aaa\u5167\u85cf\u767e\u5bf6\u7d93"}, "originalTitle": "Taisho Tripitaka, Electronic version, No. 0807 \u4f5b\u8aaa\u5167\u85cf\u767e\u5bf6\u7d93: \u4f5b\u8aaa\u5167\u85cf\u767e\u5bf6\u7d93: \u4f5b\u8aaa\u5167\u85cf\u767e\u5bf6\u7d93", "source": "CBETA"}