u4e0a\u8a16\u59ac\u4e8c\u5408\u5f15\u9298\u4e0a\u5a46\u56a9\u4e03\u8607\u5831\u5f15\u4f7f\u55bb\u4e8c\u5408\u5f15", "202": "\u9298\u4e0a\u5a46\u4e0a\u56a9\u516b\u85a9\u56a9\u6089\u6715\u6575\u7981\u53cd\u9298\u4e0a\u9262\u56c9\u4e8c\u5408\u5f15\u91ce", "203": "\u7e12\u4e0a\u4e5d\u85a9\u56a9\u7faf\u6469\u8607\u8005\u9298\u4e0a\u5341\u8cea\u8de2\u4e0a\u5ba4\u550e\u4e8c\u5408", "204": "\u85e5\u77e9\u5695\u5341\u4e00\u543d\u8a36\u8a36\u8a36\u8a36\u7a40\u5a46\u8a90\u9441\u5341\u4e8c\u85a9\u56a9\u601b", "205": "\u4ed6\u53bb\u5f15\u8a90\u8de2\u5341\u4e09\u56a9\u65e5\u56c9\u4e8c\u5408\u5f15\u83bd\u5f15\u9298\u4e0a\u60b6\u4e0a\u8005\u5341", "206": "\u56db\u56a9\u65e5\u54e9\u4e8c\u5408\u5a46\u4e0a\u56a9\u4e0a\u5341\u4e94\u6469\u8a36\u4e09\u53bb\u9ebc\u8036\u85a9\u601b", "207": "\u56a9\u4e8c\u5408\u60e1\u5f15\u5165\u5341\u516d", "208": "\u7531\u4ee5\u767e\u5b57\u771f\u8a00\u5949\u95bc\u4f3d\u6c34\u6545\u3002\u6240\u6709\u9858\u6c42\u901f\u5f97", "209": "\u6210\u5c31\u3002", "210": "\u6b21\u8aa6\u91d1\u525b\u5b09\u6232\u771f\u8a00\u66f0\u3002", "211": "\u7cfb\u56c9\u5e95\u56a9\u65e5\u56c9\u4e8c\u5408\u4e00\u5c3e\u908f\u8cdc\u511e\u4e8c\u601b\u56c9\u4e8c\u5408\u5412\u534a", "212": "\u97f3", "213": "\u6b21\u91d1\u525b\u9b18\u771f\u8a00\u66f0\u3002", "214": "\u7cfb\u56c9\u5e95\u56a9\u65e5\u56c9\u4e8c\u5408\u5f15\u4e00\u8cc0\u7d30\u4e8c\u8a36\u8a36\u4e09", "215": "\u6b21\u91d1\u525b\u6b4c\u771f\u8a00\u66f0\u3002", "216": "\u7cfb\u56c9\u5e95\u56a9\u65e5\u56c9\u4e8c\u5408\u5f15\u4e00\u64ec\u8ae6\u8ae6\u8ae6", "217": "\u6b21\u91d1\u525b\u821e\u771f\u8a00\u66f0\u3002", "218": "\u7cfb\u56c9\u5e95\u56a9\u65e5\u56c9\u4e8c\u5408\u5f15\u4e00\u511e\u54e9\u4e8c\u5408\u5e95\u66f3\u4e8c\u5408\u4e8c\u5420\u6ce2\u5420", "219": "\u6ce2\u4e09", "220": "\u5fa9\u4f5c\u5916\u56db\u4f9b\u990a\u800c\u5949\u737b\u4e4b\u3002", "221": "\u6b21\u91d1\u525b\u82b1\u9b18\u771f\u8a00\u66f0\u3002", "222": "\u7cfb\u4e00\u56a9\u65e5\u56c9\u4e8c\u5408\u5f15\u4e8c\u56c9\u5e95\u4e09", "223": "\u6b21\u91d1\u525b\u9999\u771f\u8a00\u66f0\u3002", "224": "\u6469\u8a36\u56c9\u591a\u56a9\u65e5\u54e9\u4e8c\u5408\u4e00\u659b\u4e8c", "225": "\u6b21\u91d1\u525b\u71c8\u771f\u8a00\u66f0\u3002", "226": "\u5535\u4e00\u56a9\u65e5\u56c9\u4e8c\u5408\u5f15\u4e8c\u8def\u8005\u5be7\u4e09", "227": "\u6b21\u91d1\u525b\u5857\u9999\u771f\u8a00\u66f0\u3002", "228": "\u6469\u8a36\u5ba4\u550e\u4e8c\u5408\u4e00\u56a9\u65e5\u54e9\u4e8c\u5408\u4e8c\u546c\u4e09", "229": "\u4fee\u884c\u8005\u7e94\u4f4f\u662f\u516b\u4f9b\u990a\u83e9\u85a9\u4e09\u6469\u5730\u3002\u5373\u80fd\u51fa", "230": "\u751f\u76e1\u5341\u65b9\u754c\u4e00\u5207\u4f5b\u524e\u5ee3\u5927\u96f2\u6d77\u4f9b\u990a\u5100\u5f0f\u3002", "231": "\u6b21\u5373\u7d50\u672c\u5c0a\u4e09\u6627\u8036\u5370\u3002\u4e8c\u7fbd\u91d1\u525b\u5408\u638c\u3002\u4ee5\u5927", "232": "\u6307\u5404\u637b\u4e8c\u982d\u6307\u5074\u8aa6\u672c\u5c0a\u771f\u8a00\u4e03\u904d\u3002", "233": "\u771f\u8a00\u66f0\u3002", "234": "\u5a1c\u8b28\u5a46\u53bb\u8a90\u56a9\u601b\u66f3\u4e8c\u5408\u5f15\u4e00\u9262\u56c9\u4e8c\u5408\u5f15\u67b3\u5b43\u4e8c\u5408\u64ad", "235": "\u5f15\u56c9\u5f2d\u8de2\u5f15\u66f3\u4e8c\u5535\u4e09\u7d07\u550e\u4e8c\u5408\u5165\u5f15\u5730\u5f15\u5165\u5ba4\u54e9", "236": "\u4e8c\u5408\u5f15\u56db\udb82\udc4a\u5695\u4e8c\u5408\u5e95\u5c3e\u60f9\u66f3\u4e94\u5a11\u56a9\u4e8c\u5408\u5f15\u8cc0\u8aa6\u771f\u8a00\u5df2\u9802\u4e0a\u6563\u5370\u3002", "237": "\u6b21\u7576\u8aa6\u5ff5\u73e0\u771f\u8a00\u66f0\u3002", "238": "\u5535\u56a9\u65e5\u56c9\u4e8c\u5408\u5f15\u611a\u546c\u8036\u4e8c\u5408\u5f15\u60f9\u8ddb\u4e09\u9ebc\u66f3\u543d", "239": "\u7531\u8aa6\u6b64\u771f\u8a00\u52a0\u6301\u5ff5\u73e0\u4e03\u904d\u6545\u3002\u5ff5\u8aa6\u4e00\u904d\u5247", "240": "\u6210\u5343\u904d\u3002\u5982\u662f\u52a0\u6301\u7562\u5df2\u3002\u56db\u6642\u4e09\u6642\u4e43\u81f3\u4e8c\u6642\u3002", "241": "\u6216\u4e00\u767e\u516b\u904d\u6216\u4e00\u5343\u904d\u3002\u51e1\u6642\u5206\u904d\u6578\u4e00\u5247\u5e38", "242": "\u5b9a\u3002\u6642\u8aa6\u6578\u6eff\u5982\u524d\u9802\u6234\u3002\u5373\u5165\u822c\u82e5\u6ce2\u7f85\u871c", "243": "\u4e09\u6469\u5730\u3002\u5fa9\u65bc\u5fc3\u4e2d\u60f3\u5713\u6eff\u6708\u8f2a\u91cf\u5468\u6cd5\u754c\u3002\u65bc", "244": "\u4e2d\u89c0\u5730\u5b57\u9580\u3002\u4e86\u4e86\u5206\u660e\u8272\u5982\u73c2\u96ea\u5c08\u6ce8\u800c\u4f4f\u3002", "245": "\u5373\u540d\u70ba\u5962\u6469\u4ed6\u3002\u651d\u8af8\u6563\u52d5\u5fc3\u65e2\u5b9a\u5df2\u3002\u5373\u8207\u6167", "246": "\u76f8\u61c9\u3002\u601d\u5730\u5b57\u7fa9\u4e00\u5207\u6cd5\u6027\u76f8\u5982\u6cd5\u754c\u4e0d\u53ef\u5f97", "247": "\u6545\u3002\u5373\u540d\u70ba\u6bd8\u9262\u820d\u90a3\u3002\u6216\u8207\u5b9a\u76f8\u61c9\u6216\u8207\u6167\u76f8", "248": "\u61c9\u3002\u4e45\u5df2\u7d14\u719f\u5b9a\u6167\u96d9\u904b\u3002\u80fd\u89c0\u6240\u89c0\u5e73\u7b49\u5e73\u7b49\u3002", "249": "\u80fd\u53d6\u6240\u53d6\u6089\u61c9\u9060\u96e2\u3002\u5e38\u4f5c\u5982\u662f\u89c0\u7167\u3002\u73fe\u751f\u5f97", "250": "\u5165\u521d\u5730\u3002\u5f8c\u5341\u516d\u5927\u751f\u6210\u666e\u8ce2\u83e9\u85a9\u3002\u5fa9\u7d50\u672c\u5c0a", "251": "\u5370\u8aa6\u767e\u5b57\u771f\u8a00\u3002\u91cd\u7d50\u516b\u4f9b\u990a\u5370\u6367\u95bc\u4f3d\u6c34\u3002\u4ee5", "252": "\u91d1\u525b\u773c\u5de6\u8996\u89e3\u754c\u3002\u5373\u7d50\u6cd5\u6ce2\u7f85\u871c\u5370\u5949\u9001\u8056", "253": "\u773e\u8aa6\u91d1\u525b\u89e3\u812b\u771f\u8a00\u66f0\u3002", "254": "\u5535\u4e00\u8a16\u54e9\u4e8c\u5408\u89a9\u56a9\u5165\u4e8c\u85a9\u56a9\u85a9\u601b\u56a9\u4e8c\u5408\u5f15\u56c9\u4ed6", "255": "\u4e8c\u5408\u5f15\u6089\u5730\u56c9\u5a1c\u4e8c\u5408\u591a\u4e09\u91ce\u4ed6\u5f15\u5f29\u8a90\u5f15\u56db\u8616\u8e49\u5f15", "256": "\u7279\u9441\u4e8c\u5408\u4e94\u6c92\u99b1\u5c3e\u7051\u7130\u516d\u5e03\u66e9\u56c9\u5f15\u8a90\u4e03\u9ebc\u66e9", "257": "\u91ce\u89a9\u516b\u5535\u56a9\u65e5\u56c9\u4e8c\u5408\u6e34\u8a90\u85a9\u601b\u56a9\u4e8c\u5408\u7a46", "258": "\u5fa9\u4ee5\u91d1\u525b\u5bf6\u5370\u3002\u704c\u9802\u88ab\u7532\u79ae\u4e94\u65b9\u4f5b\u3002\u5373\u51fa\u9053", "259": "\u5834\u96a8\u610f\u7d93\u884c\u8b80\u5927\u4e58\u7d93\u5178\u3002\u4ee5\u6b64\u6b8a\u52dd\u798f\u8ff4\u5411", "260": "\u4e00\u5207\u6709\u60c5\u3002\u901f\u8b49\u822c\u82e5\u6ce2\u7f85\u871c\u7121\u4e0a\u6b63\u7b49\u83e9\u63d0\u3002", "261": "\u6b21\u5f8c\u4fbf\u8aaa\u529f\u80fd\u9817\u6b8a\u5999\u77e3\u3002", "262": "\u822c\u82e5\u6ce2\u7f85\u871c\u591a\u6839\u672c\u771f\u8a00\u66f0\u3002", "263": "\u66e9\u8b28\u5a46\u53bb\u8a90\u56a9\u6234\u5f15\u9262\u56c9\u4e8c\u5408\u67b3\u5b43\u4e8c\u5408\u64ad\u5f15\u56c9", "264": "\u5f2d\u8de2\u5f15\u66f3\u5535\u7d07\u54e9\u4e8c\u5408\u5165\u5f15\u5730\u5165\u5f15\u5ba4\u54e9\u4e8c\u5408\u5165\u5f15\u620d\u5695\u4e8c\u5408", "265": "\u5e95\u5c3e\u60f9\u66f3\u5a11\u56a9\u4e8c\u5408\u5f15\u8a36\u5f15\u4f5b\u8a00\u6b64\u9640\u7f85\u5c3c\u5370\u6709", "266": "\u56db\u7a2e\u540d\u3002\u4e00\u540d\u822c\u82e5\u7121\u76e1\u85cf\u3002\u4e8c\u540d\u822c\u82e5\u773c\u3002\u4e09\u540d", "267": "\u822c\u82e5\u6839\u672c\u3002\u56db\u540d\u91d1\u525b\u822c\u82e5\u5fc3\u3002\u6b64\u9640\u7f85\u5c3c\u5370\u6709", "268": "\u5927\u529f\u5fb7\u3002\u82e5\u80fd\u81f3\u5fc3\u5982\u6cd5\u53d7\u6301\u3002\u96a8\u8aa6\u4e00\u904d\u51fa\u751f", "269": "\u4e00\u842c\u516b\u5343\u4fee\u591a\u7f85\u85cf\u3002\u53c8\u5f7c\u4e00\u4e00\u4fee\u591a\u7f85\u4e2d\u3002\u5404", "270": "\u5404\u51fa\u751f\u4e8c\u842c\u4e94\u5343\u4fee\u591a\u7f85\u85cf\u3002\u53c8\u5f7c\u4e00\u4e00\u4fee\u591a", "271": "\u7f85\u4e2d\u3002\u51fa\u751f\u767e\u842c\u4fee\u591a\u7f85\u85cf\u3002\u53c8\u5f7c\u4e00\u4e00\u4fee\u591a\u7f85", "272": "\u85cf\u4e2d\u3002\u51fa\u751f\u7121\u91cf\u767e\u5343\u842c\u5104\u90a3\u7531\u4ed6\u963f\u50e7\u7947\u4fee", "273": "\u591a\u7f85\u85cf\u3002\u5982\u662f\u4e43\u81f3\u5c55\u8f49\u3002\u51fa\u4e16\u7121\u91cf\u7121\u76e1\u4fee\u591a", "274": "\u7f85\u85cf\u3002\u6240\u51fa\u7d93\u984c\u540d\u53e5\u7fa9\u5473\u3002\u5404\u5404\u4e0d\u540c\u800c\u4e0d\u91cd", "275": "\u51fa\u3002\u5982\u662f\u5ff5\u5ff5\u51fa\u751f\u7121\u76e1\u3002\u662f\u6545\u540d\u70ba\u7121\u76e1\u85cf\u9640", "276": "\u7f85\u5c3c\u5370\u3002\u6b64\u9640\u7f85\u5c3c\u5370\u3002\u5373\u662f\u5341\u65b9\u4e09\u4e16\u8af8\u4f5b\u5b97", "277": "\u7956\u3002\u4ea6\u662f\u5341\u65b9\u4e09\u4e16\u8af8\u4f5b\u7121\u76e1\u6cd5\u85cf\u3002\u4e00\u5207\u822c\u82e5", "278": "\u6ce2\u7f85\u871c\u6bcd\u3002\u904e\u73fe\u672a\u4f86\u8af8\u4f5b\u83e9\u85a9\u5e38\u6240\u4f9b\u990a\u606d", "279": "\u656c\u8b9a\u6b4e\u3002\u82e5\u5584\u7537\u5b50\u5584\u5973\u4eba\u7b49\u3002\u4ee5\u81f3\u8aa0\u5fc3\u66f8\u5beb", "280": "\u8b80\u8aa6\u5982\u8aaa\u4fee\u884c\u3002\u662f\u4eba\u6240\u6709\u767e\u5343\u842c\u5104\u6052\u6cb3\u6c99", "281": "\u52ab\u751f\u6b7b\u91cd\u7f6a\u3002\u65bc\u9808\u81fe\u9803\u6089\u6ec5\u7121\u9918\u3002\u6b64\u9640\u7f85\u5c3c", "282": "\u5370\u6240\u6709\u529f\u5fb7\u3002\u6211\u82e5\u4f4f\u65bc\u767e\u5343\u842c\u5104\u963f\u50e7\u7947\u52ab\u3002", "283": "\u6b4e\u7336\u4e0d\u76e1\u3002\u4f55\u6cc1\u9918\u4eba\u6b4e\u4e4b\u80fd\u76e1\u3002\u82e5\u6b32\u4fee\u884c\u822c", "284": "\u82e5\u6cd5\u8005\u3002\u4e00\u98df\u9f4b\u6212\u9999\u6e6f\u6d17\u6d74\u3002\u8457\u65b0\u6de8\u8863\u5165\u65bc", "285": "\u9053\u5834\u3002\u8981\u7576\u5148\u8aa6\u6b64\u9640\u7f85\u5c3c\u5e76\u4f5c\u6b64\u5370\u6eff\u767e\u842c", "286": "\u904d\u7136\u5f8c\u4fee\u884c\u9918\u822c\u82e5\u6cd5\u6c7a\u5b9a\u6210\u5c31\u3002\u662f\u6545\u540d\u70ba", "287": "\u822c\u82e5\u6839\u672c\u3002\u6b64\u9640\u7f85\u5c3c\u5370\u3002\u6089\u80fd\u7167\u4e86\u4e00\u5207\u822c\u82e5", "288": "\u6ce2\u7f85\u871c\u6cd5\u3002\u6545\u540d\u822c\u82e5\u6ce2\u7f85\u871c\u773c\u3002\u6b64\u9640\u7f85\u5c3c\u5370\u3002", "289": "\u6089\u80fd\u6467\u6ec5\u4e00\u5207\u969c\u7919\u3002\u6089\u80fd\u4f4f\u6301\u4e00\u5207\u8af8\u4f5b\u83e9", "290": "\u85a9\u529f\u5fb7\u3002\u6545\u540d\u91d1\u525b\u822c\u82e5\u5fc3\u4e5f\u3002", "291": "\u4fee\u7fd2\u822c\u82e5\u6ce2\u7f85\u871c\u83e9\u85a9\u89c0\u884c\u5ff5\u8aa6\u5100\u8ecc\u4e00\u5377"}, "originalTitle": "Taisho Tripitaka, Electronic version, No. 1151 \u4fee\u7fd2\u822c\u82e5\u6ce2\u7f85\u871c\u83e9\u85a9\u89c0\u884c\u5ff5\u8aa6\u5100\u8ecc", "source": "CBETA"}