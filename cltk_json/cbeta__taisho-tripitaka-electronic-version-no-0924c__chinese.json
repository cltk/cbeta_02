u65e5\u7f85\u4e8c\u5408", "128": "\u534a\u60f9\u7f85\u543d\u6cee\u5412\u6b21\u4e0a\u4e0b\u65b9\u3002", "129": "\u6b21\u706b\u9662\u771f\u8a00\u66f0\u3002", "130": "\u5535\u963f\u4e09\u83be\u64ec\u511e\u543d\u6cee\u5412", "131": "\u6b21\u91cd\u7d50\u5927\u754c\u3002\u771f\u8a00\u66f0\u3002", "132": "\u5535\u6607\u7faf\udb83\ude9c\u83be\u8a36\u4e09\u6469\u7130\u838e\u8a36\u4e03\u904d", "133": "\u6b21\u5949\u5857\u9999\u3002\u771f\u8a00\u66f0\u3002", "134": "\u4f0a\ud843\udc22\u8a00\u99b1\u8f38\u5a46\u511e\u5fae\u591c\u4e8c\u5408\u8f38\u8005\u4e5f\u8f38\u8005\u8f38\u8005", "135": "\u8f38\u8005\u4e5f\u83be\u591c\u511e\u5420\u511e\u59cb\u8584\u8a16\u5e95\u4e5f\u4e8c\u5408\u9262\u7f85\u4e8c\u5408", "136": "\u5e95\u4ee1\u35da\u546c\u4e5f\u9262\u7f85\u4e8c\u5408\u56db\u6930\ud843\udc22\u5535\u963f\u8cc0\u7f85\u963f\u8cc0", "137": "\u7f85\u85a9\u7f85\u7e1b\u5c3e\u511e\u591c\u9054\u7f85\u5e03\u723e\u5e1d\u838e\u8a36\u4e03\u904d", "138": "\u5949\u83ef\u771f\u8a00", "139": "\u4f0a\ud843\udc22\u8607\u83be\u56ca\u7d20\u511e\u5c3e\u591c\u8f2a\u8005\u8f38\u8f38\u8005\u8f38\u56ca\u4e5f\u83bd", "140": "\u591c\u5be7\u5535\u4f0f\u511e\u591a\u8584\u8a16\u5e95\u4e8c\u5408\u591c\u9262\u7f85\u5e95\u4ee1\u35da\u546c", "141": "\u4e5f\u9262\u7f85\u56db\u632a\ud843\udc22\u5535\u963f\u8cc0\u7f85\u963f\u8cc0\u7f85\u85a9\u7f85\u7e1b\u5c3e\u511e", "142": "\u591c\u9054\u7f85\u5e03\u723e\u5e1d\u838e\u8a36", "143": "\u5949\u71d2\u9999\u3002\u771f\u8a00\u66f0\u3002", "144": "\u963f\u95bb\u7e1b\u56ca\u6c99\u9262\u5e95\u7f85\u7d20\u8cc0\u35da\u511e\u4e5f\u5065\u9640\u503c\u4e5f\u8607", "145": "\u7f85\u90e8\u82e5\u66e9\u83bd\u591c\u5be7\u5420\u511e\u59cb\u8584\u8a16\u5e95\u591c\u5ea6\u5831\u95bb\u9262", "146": "\u7f85\u5e95\u4ee1\u35da\u546c\u4e5f\u96e3\u5535\u963f\u53bb\u8cc0\u7f85\u963f\u8cc0\u7f85\u85a9\u7f85\u7e1b", "147": "\u5c3e\u511e\u591c\u9054\u7f85\u5e03\u723e\u838e\u8a36", "148": "\u5949\u98df\u3002\u771f\u8a00\u66f0\u3002", "149": "\u5967\u7051\u6d95\u96e3\u7f85\u7d20\u8cc0\u35da\u4e8c\u5408\u4ed6\u4e5f\u4e8c\u5408\ud86b\udfbc\u7051\u6d8c\u591a\u7f85", "150": "\u4e8c\u5408\u8a2d\u6012\u9ebc\u91cc\u83bd\u4e5f\u5be7\u5535\u5420\u511e\u59cb\u8584\u8a16\u5e95\u591c\u4e8c\u5408", "151": "\u9262\u7f85\u5e95\u4ee1\u546c\u4e5f\u4e8c\u5408\u9262\u7f85\u56db\u6930\u543d\u5535\u963f\u53bb\u7f85\u8cc0", "152": "\u963f\u7f85\u8cc0\u85a9\u7f85\u7e1b\u5c3e\u511e\u591c\u4e8c\u5408\u9054\u7f85\u5e03\u723e\u5e1d\u838e\u8a36", "153": "\u5949\u71c8\u3002\u771f\u8a00\u66f0\u3002", "154": "\u7f85\u8a16\u84ad\u4e8c\u5408\u8fd1\u66e9\u4e8c\u5408\u7051\u8005\u4e8c\u5408\u8ddb\u5c3e\u601b\u7f85\u4e8c\u5408\u5ba4", "155": "\u8005\u4e8c\u5408\u77bb\u8b28\u5c3e\u66c7\u83bd\u66e9\u77ed\u540d\u8f38\u5a46\u83bd\u591c\u5be7\u5420\u511e\u59cb", "156": "\u8584\u8a16\u5e95\u591c\u4e8c\u5408\u511e\u5831\u95bb\u9262\u7f85\u5e95\u8a16\u35da\u4e8c\u5408\u546c\u4e5f\u96e3", "157": "\u5535\u963f\u8def\u8fe6\u4e5f\u85a9\u7f85\u5c3e\u511e\u591c\u4e8c\u5408\u9054\u7f85\u5e03\u723e\u5e1d\u838e", "158": "\u5a46\u8cc0", "159": "\u6b21\u904b\u5fc3\u4f9b\u990a\u3002\u4ee5\u5fc3\u904b\u60f3\u3002\u6c34\u9678\u8af8\u83ef\u3002\u7121\u66f0\u4e3b", "160": "\u6240\u3002\u904d\u6eff\u865b\u7a7a\u3002\u76e1\u5341\u65b9\u754c\u3002\u53ca\u4ee5\u4eba\u5929\u3002\u5999\u5857\u9999", "161": "\u96f2\u3002\u71d2\u9999\u71c8\u660e\u3002\u5e62\u5e61\u5bf6\u84cb\u3002\u7a2e\u7a2e\u9f13\u6a02\u3002\u6b4c\u821e\u5993", "162": "\u5531\u3002\u771f\u73e0\u7f85\u7db2\u3002\u61f8\u8af8\u5bf6\u9818\u3002\u9ae3\u767d\u62c2\u5fae\u5999\u8072\u3002\u92d2", "163": "\u77dc\u7faf\u5c3c\u7db2\u3002\u5982\u610f\u5bf6\u6a39\u3002\u8863\u670d\u4e4b\u96f2\u5929\u3002\u8af8\u5eda\u81b3\u4e0a\u3002", "164": "\u5999\u9999\u7f8e\u5473\u3002\u5bae\u6bbf\u6a13\u95a3\u5bf6\u67f1\u3002\u838a\u56b4\u8eab\u3002\u982d\u51a0\u74d4\u73de\u3002", "165": "\u5982\u662f\u7b49\u96f2\u3002\u884c\u8005\u904b\u5fc3\u3002\u904d\u6eff\u865b\u7a7a\u3002\u4ee5\u81f3\u8aa0\u5fc3\u3002\u5982", "166": "\u662f\u4f9b\u990a\u3002\u6700\u70ba\u52dd\u4e0a\u3002\u662f\u6545\u884c\u8005\u4ee5\u6c7a\u5b9a\u5fc3\u3002\u800c\u884c", "167": "\u6b64\u6cd5\u3002\u904b\u5fc3\u4f9b\u990a\u3002\u8aa6\u6301\u771f\u8a00\u3002\u53ca\u4f5c\u624b\u5370\u3002\u5982\u4e0a", "168": "\u6240\u60f3\u3002\u7686\u6089\u6210\u5c31\u3002\u771f\u8a00\u66f0\u3002", "169": "\u5535\u85a9\u7f85\u56a9\u4ed6\u9f95\u6435\u5a1c\udb83\udd72\u5e1d\u5a11\u9817\u7f85\u546c\u9580\u4f3d\u4f3d\u66e9", "170": "\u91f0\u838e\u8a36\u4e03\u904d", "171": "\u6b21\u8b9a\u6b4e\u4f5b\u66f0\u3002", "172": "\u6469\u8cc0\u5f15\u8fe6\u5f15\u62b3\u5695\u5f15\u5efa\u66e9\u5f15\u8caa\u4e00\u6368\u5a11\u8de2\u4e8c\u5408\u5f15\u85cd", "173": "\u85a9\u56a9\u5420\u5f97\u5fae\u9589\u53cd\u5f15\u511e\u6ce5\u4ee5\u53cd\u5357\u4e8c\u5408\u5954\u6d66\u959f\u53cd\u53bb\u5973\u9f3b\u97f3\u5a1c\u5730", "174": "\u8805\u4e8c\u5408\ud864\udd3d\u62cf\u5f15\u99b1\u85cd\u4e09\u9262\u7f85\u4e8c\u5408\u62cf\u4e0a\u6469\u5f15\u5f2d\u601b\u4ed6", "175": "\u5f15\u8a90\u7708\u56db\u4f5b", "176": "\u5535\u7f85\u5117\u91ce\u4e8c\u5408\u60f9\u5f15\u66e9\u5357\u5f15\udb82\udc4a\u8a69\u4f5b\u53cd\u6de1\u4e00\u620d\u5a46\u5f15", "177": "\u8a25\udb83\udd72\u5e95\u4e01\u4ee5\u53cd\u8b28\u5f15\u5de6\u91f0\u4e8c\u672c\u64ad\u5f15\u7f85\u6cab\u9ad4\u4ed6\u4ee5\u53cd\u8fe6\u8b0e", "178": "\u5f15\u5efa\u8ead\u4e09\u66e9\u8b0e\u5f15\u9054\ud851\udea5\u820d\u9ebc\u56a9\u61be\u56db\u6cd5", "179": "\u7a46\u8a16\u8ead\u4e8c\u5408\u7a46\u8a16\u5e95\u4e8c\u5408\u64ad\u5f15\u4ed6\u5f15\u9262\u7f85\u4e8c\u5408\u8ddb", "180": "\u8ead\u4e8c\u5408\u5f15\u8a90\u4e5e\u7051\u4e8c\u5408\u91ce\u7d22\u5f15\u5f2d\u91ce\u4e8c\u56a9\u5f15\u6c99\u9ad4", "181": "\u4e8c\u5408\u8ead\u4e5e\u7051\u4e8c\u5408\u601b\u89bd\u4e8c\u5408\u5c3e\u59cb\u745f\u9d42\ud864\udd3d\u4e0a\u62cf\u520e", "182": "\u7121\u672c\u53cd\u4e00\u66e9\u8b0e\u5f15\u50e7\u53bb\u5065\u5de6\u5a46\u5f15\u56a9\u4e0a\u8de2\u90fd\u5404\u53cd\u56db\u50e7", "183": "\u85a9\u56a9\u6bcd\u99b1\u5a46\u7f85\u4e8c\u5408\u6368\u8de2\u4e8c\u5408\u5f15\u91ce\u4e09\u52c3\u54e9\u4e8c\u5408", "184": "\u8de2\u5f15\u91ce\ud864\udd3d\udb83\ude9c\ud864\udd3d\u89bd\u4e8c\u963f\u56a9\u8def\u5f15\u67b3\u591a\u50e7\u53bb\u67b3\u5b43\u4e8c", "185": "\u5408\u91ce\u4e09\u66e9\u8b28\u5f15\ud865\udd73\u9afb\u6469\u8cc0\u7b54\u6469\u4e8c\u5408\u511e\u56db\u83e9\u85a9", "186": "\u6469\u8cc0\u5f15\u6469\u651e\u52e4\u53ef\u53cd\u91ce\u8d0a\u62cf\u91ce\u4e0a\u5c3e\u511e\u91ce\u4e8c\u5408\u7f85\u60f9", "187": "\u65e5\u7f85\u53cd\u91ce\u5a11\u5f15\u99b1\u5420\u5f15\u51c6\u53cd\u4e8c\u8a25\u96e3\u4e0a\u591a\u5a1c\u6469\u8fe6\u591c\u5f15\u4e09", "188": "\u66e9\u8b28\u5f15\u6089\u5e1d\u56a9\u65e5\u7f85\u4e8c\u5408\u64ad\u5f15\u62cf\u4e0a\u66f3\u5f15\u53bb\u56db\u91d1\u525b\u773e", "189": "\u963f\u5f15\u6f14\u5f15\u89a9\u6ce5\u56a9\u5de6\u8a90\u7d20\u7f85\u4e00\u7dca\u90a3\u4e0a\u7f85\u90a3\u56c9\u9460", "190": "\u8fe6\u7f85\u4e8c\u5408\u90a3\u91ce\u4e8c\u9262\u7f85\u4e8c\u5408\u56a9\u7f85\u4e8c\u5408\u9054\u9ebc\u8616\u91cc", "191": "\u4e8c\u5408\u591a\u5730\u4f3d\u7f85\u4e09\u5c3e\u9054\u9ebc\u5de6\u9262\u7f85\u4e8c\u5408\u6368\u9ebc\u64cd\u723e", "192": "\u4e5f\u4e8c\u5408\u56db\u511e\u9298\u4e8c\u5408\u591a\u90e8\u591a\u9298\u591a\u9262\u7f85\u4e8c\u5408\u8fe6\u6368\u591c", "193": "\u4e94\u601b\u511e\u8cc0\u5ba4\u7f85\u4e8c\u5408\u6469\u62cf\u4e5f\u99b1\u5f15\ud851\udea5\u5f15\u516d\u4e8c\u5408\u8af8\u5929", "194": "\u6b21\u767c\u9858\u3002\u6b21\u6563\u5ff5\u8aa6\u3002\u5148\u79ae\u4e09\u5bf6\u3002\u6b21\u79ae\u672c\u5c0a\u3002\u7136\u5f8c", "195": "\u666e\u79ae\u8af8\u4f5b\u3002\u6b21\u6301\u8aa6\u4eba\u5148\u9ebb\u5ea7\u3002\u637a\u5a46\u7b49\u5ea7\u8b02\u8dcf", "196": "\u8dba\u534a\u8dcf\u8ce2\u9ebb\u5ea7\u3002\u4ee4\u8eab\u7aef\u76f4\u3002\u6b21\u628a\u6578\u73e0\u3002\u53f3\u4e09", "197": "\u8ff4\u8f49\u3002\u6b21\u5b89\u73e0\u4e8c\u638c\u4e2d\u9802\u6234\u4e91\u4e91\u6b21\u7d50\u6839\u672c\u5370\u3002", "198": "\u8aa6\u6839\u672c\u771f\u8a00\u4e03\u904d\u3002\u6b21\u5ff5\u8aa6\u4e86\u3002\u5373\u9802\u6234\u73e0\u3002\u767c\u9858", "199": "\u5b89\u7f6e\u672c\u8655\u3002\u6b21\u7d50\u6839\u672c\u5370\u3002\u8aa6\u6839\u672c\u771f\u8a00\u4e03\u904d\u3002\u6b21", "200": "\u66f4\u5949\u5857\u9999\u83ef\u71d2\u9999\u98ef\u71c8\u3002\u6b21\u904b\u5fc3\u4f9b\u990a\u3002\u6b21\u8b9a\u3002\u6b21", "201": "\u5949\u95bc\u4f3d\u3002\u6b21\u767c\u9858\u3002\u6b21\u89e3\u754c\u3002\u6b21\u793a\u4e09\u6627\u3002\u53ca\u79ae\u62dc\u8af8", "202": "\u5c0a\u3002\u6b21\u8aa6\u73fe\u5728\u8af8\u5982\u4f86\u4e4b\u5048\u3002\u6b21\u5949\u9001\u771f\u8a00\u3002\u6b21\u8ff4", "203": "\u4e5d\u65b9\u4fbf\u3002\u6b21\u8ff4\u5411\u3002\u6b21\u4e09\u90e8\u4e09\u6627\u8036\u3002\u6b21\u8b77\u8eab\u3002\u6b21\u8b77", "204": "\u672c\u5c0a\u7b49\u3002\u6b21\u904d\u79ae\u8af8\u5c0a\u3002\u7136\u5f8c\u53bb\u3002\u7576\u5802\u9053\u5834\u3002\u4efb", "205": "\u610f\u7d93\u884c\u3002", "206": "\u5c0f\u6307\u6a80\u6167\u4e0a\u5de6\u4e0b\u53f3\u7121\u540d\u6307\u6212\u65b9\u4e0a\u5de6\u4e0b\u53f3\u4e2d\u6307\u5fcd\u9858\u4e0a\u53f3\u4e0b\u5de6", "207": "\u982d\u6307\u9032\u529b\u4e0a\u5de6\u4e0b\u53f3\u5927\u6307\u79aa\u667a\u4e0a\u5de6\u4e0b\u53f3\u3002", "208": "\u5584\u4f86\u5c0a\u8005\u3002\u5584\u4f86\u5c0a\u8005\u3002\u7531\u672c\u9858\u529b\u3002\u800c\u4f86\u964d\u8d74\u3002\u9867", "209": "\u4e58\u52a0\u6301\u3002\u6388\u6b64\u8acb\u3002\u5e76\u53d7\u6211\u6240\u8fa6\u8af8\u4f9b\u990a\u3002\u8d77\u5927", "210": "\u6148\u60b2\u3002\u552f\u5782\u7d0d\u53d7\u3002", "211": "\u73fe\u5728\u8af8\u5982\u4f86\u6551\u4e16\u8af8\u83e9\u85a9", "212": "\u4e0d\u65b7\u5927\u4e58\u6559\u5230\u6b8a\u52dd\u4f4d\u8005", "213": "\u552f\u9858\u8056\u5929\u773e\u6c7a\u5b9a\u8b49\u77e5\u6211", "214": "\u5404\u7576\u96a8\u8655\u5b89\u5f8c\u6642\u5782\u54c0\u8d74", "215": "\u7d42\u980c\u3002", "216": "\u6211\u4eca\u5ff5\u8aa6\u7955\u5bc6\u546a\u666e\u901a\u4e09\u754c\u5f97\u6e05\u6de8", "217": "\u9858\u6211\u751f\u4e2d\u9664\u696d\u969c\u798f\u5fb7\u5713\u6eff\u5229\u6709\u60c5", "218": "\u751f\u751f\u4e16\u4e16\u6052\u53d7\u6301\u56db\u6069\u773e\u751f\u5171\u6210\u4f5b", "219": "\u85e5\u5e2b\u5100\u8ecc\u4e00\u5177\u7d42\u3000\u50b3\u6559\u5927\u5e2b\u5c07\u4f86"}, "originalTitle": "Taisho Tripitaka, Electronic version, No. 0924C \u85e5\u5e2b\u5100\u8ecc\u4e00\u5177", "source": "CBETA"}