: "\u5a46\u8a90\u56a9\u5e95\u70cf\u637a\u56c9\u9f3b\u723e\u62cf\u511e\u54e9\u511e\u54e9\u56c9\u601b", "48": "\u90a3\u4e8c\u5408\u4ff1\u651e\u5a11\u6469\u5931\u54e9\u4e8c\u5408\u8ae6\u5f2d\u91cc\u5f2d\u91cc\u963f\u8fe6", "49": "\u6368\u99c4\u89a9\u865e\u5de6\u3611\u4f01\u91cc\u4f01\u91cc\u85a9\u56a9\u601b\u4ed6\u8616\u591a\u54e9", "5": "\u7a7a\u8ae1\u5927\u9452\u6b63\u865f\u5927\u5ee3\u667a\u5927\u8208\u5584", "50": "\u4e5f\u4e8c\u5408\u5ba4\u56c9\u4e8c\u5408\u8fe6\u695e\u8fe6\u7f85\u6b65\u8ae6\u6ce5\u5c3e\u637b\u5974\u63d6\u53cd", "51": "\u5c3e\u4e5f\u4e8c\u5408\u54e9\u4e5f\u4e8c\u5408\u6c92\u56c9\u4e8c\u5408\u61be\u9ebc\u4e8c\u5408\u601b\u4ed6\udb80\udef2", "52": "\u591a\u52aa\udb80\udef2\u8ae6\u5c3e\u6fd5\u56a9\u4e8c\u5408\u9032\u5e95\u4e5f\u4e8c\u5408\u56a9\u56c9\u6ce2", "53": "\u56c9\u6ce2\u56c9\u8a16\u56c9\u4e8c\u5408\u8b0e\u66e9\u8b28\u5a46\u8a90\u56a9\u5e95\u6ce2\u56c9\u723e", "54": "\u8ae6\ud843\ude2a\u4e5e\u7051\u4e8c\u5408\ud843\ude2a\u4e5e\u7051\u4e8c\u5408\u9ebc\u9ebc\u85a9\u56a9\u8a25\u745f\u8dd3", "55": "\u4e8c\u5408\u9262\u637a\u56c9\u4e8c\u5408\u5420\u7121\u80ba\u53cd\u6ce2\u4e5f\u7d30\u6bd8\u85e5\u4e8c\u5408\u5a11\u56a9", "56": "\u4e8c\u5408\u5f15\u8a36\u5f15", "57": "\u8aaa\u662f\u9640\u7f85\u5c3c\u4e16\u9593\u6089\u7686\u805e", "58": "\u662f\u5927\u7121\u80fd\u52dd\u80fd\u58de\u4e00\u5207\u9b54", "59": "\u80fd\u589e\u52e4\u52c7\u529b\u5247\u4f4f\u4e09\u6627\u5f62", "6": "\u5bfa\u4e09\u85cf\u6c99\u9580\u4e0d\u7a7a\u5949\u3000\u8a54\u8b6f", "60": "\u540d\u70ba\u7121\u80fd\u52dd\u8aaa\u5f7c\u5927\u5fc3\u660e", "61": "\u5927\u529b\u6975\u52c7\u731b\u4e0d\u7570\u65bc\u524d\u660e", "62": "\u662f\u5fc3\u4e16\u5c0a\u8aaa", "63": "\u771f\u8a00\u66f0\u3002", "64": "\u66e9\u8b28\u98af\u8de2\u5583\u4e09\u85d0\u4e09\u6c92\u99b1\u4ff1\u81f4\u5357\u85a9\u5931\u56c9\u4e8c", "65": "\u5408\u56a9\u8fe6\u50e7\u4f3d\u5583\u85a9\u56a9\u8b0e\u56c9\u5a46\u4e5f\u5e95\u8de2\u5583\u5c3e\u6ce2", "66": "\u5c38\u66e9\u85a9\u8ae6\u4e8c\u5408\u60f9\u5a11\u35da\ud840\udd99\u5de6\u59cb\u4f01\u66e9\u85a9\u601b\u4e8c\u5408", "67": "\u4e5f\u5c3e\u6fd5\u56a9\u4e8c\u5408\u6b65\u5165\u9262\u56c9\u4e8c\u5408\u67b3\u5b43\u4e8c\u5408\u4e5f\u5236", "68": "\u9441\u8a16\u3601\u4e8c\u5408\u4ff1\u5b6b\u90a3\u56a9\u96b7\u66e9\u5de6\u7faf\u8afe\u8fe6\u725f\u5c3c", "69": "\u59cb\u4e5e\u7051\u4e8c\u5408\u4e5f\u8fe6\u6368\u8ddb\u5beb\u4fc1\u62cf\u56c9\u6bd4\u820d\u67b3\u4e5f", "7": "11 \u7121\u80fd\u52dd\u52a0\u6301\u54c1\u7121\u80fd\u52dd\u52a0\u6301\u54c1\u7b2c\u5341\u4e00", "70": "\u4e8c\u5408\u50e7\u8cc0\u5beb\u5f2d\u91cc\u66f3\u4e8c\u5408\u62cf\u585e\u56a9\u4e8c\u5408\u5a11\u5e95\u5a46", "71": "\u56a9\u89a9\u6469\u6469\u85a9\u56a9\u85a9\u601b\u56a9\u4e8c\u5408\u96e3\u8005\u85a9\u56a9\u5a46\u5ebe", "72": "\u9262\u637a\u56c9\u4e8c\u5408\u5420\u540c\u4e0a\u6bd8\u85e5\u4e8c\u5408\u601b\u511e\u4e5f\u4e8c\u5408\u4ed6", "73": "\u60f9\u66f3\u5c3e\u60f9\u66f3\u60f9\u6f14\u5e95\u5c3e\u60f9\u6f14\u5e95\u963f\u723e\u55ae\u60f9", "74": "\u66f3\u82e5\u6f14\u5e95\u963f\u723e\u8ae6\u963f\u6ce2\u56c9\u4e8c\u5408\u723e\u8ae6\u6469", "75": "\u56c9\u67b3\u5b43\u4e8c\u5408\u9262\u7f85\u672b\u5a1c\u5be7\u66f3\u5a11\u56a9\u4e8c\u5408\u5f15\u8a36", "76": "\u8aaa\u6b64\u5fc3\u771f\u8a00\u61c9\u7b49\u6b63\u89ba\u8aaa", "77": "\u4e03\u4f5b\u4e4b\u4e16\u5c0a\u986f\u63da\u8af8\u529f\u5fb7", "78": "\u5373\u8aaa\u662f\u5927\u660e\u5229\u76ca\u4fee\u884c\u8005", "79": "\u666e\u904d\u8af8\u4e16\u754c\u516d\u7a2e\u800c\u632f\u52d5", "8": "\u723e\u6642\u4e16\u5c0a\u89c0\u91d1\u525b\u624b\u7955\u5bc6\u4e3b\u5fa9\u4ee5\u4f3d\u4ed6\u800c\u8aa5\u4e4b", "80": "\u4e00\u5207\u9b54\u5bae\u6bbf\u6089\u7686\u5927\u632f\u52d5", "81": "\u91d1\u525b\u624b\u6b64\u771f\u8a00\u53e5\u3002\u4e00\u5207\u8af8\u4f5b\u6240\u8aaa\u3002\u5229\u76ca\u773e\u751f", "82": "\u6545\u3002\u7955\u5bc6\u4e3b\u6216\u6301\u8aa6\u8f2a\u738b\u771f\u8a00\u8005\u3002\u6216\u6301\u9918\u771f\u8a00", "83": "\u8005\u4ee5\u6b64\u771f\u8a00\u52a0\u6301\u7d50\u7e37\u3002\u6216\u7d50\u8888\u88df\u89d2\u3002\u6216\u7d50\u9802", "84": "\u9aee\u3002\u6216\u6a3a\u76ae\u4e0a\u66f8\u5e36\u9838\u81c2\u3002\u5f7c\u4eba\u901f\u75be\u6613\u5f97\u6210\u5c31\u3002", "85": "\u672c\u5c0a\u901f\u73fe\u5176\u524d\u3002\u5ff5\u8aa6\u6642\u82e5\u80fd\u61b6\u6301\u3002\u91d1\u525b\u624b\u6211", "86": "\u4e0d\u898b\u3002\u5929\u4e16\u9b54\u4e16\u6c99\u9580\u5a46\u7f85\u9580\u773e\u4e2d\u3002\u82e5\u6b64\u771f\u8a00", "87": "\u52a0\u8b77\u3002\u82e5\u7a62\u8005\u82e5\u6de8\u8005\u524d\u3002\u82e5\u4eba\u82e5\u975e\u4eba\u6216\u9b54\u5b50", "88": "\u6216\u5fc5\u820d\u906e\u3002\u6216\u6bd8\u90a3\u4e5f\u8fe6\u6216\u85e5\u53c9\u6216\u9ce9\u69c3\u8336\u6216", "89": "\u7f85\u524e\u5a11\u3002\u6216\u9918\u985e\u6709\u60c5\u6b32\u4f86\u969c\u96e3\u3002\u4f5c\u662f\u601d\u60df\u3002", "9": "\u66f0\u3002", "90": "\u65bc\u963f\u5412\u8fe6\u56a9\u5e95\u4e01\u4ee5\u53cd\u738b\u5bae\u4e0d\u5f97\u5165\u3002\u82e5\u6709\u9055\u8d8a", "91": "\u6b64\u660e\u6e05\u6de8\u4fee\u884c\u8005\u3002\u5f7c\u6089\u7686\u9055\u9006\u91d1\u525b\u7a2e\u65cf\u53ca", "92": "\u81ea\u7a2e\u65cf\u5e76\u89aa\u65cf\u670b\u53cb\u3002\u4e0d\u5bb9\u4f4f\u65bc\u5f7c\u3002\u91d1\u525b\u624b\u6b64", "93": "\u660e\u771f\u8a00\u6709\u5927\u5a01\u529b\u3002\u65bc\u4e00\u5207\u4e8b\u696d\u61c9\u4f5c\u52a0\u8b77\u3002\u61c9", "94": "\u4f9b\u6b63\u904d\u77e5\u5370\u53ef\u3002\u4e00\u5207\u8af8\u83e9\u85a9\u5370\u53ef\u3002", "95": "12 \u8b49\u5b78\u6cd5\u54c1\u83e9\u63d0\u5834\u6240\u8aaa\u4e00\u5b57\u9802\u8f2a\u738b\u7d93\u8b49\u5b78\u6cd5\u54c1\u7b2c", "96": "\u5341\u4e8c", "97": "\u723e\u6642\u4e16\u5c0a\u77e5\u7121\u76e1\u6cd5\u754c\u5df2\u3002\u9664\u9063\u4e00\u5207\u8af8\u969c\u3002\u5fa9", "98": "\u8aa5\u91d1\u525b\u624b\u83e9\u85a9\u8a00\u3002\u91d1\u525b\u624b\u82e5\u5584\u7537\u5b50\u5584\u5973\u4eba", "99": "\u82fe\u82bb\u82fe\u82bb\u5c3c\u3002\u82e5\u6b32\u4fee\u7fd2\u4f5b\u9802\u4e0d\u601d\u8b70\u5370\u4e09\u6469"}, "source": "CBETA", "language": "chinese", "originalTitle": "Taisho Tripitaka, Electronic version, No. 0950 \u83e9\u63d0\u5834\u6240\u8aaa\u4e00\u5b57\u9802\u8f2a\u738b\u7d93: \u83e9\u63d0\u5834\u6240\u8aaa\u4e00\u5b57\u9802\u8f2a\u738b\u7d93: \u83e9\u63d0\u5834\u6240\u8aaa\u4e00\u5b57\u9802\u8f2a\u738b\u7d93", "author": "\u5510 \u4e0d\u7a7a\u8b6f", "sourceLink": "http://www.cbeta.org/"}