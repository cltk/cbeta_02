": "\u300c\u6211\u4eca\u8aaa\u4e00\u5207\u90fd\u81ea\u8eab\u3001\u53e3\u3001\u5fc3\u91d1\u525b\u4e2d\uff0c\u4ee4\u4f5c\u5982\u91d1", "427": "\u525b\u5100\u8ecc\u3002\u82e5\u5370\u52a0\u6301\u7de9\u6162\uff0c\u82e5\u610f\u6b32\u89e3\uff0c\u5247\u4ee5\u6b64\u5fc3", "428": "\u771f\u8a00\u4ee4\u4f5c\u5805\u56fa\u3002\u771f\u8a00\u66f0\uff1a\u300c\u5535\u56a9\u65e5\u7f85\u4e8c", "429": "\u5408\u85a9\u601b\u56a9\u4e09\u6469\u8036\u9ebc\u52aa\u6ce2\u5f15\u651e\u8036\u56a9\u65e5\u7f85", "43": "\u300c\u5373\u524d\u91d1\u525b\u5408\u638c\u7576\u5fc3\uff0c\u4ee5\u9802\u8457\u5730\uff0c\u79ae\u5317\u65b9\uff0c\u771f\u8a00", "430": "\u85a9\u601b\u56a9\u601b\u5c3e\u6012\u6ce2\u5e95\u745f\u59f9\u634f\u54e9\u6fc1\u5bd0\u5a46", "431": "\u56a9\u8607\u90fd\u4f7f\u5ebe\u5bd0\u5a46\u56a9\u963f\u52aa\u56c9\u7faf\u90fd\u5bd0", "432": "\u5a46\u56a9\u8607\u5e03\u4f7f\u5ebe\u5bd0\u5a46\u56a9\u85a9\u56a9\u6089\u6715\u5bd0\u9262\u56c9", "433": "\u4e5f\u8eca\u85a9\u56a9\u7faf\u6469\u7d20\u8005\u5bd0\u8cea\u591a\u5ba4\u54e9\u85e5\u77e9\u5695", "434": "\u543d\u5475\u5475\u5475\u5475\u659b\u5f15\u5a46\u4f3d\u68b5\u85a9\u56a9\u601b\u4ed6\u8616\u591a", "435": "\u56a9\u65e5\u56c9\u6469\u5f2d\u60b6\u906e\u56a9\u65e5\u54e9\u5a46\u56a9\u6469\u8a36\u4e09\u6469", "436": "\u8036\u85a9\u601b\u56a9\u5641\u5f15", "437": "\u300c\u7531\u6b64\u771f\u8a00\uff0c\u8a2d\u4f5c\u7121\u9593\u7f6a\u3001\u8b17\u4e00\u5207\u5982\u4f86\u53ca\u65b9\u5ee3", "438": "\u5927\u4e58\u6b63\u6cd5\uff0c\u4e00\u5207\u60e1\u4f5c\uff0c\u5c1a\u5f97\u6210\u5c31\u3002\u300c\u4e00\u5207\u5982\u4f86\u5370", "439": "\u8005\uff0c\u7531\u91d1\u525b\u85a9\u57f5\u5805\u56fa\u9ad4\u6545\u73fe\u751f\u901f\u75be\uff0c\u96a8\u6a02\u5f97", "44": "\u66f0\uff1a\u300c\u5535\u85a9\u56a9\u601b\u4ed6\u8616\u591a\u5e03\u60f9\u7faf\u78e8\u5c3c\u8f15\u547c\u963f", "440": "\u4e00\u5207\u6700\u52dd\u6210\u5c31\u2026\u2026\uff0c\u4e43\u81f3\u7372\u5f97\u5982\u4f86\u6700\u52dd\u6089\u5730\u3002\u300d\u5a46", "441": "\u4f3d\u68b5\u4e00\u5207\u5982\u4f86\u91d1\u525b\u85a9\u57f5\u4f5c\u5982\u662f\u8aaa\uff1a\u300c\u6211\u4eca", "442": "\u90fd\u8aaa\u4e00\u5207\u5370\u89e3\u812b\u5100\u5247\u3002", "443": "\u300c\u5f9e\u5f7c\u5f7c\u51fa\u751f\u6240\u6709\u4e00\u5207\u5370\uff0c\u65bc\u5f7c\u5f7c\u7576\u89e3\uff0c\u7531", "444": "\u6b64\u771f\u8a00\u5fc3\uff1a\u300e\u56a9\u65e5\u56c9\u7a46\u3002\u300f", "445": "\u300c\u5f9e\u81ea\u5fc3\u8d77\u91d1\u525b\u5bf6\u5370\uff0c\u5b89\u65bc\u704c\u9802\u8655\uff0c\u4ee5\u52dd\u6307\u81ea", "446": "\u704c\u9802\uff0c\u5206\u624b\u7e8f\u982d\u7e6b\u9b18\u3002", "447": "\u300c\u6b21\u7d50\u7532\u5191\uff0c\u4ee5\u6b64\u5fc3\u771f\u8a00\uff1a\u300c\u5535\u56a9\u65e5\u56c9\u56c9\u601b\u90a3", "448": "\u6bd8\u8a75\u8005\ud851\udea5\u85a9\u56a9\u6bcd\u637a\u7f85\u4e8c\u5408\u54a9\u637a\u54e9\u4e8c\u5408", "449": "\u6468\u77e9\u5695\u56a9\u56c9\u8fe6\u56a9\u5236\u90a3\u9441\u300c\u88ab\u7532\u5df2\uff0c\u4ee5", "45": "\u601b\u9ebc\u5357\u6d85\u54e9\u591c\u591a\u591c\u5f2d\u85a9\u56a9\u601b\u4ed6\u8616\u591a\u56a9", "450": "\u9f4a\u638c\u62cd\u4ee4\u6b61\u559c\uff0c\u4ee5\u6b64\u5fc3\u771f\u8a00\uff1a\u300c\u56a9\u65e5\u56c9\u89a9", "451": "\u4f7f\u8036\u659b\u5f15", "452": "\u300c\u7531\u6b64\u5fc3\u771f\u8a00\uff0c\u89e3\u7e1b\u5f97\u6b61\u559c\uff0c", "453": "\u7372\u5f97\u91d1\u525b\u9ad4\uff0c\u5982\u91d1\u525b\u85a9\u57f5\u3002", "454": "\u4e00\u904d\u8aa6\u91d1\u525b\u85a9\u57f5\uff0c\u96a8\u610f\u611b\u6a02\u4f4f\u5b89\u6a02\uff0c", "455": "\u8b92\u8aa6\u7686\u5f97\u901f\u6210\u5c31\uff0c\u5982\u91d1\u525b\u624b\u4e4b\u6240\u8aaa\u3002\u300d", "456": "\u5a46\u4f3d\u68b5\u666e\u8ce2\u4f5c\u5982\u662f\u8aaa\uff1a", "457": "\u300c\u91d1\u525b\u85a9\u57f5\u7b49\u85a9\u57f5\uff0c\u4e00\u5207\u6210\u5c31\u4f5c\u4e8b\u696d\uff0c", "458": "\u96a8\u610f\u5ff5\u8aa6\u65bc\u6b64\u4e2d\uff0c\u65bc\u8af8\u4e8b\u696d\u7686\u6210\u5c31\u3002", "459": "\u771f\u8a00\u5fc3\u5370\u53ca\u8af8\u660e\uff0c\u96a8\u6a02\u4fee\u7fd2\u8af8\u7406\u8da3\uff0c", "46": "\u65e5\u56c9\u7faf\u78e8\u53e5\ud844\udc14\ud851\udea5", "460": "\u65bc\u6559\u6240\u8aaa\u53ca\u81ea\u4f5c\uff0c\u7686\u5f97\u6210\u5c31\u904d\u4e00\u5207\u3002", "461": "\u300c\u6b21\u8aaa\u56db\u7a2e\u7955\u5bc6\u4f9b\u990a\uff0c\u61c9\u4f5c\u4ee5\u6b64\u91d1\u525b\u6b4c\u8a60\uff0c\u771f", "462": "\u8a00\uff1a\u300c\u5535\u56a9\u65e5\u56c9\u4e8c\u5408\u85a9\u601b\u56a9\u50e7\u8616\u56c9\u8cc0\u56a9\u65e5", "463": "\u56c9\u56c9\u601b\u90a3\u9ebc\u52aa\u601b\u3615\u56a9\u65e5\u56c9\u9054\u6469\u8a90\u8036", "464": "\u5948\u5f15\u56a9\u65e5\u56c9\u7faf\u78e8\u8fe6\u5695\u5a46\u56a9", "465": "\u300c\u65bc\u66fc\u837c\u7f85\u4e2d\u4ee5\u6b64\u91d1\u525b\u8b9a\u8a60\u800c\u6b4c\uff0c\u4ee5\u91d1\u525b\u821e\uff0c", "466": "\u4ee5\u4e8c\u624b\u638c\u53ca\u4f9b\u990a\u82b1\u7b49\u4f5c\u4f9b\u990a\u3002\u65bc\u5916\u66fc\u837c\u7f85", "467": "\u91d1\u525b\u9999\u7b49\u4f9b\u990a\u5df2\uff0c\u5b89\u65bc\u672c\u8655\uff0c\u4e00\u5207\u96a8\u529b\u800c\u4f9b", "468": "\u990a\uff0c\u555f\u767d\u4e00\u5207\u5982\u4f86\u3002\u96a8\u610f\u9999\u7b49\u4f9b\u990a\u5df2\uff0c\u5df2\u5165", "469": "\u66fc\u837c\u7f85\u8005\u96a8\u529b\u5df2\u737b\u5927\u66fc\u837c\u7f85\u4e00\u5207\u6ecb\u5473\u98f2", "47": "\u300c\u5247\u4ee5\u7dcb\u7e52\u89d2\u7d61\u62ab\uff0c\u4ee5\u7dcb\u5e1b\u8986\u9762\uff0c\u4ee4\u5f1f\u5b50\u7d50\u85a9", "470": "\u98df\u3001\u5b89\u6a02\u7b49\u4e00\u5207\u8cc7\u5177\uff0c\u4ee4\u5145\u8db3\u53d7\u7528\uff0c\u61c9\u53d7\u8207\u4e00", "471": "\u5207\u5982\u4f86\u6210\u5c31\u91d1\u525b\u7981\u6212\uff1a", "472": "\u300c\u6b64\u662f\u4e00\u5207\u4f5b\u9ad4\u6027\uff0c\u4f4f\u65bc\u91d1\u525b\u85a9\u57f5\u624b\uff0c", "473": "\u6c5d\u4eca\u61c9\u7576\u800c\u53d7\u6301\uff0c\u91d1\u525b\u85a9\u57f5\u5805\u56fa\u7981\u3002\u300d", "474": "\u300c\u5535\u85a9\u56a9\u601b\u4ed6\u8616\u591a\u6089\u5730\u56a9\u65e5\u56c9\u4e09\u6469\u8036\u5e95", "475": "\u745f\u59f9\u4e8c\u5408\u7ff3\u6c99\u601b\u9441\u4e8c\u5408\u99b1\u56c9\u591c\u5f2d\u56a9\u65e5\u56c9", "476": "\u85a9\u601b\u7e1b\u4e8c\u5408\u546c\u546c\u546c\u546c\u543d", "477": "\u300c\u5247\u5404\u5404\u5fa9\u544a\u8a00\uff1a\u300e\u52ff\u5f97\u8aaa\u65bc\u9918\u4eba\u3002\u300f\u5247\u8aa6\u8a93\u5fc3\u771f", "478": "\u8a00\uff0c\u5148\u5df2\u5165\u8005\u555f\u767d\u4e00\u5207\uff0c\u5982\u4f86\u7d50\u85a9\u57f5\u91d1\u525b\u5370", "479": "\u5f9e\u4e0b\u5411\u4e0a\u89e3\u3002\u4ee5\u6b64\u771f\u8a00\u5fc3\uff1a\u300c\u5535\u5f15\u5403\u54e9", "48": "\u57f5\u91d1\u525b\u5370\uff0c\u4ee5\u6b64\u5fc3\uff1a\u300c\u4e09\u6469\u8036\u85a9\u601b\u9441\u4e8c\u5408", "480": "\u4e8c\u5408\u90fd\u56a9\u85a9\u56a9\u85a9\u601b\u56a9\u4e8c\u5408\u35da\u4ed6\u4e8c\u5408\u6089\u5730\u637a", "481": "\u591a\u4e5f\u4ed6\u52aa\u8a90\u8616\u4ed6\u8eca\u99c4\udb81\udccb\u4e8c\u5408\u52c3\u99b1", "482": "\u5fae\u7051\u95bb\u88dc\u90a3\u56c9\u8a90\u9ebc\u90a3\u4e5f\u90fd\u56a9\u65e5\u56c9\u4e8c\u5408", "483": "\u85a9\u601b\u56a9\u4e8c\u5408\u7a46", "484": "\u300c\u5982\u662f\uff0c\u65bc\u4e00\u5207\u66fc\u837c\u7f85\u4e09\u6627\u8036\u52dd\u5370\u800c\u4f5c\u89e3\u3002\u300d", "485": "\u91d1\u525b\u9802\u4e00\u5207\u5982\u4f86\u771f\u5be6\u651d\u5927\u4e58\u73fe\u8b49\u5927\u6559\u738b", "486": "\u7d93\u5377\u4e0b", "49": "\u300c\u5247\u4ee5\u4e8c\u4e2d\u6307\u4ee4\u6301\u82b1\u9b18\uff0c\u4ee5\u6b64\u5fc3\u771f\u8a00\u4ee4\u5165\u4e09", "5": "\u6b63\u865f\u5927\u5ee3\u667a\u5927\u8208\u5584\u5bfa\u4e09\u85cf\u6c99\u9580", "50": "\u6469\u8036\u543d\u3002\u5165\u5df2\uff0c\u4f5c\u662f\u8a00\uff1a\u300c\u963f\u511e\u4e5f\u4e8c\u5408\u85a9\u601b\u9441\u4e8c\u5408", "51": "\u85a9\u5a46\u601b\u4ed6\u8616\u591a\u53e5\u7281\u9262\u56c9\u5c3e\u745f\u5412\u4e8c\u5408\u85a9", "52": "\u591a\u4e8c\u5408\u5a1c\u608d\u8ae6\u56a9\u65e5\u56c9\u67b3\u82e5\u4e8c\u5408\u90a3\u6bcd\u601b", "53": "\u8ddb\u4e8c\u5408\u90a3\u4ee5\u4f7f\u4e5f\u4e8c\u5408\u5f2d\u66f3\u90a3\u67b3\u5a18\u4e8c\u5408\u6ce5\u90a3", "54": "\u601b\u9441\u85a9\u5a46\u601b\u4ed6\u8616\u591a\u6089\u5730\u56c9\u6bd4\u907f\u56c9\u4e8c\u5408", "55": "\u9262\u65e6\u4e8c\u5408\u5636\u91d1\u5409\u6eaa\u53cd\u5e03\u90a3\u56c9\u60f9\u6089\u99b1\u85e5\u56c9", "56": "\u60f9\u90a3\u906e\u601b\u56a9\u8036\u6d85\u54e9\u745f\u5412\u6469\u8a36\u66fc\u8336\u7f85\u5beb", "57": "\u4e5f\u4e8c\u5408\u56a9\u7faf\u54c6\u4e8c\u5408\u5c3e\u95bb\u4e8c\u5408\u9ebc\u63d0\u4e09\u6469", "58": "\u6e1d\u5c3e\u4e5f\u4e8c\u5408\u4ed6\u511e\u6e27\u4e01\u7ffc\u53cd", "59": "\u300c\u91d1\u525b\u963f\u95cd\u9ece\u81ea\u61c9\u7d50\u85a9\u57f5\u91d1\u525b\u5370\uff0c\u53cd\u5b89\u5f1f\u5b50", "6": "\u4e0d\u7a7a\u5949\u3000\u8a54\u8b6f", "60": "\u9802\uff0c\u4f5c\u662f\u8a00\uff1a\u300e\u6b64\u662f\u4e09\u6627\u8036\u91d1\u525b\uff0c\u6467\u6c5d\u9802\u4e0d\u61c9\u8aaa\u3002\u300f", "61": "\u300c\u52a0\u6301\u8a93\u6c34\u4e00\u904d\uff0c\u4ee4\u5f1f\u5b50\u98f2\u8a93\u6c34\uff0c\u771f\u8a00\u66f0\uff1a\u300c\u56a9\u65e5", "62": "\u56c9\u85a9\u57f5\u85a9\u56a9\u5ef6\u8ae6\u511e\u8036\u4e8c\u5408\u7d07\u550e\u90a3\u66f3", "63": "\u5a11\u6469\u56a9\u6089\u9ad4\u6c40\u4ee5\u53cd\u54c6\u634f\u5c3c\u9038\u53cd\u907f\u511e\u4e5f\u4e8c\u5408", "64": "\u85a9\u601b\u4e5e\u53c9\u4e8c\u5408\u5583\u591c\u8036\u511e\u4e5f\u4e8c\u5408\u511e\u6c92\u5695", "65": "\u4e8c\u5408\u8036\u511e\u96e3\u90a3\u53bb\u95bb\u56a9\u65e5\ud844\udc14\u5a1c\u8fe6\u577c", "66": "\u300c\u5247\u544a\u5f1f\u5b50\uff1a\u300e\u81ea\u4eca\u5df2\u5f8c\uff0c\u6c5d\u89c0\u5a11\u8de2\u6211\u5982\u91d1\u525b\u624b\u3002", "67": "\u6211\u6240\u61c9\u8a00\uff0c\u6c5d\u7576\u5982\u662f\u4f5c\u3002\u6c5d\u4e0d\u61c9\u8f15\u6162\u65bc\u6211\uff0c\u52ff", "68": "\u4ee4\u6c5d\u62db\u707d\u798d\u6b7b\u5df2\u7576\u58ae\u5730\u7344\u3002\u300f\u4f5c\u5982\u662f\u8a9e\u5df2\uff0c", "69": "\u552f\u9858\u4e00\u5207\u5982\u4f86\u52a0\u6301\uff0c\u9858\u91d1\u525b\u85a9\u57f5\u904d\u5165\u91d1\u525b", "7": "\u5927\u66fc\u837c\u7f85\u5ee3\u5927\u5100\u8ecc\u54c1\u4e4b\u4e09", "70": "\u963f\u95cd\u68a8\uff0c\u61c9\u7d50\u85a9\u57f5\u91d1\u525b\u5370\uff0c\u4f5c\u662f\u8a00\uff1a\u300c\u963f\u884d\u601b\u4e09", "71": "\u6469\u6b32\u958b\u53e3\u547c\u7e1b\u65e5\u3615\u4e8c\u5408\u56a9\u65e5\u56c9\u85a9\u601b\u56a9\u5f2d", "72": "\u5e95\u4e01\u4ee5\u53cd\u85a9\u5bc6\u54e9\u4e8c\u5408\u803d\u963f\u5c3e\u6368\u91ce\u90fd\u8ae6\u66f3", "73": "\u56a9\u4e8c\u5408\u56a9\u65e5\u56c9\u67b3\u5b43\u90a3\u6469\u5f29\u601b\u3615\u56a9\u65e5\u56c9", "74": "\u5f15\u5420\u5962\u5641", "75": "\u300c\u5247\u7d50\u5fff\u6012\u62f3\u6467\u85a9\u57f5\u91d1\u525b\u5370\uff0c\u96a8\u610f\u91d1\u525b\u8a9e\uff0c\u8aa6", "76": "\u5927\u4e58\u73fe\u8b49\u767e\u5b57\u771f\u8a00\uff0c\u5247\u963f\u5c3e\u6368\u3002\u7e94\u963f\u5c3e\u6368", "77": "\u5df2\uff0c\u5247\u767c\u751f\u5fae\u5999\u667a\uff0c\u7531\u6b64\u77e5\u4ed6\u5fc3\u3001\u609f\u4ed6\u5fc3\u3001\u65bc\u4e00", "78": "\u5207\u4e8b\u77e5\u4e09\u4e16\uff0c\u5176\u5fc3\u5247\u5f97\u5805\u56fa\uff0c\u65bc\u4e00\u5207\u5982\u4f86\u6559", "79": "\u4e2d\u6089\u9664\u4e00\u5207\u82e6\u60f1\uff0c\u96e2\u4e00\u5207\u8af8\u60e1\u8da3\uff0c\u65bc\u4e00\u5207", "8": "\u300c\u6b21\u7576\u5ee3\u8aaa\u91d1\u525b\u5f1f\u5b50\u5165\u91d1\u525b\u5927\u66fc\u837c\u7f85\u5100\u8ecc\u3002", "80": "\u6709\u60c5\u7121\u6cae\u58de\uff0c\u4e00\u5207\u5982\u4f86\u52a0\u6301\uff0c\u4e00\u5207\u6089\u5730\u73fe\u524d", "81": "\u5f97\u672a\u66fe\u6709\uff0c\u751f\u559c\u6085\u5b89\u6a02\u6085\u610f\u3002\u7531\u6b64\u5b89\u6a02\u7b49\uff0c\u6216", "82": "\u6210\u5c31\u4e09\u6469\u5730\u3001\u6216\u9640\u7f85\u5c3c\u9580\u3001\u6216\u4e00\u5207\u610f\u9858\u7686\u5f97", "83": "\u6eff\u8db3\u3001\u4e43\u81f3\u6210\u5c31\u4e00\u5207\u5982\u4f86\u9ad4\u6027\u3002", "84": "\u300c\u5247\u7d50\u5f7c\u5370\uff0c\u4ee5\u89e3\u65bc\u5f1f\u5b50\u5fc3\uff0c\u8aa6\u6b64\u5fc3\u771f\u8a00\uff1a\u300c\u5e95\u745f", "85": "\u59f9\u4e8c\u5408\u56a9\u65e5\u56c9\u6d85\u54e9\u6fc1\u54a9\u5a46\u56a9\u6368\u6fd5\u3002\u56a9", "86": "\u4e8c\u5408\u90fd\u54a9\u5a46\u56a9\u7d07\u550e\u5a1c\u95bb\u54a9\u5730\u5e95\u745f\u59f9", "87": "\u85a9\u56a9\u6089\u6715\u4ead\u6deb\u53cd\u3001\u4e0a\u906e\u54a9\u9262\u56c9\u4e8c\u5408\u4e5f\u8eca\u543d\u5475", "88": "\u5475\u5475\u5475\u659b\u5f15", "89": "\u300c\u5247\u4ee5\u5176\u82b1\u9b18\u4ee4\u5f1f\u5b50\u64f2\u65bc\u5927\u66fc\u837c\u7f85\uff0c\u4ee5\u6b64\u5fc3", "9": "\u65bc\u4e2d\u6211\u5148\u8aaa\u4ee4\u5165\u76e1\u7121\u9918\u6709\u60c5\u754c\u62d4\u6fdf\u5229\u76ca", "90": "\u771f\u8a00\uff1a\u300c\u9262\u56c9\u5e95\u8eca\u56a9\u65e5\u56c9\u659b\u5f15", "91": "\u300c\u96a8\u82b1\u843d\u8655\uff0c\u5247\u5f7c\u5c0a\u6210\u5c31\uff0c\u5247\u53d6\u5f7c\u82b1\u9b18\u7e6b\u5f1f\u5b50", "92": "\u982d\u4e0a\uff0c\u4ee5\u6b64\u5fc3\u771f\u8a00\uff1a\u300c\u5535\u9262\u56c9\u5e95\u4ee1\u54e9\u7d07\u62cf", "93": "\u601b\u56a9\u5f2d\ud851\udea5", "94": "\u300c\u85a9\u601b\u56a9\u6469\u8a36\u9ebc\u651e", "95": "\u300c\u7531\u6b64\u5247\u5927\u85a9\u57f5\u651d\u53d7\u901f\u5f97\u6210\u5c31\uff0c\u6210\u5165\u5df2\u5247\u89e3", "96": "\u9762\uff0c\u4ee5\u6b64\u5fc3\u771f\u8a00\uff1a\u300c\u5535\u56a9\u65e5\u56c9\u85a9\u57f5\u85a9\u56a9\u5ef6\u5e1d", "97": "\u511e\u8036\u4e8c\u5408\u707c\u4e5e\u82bb\u4e8c\u5408\u3001\u5f15\u5a1c\u4f3d\u59f9\u66e9\u601b\u9262\u4e8c\u5408", "98": "\u56c9\u70cf\u90a3\u4f3d\u54a4\u91ce\u5e95\u4e01\u4ee5\u53cd\u85a9\u56a9\u4e5e\u35d9\u56a9\u65e5", "99": "\u56c9\u4e8c\u5408\u707c\u4e5e\u35d9\u56c9\u5f29\u591a\u3615"}, "source": "CBETA", "language": "chinese", "originalTitle": "Taisho Tripitaka, Electronic version, No. 0865 \u91d1\u525b\u9802\u4e00\u5207\u5982\u4f86\u771f\u5be6\u651d\u5927\u4e58\u73fe\u8b49\u5927\u6559\u738b\u7d93", "author": "\u5510 \u4e0d\u7a7a\u8b6f", "sourceLink": "http://www.cbeta.org/"}