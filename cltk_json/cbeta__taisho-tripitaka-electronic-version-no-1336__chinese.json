u8af8\u5929", "347": "\u7a2e\u5f9e\u662f\u6ec5\u3002\u5f9e\u5176\u4e2d\u81f4\u8655\u54c0\u5ff5\u3002\u6b32\u4ee4\u75be\u89e3\u812b\u6b78", "348": "\u547d\u65bc\u9053\u4eba\u3002\u5927\u5929\u738b\u738b\u56db\u5929\u4e0b\u8005\u3002\u807d\u6211\u8a00\u6211\u8aaa", "349": "\u546a\u7d93\u3002\u7528\u6551\u4eba\u6c11\u6545\u3002\u70ba\u9b3c\u795e\u4f5c\udb80\ude53\u6f3f\u3002\u7136\u4e43\u8aaa\u546a\u3002", "350": "\u68b5\u5929\u8b77\u6c5d\u982d\u653e\u9aee\u6642\u3002\u820d\u820d\u5f4c\u8b77\u4e4b\u76e3\u7576\u8b77\u6c5d", "351": "\u8eab\u3002\u6708\u7576\u8b77\u6c5d\u984d\u3002\u65e5\u7576\u8b77\u6c5d\u773c\u3002\u661f\u7576\u8b77\u6c5d\u814e\u3002", "352": "\u63f5\u9640\u7f85\u7576\u8b77\u6c5d\u81cd\u3002\u963f\u6bd4\u5c3c\u7576\u8b77\u6c5d\u5c3b\u3002\u5176\u68a8\u95cd", "353": "\u62d8\u68a8\u7576\u8b77\u6c5d\u5c0f\u8179\u3002\u661f\u5bbf\u7576\u8b77\u6c5d\u547d\u3002\u5176\u6709\u5728\u56db", "354": "\u5929\u4e0b\u8005\u3002\u82e5\u5584\u82e5\u60e1\u8607\u6478\u7576\u8b77\u6c5d\u813e\u3002\u5929\u7576\u548c\u6069", "355": "\u5412\u7576\u8b77\u4e4b\u3002\u90a3\u884c\u8005\u548c\u4fee\u63d0\u3002\u662f\u56db\u5927\u5929\u738b\u4e3b\u56db", "356": "\u5929\u4e0b\u8005\u3002\u7576\u8b77\u6c5d\u5bb6\u3002\u820d\u4f3d\u982d\u7576\u8b77\u6c5d\u819d\u3002\u98df\u4e00\u5207", "357": "\u5473\u8005\u7576\u8b77\u6c5d\u8db3\u3002\u6469\u8a36\u4f3d\u7f85\u7576\u8b77\u6c5d\u624b\u3002\u83ab\u6050\u6016", "358": "\u65bc\u662f\u7121\u61fc\u3002\u4f7f\u5409\u5584\u3002", "359": "\u5f7c\u6bd8\u6c99\u9580\u5927\u5929\u738b\u65bc\u4e00\u5207\u9b3c\u795e\u7f85\u524e\u4e2d\u6700\u5c0a\u3002", "360": "\u52c5\u4ee4\u4e00\u5207\u9b3c\u795e\u7f85\u524e\u3002\u4fbf\u8aaa\u662f\u546a\u5f97\u546a\u5927\u546a\u6240\u884c", "361": "\u5373\u75be\u3002\u8017\u4e82\u7761\u7720\u3002\u5f97\u7121\u4e00\u5207\u7686\u4e0d\u6210\u3002\u5982\u662f\u3002\u5475", "362": "\u5c3c\u5475\u5c3c\u4f49\u5c3c\u4f49\u5c3c\u982d\u4f49\u5c3c\u4f49\u77e5\u4f49\u77e5\u4f49\u77e5\u98e2\u98e2", "363": "\u63d0\u982d\u8cf4\u5412\u5927\u5929\u738b\u3002\u65bc\u4e00\u5207\u63f5\u9640\u7f85\u4e2d\u6700\u5c0a\u3002\u7686", "364": "\u52c5\u4ee4\u8af8\u63f5\u9640\u7f85\u3002\u4fbf\u8aaa\u662f\u546a\u3002\u5f97\u546a\u5927\u546a\u6240\u884c\u5373", "365": "\u75be\u3002\u8017\u4e82\u7761\u7720\u5f97\u7121\u4e00\u5207\u7686\u4e0d\u6210\u5982\u662f\u3002", "366": "\u5475\u5b89\u5475\u5b89\u5475\u5b89\u3000\u6ce2\u77e5\u6ce2\u77e5\u6ce2\u77e5\u3000\u77e5\u5937\u7281", "367": "\u548c\u5937\u548c\u62b5\u3000\u6587\u591a\u5c3c\u3000\u6ce2\u77e5\u591a\u5c3c", "368": "\u6bd8\u6a13\u52d2\u53c9\u5927\u5929\u738b\u3002\u65bc\u4e00\u5207\u9ce9\u69c3\u8336\u4e2d\u6700\u5c0a\u3002\u52c5", "369": "\u4ee4\u4e00\u5207\u9ce9\u69c3\u8336\u3002\u4fbf\u8aaa\u662f\u546a\u3002\u5f97\u546a\u5927\u546a\u6240\u884c\u5373", "370": "\u75be\u3002\u8017\u4e82\u7761\u7720\u5f97\u7121\u4e8b\u4e0d\u6210\u5982\u662f\u3002", "371": "\u963f\u90a3\u5c3c\u3000\u963f\u9588\u9640\u7f85\u3000\u9588\u6469\u62b5\u6469\u62b5\u3000\u6469\u8a36\u6469", "372": "\u62b5\u6469\u5e1d\u6469\u5e1d\u3000\u6469\u591a\u6469\u8ae6\u3000\u4e5d\u77e5\u4e5d\u77e5\u6bd8\u4e5d", "373": "\u77e5", "374": "\u6bd8\u6a13\u535a\u53c9\u5927\u5929\u738b\u3002\u65bc\u4e00\u5207\u9f8d\u4e2d\u6700\u5c0a\u3002\u52c5\u4ee4\u8af8", "375": "\u9f8d\u4fbf\u8aaa\u662f\u546a\u3002\u5927\u546a\u6240\u884c\u5373\u75be\u3002\u8017\u4e82\u7761\u7720\u5f97\u7121", "376": "\u4e8b\u4e0d\u6210\u5982\u662f\u3002", "377": "\u6469\u501a\u3000\u6469\u501a\u3000\u6469\u501a\u3000\u6469\u5e1d\u3000\u62d8\u77e5\u6bd8\u62d8\u77e5", "378": "\u5929\u738b\u8aaa\u662f\u546a\u7d93\u6642\u3002\u5730\u7686\u70ba\u52d5\u3002\u5b08\u4eba\u6c11\u8005\u6089\u6050", "379": "\u6016\u8863\u6bdb\u70ba\u7aea\u3002\u5176\u6709\u77e5\u662f\u546a\u738b\u7d93\u8005\u3002\u7121\u5fa9\u754f\u6050\u3002", "380": "\u4e0d\u82e6\u982d\u75db\u76ee\u4e0d\u7729\u8033\u4e0d\u75db\u3002\u9f3b\u8123\u53e3\u9f52\u820c\u9830\u9824", "381": "\u81cd\u5fc3\u4ea6\u4e0d\u75db\u3002\u82e5\u8179\u816b\u8107\u816b\u624b\u75db\u81cd\u75db\u813e\u75db\u819d", "382": "\u75db\u8db3\u75db\u3002\u4e00\u5207\u8af8\u6240\u82e6\u75db\u6089\u9664\u53bb\u3002\u82e5\u4eba\u82e5\u975e\u4eba", "383": "\u7121\u80fd\u5f97\u5984\u5b08\u3002\u6d6e\u9640\u4ea6\u4e0d\u5f97\u5b08\u3002\u5929\u4ea6\u4e0d\u5f97\u5b08\u3002\u82e5", "384": "\u9f8d\u82e5\u63f5\u9640\u7f85\u3002\u82e5\u91d1\u7fc5\u9ce5\u82e5\u4ec7\u6853\u3002\u82e5\u9b3c\u795e\u82e5\u7f85", "385": "\u524e\u3002\u82e5\u9ce9\u69c3\u8336\u82e5\u6eb7\u908a\u9b3c\u795e\u3002\u82e5\u9913\u9b3c\u82e5\u820d\u820d\u3002\u82e5", "386": "\u9ce5\u7f85\u82e5\u963f\u9640\u6469\u7f85\u3002\u82e5\u63f5\u9640\u6a13\u9640\u7686\u4e0d\u5f97\u5984\u5b08\u3002", "387": "\u53ca\u65e5\u6708\u661f\u5bbf\u98a8\u5bd2\u6c23\u3002\u7686\u4f7f\u9664\u6108\u64c1\u8b77\u5176\u8eab\u3002\u4ee4", "388": "\u5176\u8af8\u90aa\u60e1\u6d88\u6ec5\u767e\u75c5\u9664\u6108\u3002\u5357\u7121\u4e00\u5207\u5409\u5584\u5409", "389": "\u7965\u8af8\u7fa9\u5584\u7cbe\u9032\u3002", "390": "\u5357\u7121\u8af8\u5982\u4f86\u7121\u6240\u8457\u7b49\u6b63\u89ba\u3002\u79ae\u8db3\u5df2\u4fbf\u8aaa\u662f", "391": "\u546a\u3002\u4ee4\u6211\u6240\u546a\u5373\u5f9e\u5982\u9858\u3002", "392": "\u9640\u7f85\u5c3c\u96dc\u96c6\u5377\u7b2c\u5341"}, "originalTitle": "Taisho Tripitaka, Electronic version, No. 1336 \u9640\u7f85\u5c3c\u96dc\u96c6", "source": "CBETA"}