": "\u7576\u77e5\u570b\u571f\u7336\u5982\u7f85\u7db2\u3002\u4ea6\u5982\u7f82\u5f36\u3002\u5982\u6df1\u6de4\u6ce5\u3002\u4ea6", "133": "\u5982\u6d04\u6ce2\u3002\u53c8\u5982\u6d77\u6d6a\u3002\u5982\u6797\u88ab\u71d2\u3002\u4ea6\u5982\u5371\u5cb8\u3002\u7336\u5982", "134": "\u5730\u7344\u3002\u4f55\u6709\u667a\u8005\u3002\u7576\u6a02\u8caa\u8457\u3002\u5982\u662f\u5927\u82e6\u3002\u4f55\u6709", "135": "\u667a\u8005\u3002\u7576\u751f\u6a02\u60f3\u3002\u5982\u662f\u5927\u738b\u3002\u55da\u547c\u602a\u54c9\u3002\u88ab\u6b3a\u4e43", "136": "\u723e\u3002\u88ab\u8a91\u4e43\u723e\u7336\u5982\u7a7a\u62f3\u8a91\u65bc\u5c0f\u5152\u3002\u901f\u75be\u4e0d", "137": "\u505c\u3002\u7336\u5982\u5e7b\u5316\u3002\u4e94\u6b32\u6b3a\u8a91\u3002\u4ea6\u5fa9\u5982\u662f\u3002\u7336\u5982\u733f", "138": "\u7334\u5728\u9ad8\u5c71\u9802\u898b\u96f2\u5f4c\u5e03\u4ee5\u70ba\u5805\u5be6\u3002\u8b02\u70ba\u662f\u5730", "139": "\u4fbf\u4ee5\u8eab\u6295\u58ae\u767e\u4e08\u5dd6\u3002\u55aa\u5176\u8eab\u547d\u3002\u4e00\u5207\u788e\u6ec5\u3002\u4ea6", "140": "\u5982\u91ce\u5e72\u898b\u7504\u53d4\u8fe6\u6a39\u3002\u5176\u679c\u4f3c\u8089\u3002\u898b\u843d\u5730\u6642\u3002\u4fbf", "141": "\u5f80\u6b32\u98df\u3002\u77e5\u5176\u975e\u8089\u3002\u66f4\u5fa9\u751f\u5ff5\u3002\u4eca\u6b64\u975e\u8089\u3002\u5f7c", "142": "\u6a39\u4e0a\u8005\u3002\u5fc5\u7576\u662f\u8089\u3002\u9042\u4fbf\u5b88\u4e4b\u3002\u70ba\u5176\u6240\u56f0\u4e94\u6b32", "143": "\u8a91\u738b\u3002\u4ea6\u5fa9\u5982\u662f\u3002\u4ea6\u5982\u5546\u4f30\u4ee5\u507d\u73e0\u8a91\u4eba\u3002\u4e94", "144": "\u6b32\u8a91\u738b\u3002\u4ea6\u5fa9\u5982\u662f\u3002\u53c8\u5982\u5b30\u611a\u55dc\u5473\u8caa\u6b61\u559c", "145": "\u4e38\u3002\u4eba\u4ee5\u6ce5\u5718\u3002\u800c\u4f86\u8a91\u4e4b\u3002\u8b02\u70ba\u771f\u5be6\u3002\u8d70\u9010\u75b2", "146": "\u82e6\u3002\u4e43\u5f97\u6ce5\u5718\u3002\u5982\u71b1\u6642\u7130\u8a91\u6e34\u611a\u592b\u3002\u7336\u5982\u773e", "147": "\u4eba\u524d\u7aea\u65bc\u5e7b\u6a5b\u3002\u80fd\u4f7f\u6642\u773e\u898b\u7a2e\u7a2e\u4e8b\u3002\u82e5\u62d4\u5e7b", "148": "\u6a5b\u3002\u8272\u50cf\u5373\u6ec5\u3002\u7336\u5982\u756b\u5320\u53ca\u6a5f\u95dc\u5e2b\u3002\u5982\u72d7\u5420", "149": "\u4e95\u81ea\u898b\u5f62\u5f71\u3002\u6012\u773c\u7aea\u6bdb\u3002\u8b02\u4e95\u5e95\u5f71\u6b32\u5171\u5df1\u9b2a\u3002", "150": "\u6a6b\u751f\u778b\u5fff\u3002\u6295\u4e95\u800c\u6b7b\u3002\u5927\u738b\u3002\u5b9c\u5584\u89c0\u5bdf\u3002\u4f55\u6709\u4e94", "151": "\u6b32\u800c\u5f97\u5e38\u8005\u4f55\u6709\u738b\u4f4d\u800c\u5f97\u4e45\u505c\u3002\u5c0a\u8c6a\u5a01\u52e2\u3002", "152": "\u7121\u5f97\u4f4f\u8005\u3002\u4f55\u6709\u570b\u754c\u800c\u4e0d\u9077\u58de\u3002\u4f55\u6709\u73cd\u5bf6\u800c", "153": "\u4e0d\u6563\u5931\u3002\u4f55\u6709\u6b32\u6a02\u5e38\u6052\u4e0d\u8b8a\u3002\u82e6\u4e4b\u5c01\u6388\u5fc5\u53d7", "154": "\u8870\u6ec5\u3002\u4f55\u6709\u5408\u6703\u800c\u4e0d\u5225\u96e2\u3002\u4e00\u5207\u4e94\u6b32\u3002\u9ad4\u6027\u5be6", "155": "\u82e6\u3002\u7686\u5f9e\u5984\u60f3\u800c\u751f\u65bc\u6a02\u3002\u4f55\u6709\u8af8\u884c\u4e0d\u4f3c\u82ad\u8549", "156": "\u63f5\u95e5\u5a46\u57ce\u3002\u5927\u738b\u4e91\u4f55\u3002\u8655\u751f\u8001\u75c5\u6b7b\u8870\u798d\u6050\u6016", "157": "\u903c\u8feb\u4e4b\u4e2d\u3002\u4e91\u4f55\u80fd\u70ba\u570b\u571f\u5c11\u6a02\u751f\u611b\u6a02\u60f3\u3002", "158": "\u5982\u6797\u4e2d\u9e7f\u3002\u56db\u908a\u706b\u8d77\u3002\u5982\u9ce5\u5728\u7c60\u3002\u5982\u9b5a\u8655\u7db2\u3002\u5982", "159": "\u9f9c\u541e\u9264\u3002\u5982\u5e2b\u5b50\u6bd2\u7bad\u5165\u5fc3\u3002\u5982\u9f8d\u8655\u546a\u5834\u3002\u5982\u4eba", "160": "\u5728\u5c4b\u4e2d\u56db\u908a\u706b\u8d77\u3002\u5982\u8655\u5371\u673d\u83ef\u5802\u901f\u75be\u5d29\u589c\u3002", "161": "\u5982\u597d\u83ef\u6c60\u6709\u6c34\u7f85\u524e\u564f\u98df\u65bc\u4eba\u3002\u91cd\u8aaa\u5048\u8a00\u3002", "162": "\u751f\u8001\u75c5\u6b7b\u60a3\u65bc\u4e2d\u672a\u89e3\u812b", "163": "\u7121\u660e\u611b\u6bd2\u7bad\u7336\u672a\u5f97\u62d4\u51fa", "164": "\u4eba\u5e1d\u6c5d\u4e91\u4f55\u800c\u751f\u6a02\u8457\u60f3", "165": "\u5982\u8c61\u8655\u6797\u4e2d\u56db\u908a\u5927\u706b\u8d77", "166": "\u8655\u6b64\u6025\u96e3\u8655\u4e91\u4f55\u6709\u6b61\u559c", "167": "\u5927\u738b\u61c9\u7576\u77e5\u69ae\u4f4d\u9808\u81fe\u9593", "168": "\u667a\u8005\u6df1\u89c0\u5bdf\u4e0d\u61c9\u65bc\u6b64\u4e8b", "169": "\u800c\u751f\u5e0c\u6709\u60f3\u6c5d\u4f55\u6545\u932f\u89e3", "170": "\u5be6\u662f\u611b\u5974\u50d5\u800c\u751f\u9ad8\u8cb4\u60f3", "171": "\u6368\u4e0a\u5999\u8ca1\u5bf6\u800c\u751f\u5927\u5bcc\u60f3", "172": "\u4e0d\u5584\u89e3\u65b9\u4fbf\u6a6b\u751f\u667a\u6167\u60f3", "173": "\u70ba\u773e\u7169\u60f1\u60a3\u6a6b\u751f\u7121\u75c5\u60f3", "174": "\u672a\u812b\u751f\u6b7b\u80ce\u6a6b\u751f\u7121\u754f\u60f3", "175": "\u8655\u5341\u4e8c\u523a\u6797\u6a6b\u751f\u7121\u523a\u60f3", "176": "\u6b32\u8cca\u52ab\u8af8\u6839\u6a6b\u751f\u7121\u8cca\u60f3", "177": "\u5927\u738b\u800c\u6b64\u8eab\u8005\u5fc5\u6b78\u6557\u58de\u3002\u5c0a\u8c6a\u69ae\u8cb4\u3002\u5fc5\u6709\u8870", "178": "\u6ec5\u3002\u8ca1\u5bf6\u5eab\u85cf\u3002\u5fc5\u6709\u6563\u5931\u3002\u5927\u738b\u3002\u5982\u4f5b\u8a00\u66f0\u3002", "179": "\u69ae\u4f4d\u5982\u5922\u3002\u6069\u611b\u66ab\u6709\u3002\u6c5d\u65bc\u4e94\u6b32\u3002\u751f\u65bc\u5e0c\u6709\u96e3", "180": "\u906d\u4e4b\u60f3\u3002\u8ce2\u5fb7\u65bc\u6b64\u3002\u8c48\u5f97\u540d\u70ba\u80fd\u5584\u89c0\u5bdf\u3002\u4f55\u4ee5", "181": "\u6545\u3002\u69ae\u4f4d\u6069\u611b\u3002\u5fc5\u6709\u5225\u96e2\u3002\u5982\u773e\u98db\u9ce5\u591c\u6816\u4e00\u6a39\u3002", "182": "\u6668\u5247\u56db\u6563\u3002\u53c8\u5982\u5ba2\u820d\u5915\u5247\u805a\u8cd3\u3002\u660e\u5404\u7570\u8def\u3002\u4ea6", "183": "\u5982\u4e58\u8239\u3002\u7570\u4eba\u540c\u8f09\u3002\u65e2\u81f3\u5cb8\u5df2\u3002\u5404\u81ea\u6b8a\u9053\u3002\u4ea6\u5982", "184": "\u99db\u6d41\u6f02\u96c6\u773e\u6728\u3002\u9808\u81fe\u4e4b\u9593\u3002\u96a8\u6d41\u5206\u6563\u3002\u7336\u5982\u6d6e", "185": "\u96f2\u3002\u9808\u81fe\u6563\u6ec5\u3002\u4f5c\u97f3\u6a02\u8655\u3002\u7537\u5973\u805a\u96c6\u4f5c\u6a02\u5df2", "186": "\u5f8c\u3002\u5404\u81ea\u6563\u53bb\u3002\u5bae\u4eba\u5a47\u5973\u7aef\u6b63\u7f8e\u5999\u3002\u7121\u5e38\u7406", "187": "\u6703\u3002\u6703\u6b78\u6368\u68c4\u3002\u8b6c\u5982\u83ef\u6a39\u8702\u96c6\u5176\u4e0a\u3002\u82b1\u51cb\u843d", "188": "\u76e1\u3002\u8af8\u8702\u9060\u96e2\u3002\u5982\u82b1\u6c60\u67af\u6db8\u7278\u8c61\u4e0d\u5165\u3002\u5982\u5927\u6c60", "189": "\u6c34\u9d60\u6a02\u904a\u5c45\u3002\u53ca\u5176\u4e7e\u7aed\u66f4\u4e0d\u5fa9\u8fd1\u3002\u798f\u76e1\u4e4b", "190": "\u5bb6\u3002\u69ae\u5229\u4e0d\u8fd1\u3002\u5982\u5bc6\u96f2\u805a\u96c6\u3002\u96fb\u5149\u66ab\u73fe\u3002\u5982\u98a8", "191": "\u5439\u96f2\u3002\u96fb\u5149\u4e0d\u73fe\u3002\u5f7c\u4e0d\u6368\u6c5d\u3002\u6c5d\u5fc5\u6368\u4e4b\u3002\u5982\u4f3c", "192": "\u590f\u76e1\u5b54\u96c0\u6bdb\u7fbd\u6089\u7686\u81ea\u843d\u3002\u5982\u5bd2\u65e2\u81f3\u9d3b\u9d60\u9060", "193": "\u6c60\u3002\u5982\u963f\u8f38\u4f3d\u6a39\u82b1\u8449\u76db\u6642\u3002\u4eba\u6240\u611b\u6a02\u53ca\u5176\u67af", "194": "\u60b4\u3002\u7121\u6709\u82b1\u8449\u3002\u4eba\u4e0d\u9867\u8996\u3002\u7336\u5982\u82b1\u5e62\u3002\u8cb4\u8005\u611b", "195": "\u656c\u3002\u83ef\u840e\u7e37\u7d55\u800c\u4fbf\u68c4\u4e4b\u3002\u5373\u8aaa\u5048\u8a00\u3002", "196": "\u7121\u5e38\u4e0d\u5805\u56fa\u5982\u82ad\u8549\u6c34\u6cab", "197": "\u4ea6\u5982\u6d6e\u96f2\u6563\u5929\u738b\u5c0a\u52dd\u4f4d", "198": "\u5371\u8106\u4ea6\u5982\u662f\u4eba\u5e1d\u61c9\u7576\u77e5", "199": "\u8caa\u5229\u6975\u901f\u99db\u5982\u6c34\u6f8d\u6df1\u8c37", "200": "\u55dc\u6b32\u6975\u8f15\u75be\u52d5\u8f49\u5982\u6389\u7d22", "201": "\u611a\u7661\u67d3\u70ba\u6b32\u4e0d\u89ba\u81f4\u58ae\u843d", "202": "\u5c0a\u8005\u8a00\u3002\u5927\u738b\u3002\u6211\u4eca\u70ba\u738b\u7565\u8aaa\u8b6c\u55bb\u3002\u8af8\u6709\u751f\u6b7b\u3002", "203": "\u8457\u5473\u904e\u60a3\u3002\u738b\u81f3\u5fc3\u807d\u3002\u6614\u65e5\u6709\u4eba\u3002\u884c\u5728\u66e0\u8def\u3002\u9022", "204": "\u5927\u60e1\u8c61\u3002\u70ba\u8c61\u6240\u9010\u3002\u72c2\u61fc\u8d70\u7a81\u3002\u7121\u6240\u4f9d\u6019\u898b\u4e00", "205": "\u4e18\u4e95\u3002\u5373\u5c0b\u6a39\u6839\u3002\u5165\u4e95\u4e2d\u85cf\u3002\u6709\u767d\u9ed1\u9f20\u3002\u7259\u9f67\u6a39", "206": "\u6839\u3002\u6b64\u4e95\u56db\u908a\u3002\u6709\u56db\u6bd2\u86c7\u3002\u6b32\u87ab\u5176\u4eba\u3002\u800c\u6b64\u4e95", "207": "\u4e0b\u3002\u6709\u5927\u6bd2\u9f8d\u3002\u508d\u754f\u56db\u86c7\u3002\u4e0b\u754f\u6bd2\u9f8d\u3002\u6240\u6500\u4e4b", "208": "\u6a39\u3002\u5176\u6839\u52d5\u6416\u3002\u6a39\u4e0a\u6709\u871c\u4e09\u6e27\u3002\u58ae\u5176\u53e3\u4e2d\u3002", "209": "\u4e8e\u6642\u52d5\u6a39\ud84b\udd24\u58de\u8702\u7aa0\u3002\u773e\u8702\u6563\u98db\u3002\u553c\u87ab\u5176\u4eba\u3002\u6709", "210": "\u91ce\u706b\u8d77\u3002\u5fa9\u4f86\u71d2\u6a39\u3002\u5927\u738b\u7576\u77e5\u3002\u5f7c\u4eba\u82e6\u60f1\u3002\u4e0d\u53ef", "211": "\u7a31\u8a08\u3002\u738b\u6101\u6182\u53ad\u60e1\u800c\u8a00\u3002\u5f7c\u4eba\u5f97\u5473\u751a\u5c11\u3002\u82e6\u60a3", "212": "\u751a\u591a\u3002\u5176\u6240\u5473\u8005\u3002\u5982\u725b\u8de1\u6c34\u3002\u5176\u6240\u82e6\u60a3\u7336\u5982\u5927", "213": "\u6d77\u3002\u5473\u5982\u82a5\u5b50\u3002\u82e6\u5982\u9808\u5f4c\u3002\u5473\u5982\u87a2\u706b\u3002\u82e6\u5982\u65e5", "214": "\u6708\u3002\u5982\u85d5\u6839\u5b54\u6bd4\u65bc\u592a\u865b\u3002\u4ea6\u5982\u868a\u5b50\u6bd4\u91d1\u7fc5", "215": "\u9ce5\u3002\u5176\u5473\u82e6\u60f1\u591a\u5c11\u5982\u662f\u3002\u5c0a\u8005\u8a00\u3002\u5927\u738b\u3002\u66e0\u91ce", "216": "\u8005\u3002\u55bb\u65bc\u751f\u6b7b\u3002\u5f7c\u7537\u5b50\u8005\u3002\u55bb\u65bc\u51e1\u592b\u3002\u8c61\u55bb\u7121", "217": "\u5e38\u3002\u4e18\u4e95\u55bb\u65bc\u4eba\u8eab\u3002\u6a39\u6839\u55bb\u4eba\u547d\u3002\u767d\u9ed1\u9f20\u8005\u3002", "218": "\u55bb\u665d\u591c\u3002\u9f67\u6a39\u6839\u8005\u3002\u55bb\u5ff5\u5ff5\u6ec5\u3002\u56db\u6bd2\u86c7\u55bb\u56db\u5927\u3002", "219": "\u871c\u8005\u55bb\u4e94\u6b32\u3002\u773e\u8702\u55bb\u60e1\u89ba\u89c0\u3002\u91ce\u706b\u71d2\u8005\u55bb", "220": "\u8001\u3002\u4e0b\u6bd2\u9f8d\u8005\u55bb\u6b7b\u3002\u662f\u6545\u7576\u77e5\u3002\u6b32\u5473\u751a\u5c11\u3002\u82e6", "221": "\u60a3\u751a\u591a\u3002\u751f\u8001\u75c5\u6b7b\u3002\u65bc\u4e00\u5207\u4eba\u3002\u7686\u5f97\u81ea\u5728\u3002\u4e16", "222": "\u9593\u4e4b\u4eba\u3002\u8eab\u5fc3\u52de\u82e6\u3002\u7121\u6b78\u4f9d\u8655\u3002\u773e\u82e6\u6240\u903c\u3002\u8f15", "223": "\u75be\u5982\u96fb\u3002\u662f\u53ef\u6182\u6101\u3002\u4e0d\u61c9\u611b\u8457\u3002\u5927\u738b\u3002\u4eca\u6211\u8a9e", "224": "\u738b\u3002\u8a00\u96d6\u9e81\u60e1\u3002\u5be6\u662f\u5229\u76ca\u3002\u738b\u805e\u662f\u8a9e\u3002\u8863\u6bdb\u7686", "225": "\u7aea\u3002\u60b2\u559c\u4ea4\u96c6\u3002\u6d95\u6ce3\u6d41\u6dda\u3002\u5373\u8d77\u5408\u638c\u3002\u4e94\u9ad4\u6295", "226": "\u5730\u3002\u767d\u5c0a\u8005\u8a00\u3002\u6211\u4e4b\u5b30\u611a\u3002\u7121\u6709\u667a\u6167\u3002\u6211\u4e4b\u4e0b", "227": "\u8ce4\u3002\u4f5c\u65af\u72c2\u8a00\u3002\u5982\u662f\u72c2\u8a00\u3002\u807d\u6211\u61fa\u6094\u3002\u5c0a\u8005\u8a00\u3002", "228": "\u6211\u65bc\u4eca\u8005\u3002\u4ee5\u5fcd\u51fa\u5bb6\u3002\u7121\u4e0d\u5fcd\u53d7\u3002\u6211\u5fc3\u6e05\u6de8\u3002", "229": "\u7336\u5982\u79cb\u6708\u6de8\u7121\u96f2\u7ff3\u3002\u738b\u4eca\u61fa\u6094\u3002\u9858\u4f7f\u5927\u738b\u7336", "230": "\u5982\u5929\u5e1d\u5f97\u898b\u9053\u8de1\u3002\u738b\u5927\u6b61\u559c\u3002\u8207\u8af8\u7737\u5c6c\u3002\u4f5c\u79ae", "231": "\u9084\u5bae\u3002", "232": "\u8cd3\u982d\u76e7\u7a81\u7f85\u95cd\u70ba\u512a\u9640\u5ef6\u738b\u8aaa\u6cd5\u7d93"}, "originalTitle": "Taisho Tripitaka, Electronic version, No. 1690 \u8cd3\u982d\u76e7\u7a81\u7f85\u95cd\u70ba\u512a\u9640\u5ef6\u738b\u8aaa\u6cd5\u7d93", "source": "CBETA"}