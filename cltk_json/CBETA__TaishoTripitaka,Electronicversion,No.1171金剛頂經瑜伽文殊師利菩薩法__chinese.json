u5ff5\u73e0\u7576\u5fc3\u5ff5\u8aa6\u4e0d", "312": "\u7de9\u4e0d\u6025\u3002\u6216\u4e00\u767e\u516b\u904d\u6216\u4e00\u5343\u4e43\u81f3\u4e00\u842c\u904d\u3002\u5ff5", "313": "\u8aa6\u6578\u7562\u3002\u4e8c\u624b\u6367\u73e0\u5b89\u65bc\u9802\u4e0a\u3002\u7136\u5f8c\u7f6e\u672c\u8655\u3002", "314": "\u82e5\u4e09\u6469\u5730\u5ff5\u8aa6\u8005\u3002\u7576\u5fc3\u89c0\u5927\u5713\u93e1\u667a\u3002\u4e2d\u5e03", "315": "\u4e94\u5b57\u9580\u3002\u4e86\u4e86\u8ae6\u89c0\u3002\u96a8\u7fa9\u76f8\u61c9\u3002\u5fc3\u8207\u822c\u82e5\u6ce2\u7f85", "316": "\u871c\u5408\u3002\u6b64\u540d\u4e09\u6469\u5730\u5ff5\u8aa6\u3002\u82e5\u8eab\u75b2\u61c8\u3002\u5373\u7d50\u672c\u5c0a", "317": "\u528d\u5370\u3002\u8aa6\u4e94\u5b57\u9640\u7f85\u5c3c\u4e03\u904d\u3002\u5fa9\u4ee5\u516b\u5927\u4f9b\u990a\u4f9b", "318": "\u990a\u8af8\u4f5b\u3002\u4ee5\u5999\u97f3\u8fad\u7a31\u63da\u8b9a\u6b4e\u3002\u737b\u95bc\u4f3d\u6c34\u3002\u4ee5\u964d", "319": "\u4e09\u4e16\u5370\u5de6\u65cb\u89e3\u754c\u3002\u5373\u7d50\u91d1\u525b\u5229\u528d\u5370\u3002\u5949\u9001\u8af8", "32": "\u751f\u7fa9\u3002\u8ddb\u5b57\u8005\u7b2c\u4e00\u7fa9\u8ae6\u7fa9\u3002\u8005\u5b57\u8005\u5999\u884c\u7fa9\u3002\u66e9", "320": "\u8056\u3002\u5404\u9084\u672c\u5bae\u3002\u771f\u8a00\u66f0\u3002", "321": "\u5535\u8a16\u54e9\u4e8c\u5408\u89a9\u5f15\u56a9\u7121\u83ab\u53cd\u4e00\u85a9\u56a9\u85a9\u601b\u56a9\u4e8c\u5408\u56c9\u4ed6", "322": "\u4e8c\u5408\u4e8c\u6089\u5730\u637a\u591a\u5f15\u4e09\u4e5f\u4ed6\u5f15\u52aa\u8a90\u5f15\u4e0a\u56db\u8616\u7e12\u6301", "323": "\u633d\u4e8c\u5408\u6c92\u99b1\u5c3e\u7051\u7130\u4e94\u5e03\u66e9\u56c9\u5f15\u8a90\u516d\u9ebc\u56ca\u5f15\u4e5f", "324": "\u90fd\u4e03\u5535\u6e34\u8a90\u85a9\u601b\u56a9\u4e8c\u5408\u76ee\u516b", "325": "\u4f5c\u6b64\u6cd5\u5df2\u3002\u91cd\u4ee5\u4e09\u9ebc\u8036\u5370\u8aa6\u52a0\u6301\u660e\u4ee5\u5370\u56db", "326": "\u8655\u3002\u7136\u5f8c\u704c\u9802\u3002\u88ab\u91d1\u525b\u7532\u5191\u3002\u4f9d\u524d\u56db\u79ae\u79ae\u56db", "327": "\u65b9\u4f5b\u3002\u61fa\u6094\u767c\u9858\u7b49\u3002\u7136\u5f8c\u4f9d\u9591\u975c\u8655\u3002\u56b4\u4ee5\u9999\u82b1", "328": "\u4f4f\u672c\u5c0a\u4e09\u6469\u5730\u3002\u8b80\u8aa6\u65b9\u5ee3\u4e00\u5207\u5927\u4e58\u7d93\u5178\u5927", "329": "\u822c\u82e5\u5927\u54c1\u3002\u4e43\u81f3\u6587\u6b8a\u822c\u82e5\u7b49\u3002\u96a8\u610f\u7d93\u884c\u3002", "33": "\u5b57\u8005\u7121\u81ea\u6027\u7fa9\u3002\u6a02\u6b32\u83e9\u63d0\u4e0d\u6368\u773e\u751f\u3002\u6df1\u5165\u7b2c", "330": "\u82e5\u6709\u667a\u8005\u4f9d\u6b64\u6cd5\u665d\u591c\u56db\u6642\u7cbe\u9032\u4fee", "331": "\u898b\u4e16\u8b49\u5f97\u6b61\u559c\u5730\u5f8c\u5341\u516d\u751f\u6210\u6b63\u89ba", "332": "\u6587\u6b8a\u5e2b\u5229\u5fff\u6012\u9640\u7f85\u5c3c", "333": "\u5535\u56a9\u65e5\u56c9\u4e8c\u5408\u5e95\u4e5e\u53c9\u62cf\u4e09\u5408\u4ff1\u5695\u4e8c\u5408\u99b1\u778b\u90a3", "334": "\u778b\u90a3\u543d\u5f15\u6cee\u5412", "335": "\u7528\u6b64\u771f\u8a00\u8b77\u8eab\u8f9f\u9664\u7d50\u754c\u3002\u6de8\u8af8\u9999\u82b1\u53ca\u4e00\u5207", "336": "\u4f9b\u5177\u7b49\u4e26\u5f97\u3002", "337": "\u7576\u9858\u773e\u751f\u9047\u6b64\u6559\u66fc\u6b8a\u5e38\u70ba\u5584\u77e5\u8b58", "338": "\u901f\u8b49\u822c\u82e5\u5584\u5de7\u667a\u75be\u6210\u7121\u4e0a\u5169\u8db3\u5c0a", "339": "\u4e94\u5b57\u9640\u7f85\u5c3c", "34": "\u4e00\u7fa9\u8ae6\u4e2d\u884c\u884c\u3002\u4fee\u7fd2\u8af8\u6cd5\u7121\u6709\u81ea\u6027\u3002\u82e5\u5982\u662f", "340": "\u963f\u56c9\u8ddb\u8005\u66e9\u5535\u56a9\u65e5\u56c9\u4e8c\u5408\u5e95\u4e5e\u53c9\u62cf\u4e09\u5408\u5535\u8028", "341": "\u4f49\u6cda\u53bb\u5a1c\u5535\u6e34\u8a90\u8616\u56c9\u4e8c\u5408\u6b20\u5e73\u5535\u6e34\u8a90\u85a9\u601b", "342": "\u56a9\u4e8c\u5408", "343": "\u91d1\u525b\u9802\u7d93\u745c\u4f3d\u6587\u6b8a\u5e2b\u5229\u83e9\u85a9\u6cd5\u4e00\u54c1", "35": "\u4fee\u6eff\u4e00\u5207\u9858\u3002\u6b64\u8af8\u9858\u4e2d\u8b49\u5982\u4f86\u4f4d\u53ca\u57f7\u91d1\u525b\u3002", "36": "\u4e0d\u6c42\u7576\u5f97\u3002", "37": "\u6211\u4eca\u53c8\u8aaa\u5951\u5370\u66fc\u8336\u7f85\u3002\u58c7\u4e2d\u756b\u91d1\u525b\u528d\u3002\u56db", "38": "\u9762\u5404\u65bc\u672c\u65b9\u3002\u756b\u516b\u4f9b\u990a\u5951\u53ca\u56db\u651d\u5951\u3002\u5c0d\u65bc\u6b64", "39": "\u58c7\u5ff5\u8aa6\u3002\u4e0d\u4e45\u5373\u7576\u6210\u5c31\u3002", "4": "\u8085\u570b\u516c\u98df\u9091\u4e09\u5343\u6236\u8cdc\u7d2b\u8d08\u53f8\u7a7a", "40": "\u6211\u4eca\u53c8\u8aaa\u4e09\u6469\u8036\u66fc\u837c\u7f85\u3002\u58c7\u4e2d\u66f8\u4e94\u5b57\u53ca", "41": "\u516b\u4f9b\u990a\u56db\u651d\u7a2e\u5b50\u5b57\u3002\u5c0d\u6b64\u58c7\u5ff5\u8aa6\u800c\u4f5c\u662f\u8a00\u3002", "42": "\u963f\u5b57\u9580\u8005\u8af8\u6cd5\u672c\u4e0d\u751f\u3002\u65e5\u65e5\u5ff5\u8aa6\u3002\u4e0d\u4e45\u4e00\u5207", "43": "\u7f6a\u969c\u6d88\u6ec5\u901f\u5f97\u6210\u5c31\u3002", "44": "\u6211\u4eca\u53c8\u8aaa\u7faf\u78e8\u66fc\u837c\u7f85\u3002\u58c7\u4e2d\u5b89\u822c\u82e5\u6ce2\u7f85\u871c", "45": "\u7d93\u5377\u3002\u65e5\u65e5\u8b80\u8aa6\u5ff5\u8aa6\u3002\u4ee5\u7a2e\u7a2e\u4f9b\u990a\u800c\u4f9b\u990a", "46": "\u4e4b\u3002\u4e0d\u4e45\u5373\u7576\u6210\u5c31\u3002", "47": "\u6211\u4eca\u7576\u8aaa\u756b\u50cf\u6cd5\u3002\u6216\u767d\u3cb2\u7d79\u7d20\u7b49\u4e2d\u3002\u756b\u6587\u6b8a", "48": "\u5e2b\u5229\u83e9\u85a9\u3002\u5750\u6708\u8f2a\u4e2d\u3002\u8f2a\u5167\u5468\u65cb\u66f8\u4e94\u5b57\u3002\u56db\u9762", "49": "\u756b\u516b\u4f9b\u990a\u53ca\u56db\u651d\u3002\u5982\u5927\u58c7\u6cd5\u3002\u5c0d\u6b64\u50cf\u524d\u5982\u6cd5", "5": "\u8b1a\u5927\u9452\u6b63\u865f\u5927\u5ee3\u667a\u5927\u8208\u5584\u5bfa", "50": "\u5ff5\u8aa6\u800c\u4f5c\u662f\u8a00\u3002\u8af8\u6cd5\u81ea\u6027\u6210\u5c31\u3002\u5ff5\u8aa6\u6578\u6eff\u4e94", "51": "\u5341\u842c\u904d\u3002\u5373\u7372\u7121\u76e1\u8faf\u624d\u3002\u5982\u6587\u6b8a\u5e2b\u5229\u83e9\u85a9\u7b49", "52": "\u7121\u6709\u7570\u3002\u98db\u9a30\u865b\u7a7a\u6240\u6c42\u4e16\u9593\u51fa\u4e16\u9593\u4e8b\u6089\u5f97", "53": "\u6210\u5c31\u3002", "54": "\u53c8\u5ff5\u8aa6\u6578\u6eff\u4e00\u4ff1\u80dd\u3002\u96e2\u8af8\u82e6\u60f1\u3002\u6eff\u4e8c\u4ff1\u80dd", "55": "\u904d\u3002\u4e94\u7121\u9593\u7b49\u4e00\u5207\u7f6a\u969c\u6c38\u76e1\u7121\u9918\u3002\u4e09\u4ff1\u80dd\u904d\u3002", "56": "\u8b49\u609f\u4e00\u5207\u8af8\u4e09\u6627\u9580\u3002\u56db\u4ff1\u80dd\u904d\u7372\u5927\u805e\u6301\u3002\u4e94", "57": "\u4ff1\u80dd\u904d\u6210\u963f\u8028\u591a\u7f85\u4e09\u85d0\u4e09\u83e9\u63d0\u3002", "58": "\u53c8\u6cd5\u65bc\u820d\u5229\u5854\u56db\u9762\u3002\u5468\u65cb\u53f3\u8f49\u66f8\u4e94\u5b57\u9640\u7f85", "59": "\u5c3c\u3002\u9076\u5854\u884c\u9053\u5ff5\u8aa6\u52ff\u4ee4\u65b7\u7d55\u3002\u6eff\u4e94\u843d\u53c9\u904d\u3002", "6": "\u4e09\u85cf\u6c99\u9580\u4e0d\u7a7a\u5949\u3000\u8a54\u8b6f", "60": "\u723e\u6642\u5982\u4f86\u53ca\u6587\u6b8a\u5e2b\u5229\u57f7\u91d1\u525b\u7b49\u3002\u65bc\u865b\u7a7a\u4e2d", "61": "\u800c\u73fe\u5176\u8eab\u4ecd\u70ba\u8aaa\u6cd5\u3002", "62": "\u91d1\u525b\u9802\u7d93\u745c\u4f3d\u6587\u6b8a\u5e2b\u5229\u83e9\u85a9\u5100\u8ecc\u4f9b\u990a\u6cd5\u54c1\u91d1\u525b\u9802\u7d93\u745c\u4f3d\u6587\u6b8a\u5e2b\u5229\u83e9\u85a9\u5100\u8ecc\u4f9b\u990a\u6cd5", "63": "\u4e00\u54c1", "64": "\u6b78\u547d\u7ae5\u771f\u5999\u5409\u7965\u6211\u4f9d\u745c\u4f3d\u8aaa\u5ff5\u8aa6", "65": "\u8eab\u53e3\u610f\u696d\u91d1\u525b\u5ff5\u5982\u4f86\u751a\u6df1\u4e09\u5bc6\u9580", "66": "\u884c\u8005\u61c9\u767c\u666e\u8ce2\u5fc3\u5f9e\u5e2b\u61c9\u53d7\u91d1\u525b\u6212", "67": "\u4e0d\u9867\u8eab\u547d\u8d77\u6148\u60b2\u65b9\u53ef\u582a\u5165\u89e3\u812b\u8f2a", "68": "\u61c9\u5f9e\u5e2b\u53d7\u4e09\u9ebc\u8036\u5951\u5370\u5bc6\u8a9e\u5982\u7d93\u8aaa", "69": "\u656c\u963f\u95cd\u68a8\u5982\u4f5b\u60f3\u65bc\u540c\u5b78\u6240\u6147\u91cd\u5fc3", "7": "\u723e\u6642\u6587\u6b8a\u5e2b\u5229\u83e9\u85a9\u3002\u5728\u6bd8\u76e7\u906e\u90a3\u5927\u6703\u4e2d\u3002\u5f9e\u5ea7", "70": "\u6216\u65bc\u5c71\u9593\u963f\u862d\u82e5\u6d41\u6cc9\u6d74\u6c60\u6085\u610f\u6a39", "71": "\u5c71\u5cef\u77f3\u7a9f\u9008\u6a39\u908a\u5efa\u7acb\u58c7\u5834\u5982\u672c\u6cd5", "72": "\u838a\u56b4\u7cbe\u5ba4\u7f6e\u672c\u5c0a\u96a8\u529b\u4f9b\u990a\u4e00\u5fc3\u4f4f", "73": "\u904d\u89c0\u5341\u65b9\u8af8\u4f5b\u6d77\u4f9b\u990a\u79ae\u8af8\u5982\u4f86\u8db3", "74": "\u70ba\u6210\u4e09\u696d\u91d1\u525b\u6545\u7576\u65bc\u4e8c\u624b\u820c\u5fc3\u4e2d", "75": "\u61c9\u60f3\u4e94\u667a\u91d1\u525b\u6775\u7531\u6b64\u52a0\u6301\u7686\u6089\u5730", "76": "\u6b21\u61c9\u7d50\u5951\u540d\u8b66\u89ba\u4e8c\u624b\u7686\u4f5c\u91d1\u525b\u62f3", "77": "\u6a80\u6167\u76f8\u9264\u7aea\u9032\u529b\u4e8c\u5ea6\u5074\u62c4\u6210\u89ba\u609f", "78": "\u8b66\u89ba\u771f\u8a00\u66f0\u3002", "79": "\u5535\u56a9\u65e5\u56c9\u4e8c\u5408\u5e95\u745f\u59f9\u4e8c\u5408", "8": "\u800c\u8d77\u9802\u79ae\u4f5b\u8db3\u3002\u767d\u4f5b\u8a00\u4e16\u5c0a\u3002\u6211\u4eca\u8aaa\u672c\u4e94\u5b57", "80": "\u656c\u79ae\u6771\u65b9\u963f\u95a6\u5c0a\u6368\u8eab\u6c42\u8acb\u4e0d\u9000\u8f49", "81": "\u5168\u8eab\u8457\u5730\u4ee5\u5fc3\u79ae\u91d1\u525b\u5408\u638c\u8212\u9802\u4e0a", "82": "\u6368\u8eab\u6c42\u8acb\u52a0\u6301\u771f\u8a00\u66f0\u3002", "83": "\u5535\u85a9\u56a9\u601b\u4ed6\u8616\u591a\u4e00\u5e03\u5f15\u5112\u8ddb\u5a11\u4ed6\u4e8c\u5408\u5f15\u56ca\u5f15", "84": "\u8036\u5f15\u7b54\u9ebc\u4e8c\u5408\u5f15\u5357\u4e8c\u6d85\u54e9\u591c\u4e8c\u5408\u5f15\u591a\u591c\u5f4c\u4e09\u85a9\u56a9", "85": "\u601b\u4ed6\u5f15\u8616\u591a\u56db\u56a9\u65e5\u56c9\u4e8c\u5408\u85a9\u601b\u56a9\u4e8c\u5408\u5730\u745f", "86": "\u59f9\u4e8c\u5408\u5a11\u56a9\u4e8c\u5408\ud851\udea5\u5fc3\u60f3\u543d\u5b57\u9752\u8272", "87": "\u6b21\u7576\u656c\u79ae\u5bf6\u751f\u5c0a\u6368\u8eab\u6c42\u8acb\u704c\u9802\u4f4d", "88": "\u91d1\u525b\u5408\u638c\u7576\u65bc\u5fc3\u4ee5\u984d\u8457\u5730\u8654\u8aa0\u79ae", "89": "\u6368\u8eab\u6c42\u8acb\u704c\u9802\u771f\u8a00\u66f0\u3002", "9": "\u9640\u7f85\u5c3c\u3002\u82e5\u6709\u5584\u7537\u5b50\u5584\u5973\u4eba\u3002\u7e94\u8aa6\u4e00\u904d\u8005\u3002\u4e00", "90": "\u5535\u85a9\u56a9\u601b\u4ed6\u8616\u591a\u5e03\u60f9\u6bd8\u66ec\u8fe6\u5f15\u8036\u5f15\u7b54\u9ebc\u4e8c\u5408", "91": "\u5357\u6d85\u54e9\u591c\u4e8c\u5408\u591a\u591c\u5f4c\u85a9\u56a9\u601b\u4ed6\u5f15\u8616\u591a\u56a9\u65e5", "92": "\u56c9\u4e8c\u5408\u56c9\u601b\u90a3\u4e8c\u5408\u6bd8\u8a75\u5de6\ud851\udea5\u984d\u60f3\u9ec3\u8272", "93": "\u6b21\u79ae\u89c0\u81ea\u5728\u738b\u5c0a\u6368\u8eab\u6c42\u8acb\u4e09\u9ebc\u5730", "94": "\u91d1\u525b\u5408\u638c\u7f6e\u9802\u4e0a\u4ee5\u53e3\u8457\u5730\u8654\u8aa0\u79ae", "95": "\u6368\u8eab\u6c42\u8acb\u4e09\u6627\u771f\u8a00\u66f0\u3002", "96": "\u5535\u85a9\u56a9\u601b\u4ed6\u5f15\u8616\u591a\u5e03\u60f9\u9262\u56c9\u4e8c\u5408\u977a\u591a\u56ca\u5f15", "97": "\u7b54\u9ebc\u4e8c\u5408\u5357\u6d85\u54e9\u591c\u4e8c\u5408\u591a\u591c\u5f4c\u85a9\u56a9\u601b\u4ed6\u5f15", "98": "\u8616\u591a\u56a9\u65e5\u56c9\u4e8c\u5408\u9054\u9ebc\u9262\u56c9\u4e8c\u5408\u977a\u591a\u91ce\ud851\udea5\u53e3", "99": "\u60f3\u8d64"}, "source": "CBETA", "language": "chinese", "originalTitle": "Taisho Tripitaka, Electronic version, No. 1171 \u91d1\u525b\u9802\u7d93\u745c\u4f3d\u6587\u6b8a\u5e2b\u5229\u83e9\u85a9\u6cd5", "author": "\u5510 \u4e0d\u7a7a\u8b6f", "sourceLink": "http://www.cbeta.org/"}