951\u3002\u53ca\u8b8a\u5316\u7b49\u7a2e\u7a2e\u4e4b\u6cd5\u3002", "128": "\u7686\u9808\u65e5\u5225\u4e09\u6642\u4f5c\u5176\u89c0\u60f3\u3002\u5f97\u60f3\u61c9\u5df2\u3002\u65b9\u53ef\u904b", "129": "\u5fc3\u4f5c\u7a2e\u7a2e\u6cd5\u3002\u5982\u6b64\u7d93\u4e00\u6708\u6216\u4e09\u6708\u4e43\u81f3\u516d\u6708\u3002", "130": "\u4fee\u5176\u5148\u884c\u4ee4\u5f97\u7cbe\u719f\u3002\u4ecd\u6c42\u8af8\u4f5b\u8ce2\u8056\u5a01\u529b\u52a0", "131": "\u88ab\u793a\u73fe\u5883\u50cf\u3002\u5f97\u5409\u7965\u5df2\u3002\u7136\u5f8c\u8d77\u9996\u64c7\u5730\u4f5c\u66fc", "132": "\u62cf\u7f85\u3002\u53ca\u7c89\u756b\u8056\u8ce2\u7b49\u3002", "133": "\u5fa9\u6b21\u963f\u95cd\u68a8\u6b32\u5efa\u66fc\u62cf\u7f85\u3002\u9808\u64c7\u52dd\u5730\u6216\u5bfa\u820d\u3002", "134": "\u6216\u805a\u843d\u5712\u6797\u7b49\u8655\u3002\u6216\u5f97\u66fe\u7d50\u754c\u5730\u4e0d\u9808\u4fee\u6301\u3002", "135": "\u5982\u975e\u7d50\u754c\u5730\u7576\u6398\u6df1\u81f3\u9838\u3002\u6216\u81f3\u81cd\u6216\u81f3\u819d\u3002\u963f", "136": "\u95cd\u68a8\u6b63\u8996\u3002\u82e5\u6709\u7070\u70ad\u7ce0\u9aa8\u4e43\u81f3\u7802\u77f3\u4e4b\u985e\u3002\u7576", "137": "\u6089\u53bb\u9664\u3002\u5225\u7528\u9999\u6de8\u597d\u571f\u586b\u6eff\u5e73\u6b63\u3002\u963f\u95cd\u68a8\u8aa6", "138": "\u4f5b\u773c\u83e9\u85a9\u91d1\u525b\u591c\u53c9\u660e\u738b\u53ca\u7518\u9732\u8ecd\u8336\u68a8\u771f", "139": "\u8a00\u3002\u52a0\u6301\u6c34\u7051\u6de8\u5176\u5730\u3002\u963f\u95cd\u68a8\u5373\u65bc\u5f7c\u8655\u3002\u5165\u91d1", "140": "\u525b\u767c\u9063\u8af8\u9b54\u4e09\u6469\u5730\u3002\u53c8\u60f3\u5fc3\u4e2d\u5f9e\u771f\u8a00\u3002\u51fa\u751f", "141": "\u5927\u5fff\u6012\u660e\u738b\u3002\u4e8c\u76ee\u5982\u65e5\u6708\u3002\u624b\u6301\u7faf\u78e8\u6775\u5149\u7130", "142": "\u71be\u76db\u3002\u4f5c\u91d1\u525b\u6b65\u5de6\u53f3\u9867\u8996\u3002\u65bc\u4e00\u524e\u90a3\u4e2d\u80fd\u964d", "143": "\u4e00\u5207\u9b54\u3002\u963f\u95cd\u68a8\u5373\u5de6\u624b\u6301\u9234\u3002\u53f3\u624b\u6301\u91d1\u525b\u6775\u3002", "144": "\u4f5c\u91d1\u525b\u6b65\u884c\u66fc\u62cf\u7f85\u5730\u3002\u4e43\u81f3\u91d1\u525b\u570d\u3002\u53c8\u5fa9\u7d50", "145": "\u6839\u672c\u5370\u3002\u8aa6\u9063\u9b54\u771f\u8a00\u53ca\u7a31\u543d\u5b57\u3002\u5982\u53f3\u821e\u52e2\u53ca", "146": "\u5982\u660e\u738b\u81ea\u5728\u76f8\u3002\u65cb\u7e5e\u5f7c\u5730\u3002\u8aa6\u771f\u8a00\u767c\u9063\u4e00\u5207", "147": "\u9b54\u3002\u771f\u8a00\u66f0\u3002", "148": "\u5535\u5f15\u56a9\u65e5\u56c9\u4e8c\u5408\u8a08\u54e9\u5409\u54e9\u4e00\u85a9\u54e9\u56a9\u4e8c\u5408\u5c3e\u4f3d", "149": "\u66e9\u4e8c\u5408\u6eff\u99b1\u543d\u5f15\u767c\u5412\u534a\u97f3\u4e8c", "150": "\u6b21\u8aa6\u91d1\u525b\u6a5b\u771f\u8a00\u3002\u4ee5\u6a5b\u91d8\u5730\u771f\u8a00\u66f0\u3002", "151": "\u5535\u5f15\u7aed\u7aed\u4e00\u4f3d\u5f15\u601b\u91ce\u4f3d\u5f15\u601b\u91ce\u4e8c\u85a9\u54e9\u56a9\u4e8c\u5408", "152": "\u8a25\u745f\u9d6e\u4e8c\u5408\u5f15\u767c\u5412\u534a\u97f3\u4e09\u8a08\u5f15\u7f85\u91ce\u8a08\u5f15\u7f85\u91ce\u56db\u85a9", "153": "\u54e9\u56a9\u4e8c\u5408\u64ad\u5f15\u8b17\u767c\u5412\u534a\u97f3\u4e94\u56a9\u65e5\u56c9\u4e8c\u5408\u8a08\u5f15\u7f85\u516d\u56a9", "154": "\u65e5\u56c9\u4e8c\u5408\u9054\u5695\u502a\u4e5f\u4e8c\u5408\u5f15\u9262\u91ce\u5e95\u5a11\u56a9\u4e8c\u5408\u5f15\u8cc0\u5f15\u4e03", "155": "\u963f\u95cd\u68a8\u4f5c\u66fc\u62cf\u7f85\u3002\u9078\u5f97\u52dd\u5730\u5df2\u3002\u5373\u5148\u4ee5\u6240\u70ba", "156": "\u4e8b\u91cf\u5176\u5927\u5c0f\u3002\u82e5\u70ba\u570b\u738b\u7576\u4f5c\u4e00\u767e\u8098\u6216\u4e94\u5341", "157": "\u8098\u3002\u82e5\u70ba\u5927\u81e3\u4e43\u81f3\u5eb6\u6c11\u3002\u53ef\u4f5c\u4e8c\u5341\u4e94\u8098\u6216\u5341", "158": "\u4e8c\u8098\u3002\u82e5\u7121\u529b\u8005\u53ef\u4f5c\u4e00\u8098\u3002\u5982\u662f\u91cf\u5ea6\u5df2\u3002\u5373\u7528", "159": "\u77bf\u6469\u5937\u5857\u7562\u3002\u5fa9\u7528\u52a0\u6301\u4e94\u6de8\u6c34\u7051\u6de8\u3002\u53c8\u7528\u5857", "160": "\u9999\u5857\u98fe\u7562\u5df2\u3002\u7136\u5f8c\u963f\u95cd\u68a8\u6c90\u6d74\u6f54\u6de8\u3002\u4ee5\u9999\u5857", "161": "\u8eab\u8457\u65b0\u9bae\u8863\u9802\u6234\u5bf6\u51a0\u3002\u53ca\u4ee5\u82b1\u9b18\u88dd\u98fe\u56b4\u6f54", "162": "\u5df2\u3002\u5373\u65bc\u66fc\u62cf\u7f85\u5730\u5148\u4f5c\u8b77\u6469\u3002\u7576\u7528\u6fd5\u67f4\u6709\u523a", "163": "\u9577\u516b\u6307\u516b\u7a1c\u8005\u3002\u5fa9\u7528\u6bd2\u85e5\u82a5\u5b50\u8840\u4eba\u9aa8\u672b\u7b49\u3002", "164": "\u4ee5\u82e6\u8fa3\u6cb9\u8abf\u548c\u3002\u8207\u6fd5\u67f4\u540c\u71d2\u4f5c\u8b77\u6469\u3002\u4ee5\u53f3\u624b", "165": "\u4f5c\u5f48\u6307\u8aa6\u771f\u8a00\u66f0\u3002", "166": "\u5535\u5f15\u85a9\u54e9\u56a9\u4e8c\u5408\u5c3e\u5484\u4e00\u56a9\u65e5\u56c9\u4e8c\u5408\u4e09\u6469\u5f15\u60f9", "167": "\ud843\ude81\u4ec1\u4f5c\u5207\u543d\u5f15\ud851\udea5\u547c\u4e8c", "168": "\u82e5\u5f97\u5df2\u7d50\u754c\u5730\u4e0d\u9808\u586b\u98fe\u3002\u5373\u5148\u65bc\u5f7c\u5730\u3002\u91cf\u5ea6", "169": "\u5ee3\u95ca\u53ef\u4e00\u725b\u76ae\u3002\u7528\u77bf\u6469\u5937\u5857\u98fe\u3002\u65bc\u4e0a\u4f9d\u6cd5\u5b89", "170": "\u4e94\u95bc\u3002\u4f3d\u74f6\u4f5c\u8b77\u6469\u6cd5\u3002\u7576\u7528\u6fd5\u67f4\u53ca\u9ec3\u677e\u6728\u9577", "171": "\u5341\u4e8c\u6307\u3002\u9ed1\u6cb9\u9ebb\u5927\u9ea5\u5c0f\u9ea5\u7cb3\u7c73\u98ef\u9165\u7b49\u3002\u5148\u4f5c", "172": "\u8b77\u6469\u800c\u544a\u5730\u5929\u8a00\u3002\u6211\u4eca\u70ba\u570b\u738b\u6216\u5927\u81e3\u4e43\u81f3", "173": "\u5f1f\u5b50\u7737\u5c6c\u7b49\u3002\u65bc\u6b64\u5730\u4f5c\u66fc\u62cf\u7f85\u3002\u70ba\u5229\u76ca\u6545\u6c5d", "174": "\u7b49\u5730\u5929\u767c\u6148\u60b2\u5fc3\u7576\u70ba\u65bc\u6211\u800c\u4f5c\u5584\u4e8b\u3002", "175": "\u5982\u662f\u8b77\u6469\u5df2\u80fd\u6210\u4e00\u5207\u4e8b\u3002\u5176\u963f\u95cd\u68a8\u7136\u5f8c\u8207", "176": "\u5176\u52a9\u4f34\u3002\u57f7\u7dda\u7d63\u91cf\u66fc\u62cf\u7f85\u3002\u7dda\u7528\u4e94\u8272\u3002\u4ee5\u9999\u6c34", "177": "\u7d93\u5bbf\u6d78\u6f2c\u3002\u7136\u5f8c\u7528\u91d1\u5668\u76db\u3002\u4e8c\u624b\u6367\u6301\u8aa6\u4e94\u4f5b", "178": "\u9802\u771f\u8a00\u3002\u52a0\u6301\u5df2\u5949\u737b\u4e94\u4f5b\u3002\u5fa9\u5f9e\u4e94\u4f5b\u5374\u6c42\u6b64", "179": "\u7dda\u3002\u9858\u4f5b\u6148\u60b2\u5171\u8cdc\u8207\u6211\u3002\u5373\u4ee5\u4e94\u8272\u90fd\u5408\u70ba\u4e00\u3002", "180": "\u8aa6\u4f3d\u9640\u66f0\u3002", "181": "\u5206\u5225\u4e00\u5207\u6cd5\u4eca\u6b63\u4e92\u76f8\u651d", "182": "\u771f\u5be6\u7406\u7121\u4e8c\u6b64\u7dda\u4ea6\u5982\u662f", "183": "\u53c8\u5fa9\u6b64\u7dda\u77ed\u9577\u4e4b\u6578\u96a8\u66fc\u62cf\u7f85\u91cf\u90fd\u5408\u6210\u5df2\u3002", "184": "\u5373\u8aa6\u771f\u8a00\u52a0\u6301\u771f\u8a00\u66f0\u3002", "185": "\u5535\u5f15\u56a9\u65e5\u56c9\u4e8c\u5408\u7d20\u601b\u56c9\u4e8c\u5408\u963f\u5f15\u54e5\u54e9\u6c99\u4e8c\u5408\u91ce", "186": "\u66fc\u62cf\u54e9\u5f15\u543d\u5f15\u4e00", "187": "\u963f\u95cd\u68a8\u5982\u662f\u52a0\u6301\u7dda\u5df2\u3002\u5373\u8207\u52a9\u4f34\u7acb\u66fc\u62cf\u7f85", "188": "\u897f\u5357\u9685\u3002\u8aa6\u771f\u8a00\u66f0\u3002", "189": "\u5535\u5f15\u56a9\u65e5\u56c9\u4e8c\u5408\u7d20\u601b\u56c9\u4e8c\u5408\u4e00\u9ebc\u54e5\u54e9\u6c99\u4e8c\u5408", "190": "\u91ce\u672b\u5e95\u8a16\u56c9\u4e8c\u5408\u6469\u543d\u5f15\u4e8c", "191": "\u8aa6\u771f\u8a00\u5df2\u3002\u963f\u95cd\u68a8\u6301\u7dda\u4e00\u982d\u9762\u6771\u884c\u3002\u81f3\u6771", "192": "\u5357\u9685\u7acb\u3002\u52a9\u4f34\u6301\u7dda\u4e00\u982d\u5411\u5317\u884c\u3002\u81f3\u897f\u5317\u9685\u7acb\u3002", "193": "\u7d63\u5df2\u3002\u6b21\u963f\u95cd\u68a8\u5411\u5317\u884c\u81f3\u6771\u5317\u9685\u52a9\u4f34\u5411\u5357", "194": "\u884c\u81f3\u897f\u5357\u9685\u3002\u7d63\u7dda\u5982\u662f\u65cb\u7e5e\u56db\u65b9\u7d63\u7dda\u3002\u7b2c", "195": "\u4e8c\u7b2c\u4e09\u91cd\u4ea6\u540c\u6b64\u6b21\u7b2c\u3002\u7d63\u7dda\u82e5\u4f9d\u6cd5\u8005\u540d\u70ba", "196": "\u5177\u8db3\u66fc\u62cf\u7f85\u3002\u82e5\u4e0d\u4f9d\u6cd5\u3002\u963f\u95cd\u68a8\u8207\u52a9\u4f34\u4ff1\u6709", "197": "\u904e\u5931\u3002\u5176\u66fc\u62cf\u7f85\u56db\u65b9\u56db\u9580\u3002\u9580\u5404\u6709\u6a13\u3002\u65bc\u9580\u6a13", "198": "\u4e0a\u756b\u65e5\u6708\u53ca\u5bf6\u74d4\u73de\u96a8\u610f\u56b4\u98fe\u3002\u65bc\u4e2d\u5fc3\u756b\u516b", "199": "\u8f3b\u8f2a\u3002\u8f2a\u5916\u756b\u91d1\u525b\u570d\u3002\u5148\u65bc\u8f2a\u5fc3\u756b\u91cb\u8fe6\u4f5b\u3002", "200": "\u6771\u8f3b\u4f4d\u756b\u91d1\u525b\u5927\u4f5b\u9802\u5982\u4f86\u3002\u5357\u8f3b\u4f4d\u756b\u5bf6\u751f", "201": "\u4f5b\u9802\u3000\u897f\u8f3b\u4f4d\u756b\u84ee\u82b1\u4f5b\u9802\u3002\u5317\u8f3b\u4f4d\u756b\u7faf\u78e8\u4f5b", "202": "\u9802\u3002\u5982\u662f\u756b\u56db\u5982\u4f86\u5df2\u3002\u6b21\u5f9e\u6771\u5357\u9685\u756b\u5149\u660e\u4f5b", "203": "\u9802\u3002\u897f\u5357\u9685\u756b\u5bf6\u5e62\u4f5b\u9802\u3002\u897f\u5317\u9685\u756b\u91d1\u525b\u5229\u4f5b", "204": "\u9802\u3002\u6771\u5317\u9685\u756b\u767d\u7e56\u84cb\u4f5b\u9802\u3002\u5982\u662f\u65bc\u8f2a\u516b\u8f3b\u3002", "205": "\u756b\u516b\u5982\u4f86\u5df2\u3002\u6b21\u65bc\u8f2a\u56db\u9685\u756b\u56db\u89aa\u8fd1\u83e9\u85a9\u3002\u65bc", "206": "\u7b2c\u4e8c\u91cd\u56db\u9685\u756b\u6232\u9b18\u6b4c\u821e\u56db\u83e9\u85a9\u3002\u65bc\u7b2c\u4e09\u91cd", "207": "\u56db\u9685\u3002\u756b\u9999\u82b1\u71c8\u5857\u56db\u83e9\u85a9\u3002\u65bc\u56db\u9580\u756b\u56db\u8b77\u9580", "208": "\u83e9\u85a9\u3002\u6b21\u65bc\u56db\u65b9\u756b\u5341\u516d\u5927\u83e9\u85a9\u3002\u65bc\u56db\u9580\u5916\u5404", "209": "\u65bc\u9830\u3002\u756b\u8c61\u53ca\u5e2b\u5b50\u3002\u65bc\u9580\u4e8c\u908a\u756b\u516b\u5409\u7965\u3002\u65bc\u5c71", "210": "\u570d\u5167\u756b\u516b\u8b77\u4e16\u3002\u6771\u65b9\u756b\u5e1d\u91cb\u5929\u3002\u53f3\u624b\u6301\u91d1", "211": "\u525b\u5fe4\u5de6\u624b\u5b89\u8170\u5074\u3002\u4e58\u516d\u7259\u767d\u8c61\u3002\u5169\u908a\u756b\u5929\u5973\u3002", "212": "\u4e00\u540d\u8a2d\u5c38\u4e8c\u540d\u70cf\u54e9\u56a9\u5c38\u3002\u6771\u5357\u65b9\u756b\u706b\u5929\u3002\u8eab", "213": "\u8d64\u8272\u5149\u660e\u71be\u76db\u3002\u53f3\u624b\u57f7\u8ecd\u6301\u5de6\u624b\u57f7\u5bf6\u6756\u3002\u4e58", "214": "\u8d64\u8272\u7f96\u7f8a\u3002\u5357\u65b9\u756b\u591c\u6469\u5929\u3002\u8eab\u4f5c\u7da0\u8272\u3002\u5de6\u624b\u4f5c", "215": "\u62f3\u5b89\u8170\u5074\u3002\u53f3\u624b\u57f7\u5bf6\u6756\u3002\u4e58\u65bc\u6c34\u725b\u3002\u5169\u908a\u756b\u4e2d", "216": "\u5929\u50cf\u3002\u897f\u5357\u65b9\u756b\u7f85\u524e\u4e3b\u3002\u8eab\u8d64\u9ed1\u8272\u5149\u5982\u706b\u805a\u3002", "217": "\u53f3\u624b\u57f7\u528d\u5de6\u624b\u5b89\u8170\u5074\u3002\u4ee5\u9b3c\u70ba\u5ea7\u3002\u897f\u65b9\u756b\u6c34", "218": "\u5929\u3002\u8eab\u9ec3\u767d\u8272\u982d\u4e0a\u73fe\u9f8d\u982d\u3002\u53f3\u624b\u6301\u7f82\u7d22\u5de6\u624b", "219": "\u6301\u5999\u5bf6\u3002\u4e58\u8d64\u8272\u6469\u7aed\u9b5a\u3002\u5169\u908a\u756b\u9f8d\u5973\u53ca\u7737\u5c6c", "220": "\u7b49\u3002\u897f\u5317\u9685\u756b\u98a8\u5929\u3002\u8eab\u5982\u865b\u7a7a\u8272\u3002\u624b\u6301\u5e61\u4e58", "221": "\u65bc\u9e7f\u3002\u5317\u65b9\u756b\u591c\u53c9\u4e3b\u3002\u8eab\u5982\u91d1\u8272\u3002\u53f3\u624b\u57f7\u5bf6\u6a39", "222": "\u679d\u5de6\u624b\u6301\u5bf6\u56ca\u3002\u4e58\u65bc\u767d\u99ac\u3002\u5169\u908a\u756b\u591c\u53c9\u5973\u3002\u6771", "223": "\u5317\u9685\u756b\u90e8\u591a\u4e3b\u3002\u8eab\u5982\u7070\u8272\u3002\u53f3\u624b\u6301\u4e09\u53c9\u5de6\u624b", "224": "\u6301\u91d1\u525b\u5b50\u8ecd\u6301\u3002\u4e58\u65bc\u9ec3\u725b\u3002\u65bc\u66fc\u62cf\u7f85\u524d\u4e0b\u9762\u3002", "225": "\u756b\u5730\u5929\u3002\u8eab\u5982\u91d1\u8272\u3002\u4e8c\u624b\u65bc\u80f8\u524d\u6301\u7518\u9732\u74f6\u3002\u5750", "226": "\u84ee\u82b1\u5ea7\u3002\u65bc\u53f3\u908a\u756b\u963f\u4fee\u7f85\u3002\u8eab\u8d64\u9ed1\u8272\u88ab\u639b\u7532", "227": "\u5191\u3002\u624b\u57f7\u5229\u528d\u4e58\u9164\u91cc\u54e5\u9f8d\u3002\u65bc\u5de6\u908a\u756b\u5927\u9f8d\u738b\u3002", "228": "\u8eab\u4f5c\u767d\u8272\u5750\u65bc\u672c\u5ea7\u3002\u624b\u6301\u5bf6\u5668\u773e\u5bf6\u76c8\u6eff\u3002\u53ca", "229": "\u756b\u4e8c\u9f8d\u5973\u3002\u8eab\u4f5c\u9ec3\u8272\u5177\u7aef\u56b4\u76f8\u3002\u624b\u6301\u822c\u82e5\u7d93", "230": "\u6578\u73e0\u8ecd\u6301\u53ca\u65bd\u9858\u76f8\u3002\u65bc\u66fc\u62cf\u7f85\u524d\u4e0a\u9762\u3002\u5de6\u756b", "231": "\u65e5\u8f2a\u53f3\u756b\u5713\u6708\u3002\u65bc\u5c71\u570d\u5916\u6771\u5357\u9685\u3002\u756b\u963f\u9f3b\u7b49", "232": "\u516b\u5730\u7344\u3002\u897f\u5357\u9685\u756b\u9913\u9b3c\u8da3\u3002\u897f\u5317\u9685\u756b\u7a2e\u7a2e\u755c", "233": "\u8da3\u3002\u6771\u5317\u9685\u756b\u4eba\u8da3\u3002\u5982\u662f\u756b\u5df2\u3002\u963f\u95cd\u68a8\u65bc\u66fc\u62cf", "234": "\u7f85\u4e2d\u3002\u737b\u606d\u4ff1\u6469\u9999\u53ca\u7a2e\u7a2e\u9999\u82b1\u98f2\u98df\u7b49\u3002\u53c8\u7528", "235": "\u6eff\u95bc\u4f3d\u74f6\u9999\u6c34\u3002\u8aa6\u91d1\u525b\u591c\u53c9\u771f\u8a00\u52a0\u6301\u3002\u53ca\u66fc", "236": "\u62cf\u7f85\u6240\u7528\u7a2e\u7a2e\u7269\u3002\u5404\u4ee5\u672c\u771f\u8a00\u52a0\u6301\u5df2\u3002\u5949\u737b", "237": "\u8af8\u8ce2\u8056\u7686\u6089\u4ee4\u6b61\u559c\u3002\u5fa9\u7528\u91d1\u525b\u591c\u53c9\u771f\u8a00\u3002\u52a0", "238": "\u6301\u66fc\u62cf\u7f85\u5df2\u3002\u5373\u8aa6\u543d\u5b57\u56db\u904d\u3002\u53d6\u51fa\u7d50\u754c\u91d1\u525b", "239": "\u6a5b\u3002\u5373\u5c0b\u4ee5\u4e94\u8272\u7c89\u586b\u6eff\u4fee\u4ee4\u5e73\u6b63\u3002\u7136\u5f8c\u963f\u95cd", "240": "\u68a8\u5f9e\u6771\u5317\u9685\u8d77\u9996\u3002\u4e0b\u4e94\u8272\u7c89\u4f5c\u8af8\u4f5b\u8ce2\u8056\u50cf\u3002", "241": "\u7576\u4e0b\u7c89\u6642\u4ee5\u767d\u9752\u8d64\u7da0\u70ba\u6b21\u3002\u4e2d\u5fc3\u7528\u9ec3\u8272\u70ba", "242": "\u516b\u8f3b\u8f2a\u3002\u6771\u65b9\u7d14\u767d\u8272\u3002\u5357\u65b9\u5927\u9752\u8272\u3002\u897f\u65b9\u8d64\u8272\u3002", "243": "\u5317\u65b9\u7da0\u8272\u3002\u5982\u662f\u7c89\u8ce2\u8056\u50cf\u7562\u3002\u963f\u95cd\u68a8\u5fa9\u540c\u524d", "244": "\u89c0\u60f3\u865b\u7a7a\u66fc\u62cf\u7f85\u3002\u5148\u4ee5\u4e8c\u624b\u91d1\u525b\u6307\u4f5c\u5805\u4ef0", "245": "\u76f8\u3002\u53ca\u60f3\u771f\u8a00\u8d77\u865b\u7a7a\u66fc\u62cf\u7f85\u3002\u5fa9\u540c\u524d\u60f3\u5408\u70ba", "246": "\u4e00\u6210\u7562\u7adf\u3002\u66fc\u62cf\u7f85\u5df2\u3002\u5373\u5fa9\u65bc\u66fc\u62cf\u7f85\u5357\u9580\u5916\u3002", "247": "\u5225\u5efa\u4e00\u5c0f\u81fa\u3002\u56db\u65b9\u5ee3\u95ca\u53ef\u4e00\u8098\u91cf\u3002\u4ee5\u4e0d\u58ae\u5730", "248": "\u77bf\u6469\u5937\u5857\u4e4b\u3002\u5f8c\u7528\u4e94\u6de8\u6c34\u7051\u6de8\u3002\u5fa9\u4ee5\u767d\u6a80\u9999\u3002", "249": "\u65bc\u81fa\u4e2d\u5fc3\u5857\u66fc\u62cf\u7f85\u3002\u5982\u5713\u6708\u76f8\u3002\u65bc\u4e0a\u737b\u9999\u82b1", "250": "\u98f2\u98df\u7a2e\u7a2e\u4f9b\u990a\u3002\u4e43\u81f3\u51fa\u751f\u7b49\u3002\u5fa9\u92ea\u9752\u5409\u7965\u8349\u3002", "251": "\u4e0a\u5b89\u95bc\u4f3d\u74f6\u74f6\u5167\u76db\u4e94\u5bf6\u4e94\u7a40\u4e94\u85e5\u3002\u53ca\u63d2\u7a2e", "252": "\u7a2e\u82b1\u679c\u6a39\u679d\u8449\u7b49\u3002\u65bc\u74f6\u9805\u4e0a\u7e6b\u4e00\u9752\u8863\u3002\u7136\u4ee5", "253": "\u672c\u771f\u8a00\u52a0\u6301\u9858\u4ee5\u6b64\u6c34\u4f5c\u5927\u5229\u76ca\u3002\u53c8\u8aa6\u672c\u771f", "254": "\u8a00\u52a0\u6301\u767d\u82a5\u5b50\u53ca\u767d\u82b1\u7b49\u3002\u53e3\u8aa6\u771f\u8a00\u3002\u4ee5\u767d\u82a5", "255": "\u5b50\u53ca\u767d\u82b1\u3002\u64ca\u4eba\u6216\u64ca\u4eba\u540d\u53ca\u8863\u7b49\u3002\u5fa9\u4ee5\u95bc", "256": "\u4f3d\u6c34\u704c\u7051\u3002\u5982\u662f\u6240\u4f5c\u606f\u707d\u589e\u76ca\u7b49\u4e8b\u7686\u7372\u529f", "257": "\u5fb7\u7121\u91cf\u7121\u908a\u3002\u4e43\u81f3\u64ca\u4ea1\u4eba\u8eab\u9aa8\u53ca\u540d\u3002\u7576\u4f7f\u4ea1", "258": "\u4eba\u96e2\u8af8\u60e1\u8da3\u5f80\u751f\u5929\u754c\u771f\u8a00\u66f0\u3002", "259": "\u5535\u5f15\u85a9\u54e9\u56a9\u4e8c\u5408\u64ad\u5f15\u6ce2\u637a\u8cc0\u90a3\u4e00\u56a9\u65e5\u56c9\u4e8c\u5408", "260": "\u543d\u5f15\u767c\u5412\u534a\u97f3\u4e8c", "261": "\u5535\u5f15\u85a9\u54e9\u56a9\u4e8c\u5408\u64ad\u5f15\u6ce2\u5c3e\u8f38\u9054\u90a3\u4e00\u56a9\u65e5\u56c9", "262": "\u4e8c\u5408\u543d\u5f15\u767c\u5412\u534a\u97f3\u4e8c", "263": "\u5535\u5f15\u85a9\u54e9\u56a9\u4e8c\u5408\u7faf\u54e9\u6469\u4e8c\u5408\u5f15\u56a9\u56c9\u62cf\u5f15\u511e\u4e00", "264": "\u8dcb\u6fd5\u5f4c\u4e8c\u5408\u5f15\u9164\u5695\u543d\u5f15\u767c\u5412\u534a\u97f3\u4e8c", "265": "\u5535\u5f15\u3a8d\u9f8d\u4e8c\u5408\u5f15\u5c3e\u90a3\u5f15\u820d\u91ce\u5f15\u56a9\u56c9\u62cf\u5f15\u511e\u4e00\u543d", "266": "\u5f15\u767c\u5412\u534a\u97f3\u4e8c", "267": "\u5535\u5f15\u7a81\u9f8d\u4e8c\u5408\u5c3e\u8f38\u9054\u91ce\u963f\u56a9\u56c9\u62cf\u5f15\u511e\u4e00\u543d\u5f15", "268": "\u767c\u5412\u534a\u97f3\u4e8c", "269": "\u5535\u5f15\u5165\u56a9\u4e8c\u5408\u7f85\u5165\u56a9\u4e8c\u5408\u7f85\u4e00\u9054\u54e5\u9054\u54e5\u4e8c\u8cc0", "270": "\u90a3\u8cc0\u90a3\u4e09\u963f\u5f15\u56a9\u56c9\u62cf\u5f15\u511e\u56db\u543d\u5f15\u767c\u5412\u534a\u97f3\u4e94", "271": "\u5535\u5f15\u7aa3\u9f8d\u4e8c\u5408\u5a11\u56c9\u5a11\u56c9\u4e00\u9262\u56c9\u4e8c\u5408\u5a11\u56c9\u9262\u56c9", "272": "\u4e8c\u5408\u5a11\u56c9\u4e8c\u963f\u5f15\u56a9\u56c9\u62cf\u5f15\u511e\u4e09\u543d\u5f15\u767c\u5412\u534a\u97f3\u56db", "273": "\u5535\u5f15\u543d\u5f15\u8cc0\u56c9\u8cc0\u56c9\u4e00\u85a9\u54e9\u56a9\u4e8c\u5408\u56a9\u56c9\u62cf\u5f15\u511e", "274": "\u5f15\u4e8c\u543d\u5f15\u767c\u5412\u534a\u97f3\u4e09", "275": "\u5535\u5f15\u543d\u5f15\u767c\u5412\u534a\u97f3\u85a9\u54e9\u56a9\u4e8c\u5408\u963f\u56a9\u56c9\u62cf\u5f15\u511e\u4e00", "276": "\u585e\u6016\u4e8c\u5408\u5f15\u5412\u91ce\u543d\u5f15\u767c\u5412\u534a\u97f3\u4e8c", "277": "\u5535\u5f15\u3a8d\u54e9\u4e8c\u5408\u591a\u3a8d\u54e9\u4e8c\u5408\u591a\u4e00\u85a9\u54e9\u56a9\u4e8c\u5408\u963f\u56a9", "278": "\u56c9\u62cf\u5f15\u511e\u4e8c\u543d\u5f15\u767c\u5412\u534a\u97f3\u4e09", "279": "\u5535\u5f15\u601b\u56c9\u4e8c\u5408\u5412\u534a\u97f3\u601b\u56c9\u4e8c\u5408\u5412\u534a\u97f3\u4e00", "280": "\u85a9\u54e9\u56a9\u4e8c\u5408\u963f\u56a9\u56c9\u62cf\u5f15\u511e\u4e8c\u543d\u5f15\u601b\u56c9\u4e8c\u5408\u5412", "281": "\u534a\u97f3\u4e09", "282": "\u5535\u5f15\u89aa\u90a3\u89aa\u90a3\u4e00\u5c3e\u637a\u56c9\u4e8c\u5408\u5f15\u9262\u91ce\u5c3e\u637a\u56c9\u4e8c\u5408", "283": "\u5f15\u9262\u91ce\u4e8c\u85a9\u54e9\u56a9\u4e8c\u5408\u963f\u56a9\u56c9\u62cf\u5f15\u511e\u4e09\u543d\u5f15\u767c", "284": "\u5412\u534a\u97f3\u56db", "285": "\u5535\u5f15\u90a3\u8cc0\u90a3\u8cc0\u4e00\u85a9\u54e9\u56a9\u4e8c\u5408\u90a3\u56c9\u54e5\u8a90\u5e95\u546c", "286": "\u5f15\u6771\u543d\u5f15\u767c\u5412\u534a\u97f3\u4e8c", "287": "\u5535\u5f15\u9262\u5de6\u9262\u5de6\u4e00\u85a9\u54e9\u56a9\u4e8c\u5408\u5fc5\u3611\u4e8c\u5408\u5f15\u591a\u8a90", "288": "\u5e95\u4e8c\u546c\u5f15\u6771\u543d\u5f15\u767c\u5412\u534a\u97f3\u4e09", "289": "\u5535\u5f15\u6469\u4ed6\u6469\u4ed6\u4e00\u85a9\u54e9\u56a9\u4e8c\u5408\u5f15\u5e1d\u54e9\u91ce\u4e8c\u5408\u8a90", "290": "\u5e95\u4e8c\u546c\u5f15\u6771\u543d\u5f15\u767c\u5412\u534a\u97f3\u4e09", "291": "\u5fa9\u8aa6\u6d17\u9664\u7f6a\u969c\u771f\u8a00\u3002\u8aa6\u771f\u8a00\u6642\u3002\u4ee5\u524d\u95bc\u4f3d\u74f6", "292": "\u6c34\u704c\u7051\u4ee5\u6210\u6d17\u9664\u7f6a\u969c\u3002\u771f\u8a00\u66f0\u3002", "293": "\u5535\u5f15\u90a3\u8b28\u5a46\u8a90\u56a9\u5e1d\u5f15\u4e00\u85a9\u54e9\u56a9\u4e8c\u5408\u8a25\u54e9\u8a90\u4e8c", "294": "\u5408\u5e95\u4e8c\u6ce2\u54e9\u8f38\u9054\u90a3\u56c9\u5f15\u60f9\u91ce\u4e09\u601b\u4ed6\u5f15\u8a90\u591a\u5f15", "295": "\u91ce\u56db\u963f\u56c9\u66f7\u4e8c\u5408\u5e1d\u5f15\u4e09\u85d0\u8a16\u4e09\u4e8c\u5408\u6c92\u99b1\u5f15", "296": "\u91ce\u4e94\u601b\udb80\udf55\u5207\u8eab\u4ed6\u5f15\u516d\u8f38\u9054\u511e\u5f15\u8f38\u9054\u511e\u5f15\u4e03\u85a9\u54e9", "297": "\u56a9\u4e8c\u5408\u64ad\u5f15\u6ce2\u5c3e\u8f38\u9054\u511e\u5f15\u516b\udb82\udc4a\u63d0\u5f15\u5c3e\udb82\udc4a\u63d0\u5f15\u4e5d", "298": "\u85a9\u54e9\u56a9\u4e8c\u5408\u7faf\u54e9\u6469\u4e8c\u5408\u963f\u56a9\u56c9\u62cf\u5341\u5c3e\udb82\udc4a\u63d0\u5f15", "299": "\u5a11\u56a9\u4e8c\u5408\u8cc0\u5f15\u5341\u4e00", "300": "\u8aa6\u6b64\u771f\u8a00\u5df2\u3002\u82e5\u70ba\u4ea1\u4eba\u8005\u3002\u7576\u5fa9\u8207\u8aa6\u8f49\u751f\u6de8", "301": "\u571f\u53ca\u6de8\u9053\u7b49\u771f\u8a00\u3002\u82e5\u70ba\u589e\u76ca\u5409\u7965\u53ca\u704c\u9802\u4e8b\u3002", "302": "\u5373\u4e0d\u7528\u4e4b\u3002", "303": "\u8f49\u751f\u6de8\u571f\u771f\u8a00\u66f0\u3002", "304": "\u5535\u5f15\u56c9\u601b\u511e\u4e8c\u5408\u56c9\u601b\u511e\u4e8c\u5408\u5f15\u4e00\u56c9\u601b\u90a3\u4e8c\u5408\u4e09", "305": "\u5a46\u5fae\u5f15\u4e8c\u56c9\u601b\u90a3\u4e8c\u5408\u67b3\u56c9\u5c3c\u5f15\u4e09\u56c9\u601b\u90a3\u4e8c\u5408", "306": "\u6469\u5f15\u7f85\u5c3e\udb82\udc4a\u63d0\u5f15\u56db\u8f38\u9054\u91ce\u85a9\u54e9\u56a9\u4e8c\u5408\u64ad\u5f15\u90a6", "307": "\u5f15\u4e94\u543d\u5f15\u601b\u56c9\u4e8c\u5408\u5412\u534a\u97f3\u516d", "308": "\u6de8\u9053\u771f\u8a00\u66f0\u3002", "309": "\u5535\u5f15\u9262\u8a25\u5f4c\u4e8c\u5408\u9262\u8a25\u5f4c\u4e8c\u5408\u5f15\u4e00\u9262\u8a25\u8b28\u4e8c\u5408\u8a25", "310": "\u5a46\u4e8c\u5408\u5fae\u5f15\u4e8c\u9165\u73c2\u5f15\u56a9\u601b\u7f8a\u4e8c\u5408\u5f15\u8a90\u74a8\u89a9\u5a11\u56a9", "311": "\u4e8c\u5408\u5f15\u8cc0\u5f15\u4e09", "312": "\u5fa9\u6b21\u5982\u4e0a\u5efa\u58c7\u6301\u8aa6\u3002\u6c42\u606f\u707d\u589e\u76ca\u5409\u7965\u7b49\u4e8b\u3002", "313": "\u7576\u9808\u4e00\u65e5\u4e09\u6642\u3002\u4ee5\u9999\u82b1\u53ca\u7a2e\u7a2e\u4f9b\u5177\u3002\u4f9b\u990a\u66fc", "314": "\u62cf\u7f85\u8af8\u4f5b\u8ce2\u8056\u3002\u53ca\u4ee5\u5409\u7965\u8b9a\u6b4e\u5949\u8b9a\u8af8\u4f5b\u8ce2", "315": "\u8056\u3002\u5c08\u5fc3\u4e0d\u61c8\u6216\u4e09\u65e5\u6216\u4e94\u65e5\u6216\u516b\u65e5\u5df2\u3002\u65b9", "316": "\u4f5c\u8b77\u6469\u6c42\u7a2e\u7a2e\u9858\u3002\u82e5\u70ba\u5f1f\u5b50\u6388\u704c\u9802\u8005\u3002\u5373\u7576", "317": "\u65bc\u5b89\u95bc\u4f3d\u74f6\u3002\u5730\u756b\u767d\u84ee\u82b1\u3002\u82b1\u4e2d\u756b\u516b\u8f3b\u8f2a\u3002\u65bc", "318": "\u8f2a\u4e2d\u5fc3\u5fa9\u5b89\u767d\u8272\u95bc\u4f3d\u74f6\u3002\u74f6\u4e2d\u76db\u4e94\u5bf6\u4e94\u85e5", "319": "\u7b49\u7a2e\u7a2e\u7269\u4f5c\u7051\u6de8\u6c34\u3002\u5fa9\u4ee5\u672c\u771f\u8a00\u52a0\u6301\u5df2\u3002\u7136", "320": "\u5f8c\u4f9d\u6cd5\u5f15\u5f1f\u5b50\u5165\u66fc\u62cf\u7f85\u3002\u8207\u6388\u704c\u9802\u5fa9\u65bd\u5999", "321": "\u6cd5\u7b49\u3002\u4e43\u81f3\u4f9d\u6cd5\u4f5c\u76f8\u61c9\u89c0\u3002\u60f3\u8f2a\u8ff4\u4e2d\u4e00\u5207\u60e1", "322": "\u8da3\u773e\u751f\u3002\u6089\u5f97\u6c90\u6d74\u6ec5\u76e1\u7f6a\u696d\u3002\u6210\u529f\u5fb7\u8eab\u8f49\u751f", "323": "\u5fc9\u5229\u5929\u6216\u515c\u7387\u5929\u3002", "324": "\u5fa9\u6b21\u8aaa\u95bc\u4f3d\u74f6\u3002\u7576\u7528\u767d\u8272\u8986\u3002\u5507\u9577\u9805\u5927\u8179\u8005", "325": "\u6eff\u8caf\u9999\u6c34\u3002\u63d2\u773e\u82b1\u679c\u6a39\u679d\u3002\u53ca\u76db\u4e94\u5bf6\u4e94\u85e5\u4e94", "326": "\u7a40\u7b49\u3002\u92ea\u9752\u5409\u7965\u8349\u5b89\u74f6\u65bc\u4e0a\u3002\u74f6\u9805\u7e6b\u4e00\u9752\u8863", "327": "\u6b64\u70ba\u74f6\u6cd5\u3002\u82e5\u4f5c\u8b77\u6469\u8005\u3002\u7576\u65bc\u66fc\u62cf\u7f85\u6771\u9580\u5982", "328": "\u662f\u5b89\u74f6\u3002\u53ca\u4e0a\u639b\u767d\u7e56\u84cb\u82b1\u9b18\u7b49\u3002\u5fa9\u65bc\u56db\u9762\u8a2d", "329": "\u4e94\u9996\u5e61\u3002\u65bc\u524d\u4f5c\u8b77\u6469\u7210\u3002\u7210\u5ee3\u95ca\u4e8c\u8098\u4e0b\u81f3\u4e00", "330": "\u8098\u3002\u9ad8\u4f4e\u6df1\u6dfa\u984f\u8272\u5e56\u5e5f\u3002\u6089\u4f9d\u6839\u672c\u5927\u6559\u6cd5\u5247\u3002", "331": "\u8b77\u6469\u6240\u7528\u5fc5\u5229\u712c\u865e\u9999\u3002\u9f8d\u82b1\u5409\u7965\u679c\u53ca\u7a2e\u7a2e", "332": "\u679c\u3002\u9ed1\u6cb9\u9ebb\u5927\u9ea5\u5c0f\u9ea5\u3002\u7cb3\u7c73\u98ef\u9165\u871c\u916a\u5177\u4e73\u6fd5", "333": "\u67f4\u7b49\u3002\u5982\u662f\u8af8\u7269\u6089\u5177\u8db3\u5df2\u3002\u963f\u95cd\u68a8\u624b\u57f7\u91d1\u525b", "334": "\u9234\u6775\u6309\u8af8\u7269\u4e0a\u3002\u5404\u4ee5\u672c\u771f\u8a00\u52a0\u6301\u4e00\u767e\u516b\u904d\u3002", "335": "\u5b89\u50cf\u6176\u8b9a\u4ea6\u540c\u6b64\u5100\u3002\u5982\u662f\u8b77\u6469\u6240\u7528\u8af8\u7269\u3002\u6089", "336": "\u5b89\u8b77\u6469\u7210\u53f3\u3002\u552f\u51fa\u751f\u9262\u5b89\u65bc\u7210\u5de6\u3002\u963f\u95cd\u68a8\u4f9d", "337": "\u6cd5\u4f5c\u4e09\u76f8\u61c9\u89c0\u3002\u7136\u5f8c\u4ee5\u6de8\u6c34\u7051\u7210\u3002\u4f5c\u7051\u6de8\u5df2\u3002", "338": "\u5373\u65bc\u7210\u5167\u71c3\u706b\u3002\u5f97\u706b\u71be\u5df2\u3002\u53c8\u7528\u95bc\u4f3d\u6c34\u5fae\u7051", "339": "\u706b\u7130\u3002\u7136\u5f8c\u963f\u95cd\u68a8\u7d50\u5370\u8aa6\u771f\u8a00\u8acb\u53ec\u706b\u5929\u60f3\u3002", "340": "\u706b\u5929\u73fe\u65bc\u706b\u5167\u3002\u7136\u4ee5\u9165\u871c\u916a\u53ca\u737b\u706b\u5929\u7269\u3002\u4e09", "341": "\u64f2\u706b\u4e2d\u3002\u8aa6\u771f\u8a00\u737b\u65bc\u706b\u5929\u3002\u7136\u5f8c\u7d50\u91d1\u525b\u8f2a\u5370\u3002", "342": "\u4ee5\u4e8c\u624b\u4f5c\u91d1\u525b\u62f3\u3002\u5fa9\u6539\u4f5c\u91d1\u525b\u7e1b\u6210\u5370\u3002\u5e76\u8aa6", "343": "\u91d1\u525b\u8f2a\u771f\u8a00\u66f0\u3002", "344": "\u5535\u5f15\u85a9\u54e9\u56a9\u4e8c\u5408\u5c3e\u5484\u534a\u97f3\u4e00\u56a9\u65e5\u56c9\u4e8c\u5408\u4f5c\u8a16\u68a8", "345": "\u4e8c\u5408\u5f15\u6089\u8fed\u5e95\u543d\u5f15\u4e8c\u56a9\u8cd2\u5f15\u56a9\u65e5\u56c9\u4e8c\u5408\u85a9\u6469\u5f15\ud844\udc60", "346": "\u5f15\u90a3\u4e09\u56a9\u65e5\u56c9\u4e8c\u5408\u8e49\u5412\u5c3e\u8b28\u5f15\u53c9\u5948\u5f15\u56db\ud843\ude81\u8f49\u8072\u547c", "347": "\u543d\u5f15\u9441\u547c\u5f15\u4e94", "348": "\u7d50\u5370\u8aa6\u771f\u8a00\u6642\u3002\u5fc3\u60f3\u66fc\u62cf\u7f85\u73fe\u65bc\u7210\u4e2d\u3002\u8af8\u4f5b", "349": "\u83e9\u85a9\u4e00\u5207\u8ce2\u8056\u7686\u6089\u73fe\u524d\u3002\u963f\u95cd\u68a8\u5373\u8654\u5fd7\u3002\u4ee5", "350": "\u7a2e\u7a2e\u8b77\u6469\u7269\u4f5c\u4e00\u767e\u516b\u7aa3\u5695\u56a9\u3002\u82e5\u552f\u7528\u9165\u53ea", "351": "\u64f2\u4e03\u7aa3\u5695\u56a9\u3002\u5982\u662f\u4f9b\u990a\u8af8\u4f5b\u8ce2\u8056\u3002\u53ca\u4f5c\u6232", "352": "\u9b18\u6b4c\u821e\u7b49\u516b\u4f9b\u990a\u3002\u4e43\u81f3\u4f5c\u4e8c\u5341\u4e94\u7a2e\u4f9b\u990a\u3002", "353": "\u963f\u95cd\u68a8\u5982\u662f\u4f5c\u89c0\u60f3\u66fc\u62cf\u7f85\u53ca\u8b77\u6469\u7a2e\u7a2e\u4e4b", "354": "\u6cd5\u3002\u6216\u70ba\u570b\u738b\u5927\u81e3\u58eb\u5eb6\u5f1f\u5b50\u7b49\u3002\u7576\u96a8\u6240\u6c42\u4e8b", "355": "\u5229\u76ca\u773e\u751f\u3002\u4e43\u81f3\u96a8\u559c\u898b\u805e\u7686\u7372\u7121\u91cf\u529f\u5fb7\u3002", "356": "\u4f5b\u8aaa\u5927\u4e58\u89c0\u60f3\u66fc\u62cf\u7f85\u6de8\u8af8\u60e1\u8da3\u7d93\u5377\u4e0b"}, "originalTitle": "Taisho Tripitaka, Electronic version, No. 0939 \u4f5b\u8aaa\u5927\u4e58\u89c0\u60f3\u66fc\u62cf\u7f85\u6de8\u8af8\u60e1\u8da3\u7d93", "source": "CBETA"}