u5f97\u6ec5\u9664\uff1b\u6cc1\u5fa9\u665d\u591c", "190": "\u6cd5\u5e62\u738b\u5982\u4f86\u3000\u5357\u7121\u5bf6\u6708\u5e62\u738b\u5982\u4f86\u3000\u5357\u7121\u8ce2", "191": "\u76f8\u5e62\u738b\u5982\u4f86\u3000\u5357\u7121\u81ea\u5728\u77bf\u90a3\u5e62\u738b\u5982\u4f86\u3000\u5357", "192": "\u7121\u53eb\u540d\u7a31\u5e62\u738b\u5982\u4f86\u3000\u5357\u7121\u666e\u773c\u904d\u6eff\u6cd5\u754c\u5e62", "193": "\u738b\u5982\u4f86\u3000\u5357\u7121\u5bf6\u805a\u5e62\u738b\u5982\u4f86\u3000\u5357\u7121\u5bf6\u7130\u9762", "194": "\u5e62\u738b\u5982\u4f86\u3000\u5357\u7121\u76e1\u6cd5\u6d77\u5bf6\u5e62\u738b\u5982\u4f86\u3000\u5357\u7121", "195": "\u83ef\u5e62\u738b\u5982\u4f86\u3000\u5357\u7121\u77bf\u90a3\u5e62\u738b\u5982\u4f86\u3000\u5357\u7121\u5e2b", "196": "\u5b50\u5e62\u738b\u5982\u4f86\u3000\u5357\u7121\u91d1\u525b\u5e62\u738b\u5982\u4f86\u3000\u5357\u7121\u5f4c", "197": "\u7559\u5e62\u738b\u5982\u4f86\u3000\u5357\u7121\u6cd5\u5e62\u738b\u5982\u4f86\u3000\u5357\u7121\u706b", "198": "\u5e62\u738b\u5982\u4f86\u3000\u5357\u7121\u5bc2\u5e62\u738b\u5982\u4f86\u3000\u5357\u7121\u5999\u5e62\u738b", "199": "\u5982\u4f86\u3000\u5357\u7121\u4e0d\u53ef\u91cf\u5e62\u738b\u5982\u4f86\u3000\u5357\u7121\u7121\u57a2\u5a01", "2": "\u5377\u7b2c\u4e00", "20": "\u53d7\u6301\u3001\u8b80\u8aa6\u3001\u61b6\u5ff5\u4e0d\u5fd8\u8005\uff0c\u662f\u4eba\u529f\u5fb7\uff0c\u4e0d\u53ef\u601d\u8b70\u3002", "200": "\u5e62\u738b\u5982\u4f86\u3000\u5357\u7121\u512a\u6ce2\u4f4e\u6c99\u5e62\u738b\u5982\u4f86\u3000\u5357\u7121", "201": "\u52c7\u5065\u5982\u4f86\u3000\u5357\u7121\u5bc2\u5e62\u5982\u4f86\u3000\u5357\u7121\u5e1d\u91cb\u5e62\u5982", "202": "\u4f86\u3000\u5357\u7121\u5927\u5e62\u5982\u4f86\u3000\u5357\u7121\u76f8\u5e62\u5982\u4f86\u3000\u5357\u7121", "203": "\u6c34\u5e62\u5982\u4f86\u3000\u5357\u7121\u653e\u5149\u5e62\u5982\u4f86\u3000\u5357\u7121\u9707\u8072\u97ff", "204": "\u5e62\u5982\u4f86\u3000\u5357\u7121\u5ee3\u540d\u667a\u6d77\u5e62\u5982\u4f86\u3000\u5357\u7121\u5a46", "205": "\u4f3d\u5a46\u5e1d\u7121\u57a2\u7130\u5bf6\u5149\u5982\u4f86\u3000\u5357\u7121\u660e\u5e62\u5982\u4f86", "206": "\u5357\u7121\u89c0\u667a\u5e62\u5982\u4f86\u3000\u5357\u7121\u5584\u6de8\u696d\u5e62\u5982\u4f86\u3000\u5357", "207": "\u7121\u91d1\u525b\u90a3\u7f85\u5ef6\u5e62\u5982\u4f86\u3000\u5357\u7121\u738b\u5149\u660e\u5e62\u5982\u4f86", "208": "\u5357\u7121\u6cd5\u754c\u97f3\u5e62\u5982\u4f86\u3000\u5357\u7121\u91d1\u83ef\u7130\u5e62\u5982\u4f86", "209": "\u5357\u7121\u6cd5\u90a3\u7f85\u5ef6\u5e62\u5982\u4f86\u3000\u5357\u7121\u4e0d\u53ef\u964d\u4f0f\u529b\u5e62", "21": "\u5f7c\u4e16\u754c\u4e2d\u6709\u4e00\u83e9\u85a9\uff0c\u540d\u66f0\u96e3\u5339\u7121\u969c\u7919\u738b\uff0c\u800c", "210": "\u5982\u4f86\u3000\u5357\u7121\u6cd5\u84ee\u83ef\u6bd8\u76e7\u906e\u90a3\u4f5b\u5e62\u5982\u4f86\u3000\u5357", "211": "\u7121\u5962\u6469\u4ed6\u5e62\u5982\u4f86\u3000\u5357\u7121\u5bf6\u5e62\u5982\u4f86\u3000\u5357\u7121\u5927", "212": "\u5bf6\u5e62\u5982\u4f86\u3000\u5357\u7121\u7121\u57a2\u5e62\u5982\u4f86\u3000\u5357\u7121\u4e16\u9593\u5e62", "213": "\u5982\u4f86\u3000\u5357\u7121\u5e1d\u91cb\u5e62\u5982\u4f86\u3000\u5357\u7121\u7130\u5e62\u5982\u4f86", "214": "\u5357\u7121\u83ef\u5e62\u5982\u4f86\u3000\u5357\u7121\u667a\u5e62\u5982\u4f86\u3000\u5357\u7121\u5bf6\u529f", "215": "\u5fb7\u71c8\u5e62\u5982\u4f86\u3000\u5357\u7121\u666e\u529f\u5fb7\u6bd8\u76e7\u906e\u90a3\u5e62\u5982\u4f86", "216": "\u5357\u7121\u5927\u5e62\u5982\u4f86\u3000\u5357\u7121\u91cb\u8fe6\u56c9\u5e62\u5982\u4f86\u3000\u5357\u7121", "217": "\u6bd8\u76e7\u906e\u90a3\u5e62\u5982\u4f86\u3000\u5357\u7121\u76f8\u5e62\u5982\u4f86\u3000\u5357\u7121\u6cd5", "218": "\u5e62\u5982\u4f86\u3000\u5357\u7121\u4e00\u5207\u5229\u6e05\u6de8\u77bf\u90a3\u5e62\u5982\u4f86\u3000\u5357", "219": "\u7121\u4eba\u6700\u52dd\u5e62\u5982\u4f86\u3000\u5357\u7121\u7121\u91cf\u529b\u5e62\u5982\u4f86\u3000\u5357", "22": "\u5982\u4f86\u6388\u5f7c\u83e9\u85a9\u8a18\uff0c\u7576\u5f97\u6210\u4f5b\uff0c\u865f\u66f0\u6beb\u76f8\u65e5\u6708", "220": "\u7121\u7121\u908a\u660e\u7130\u81ea\u5728\u5e62\u5982\u4f86\u3000\u5357\u7121\u4f5c\u5149\u6708\u5e62\u5982", "221": "\u4f86\u3000\u5357\u7121\u6cd5\u81ea\u5728\u667a\u5e62\u5982\u4f86\u3000\u5357\u7121\u96e3\u52dd\u529b\u666e", "222": "\u5e62\u5982\u4f86\u3000\u5357\u7121\u5bf6\u71c8\u666e\u660e\u5e62\u5982\u4f86\u3000\u5357\u7121\u56e0\u9640", "223": "\u7f85\u6700\u52dd\u610f\u5e62\u5982\u4f86\u3000\u5357\u7121\u5bf6\u5e62\u5982\u4f86\u3000\u5357\u7121\u5584", "224": "\u5316\u6cd5\u754c\u97f3\u5e62\u5982\u4f86\u3000\u5357\u7121\u5ee3\u6ce2\u82e5\u667a\u529f\u5fb7\u5e62\u5982", "225": "\u4f86\u3000\u5357\u7121\u71be\u5e62\u5982\u4f86\u3000\u5357\u7121\u6700\u52dd\u6cd5\u5e62\u5982\u4f86", "226": "\u5357\u7121\u5149\u5e62\u5982\u4f86\u3000\u5357\u7121\u4e0d\u53ef\u91cf\u5e62\u5982\u4f86\u3000\u5357\u7121", "227": "\u5a46\u4f3d\u5a46\u5e1d\u6cd5\u5bf6\u82b1\u529f\u5fb7\u8072\u5982\u4f86\u3000\u5357\u7121\u666e\u660e\u6cd5", "228": "\u529f\u5fb7\u8072\u5982\u4f86\u3000\u5357\u7121\u68b5\u8072\u5982\u4f86\u3000\u5357\u7121\u5e2b\u5b50\u8072", "229": "\u5982\u4f86\u3000\u5357\u7121\u5bf6\u8072\u5982\u4f86\u300d", "23": "\u5149\u660e\u7130\u5bf6\u84ee\u83ef\u56fa\u5982\u91d1\u525b\u8eab\u6bd8\u76e7\u906e\u90a3\u7121\u969c", "230": "\u4e09\u767e\u4f5b\u5df2\u7adf\u3002", "231": "\u300c\u5357\u7121\u6708\u8072\u5982\u4f86\u3000\u5357\u7121\u84ee\u83ef\u8072\u5982\u4f86\u3000\u5357\u7121\u5be6", "232": "\u8072\u5982\u4f86\u3000\u5357\u7121\u4e00\u5207\u6cd5\u4e09\u6627\u5149\u8072\u5982\u4f86\u3000\u5357\u7121", "233": "\u666e\u9580\u667a\u6bd8\u76e7\u906e\u90a3\u8072\u5982\u4f86\u3000\u5357\u7121\u91d1\u6469\u5c3c\u5c71\u8072", "234": "\u5982\u4f86\u3000\u5357\u7121\u6cd5\u70ac\u5bf6\u5e33\u8072\u5982\u4f86\u3000\u5357\u7121\u6cd5\u6d77\u543c", "235": "\u8072\u5982\u4f86\u3000\u5357\u7121\u71be\u7130\u6d77\u8072\u5982\u4f86\u3000\u5357\u7121\u4e16\u4e3b\u6700", "236": "\u52dd\u5149\u660e\u8072\u5982\u4f86\u3000\u5357\u7121\u6cd5\u754c\u97f3\u8072\u5982\u4f8