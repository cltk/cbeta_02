3000\u5357\u7121\u5f4c\u7559\u805a\u738b\u5982\u4f86\u3000\u5357\u7121\u5bf6\u84ee\u83ef\u5584\u4f4f", "377": "\u5c71\u5e1d\u91cb\u738b\u5982\u4f86\u3000\u5357\u7121\u9858\u74d4\u73de\u838a\u56b4\u738b\u5982\u4f86", "378": "\u5357\u7121\u6d77\u6301\u610f\u904a\u6232\u795e\u901a\u738b\u5982\u4f86\u3000\u5357\u7121\u96f2\u738b\u5982", "379": "\u4f86\u3000\u5357\u7121\u9952\u76ca\u738b\u5982\u4f86\u3000\u5357\u7121\u91d1\u5149\u5a01\u738b\u5982\u4f86", "38": "\u300c\u6771\u5357\u65b9\u6709\u4f5b\uff0c\u540d\u66f0\u4f5c\u71c8\u660e\u5982\u4f86\u3001\u963f\u7f85\u8a36\u3001\u4e09\u85d0", "380": "\u5357\u7121\u7834\u6563\u8af8\u591c\u53c9\u795e\u6d6e\u591a\u795e\u7b49\u738b\u5982\u4f86\u3000\u5357\u7121", "381": "\u5bf6\u5149\u838a\u56b4\u738b\u5982\u4f86\u3000\u5357\u7121\u81ea\u5728\u5a01\u8072\u738b\u5982\u4f86", "382": "\u5357\u7121\u83ef\u706b\u904a\u6232\u795e\u901a\u738b\u5982\u4f86\u3000\u5357\u7121\u958b\u6577\u83ef\u5a11", "383": "\u7f85\u738b\u5982\u4f86\u3000\u5357\u7121\u5584\u5bc2\u667a\u6708\u543c\u97f3\u81ea\u5728\u738b\u5982\u4f86", "384": "\u5357\u7121\u5730\u738b\u5982\u4f86\u3000\u5357\u7121\u5e62\u738b\u5982\u4f86\u3000\u5357\u7121\u521d\u767c", "385": "\u5fc3\u610f\u9707\u8072\u7121\u6016\u754f\u6700\u4e0a\u738b\u5982\u4f86\u3000\u5357\u7121\u7121\u9a5a\u6016", "386": "\u95c7\u4e09\u6627\u6700\u4e0a\u738b\u5982\u4f86\u3000\u5357\u7121\u5c3c\u4ff1\u9640\u738b\u5982\u4f86", "387": "\u5357\u7121\u6b61\u559c\u8e0a\u8e8d\u5bf6\u5b55\u6469\u5c3c\u805a\u738b\u5982\u4f86\u3000\u5357\u7121\u773e", "388": "\u4e3b\u738b\u5982\u4f86\u3000\u5357\u7121\u559c\u6a02\u5149\u738b\u5982\u4f86\u3000\u5357\u7121\u91d1\u525b", "389": "\u4e0a\u738b\u5982\u4f86\u3000\u5357\u7121\u5f4c\u7559\u9802\u738b\u5982\u4f86\u3000\u5357\u7121\u6e05\u5149", "39": "\u4e09\u4f5b\u9640\u3002\u4ea6\u61c9\u7576\u7a31\u5f7c\u4f5b\u540d\uff0c\u4ea6\u4fee\u606d\u656c\uff0c\u4ea6\u9808", "390": "\u738b\u5982\u4f86\u3000\u5357\u7121\u84ee\u83ef\u9b1a\u5149\u738b\u5982\u4f86\u3000\u5357\u7121\u5200\u6756", "391": "\u4e0a\u9999\u5149\u738b\u5982\u4f86\u3000\u5357\u7121\u4e0a\u820c\u738b\u5982\u4f86\u3000\u5357\u7121\u4e0d", "392": "\u601d\u8b70\u77bf\u62cf\u5149\u738b\u5982\u4f86\u3000\u5357\u7121\u8ce2\u4e0a\u738b\u5982\u4f86\u3000\u5357", "393": "\u7121\u84ee\u83ef\u4e0a\u738b\u5982\u4f86\u3000\u5357\u7121\u96e3\u4f0f\u4e0a\u738b\u5982\u4f86\u3000\u5357", "394": "\u7121\u5f4c\u7559\u5149\u738b\u5982\u4f86\u3000\u5357\u7121\u5f4c\u7559\u71c8\u738b\u5982\u4f86\u3000\u5357", "395": "\u7121\u5a11\u7f85\u81ea\u5728\u738b\u5982\u4f86\u3000\u5357\u7121\u9f13\u81ea\u5728\u97f3\u738b\u5982\u4f86", "396": "\u5357\u7121\u5f37\u5065\u8ecd\u5c07\u6230\u738b\u5982\u4f86\u3000\u5357\u7121\u666e\u5149\u6700\u4e0a\u529f", "397": "\u5fb7\u805a\u738b\u5982\u4f86\u3000\u5357\u7121\u6b63\u4f4f\u6469\u5c3c\u805a\u738b\u5982\u4f86\u3000\u5357", "398": "\u7121\u6cd5\u6c34\u6e05\u6de8\u865b\u7a7a\u7121\u9593\u738b\u5982\u4f86\u3000\u5357\u7121\u666e\u5149\u660e", "399": "\u529f\u5fb7\u5f4c\u7559\u738b\u5982\u4f86\u3000\u5357\u7121\u666e\u667a\u5e62\u97f3\u738b\u5982\u4f86", "4": "\u5982\u662f\u6211\u805e\uff1a\u4e00\u6642\uff0c\u5a46\u4f3d\u5a46\u4f4f\u738b\u820d\u5927\u57ce\u8006\u95cd\u5d1b", "40": "\u7a31\u5176\u540d\u865f\uff0c\u4ea6\u9808\u5fc3\u5ff5\u5f7c\u4f5b\u540d\u865f\u3002", "400": "\u5357\u7121\u5e1d\u91cb\u5e62\u76f8\u738b\u5982\u4f86\u3000\u5357\u7121\u5584\u4f4f\u5c71\u5e1d\u91cb\u738b", "401": "\u5982\u4f86\u3000\u5357\u7121\u7834\u6563\u96f2\u7ff3\u738b\u5982\u4f86\u3000\u5357\u7121\u666e\u77bb", "402": "\u671b\u84ee\u83ef\u904a\u6232\u738b\u5982\u4f86\u3000\u5357\u7121\u5bf6\u529f\u5fb7\u5149\u5a01\u738b\u5982", "403": "\u4f86\u3000\u5357\u7121\u5bf6\u6708\u5149\u660e\u85e5\u738b\u5982\u4f86\u300d", "404": "\u516d\u767e\u4f5b\u5df2\u7adf\u3002", "405": "\u300c\u5357\u7121\u5bbf\u738b\u5982\u4f86\u3000\u5357\u7121\u5c71\u56e0\u9640\u7f85\u738b\u5982\u4f86", "406": "\u5357\u7121\u5a11\u7f85\u5e1d\u91cb\u738b\u5982\u4f86\u3000\u5357\u7121\u9802\u6700\u4e0a\u738b\u5982\u4f86", "407": "\u5357\u7121\u5c71\u9802\u6700\u4e0a\u738b\u5982\u4f86\u3000\u5357\u7121\u53ef\u754f\u84ee\u83ef\u6700\u4e0a", "408": "\u738b\u5982\u4f86\u3000\u5357\u7121\u7cbe\u9032\u6700\u4e0a\u738b\u5982\u4f86\u3000\u5357\u7121\u7121\u908a", "409": "\u754c\u6700\u4e0a\u738b\u5982\u4f86\u3000\u5357\u7121\u85e5\u738b\u5982\u4f86\u3000\u5357\u7121\u5584\u4f4f", "41": "\u300c\u897f\u5357\u65b9\u6709\u4f5b\uff0c\u540d\u66f0\u5bf6\u4e0a\u76f8\u540d\u7a31\u5982\u4f86\u3001\u963f\u7f85\u8a36\u3001", "410": "\u738b\u5982\u4f86\u3000\u5357\u7121\u83ef\u6577\u738b\u5982\u4f86", "411": "\u4e94\u5343\u4e94\u767e\u4f5b\u540d\u795e\u546a\u9664\u969c\u6ec5\u7f6a\u7d93\u5377\u7b2c\u4e00", "42": "\u4e09\u85d0\u4e09\u4f5b\u9640\u3002\u4ea6\u61c9\u7576\u7a31\u5f7c\u4f5b\u540d\uff0c\u4ea6\u4fee\u606d\u656c\uff0c\u4ea6", "43": "\u9808\u7a31\u5176\u540d\u865f\uff0c\u4ea6\u9808\u5fc3\u5ff5\u5f7c\u4f5b\u540d\u865f\u3002", "44": "\u300c\u897f\u5317\u65b9\u6709\u4f5b\uff0c\u540d\u66f0\u7121\u754f\u89c0\u5982\u4f86\u3001\u963f\u7f85\u8a36\u3001\u4e09\u85d0", "45": "\u4e09\u4f5b\u9640\u3002\u4ea6\u61c9\u7576\u7a31\u5f7c\u4f5b\u540d\uff0c\u4ea6\u4fee\u606d\u656c\uff0c\u4ea6\u9808", "46": "\u7a31\u5176\u540d\u865f\uff0c\u4ea6\u9808\u5fc3\u5ff5\u5f7c\u4f5b\u540d\u865f\u3002", "47": "\u300c\u6771\u5317\u65b9\u6709\u4f5b\uff0c\u540d\u66f0\u7121\u754f\u7121\u602f\u6bdb\u5b54\u4e0d\u7aea\u540d\u7a31", "48": "\u5982\u4f86\u3001\u963f\u7f85\u8a36\u3001\u4e09\u85d0\u4e09\u4f5b\u9640\u3002\u4ea6\u61c9\u7576\u7a31\u5f7c\u4f5b", "49": "\u540d\uff0c\u4ea6\u4fee\u606d\u656c\uff0c\u4ea6\u9808\u7a31\u5176\u540d\u865f\uff0c\u4ea6\u9808\u5fc3\u5ff5\u5f7c\u4f5b", "5": "\u5c71\u4e2d\uff0c\u8207\u5927\u6bd4\u4e18\u773e\u5343\u4e8c\u767e\u4e94\u5341\u4eba\u4ff1\uff1b\u5fa9\u6709\u83e9", "50": "\u540d\u865f\u3002", "51": "\u300c\u4e0b\u65b9\u6709\u4f5b\uff0c\u540d\u66f0\u5e2b\u5b50\u596e\u8fc5\u6839\u5982\u4f86\u3001\u963f\u7f85\u8a36\u3001\u4e09", "52": "\u85d0\u4e09\u4f5b\u9640\u3002\u4ea6\u61c9\u7576\u7a31\u5f7c\u4f5b\u540d\uff0c\u4ea6\u4fee\u606d\u656c\uff0c\u4ea6", "53": "\u9808\u7a31\u5176\u540d\u865f\uff0c\u4ea6\u9808\u5fc3\u5ff5\u5f7c\u4f5b\u540d\u865f\u3002", "54": "\u300c\u4e0a\u65b9\u6709\u4f5b\uff0c\u540d\u66f0\u91d1\u5149\u5a01\u738b\u76f8\u4f3c\u5982\u4f86\u3001\u963f\u7f85\u8a36\u3001", "55": "\u4e09\u85d0\u4e09\u4f5b\u9640\u3002\u4ea6\u61c9\u7576\u7a31\u5f7c\u4f5b\u540d\uff0c\u4ea6\u4fee\u606d\u656c\uff0c", "56": "\u4ea6\u9808\u7a31\u5176\u540d\u865f\uff0c\u4ea6\u9808\u5fc3\u5ff5\u5f7c\u4f5b\u540d\u865f\u3002\u300d", "57": "\u723e\u6642\uff0c\u4f5b\u544a\u5f4c\u52d2\uff1a\u300c\u82e5\u6709\u6b63\u4fe1\u5584\u7537\u5b50\u3001\u6b63\u4fe1\u5584\u5973", "58": "\u4eba\uff0c\u81f3\u5fc3\u7a31\u6b64\u5341\u4e8c\u8af8\u4f5b\u540d\u865f\u4e4b\u6642\uff0c\u7d93\u65bc\u5341\u65e5\uff0c", "59": "\u7576\u4fee\u61fa\u6094\u4e00\u5207\u8af8\u7f6a\uff0c\u4e00\u5207\u773e\u751f\u6240\u6709\u529f\u5fb7\u7686", "6": "\u85a9\u6469\u8a36\u85a9\u773e\u4e00\u842c\u4e8c\u5343\u4eba\u4ff1\uff0c\u6642\uff0c\u963f\u9038\u591a\u83e9\u85a9", "60": "\u7576\u96a8\u559c\uff0c\u52f8\u8acb\u4e00\u5207\u8af8\u4f5b\u4e45\u4f4f\u65bc\u4e16\uff0c\u4ee5\u8af8\u5584\u6839", "61": "\u8ff4\u5411\u6cd5\u754c\uff0c\u662f\u6642\u5373\u5f97\u6ec5\u4e00\u5207\u8af8\u7f6a\uff0c\u5f97\u6de8\u4e00", "62": "\u5207\u696d\u969c\uff0c\u5373\u5f97\u5177\u8db3\u6210\u5c31\u838a\u56b4\u4e00\u5207\u4f5b\u571f\uff0c\u6210\u5c31", "63": "\u5177\u8db3\u7121\u754f\uff0c\u5fa9\u5f97\u5177\u8db3\u838a\u56b4\u8eab\u76f8\uff0c\u5fa9\u5f97\u5177\u8db3\u83e9", "64": "\u85a9\u7737\u5c6c\u570d\u9076\uff0c\u5fa9\u5f97\u5177\u8db3\u7121\u91cf\u9640\u7f85\u5c3c\uff0c\u5fa9\u5f97\u5177", "65": "\u8db3\u7121\u91cf\u4e09\u6627\uff0c\u5fa9\u5f97\u5177\u8db3\u5982\u610f\u4f5b\u524e\u838a\u56b4\uff0c\u4ea6\u5f97", "66": "\u5177\u8db3\u7121\u91cf\u5584\u77e5\u8b58\uff0c\u901f\u5f97\u6210\u5c31\u5982\u4e0a\u6240\u8aaa\u4e0d\u589e", "67": "\u4e0d\u6e1b\uff0c\u5728\u65bc\u7169\u60f1\u4e2d\u884c\u963f\u8028\u591a\u7f85\u4e09\u85d0\u4e09\u83e9\u63d0\uff0c", "68": "\u800c\u5f97\u7aef\u6b63\u53ef\u559c\u679c\u5831\uff0c\u4ea6\u5f97\u8ca1\u5bf6\u5145\u8db3\uff0c\u5e38\u751f\u5927", "69": "\u59d3\u8c6a\u65cf\u7a2e\u59d3\u4e4b\u5bb6\uff0c\u8eab\u76f8\u5177\u8db3\uff0c\u4ea6\u5f97\u5584\u548c\u7737", "7": "\u70ba\u9996\u3002", "70": "\u5c6c\u570d\u9076\u3002\u300d\u723e\u6642\uff0c\u4e16\u5c0a\u6b32\u91cd\u5ba3\u6b64\u7fa9\uff0c\u800c\u8aaa\u5048\u8a00\uff1a", "71": "\u300c\u82e5\u6709\u5584\u7537\u5b50\uff0c\u82e5\u5584\u5973\u4eba\u7b49\uff0c", "72": "\u53d7\u6301\u6b64\u4f5b\u540d\uff0c\u751f\u751f\u4e16\u4e16\u4e2d\uff0c", "73": "\u5f97\u4ed6\u4eba\u611b\u656c\uff0c\u5149\u660e\u5a01\u529b\u5927\uff0c", "74": "\u751f\u8655\u70ba\u4eba\u5c0a\uff0c\u65bc\u5f8c\u5f97\u6210\u4f5b\u3002", "75": "\u300c\u5357\u7121\u5a46\u4f3d\u5a46\u5e1d\u53ef\u754f\u96f7\u97f3\u5999\u5a01\u7121\u57a2\u5149\u529f\u5fb7\u5bf6", "76": "\u77bf\u90a3\u838a\u56b4\u9802\u986f\u8d6b\u958b\u6577\u5584\u751f\u751f\u529f\u5fb7\u6bd8\u76e7\u906e", "77": "\u90a3\u81ea\u5728\u738b\u5982\u4f86", "78": "\u300c\u5357\u7121\u5a46\u4f3d\u5a46\u5e1d\u7121\u91cf\u529f\u5fb7\u5bf6\u5149\u96dc\u5999\u73fe\u91d1\u5149\u5e2b", "79": "\u5b50\u543c\u738b\u5982\u4f86", "8": "\u723e\u6642\uff0c\u4e16\u5c0a\u544a\u5f4c\u52d2\u83e9\u85a9\u8a00\uff1a\u300c\u5f4c\u52d2\uff01\u6771\u65b9\u53bb\u6b64\u4f5b", "80": "\u300c\u5357\u7121\u767d\u84ee\u83ef\u9afb\u7121\u7919\u6ce2\u7130\u738b\u5982\u4f86\u300c\u5357\u7121\u5a46\u4f3d", "81": "\u5a46\u5e1d\u83e9\u63d0\u9053\u5834\u529f\u5fb7\u6708\u5982\u4f86", "82": "\u300c\u5357\u7121\u6cd5\u70ac\u7130\u529f\u5fb7\u6708\u5982\u4f86", "83": "\u300c\u5357\u7121\u6cd5\u738b\u6de8\u529f\u5fb7\u6708\u5982\u4f86", "84": "\u300c\u5357\u7121\u6cd5\u5713\u6de8\u529f\u5fb7\u6708\u5982\u4f86", "85": "\u300c\u5357\u7121\u6cd5\u773c\u751a\u6df1\u529f\u5fb7\u6708\u5982\u4f86\u3000\u5357\u7121\u4e00\u5207\u8eab\u5f62", "86": "\u5149\u660e\u529f\u5fb7\u6708\u5982\u4f86\u3000\u5357\u7121\u6834\u6a80\u529f\u5fb7\u6708\u5982\u4f86", "87": "\u5357\u7121\u77bf\u90a3\u84ee\u83ef\u529f\u5fb7\u6708\u5982\u4f86\u3000\u5357\u7121\u6cd5\u7db2\u6e05\u6de8", "88": "\u529f\u5fb7\u6708\u5982\u4f86\u3000\u5357\u7121\u6cd5\u754c\u610f\u6708\u5982\u4f86\u3000\u5357\u7121\u8af8", "89": "\u985e\u5229\u76ca\u9858\u5982\u4f86\u3000\u5357\u7121\u5149\u7130\u773c\u5f62\u6708\u5982\u4f86\u3000\u5357", "9": "\u524e\uff0c\u6709\u5341\u4e0d\u53ef\u8aaa\u8af8\u4f5b\u524e\u571f\uff0c\u5104\u767e\u5343\u5fae\u5875\u6578\u7b49\u3002", "90": "\u7121\u7a2e\u7a2e\u7130\u71be\u660e\u76db\u6708\u5982\u4f86\u3000\u5357\u7121\u8af8\u9858\u6708\u5982\u4f86", "91": "\u5357\u7121\u76f8\u56b4\u5e62\u6708\u5982\u4f86\u3000\u5357\u7121\u666e\u66dc\u6708\u5982\u4f86\u3000\u5357", "92": "\u7121\u7121\u8457\u610f\u6708\u5982\u4f86\u3000\u5357\u7121\u6700\u4e0a\u6708\u5982\u4f86\u3000\u5357\u7121", "93": "\u4f9b\u990a\u6708\u5982\u4f86\u3000\u5357\u7121\u7121\u908a\u6708\u5982\u4f86\u3000\u5357\u7121\u89e3\u812b", "94": "\u6708\u5982\u4f86\u3000\u5357\u7121\u5927\u6708\u5982\u4f86\u3000\u5357\u7121\u77bf\u90a3\u6708\u5982\u4f86", "95": "\u5357\u7121\u6708\u738b\u5982\u4f86\u3000\u5357\u7121\u4f3c\u6708\u5982\u4f86\u3000\u5357\u7121\u6708\u5982", "96": "\u4f86\u3000\u5357\u7121\u6dbc\u51b7\u5982\u4f86\u3000\u5357\u7121\u6708\u9762\u5982\u4f86\u3000\u5357\u7121", "97": "\u7121\u7591\u6ce2\u7f85\u871c\u6708\u5982\u4f86\u3000\u5357\u7121\u865b\u7a7a\u4e0b\u7121\u57a2\u667a\u6708", "98": "\u5982\u4f86\u3000\u5357\u7121\u4e0d\u601d\u8b70\u529f\u5fb7\u7167\u66dc\u6700\u52dd\u6708\u5982\u4f86", "99": "\u5357\u7121\u8af8\u529b\u5a01\u7121\u57a2\u6708\u5982\u4f86\u3000\u5357\u7121\u7cbe\u9032\u529b\u96e3\u964d"}, "source": "CBETA", "language": "chinese", "originalTitle": "Taisho Tripitaka, Electronic version, No. 0443 \u4e94\u5343\u4e94\u767e\u4f5b\u540d\u795e\u546a\u9664\u969c\u6ec5\u7f6a\u7d93: \u4e94\u5343\u4e94\u767e\u4f5b\u540d\u795e\u546a\u9664\u969c\u6ec5\u7f6a\u7d93: \u4e94\u5343\u4e94\u767e\u4f5b\u540d\u795e\u546a\u9664\u969c\u6ec5\u7f6a\u7d93", "author": "\u968b \u95cd\u90a3\u5d1b\u591a\u8b6f", "sourceLink": "http://www.cbeta.org/"}